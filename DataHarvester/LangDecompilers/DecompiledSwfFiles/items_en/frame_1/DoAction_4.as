I.u[1779] = {n:"Lard Bass",nn:"LARD BASS",t:41,d:"You can find this type of fish in glacial lakes and rivers. Fishermen really love it because of its size and its fresh flesh.",ep:2,g:290,l:40,wd:true,fm:true,w:3,p:1};
