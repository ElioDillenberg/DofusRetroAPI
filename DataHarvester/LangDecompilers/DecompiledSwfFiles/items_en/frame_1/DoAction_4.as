I.u[1815] = {n:"Skate Wing **",nn:"SKATE WING **",t:49,d:"When cooked too quickly in black Bwork bwutter, this skate wing releases an eye-watering flavour into the chewing mouth of the muncher. This meal is not a memorable one.",ep:2,g:85,l:1,wd:true,fm:true,w:3,u:true,ut:true,p:180};
I.u[1816] = {n:"Skate Wing ***",nn:"SKATE WING ***",t:49,d:"When cooked in black Bwork bwutter, this skate wing releases a rather bland flavour into the chewing mouth of the muncher. This meal wasn\'t too bad though, all things considered.",ep:2,g:86,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:340};
I.u[1817] = {n:"Steamed Carp ****",nn:"STEAMED CARP ****",t:49,d:"Steam-cooked to retain the flavour, this small carp\'s flavour will sweep you off your feet!",ep:1,g:87,l:1,wd:true,fm:true,w:1,c:"Cw<60",u:true,ut:true,p:60000};
I.u[1818] = {n:"Steamed Carp **",nn:"STEAMED CARP **",t:49,d:"Steam-cooked to retain the flavour, this small carp will make you forget all your troubles. Tomorrow\'s gonna be another day... just enjoy this steamed-carp-flavoured moment!",ep:2,g:88,l:1,wd:true,fm:true,w:3,u:true,ut:true,p:130};
I.u[1819] = {n:"Steamed Carp ****",nn:"STEAMED CARP ****",t:49,d:"Steam-cooked to retain the flavour, this small carp will make you forget all your troubles. Tomorrow\'s gonna be another day... just enjoy the steamed-carp-flavoured moment!",ep:2,g:89,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:200};
I.u[1820] = {n:"Grilled Kralove **",nn:"GRILLED KRALOVE **",t:49,d:"This grilled kralove gives off a strong smell, but it\'s a true delicacy if you manage to overcome your disgust.  \nA small piece of advice... find something long and sharp to eat it with.",ep:2,g:90,l:1,wd:true,fm:true,w:3,u:true,ut:true,p:130};
I.u[1821] = {n:"Grilled Kralove ***",nn:"GRILLED KRALOVE ***",t:49,d:"This grilled kralove doesn\'t smell too bad, and it\'s a true delicacy if you manage to overcome your indifference.",ep:2,g:91,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:200};
I.u[1822] = {n:"Seared Perch",nn:"SEARED PERCH",t:49,d:"This fish swam a long way, but failed to make it to the spawning grounds. Still, it\'s ready to be eaten, even if it is a little bland.",ep:3,g:93,l:1,wd:true,fm:true,w:3,u:true,ut:true,p:300};
I.u[1823] = {n:"Seared Perch ***",nn:"SEARED PERCH ***",t:49,d:"This fish swam a long way, and made it to the spawning grounds, where it spawned a few young. It\'s now ready to be eaten, and tastes good.",ep:3,g:94,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:400};
I.u[1824] = {n:"Seared Perch ****",nn:"SEARED PERCH ****",t:49,d:"This fish swam a long way, and spawned many young in the rivers of Amakna. It\'s now ready to be eaten, and tastes delicious.",ep:1,g:92,l:1,wd:true,fm:true,w:1,c:"Cw<100",u:true,ut:true,p:100000};
I.u[1825] = {n:"Fried Sardine **",nn:"FRIED SARDINE **",t:49,d:"You won\'t feed the family, or even yourself, with this fried sardine, but it does make an excellent starter.",ep:2,g:96,l:1,wd:true,fm:true,w:3,u:true,ut:true,p:80};
I.u[1826] = {n:"Fried Sardine ***",nn:"FRIED SARDINE ***",t:49,d:"You won\'t feed the family, or even yourself, with this fried sardine, but it does make an excellent main course.",ep:2,g:97,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:140};
I.u[1827] = {n:"Fried Sardine ****",nn:"FRIED SARDINE ****",t:49,d:"You won\'t feed the family, or even yourself, with this fried sardine, but it does make an excellent dessert.",ep:1,g:95,l:1,wd:true,fm:true,w:1,c:"Ci<40",u:true,ut:true,p:40000};
I.u[1828] = {n:"Grilled Bass ****",nn:"GRILLED BASS ****",t:49,d:"Served on top of lettuce, this fish will fill your belly for a long time. It\'s absolutely delicious!",ep:2,g:99,l:1,wd:true,fm:true,w:1,c:"Cw<70",u:true,ut:true,p:50000};
I.u[1829] = {n:"Grawn Fritters ****",nn:"GRAWN FRITTERS ****",t:49,d:"Everybody likes Grawn fritters. With a low calorie content, it would stupid to refuse them.",ep:1,g:100,l:1,wd:true,fm:true,w:1,c:"Ci<10",u:true,ut:true,p:10000};
I.u[1830] = {n:"Sliced Gudgeon ****",nn:"SLICED GUDGEON ****",t:49,d:"Slices of gudgeon, hard boiled with onions. Quite delightful... And the usual horrible odour is masked by a mysterious scent of fresh flowers too.",ep:1,g:101,l:1,wd:true,fm:true,w:1,c:"Cw<10",u:true,ut:true,p:10000};
I.u[1831] = {n:"Skate Wing ****",nn:"SKATE WING ****",t:49,d:"When cooked with care in black Bwork bwutter, this skate wing releases an incredible flavour into the chewing mouth of the muncher. This meal will truly give you wings.",ep:1,g:102,l:1,wd:true,fm:true,w:1,c:"Ci<70",u:true,ut:true,p:70000};
