I.u[1426] = {n:"Layte\'s Slicer",nn:"LAYTE\'S SLICER",t:8,d:"Layte, the creator of this shovel, was a devoted disciple of Iop. You can guess what this guy\'s major flaw was.",ep:3,g:8,l:24,wd:true,fm:true,w:24,an:16,e:[5,5,1,1,50,50,false,true],p:1800};
I.u[1427] = {n:"Zebuth Shovel",nn:"ZEBUTH SHOVEL",t:8,d:"This shovel is used by the Kanniball High Priests of Moon Island to invoke Zebuth, one of their gods who is often depicted as a cow. Only vile characters use it.",ep:1,g:40,l:55,wd:true,fm:true,w:30,an:16,e:[5,6,1,1,50,20,false,true],c:"Ps=2&CC>50",p:8000};
I.u[1428] = {n:"Everlasting Flower",nn:"EVERLASTING FLOWER",t:36,d:"This strange little plant grows in arid environments and gives off a smell of curry. It seems it can resist ageing. If only all this could be said for woman...",ep:2,g:54,l:1,wd:true,fm:true,w:1,p:5000};
I.u[1429] = {n:"Small Mechba Shovel",nn:"SMALL MECHBA SHOVEL",t:8,d:"This shovel, created by Mechba, is used by Enutrof followers to knock nuts from trees. On the battlefield, it has a similar function. Yowza!",ep:1,g:19,l:30,wd:true,fm:true,w:15,an:16,e:[5,6,1,1,40,40,false,true],c:"CC>10&CI>10",p:1500};
