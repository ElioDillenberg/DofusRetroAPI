I.u[1782] = {n:"Gudgeon",nn:"GUDGEON",t:41,d:"A small and fast-moving freshwater fish, the gudgeon is often caught by children and novice fishermen. Don\'t expect to feed your family with it.",ep:1,g:294,l:1,wd:true,fm:true,w:3,p:1};
I.u[1783] = {n:"Gutted Gudgeon",nn:"GUTTED GUDGEON",t:62,d:"This Gudgeon can now be used as an ingredient.",ep:1,g:295,l:1,wd:true,fm:true,w:2,p:1};
I.u[1784] = {n:"Blue Skate",nn:"BLUE SKATE",t:41,d:"The Blue Skate rules the sea bed in which, for some mysterious reason, it digs tiny holes all day long. Every once in a while, it returns to the surface. Sometimes long enough for you to catch it.",ep:2,g:296,l:50,wd:true,fm:true,w:3,p:1};
I.u[1785] = {n:"Gutted Blue Skate",nn:"GUTTED BLUE SKATE",t:62,d:"This Blue Skate can now be used as an ingredient.",ep:2,g:297,l:50,wd:true,fm:true,w:2,p:1};
I.u[1786] = {n:"Horror Grawn",nn:"HORROR GRAWN",t:41,d:"Transformation of the Grawn. A distinguishing characteristic of this rare species is that it makes nauseating grunting noises when it moves around.",ep:1,g:298,l:1,wd:true,fm:true,w:3,p:100};
I.u[1787] = {n:"Gutted Horror Grawn",nn:"GUTTED HORROR GRAWN",t:62,d:"This Horror Grawn can now be used as an ingredient.",ep:1,g:299,l:1,wd:true,fm:true,w:2,p:100};
I.u[1788] = {n:"Farle\'s Ray",nn:"FARLE\'S RAY",t:41,d:"Farle Ingalsse personally discovered this particular species of ray during one of his great adventures. Apparently it has the ability to enchant fish with its deep, southern voice and rhythmic train-like beating of its tail.",ep:1,g:300,l:50,wd:true,fm:true,w:3,p:20};
I.u[1789] = {n:"Gutted Farle\'s Ray",nn:"GUTTED FARLE\'S RAY",t:62,d:"This Farle\'s Ray can now be used as an ingredient.",ep:1,g:301,l:50,wd:true,fm:true,w:2,p:20};
I.u[1790] = {n:"Kiye Gudgeon",nn:"KIYE GUDGEON",t:41,d:"This rare species of gudgeon comes from Kiye Island, off Moon Island. Accustomed to warm currents, it migrates once a year to a hotter clime.",ep:1,g:302,l:1,wd:true,fm:true,w:3,p:1};
I.u[1791] = {n:"Gutted Kiye Gudgeon",nn:"GUTTED KIYE GUDGEON",t:62,d:"This Kiye Gudgeon can now be used as an ingredient.",ep:1,g:321,l:1,wd:true,fm:true,w:2,p:1};
I.u[1792] = {n:"Siktrin Bass",nn:"SIKTRIN BASS",t:41,d:"A cousin of the Lard Bass, the Siktrin Bass is officially an endangered species. The population was hunted enthusiastically ever since some quack idiot doctor claimed that it improved the singing voice.",ep:1,g:303,l:70,wd:true,fm:true,w:3,p:30};
I.u[1793] = {n:"Gutted Siktrin Bass",nn:"GUTTED SIKTRIN BASS",t:62,d:"The Siktrin Bass can now be used as an ingredient.",ep:1,g:304,l:40,wd:true,fm:true,w:2,p:30};
I.u[1794] = {n:"Ediem Carp",nn:"EDIEM CARP",t:41,d:"This creature is considered the wisest of all the freshwater fish. It\'s also surprisingly easy to catch... it\'s supreme knowledge of all things plunges many of its kind into an mind-numbing depression.",ep:2,g:305,l:40,wd:true,fm:true,w:3,p:1};
I.u[1795] = {n:"Gutted Ediem Carp",nn:"GUTTED EDIEM CARP",t:62,d:"This Ediem Carp can now be used as an ingredient.",ep:2,g:306,l:10,wd:true,fm:true,w:2,p:1};
I.u[1796] = {n:"Small Sandy Carp",nn:"SMALL SANDY CARP",t:41,d:"This small carp is sand-coloured, a distinctive feature which in theory should make it invisible to predators. What a pity there\'s not a grain of sand in the lakes of Amakna...",ep:1,g:307,l:40,wd:true,fm:true,w:3,p:1};
I.u[1797] = {n:"Gutted Small Sandy Carp",nn:"GUTTED SMALL SANDY CARP",t:62,d:"This Small Sandy Carp can now be used as an ingredient.",ep:1,g:308,l:10,wd:true,fm:true,w:2,p:1};
I.u[1798] = {n:"Gutted Kralove",nn:"GUTTED KRALOVE",t:62,d:"This Kralove can now be used as an ingredient.",ep:2,g:310,l:20,wd:true,fm:true,w:1,p:1};
I.u[1799] = {n:"Unique Kralove",nn:"UNIQUE KRALOVE",t:41,d:"This Kralove is monogamous. This is surprising when you know its habits.",ep:1,g:311,l:20,wd:true,fm:true,w:3,p:30};
I.u[1800] = {n:"Gutted Unique Kralove",nn:"GUTTED UNIQUE KRALOVE",t:62,d:"This Unique Kralove can now be used as an ingredient.",ep:1,g:312,l:20,wd:true,fm:true,w:2,p:30};
I.u[1801] = {n:"Perch",nn:"PERCH",t:41,d:"This fish doesn\'t look like much but it is the slyest and quickest of its kind. It\'s worth a try though. Its matchless taste and unique flavour will be ample reward for your efforts.",ep:3,g:313,l:70,wd:true,fm:true,w:3,p:1};
I.u[1802] = {n:"Gutted Perch",nn:"GUTTED PERCH",t:62,d:"This Perch can now be used as an ingredient.",ep:3,g:314,l:70,wd:true,fm:true,w:2,p:1};
I.u[1803] = {n:"Kittenperch",nn:"KITTENPERCH",t:41,d:"With the hideousness of its father and the savagery of its mother, this fish is the most difficult of all to catch. It\'s the rarest of its kind, too... Indeed, the mating of a Kittenfish with a Perch was once described by a famous statesman as " + "\"" + "Grotesque, unbelievable, bizarre and unprecedented." + "\"" + " The phrase GUBU has since been coined to describe such scandalous events.",ep:1,g:315,l:70,wd:true,fm:true,w:3,p:50};
I.u[1804] = {n:"Gutted Kittenperch",nn:"GUTTED KITTENPERCH",t:62,d:"That Kittenperch can now be used as an ingredient.",ep:1,g:316,l:70,wd:true,fm:true,w:2,p:50};
I.u[1805] = {n:"Shiny Sardine",nn:"SHINY SARDINE",t:41,d:"Some fish just don\'t know when to put it away... Cover up, get off the stage... Catching this bugger couldn\'t be easier.",ep:2,g:317,l:20,wd:true,fm:true,w:3,p:1};
I.u[1806] = {n:"Gutted Shiny Sardine",nn:"GUTTED SHINY SARDINE",t:62,d:"This Shiny Sardine can now be used as an ingredient.",ep:2,g:318,l:50,wd:true,fm:true,w:2,p:1};
I.u[1807] = {n:"Dark Sardine",nn:"DARK SARDINE",t:41,d:"Unlike the Shiny Sardine, the Dark Sardine is extremely difficult to catch.",ep:1,g:319,l:20,wd:true,fm:true,w:3,p:1};
I.u[1808] = {n:"Gutted Dark Sardine",nn:"GUTTED DARK SARDINE",t:62,d:"This Dark Sardine can now be used as an ingredient.",ep:1,g:320,l:50,wd:true,fm:true,w:2,p:1};
I.u[1809] = {n:"Grilled Bass **",nn:"GRILLED BASS **",t:49,d:"Served on top of lettuce, this fish will fill your belly for a little while. It\'s not the tastiest, though.",ep:2,g:98,l:1,wd:true,fm:true,w:3,u:true,ut:true,p:180};
I.u[1810] = {n:"Grilled Bass ***",nn:"GRILLED BASS ***",t:49,d:"Served on top of lettuce, this fish will fill your belly for a while. It\'s quite tasty, too.",ep:2,g:80,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:320};
I.u[1811] = {n:"Grawn Fritters **",nn:"GRAWN FRITTERS **",t:49,d:"Everybody likes crunchy Grawn Fritters. The problem is that they quickly become addictive.",ep:1,g:81,l:1,wd:true,fm:true,w:3,u:true,ut:true,p:20};
