I.u[1759] = {n:"Exotic Crab Stick",nn:"EXOTIC CRAB STICK",t:41,d:"Although a distant relative of the Crab Stick, this crab has a few brain cells to throw around... Which explains its rarity.",ep:1,g:275,l:1,wd:true,fm:true,w:3,p:20};
