I.u[716] = {n:"Staff Carver Scroll",nn:"STAFF CARVER SCROLL",t:13,d:"You gain Experience as a Staff Carver by using this scroll. (Double click to use it).",ep:1,g:31,l:1,wd:true,fm:true,w:1,u:true,p:1000};
