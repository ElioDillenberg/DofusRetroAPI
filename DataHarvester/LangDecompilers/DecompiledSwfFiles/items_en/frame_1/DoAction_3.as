I.u[1341] = {n:"Shower Potion",nn:"SHOWER POTION",t:26,d:"Thanks to the water magic it contains, this lapping potion is usually used to enchant all sorts of items.",ep:9,g:23,l:25,wd:true,fm:true,w:6,c:"Pj=48|Pj=49|Pj=50|Pj=46|Pj=44|Pj=45|Pj=43|Pj=47",p:500};
