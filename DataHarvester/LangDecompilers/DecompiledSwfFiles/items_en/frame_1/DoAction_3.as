I.u[1344] = {n:"Vervain",nn:"VERVAIN",t:36,d:"This special plant helps people go to sleep. It must be the emblem of Sadida.",ep:2,g:53,l:1,wd:true,fm:true,w:1,p:5000};
I.u[1345] = {n:"Fire Potion",nn:"FIRE POTION",t:26,d:"Thanks to the powerful fire magic it contains, this fulminating potion is usually used to powerfully enchant all sorts of items.",ep:4,g:21,l:50,wd:true,fm:true,w:6,c:"Pj=48|Pj=49|Pj=50|Pj=46|Pj=44|Pj=45|Pj=43|Pj=47",p:800};
I.u[1346] = {n:"Tsunami Potion",nn:"TSUNAMI POTION",t:26,d:"Thanks to the powerful water magic it contains, this flooding potion is usually used to powerfully enchant all sorts of items.",ep:4,g:24,l:50,wd:true,fm:true,w:6,c:"Pj=48|Pj=49|Pj=50|Pj=46|Pj=44|Pj=45|Pj=43|Pj=47",p:800};
