I.u[1325] = {n:"Walnut Shell",nn:"WALNUT SHELL",t:46,d:"Naturally resistant to all kind of aggressions, this shell symbolizes the Feca ideology.",ep:2,g:52,l:1,wd:true,fm:true,w:1,p:5000};
I.u[1326] = {n:"Bloody Flint",nn:"BLOODY FLINT",t:51,d:"This resistant and sharpened stone has apparently been used against something or someone. It is a raw and dangerous item which perfectly illustrates the characteristics of a Iop.",ep:2,g:47,l:1,wd:true,fm:true,w:1,p:5000};
I.u[1327] = {n:"Carved Bone",nn:"CARVED BONE",t:47,d:"It\'s a fearsome and gloomy item. Once worked it\'s as dangerous as a Sram.",ep:2,g:48,l:1,wd:true,fm:true,w:1,p:5000};
I.u[1328] = {n:"Enigmatic Kama",nn:"ENIGMATIC KAMA",t:15,d:"A Tofu head with a strange Tofu hat on decorates this Kama.",ep:1,g:182,l:1,wd:true,fm:true,w:10,p:1};
I.u[1329] = {n:"Flexible Wood",nn:"FLEXIBLE WOOD",t:38,d:"This big wooden branch, with its flexible and curvilinear shape makes you immediately think of Cras.",ep:2,g:49,l:1,wd:true,fm:true,w:1,p:5000};
I.u[1330] = {n:"Amulet Itbe",nn:"AMULET ITBE",t:1,d:"It is an original creation of the silversmith John-Lay Nonne, and now one of the most sought after jewels on the continent.",ep:1,g:31,l:10,wd:true,fm:true,w:5,p:500};
I.u[1331] = {n:"Oly Medal",nn:"OLY MEDAL",t:1,d:"Oly was a female Iop with only one ability - cooking. It is said that Iop himself gave her this medal in order to compensate for her weaknesses in battle. Only pure-hearted people can wear it.",ep:7,g:23,l:51,wd:true,fm:true,w:10,c:"Ps=1&Pa>10",p:7000};
I.u[1332] = {n:"Old Four-Leaf Clover",nn:"OLD FOUR-LEAF CLOVER",t:36,d:"Four-leaf clovers are rarer than five-leaf ones. So when you find one, you should rub it dry between two spell scrolls.",ep:2,g:50,l:1,wd:true,fm:true,w:1,p:5000};
I.u[1333] = {n:"Spark Potion",nn:"SPARK POTION",t:26,d:"Thanks to the feeble fire magic it contains, this warming potion is usually used to weakly enchant all sorts of items.",ep:3,g:19,l:1,wd:true,fm:true,w:6,c:"Pj=48|Pj=49|Pj=50|Pj=46|Pj=44|Pj=45|Pj=43|Pj=47",p:250};
I.u[1334] = {n:"Sulphur Pouch",nn:"SULPHUR POUCH",t:48,d:"This little pouch of sulphur gives off an unbearable smell...The same smell as an Osamodas.",ep:2,g:51,l:1,wd:true,fm:true,w:1,p:5000};
I.u[1335] = {n:"Drizzle Potion",nn:"DRIZZLE POTION",t:26,d:"Thanks to the feeble water magic it contains, this simmering potion is usually used to weakly enchant all sorts of items.",ep:3,g:22,l:1,wd:true,fm:true,w:6,c:"Pj=48|Pj=49|Pj=50|Pj=46|Pj=44|Pj=45|Pj=43|Pj=47",p:250};
I.u[1336] = {n:"Nettles",nn:"NETTLES",t:36,d:"Rapidly rubbing this plant on your skin will make you suffer torture but the stimulation produces beneficial effects... well, that\'s what Sacriers say at least.",ep:2,g:53,l:1,wd:true,fm:true,w:1,p:5000};
I.u[1337] = {n:"Draught Potion",nn:"DRAUGHT POTION",t:26,d:"Thanks to the feeble air magic it contains, this sighing potion is usually used to weakly enchant all sorts of items.",ep:3,g:25,l:1,wd:true,fm:true,w:6,c:"Pj=48|Pj=49|Pj=50|Pj=46|Pj=44|Pj=45|Pj=43|Pj=47",p:250};
