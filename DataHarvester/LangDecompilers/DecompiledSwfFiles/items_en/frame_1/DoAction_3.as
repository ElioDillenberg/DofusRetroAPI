I.u[959] = {n:"Treasure Map 5",nn:"TREASURE MAP 5",t:24,d:"-- to do --",ep:1,g:10,l:1,wd:true,fm:true,w:0,p:1};
I.u[960] = {n:"Wabbit Potion",nn:"WABBIT POTION",t:24,d:"This potion is a cure for myxomawosis. A sip of it and you will be able to visit Wabbit Island safely.",ep:5,g:2,l:1,wd:true,fm:true,w:0,p:1};
I.u[961] = {n:"Wabbit Key",nn:"WABBIT KEY",t:24,d:"One of the three keys which will enable you to enter the Wabbit Castle.",ep:8,g:3,l:1,wd:true,fm:true,w:0,p:1};
I.u[962] = {n:"Wabbit Key",nn:"WABBIT KEY",t:24,d:"One of the three keys which will enable you to enter the Wabbit Castle.",ep:8,g:4,l:1,wd:true,fm:true,w:0,p:1};
I.u[963] = {n:"Wabbit Key",nn:"WABBIT KEY",t:24,d:"One of the three keys which will enable you to enter the Wabbit Castle.",ep:8,g:5,l:1,wd:true,fm:true,w:0,p:1};
I.u[965] = {n:"Dopple Stone",nn:"DOPPLE STONE",t:50,d:"This stone comes from the Dopple World. It\'s highly-prized by some inhabitants of Amakna.",ep:2,g:663,l:41,wd:true,fm:true,w:15,p:10000};
I.u[966] = {n:"Tailor Manual",nn:"TAILOR MANUAL",t:25,d:"(Double click to learn how to become a Tailor) \nThis tailor manual will teach you the tricks of the trade. For those who lack imagination, the first pages have a few simple cut out patterns.",ep:1,g:13,l:1,wd:true,fm:true,w:1,u:true,p:100};
I.u[967] = {n:"Dazzling Cloak",nn:"DAZZLING CLOAK",t:17,d:"This magnificent cloak increases AP, as well as vitality and wisdom.",ep:13,g:78,l:55,wd:true,fm:true,w:20,p:100000};
I.u[968] = {n:"Dazzling Belt",nn:"DAZZLING BELT",t:10,d:"This magnificent belt increases action points, vitality and wisdom.",ep:1,g:10,l:30,wd:true,fm:true,w:200,p:100000};
I.u[969] = {n:"Wa Wabbit\'s Crown",nn:"WA WABBIT\'S CROWN",t:16,d:"This magnificent Crown once belonged to the notorious Wa Wabbit.",ep:8,g:21,l:30,wd:true,fm:true,w:30,s:40,p:3000};
I.u[970] = {n:"Wa Wabbit\'s Staff",nn:"WA WABBIT\'S STAFF",t:4,d:"This staff once belonged to the notorious Wa Wabbit.",ep:8,g:21,l:25,wd:true,fm:true,w:20,an:12,tw:true,e:[5,5,1,1,30,20,false,false],s:40,p:2500};
I.u[971] = {n:"Wa Wabbit\'s Cloak",nn:"WA WABBIT\'S CLOAK",t:17,d:"This cloak once belonged to the notorious Wa Wabbit. It is still impregnated with his perspiration which will bring back a few memories of the amazing fight every time you put it on.",ep:8,g:21,l:30,wd:true,fm:true,w:25,s:40,p:2525};
I.u[972] = {n:"Cawwot Dofus",nn:"CAWWOT DOFUS",t:23,d:"The Cawwot Dofus increases its bearer\'s wisdom.",ep:1,g:6,l:6,wd:true,fm:true,w:5,p:100000};
I.u[973] = {n:"Arachnid Leg",nn:"ARACHNID LEG",t:24,d:"A very thin but very hairy Leg. The best Alchemists manage to create powerful poisons with the juice extracted from these legs.",ep:2,g:11,l:1,wd:true,fm:true,w:0,p:1};
I.u[974] = {n:"Distressed Farmer\'s Head",nn:"DISTRESSED FARMER\'S HEAD",t:24,d:"He had it coming: the distressed farmer lost his head...",ep:1,g:12,l:1,wd:true,fm:true,w:0,p:1};
I.u[975] = {n:"Captive Vampire Fang",nn:"CAPTIVE VAMPIRE FANG",t:24,d:"This very pointy fang belonged to a vampire. If you wear it round your neck, or even if you simply carry it in your inventory... you\'ll be able to show it to your buddies. Sorry, were you expecting more?",ep:1,g:13,l:1,wd:true,fm:true,w:0,p:1};
I.u[976] = {n:"Pumpkwin",nn:"PUMPKWIN",t:16,d:"A distinctive feature of the Pumpkwin is that it prevents the dispersion of magic particles, making it a good magic insulator. That\'s why some warriors use this vegetable as headgear.",ep:4,g:24,l:20,wd:true,fm:true,w:10,p:500};
I.u[989] = {n:"Lemon Shigekax",nn:"LEMON SHIGEKAX",t:42,d:"Rediscover the flavour of bygone days with this bitter lemon drop. \nThis candy gives 100 energy points.",ep:1,g:23,l:1,wd:true,fm:true,w:1,u:true,p:200};
I.u[990] = {n:"Strawberry Shigekax",nn:"STRAWBERRY SHIGEKAX",t:42,d:"Rediscover the flavour of bygone days with this strawberry drop.",ep:13,g:25,l:1,wd:true,fm:true,w:1,u:true,p:140};
I.u[991] = {n:"Mint Shigekax",nn:"MINT SHIGEKAX",t:42,d:"Rediscover the flavour of bygone days with this minty drop. \nThis candy gives 200 energy points.",ep:2,g:24,l:1,wd:true,fm:true,w:1,u:true,p:400};
I.u[992] = {n:"Cherry Shigekax",nn:"CHERRY SHIGEKAX",t:42,d:"Rediscover the flavour of bygone days with this cherry drop. \nThis candy gives 50 energy points.",ep:2,g:19,l:1,wd:true,fm:true,w:1,u:true,p:100};
I.u[993] = {n:"Greenjely",nn:"GREENJELY",t:42,d:"Though made of mint jelly, this candy has repulsed more than a few vegetarians, but it\'s still a speciality of Amakna.",ep:5,g:21,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:1};
I.u[994] = {n:"Redjely",nn:"REDJELY",t:42,d:"Made out of strawberry jelly, even if it repelled quite a few vegetarian, it\'s still a specialty of Amakna.",ep:5,g:20,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:1};
I.u[995] = {n:"Blujely",nn:"BLUJELY",t:42,d:"Blue-jelly based, it\'s one of the specialties of Amakna even though it repelled many vegetarians.",ep:5,g:22,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:1};
I.u[996] = {n:"Multijely",nn:"MULTIJELY",t:42,d:"Multijely was banned after children started disappearing soon after eating it. Not long after, they realised the kids were just going to the shop to get more.",ep:5,g:26,l:1,wd:false,fm:true,w:1,c:"Sc!1056",u:true,p:100};
I.u[997] = {n:"Kokonut",nn:"KOKONUT",t:46,d:"Round as a cannonball, the Kokonut also has 3 holes, which makes it very easy to throw away.",ep:12,g:159,l:42,wd:true,fm:true,w:2,p:1};
I.u[998] = {n:"Captain Pirate\'s Cute Shorts",nn:"CAPTAIN PIRATE\'S CUTE SHORTS",t:15,d:"Captain Pirate\'s shorts are the proof that ridicule has actually killed someone. But apparently, they are also very lucky.",ep:12,g:163,l:82,wd:true,fm:true,w:2,p:1};
I.u[999] = {n:"Not Tempting Canvas",nn:"NOT TEMPTING CANVAS",t:55,d:"This large piece of canvas is slimy and smelly but yet of quite good quality. Too bad it has so many holes!",ep:12,g:164,l:1,wd:true,fm:true,w:5,p:1};
I.u[1000] = {n:"Empty Barrel",nn:"EMPTY BARREL",t:15,d:"This big barrel was emptied a long time ago, nevertheless we can still guess what it used to contain: rum, Kamas, drunken pirates... anything you can find in a barrel besides water.",ep:12,g:165,l:1,wd:true,fm:true,w:5,p:1};
I.u[1001] = {n:"Kokoko Wood Plank",nn:"KOKOKO WOOD PLANK",t:95,d:"What will you do with this plank? Build a boat? Lay a floor in your house? Do you really care? Whatever, it\'s just a simple plank for the moment.",ep:12,g:166,l:42,wd:true,fm:true,w:10,p:10};
I.u[1002] = {n:"Kokonut Palm Trunk",nn:"KOKONUT PALM TRUNK",t:38,d:"Kokonut palm trunks are similar to other trunks: straight and strong, yet they are slightly thinner than the average.",ep:12,g:167,l:42,wd:true,fm:true,w:1,p:1};
I.u[1003] = {n:"Kokoko Wood Beam",nn:"KOKOKO WOOD BEAM",t:38,d:"Solid wood beam. Don\'t put it into someone\'s eye.",ep:12,g:168,l:42,wd:true,fm:true,w:1,p:1};
I.u[1004] = {n:"Essential Rope",nn:"ESSENTIAL ROPE",t:15,d:"No self-respecting adventurer would go without such a Rope in his inventory. You should be advised to do the same, although it\'s a piece of advice seldom taken!",ep:12,g:170,l:1,wd:true,fm:true,w:1,p:1};
I.u[1005] = {n:"Raft Receipt",nn:"RAFT RECEIPT",t:24,d:"A receipt signed by Kib Rock telling you your raft will be ready soon.",ep:1,g:176,l:1,wd:true,fm:true,w:0,p:1};
I.u[1006] = {n:"Pirate Hat",nn:"PIRATE HAT",t:24,d:"Large brim, the insignia of death embroidered on, fur to make it nice...This pirate hat is a classic of its kind among captains.",ep:12,g:14,l:1,wd:true,fm:true,w:0,p:1};
I.u[1007] = {n:"Hook",nn:"HOOK",t:24,d:"Essential accessory for unlucky pirates, this hook can be used as a hand. Its colour gives it a classy style...",ep:12,g:15,l:1,wd:true,fm:true,w:0,p:1};
I.u[1008] = {n:"Falling Jaw",nn:"FALLING JAW",t:24,d:"People say that this Jaw necklace was created when a captain caught one of his sailors smiling at him.",ep:12,g:16,l:1,wd:true,fm:true,w:0,p:1};
I.u[1009] = {n:"Pirate Skull",nn:"PIRATE SKULL",t:24,d:"This skull wearing a headband proves that a even a dead pirate can hide his baldness.",ep:12,g:17,l:1,wd:true,fm:true,w:0,p:1};
I.u[1010] = {n:"Green Shell",nn:"GREEN SHELL",t:24,d:"The shell of a mad turtle that thinks it\'s a ninja.",ep:12,g:18,l:1,wd:true,fm:true,w:0,p:1};
I.u[1011] = {n:"Yellow Shell",nn:"YELLOW SHELL",t:24,d:"The Shell of a mad turtle who had probably ingested too many dangerous substances.",ep:12,g:19,l:1,wd:true,fm:true,w:0,p:1};
I.u[1012] = {n:"Red Shell",nn:"RED SHELL",t:24,d:"The shell of a mad turtle that liked tomatoes and cheese on a warm slice of bread.",ep:12,g:20,l:1,wd:true,fm:true,w:0,p:1};
I.u[1013] = {n:"Blue Shell",nn:"BLUE SHELL",t:24,d:"The shell of a mad turtle who wanted to imitate Zowwo, the Masked Fox.",ep:12,g:21,l:1,wd:true,fm:true,w:0,p:1};
I.u[1014] = {n:"Green Mask",nn:"GREEN MASK",t:24,d:"This Mask of Cheerful Kanniball has been made in the image of Greenery, element which is linked to meat seasoning.",ep:12,g:22,l:1,wd:true,fm:true,w:0,p:1};
I.u[1015] = {n:"Yellow Mask",nn:"YELLOW MASK",t:24,d:"This Whining Kanniball mask has been made in the image of Lightning. What does that mean? Hey, I just work here, buddy.",ep:12,g:23,l:1,wd:true,fm:true,w:0,p:1};
I.u[1016] = {n:"Red Mask",nn:"RED MASK",t:24,d:"This Mask of Irascible Kanniball has been made in the image of Fire, element of roasting meat.",ep:12,g:24,l:1,wd:true,fm:true,w:0,p:1};
I.u[1017] = {n:"Blue Mask",nn:"BLUE MASK",t:24,d:"Mask of a Surprised Kanniball that has been made in the image of Water, element which slakes thirst between two mouthfuls of meat.",ep:12,g:25,l:1,wd:true,fm:true,w:0,p:1};
I.u[1018] = {n:"Glu",nn:"GLU",t:15,d:"This pot of pink jelly might smell good... But you must never taste it or you won\'t ever speak again. It\'s an excellent fixative.",ep:12,g:171,l:1,wd:true,fm:true,w:1,p:1};
I.u[1019] = {n:"Helmet",nn:"HELMET",t:16,d:"With that Helmet on, you\'ll have a very special look... halfway between ridiculous and  comical.",ep:1,g:25,l:20,wd:true,fm:true,w:10,p:200};
I.u[1020] = {n:"Broken Helmet",nn:"BROKEN HELMET",t:15,d:"This helmet took a bullet for you pal... He died for you, man...",ep:2,g:173,l:1,wd:true,fm:true,w:5,p:1};
I.u[1021] = {n:"Wooden Wings",nn:"WOODEN WINGS",t:17,d:"For those who have always dreamt of flying like a Kwak, go back to bed because these wings will turn you into a public menace and an idiot.",ep:1,g:22,l:20,wd:true,fm:true,w:20,p:1200};
I.u[1022] = {n:"Broken Wings",nn:"BROKEN WINGS",t:104,d:"Looking at these broken wings makes you realise just how badly you wanted to stay in bed this morning.",ep:2,g:174,l:1,wd:true,fm:true,w:10,p:1};
I.u[1023] = {n:"Bottle of Rum",nn:"BOTTLE OF RUM",t:15,d:"The label says \'10 year old rum\' but it is quite impossible to check if it tells the truth. Don\'t drink it excessively on the road for fear of seasickness.",ep:12,g:177,l:1,wd:true,fm:true,w:2,p:1};
I.u[1026] = {n:"Small Stek Knife",nn:"SMALL STEK KNIFE",t:5,d:"One day, the infamously crazy T. Bun Stek found two strange yet impressive knives... No one could figure out what they were for. He used the very same knives to sacrifice half his livestock to the Gods and soon after they bellowed the answer from the heave\'s. He was slaughtering a particularly noisy cow at that very moment... The poor guy didn\'t hear a word.",ep:1,g:13,l:32,wd:true,fm:true,w:10,an:13,e:[15,3,1,1,30,50,false,false],c:"CA>32&CS>10",p:3000};
