I.u[8341] = {n:"Pineapple Boots",nn:"PINEAPPLE BOOTS",t:11,d:"These boots smell of pineapple juice marinated with wet socks. The great thing is that when you visit your friends, they will never ask you to take your shoes off.",ep:1,g:90,l:1,wd:true,fm:true,w:10,s:74,p:100};
I.u[8342] = {n:"Dreggon Sanctuary Key",nn:"DREGGON SANCTUARY KEY",t:84,d:"This key, made of pressure-bonded Dreggon shells, will give you access to the terrible Dreggon Sanctuary, reputed for its awkwardly silent inhabitants.",ep:10,g:18,l:1,wd:true,fm:true,w:1,p:100};
I.u[8343] = {n:"Dreggon Dungeon Key",nn:"DREGGON DUNGEON KEY",t:84,d:"You thought you\'d seen it all in the Dreggon Sanctuary, but NO! Not by a long shot. In fact, I\'m betting this key leads you to your death.",ep:10,g:17,l:1,wd:true,fm:true,w:1,p:100};
I.u[8344] = {n:"Black Dragoss Horn",nn:"BLACK DRAGOSS HORN",t:47,d:"It\'s not as pointy as a Xelor clock-hand but it\'s a lot more solid. This horn enables the Dragoss to transform a Spanking session into a Cracking one.",ep:11,g:648,l:50,wd:true,fm:true,w:1,p:10};
