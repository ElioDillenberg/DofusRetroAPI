I.u[8557] = {n:"Batofu Feather",nn:"BATOFU FEATHER",t:53,d:"This feather was plucked from the Batofu\'s armpit and it smells like a marinated ancestral Doobitchoo.",ep:3,g:120,l:20,wd:true,fm:true,w:1,p:10};
I.u[8558] = {n:"Coney Hat",nn:"CONEY HAT",t:16,d:"If you\'re planning to communicate with Wabbits, this might be the hat you need. If you want to look good, put it on someone next to you.",ep:1,g:170,l:6,wd:true,fm:true,w:10,p:1200};
I.u[8559] = {n:"The HmmMmmmMmmm",nn:"THE HMMMMMMMMMM",t:16,d:"Mmmmm mm mmmmmm mmmmm, mmm mmm mm mmmmmm mmmmm mmmmmmm mmmmmm mmm mmmm\'m mmmmm mmmm mmmmmmm.",ep:2,g:171,l:42,wd:true,fm:true,w:1,p:420};
I.u[8561] = {n:"Pandawa Cub",nn:"PANDAWA CUB",t:18,d:"This cute Pandawa Cub will gladly travel round the world with you and discover its mysteries with its big, round eyes. This tamed pet eats rice, but can also be fed with hazelnuts.",ep:3,g:42,l:1,wd:true,fm:true,w:10,c:"PZ=1",p:1};
I.u[8563] = {n:"Litol Goblait",nn:"LITOL GOBLAIT",t:32,d:"Litol must meet his future magic teacher.",ep:1,g:18,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[8564] = {n:"Bow Kennel Certificate: Pandawa Cub",nn:"BOW KENNEL CERTIFICATE: PANDAWA CUB",t:77,d:"This certificate describes the characteristics of the Pandawa Cub you\'ve put in the Bow Kennels.",ep:3,g:44,l:1,wd:false,fm:true,w:1,p:100};
I.u[8565] = {n:"Pandawa Cub Ghost",nn:"PANDAWA CUB GHOST",t:90,d:"This Ghost hasn\'t left the living world yet. Will you find a way to resurrect it or cowardly abandon it once and for all?",ep:3,g:44,l:1,wd:true,fm:true,w:1,p:1};
I.u[8566] = {n:"Pingoku Transformation",nn:"PINGOKU TRANSFORMATION",t:27,d:"You\'ve been transformed into a ghost to be able to get in the Pet Sanctuary.",ep:20,g:3,l:1,wd:false,fm:true,w:1,m:true,p:1};
I.u[8567] = {n:"Pandawa Cub Ghost Transformation",nn:"PANDAWA CUB GHOST TRANSFORMATION",t:27,d:"You\'ve been transformed into a ghost to be able to get in the Pet Sanctuary.",ep:20,g:3,l:1,wd:false,fm:true,w:1,m:true,p:1};
I.u[8568] = {n:"Gift: Pandawa Cub",nn:"GIFT: PANDAWA CUB",t:89,d:"If you shake this object vigorously and hold your ear to it, you\'ll hear a strange noise... like a tiny animal weeping... yeah, actually maybe you don\'t want to shake it... Ahhh, just open the damn thing. You\'ll find a wonderful pet inside it, as well as some food and healing Eniripsa powders.",ep:1,g:42,l:1,wd:true,fm:true,w:1,u:true,p:1};
I.u[8569] = {n:"Doro L. Blak",nn:"DORO L. BLAK",t:16,d:"This cap used to belong to a little Xelorette who got a kick out of disrupting the passage of time. Legend has it that she once stole several days off the inhabitants of Amakna. She had to hand out quite a few candies to make up for that little trick.",ep:22,g:172,l:129,wd:true,fm:true,w:10,p:42512};
I.u[8570] = {n:"Bontarian Rat Hair",nn:"BONTARIAN RAT HAIR",t:54,d:"This hair used to belong to the Bonta sewer rats. Indeed, they were the only ones to have hair tufts which matched the colour of their city wall.",ep:2,g:672,l:40,wd:true,fm:true,w:1,p:10};
I.u[8571] = {n:"Brakmarian Rat Hair",nn:"BRAKMARIAN RAT HAIR",t:54,d:"This hair used to belong to the Brakmar sewer rats. Indeed, they were the only ones to have hair tufts which matched the colour of their city wall.",ep:2,g:673,l:40,wd:true,fm:true,w:1,p:10};
I.u[8572] = {n:"Forgetfulness Potion: \'Mount Taming\'",nn:"FORGETFULNESS POTION: \'MOUNT TAMING\'",t:43,d:"Using this potion will cause you to unlearn one level of the spell \'Mount Taming\'.",ep:5,g:36,l:1,wd:true,fm:true,w:1,u:true,p:1000};
I.u[8573] = {n:"Forgetfulness Potion: \'Perfidious Boomerang\'",nn:"FORGETFULNESS POTION: \'PERFIDIOUS BOOMERANG\'",t:43,d:"Using this potion will cause you to unlearn one level of the spell \'Perfidious Boomerang\'.",ep:5,g:36,l:1,wd:true,fm:true,w:1,u:true,p:1000};
I.u[8574] = {n:"Dofus Seasoning",nn:"DOFUS SEASONING",t:23,d:"Either with or without bacon, this Dofus is delicious.",ep:0,g:13,l:1,wd:false,fm:true,w:1,p:1};
I.u[8575] = {n:"First Blood Staff",nn:"FIRST BLOOD STAFF",t:4,d:"A famous Feca warrior who lived in the jungle of Pandala was often heard to say: " + "\"" + "To win the war, you have to become the war" + "\"" + ".",ep:1,g:71,l:200,wd:false,fm:true,w:20,an:12,tw:true,e:[7,3,1,2,42,42,false,true],c:"PN~xxramboplxx",p:100000};
I.u[8576] = {n:"Yse Vewybad\'s Recommendation Scroll",nn:"YSE VEWYBAD\'S RECOMMENDATION SCROLL",t:24,d:"Yse Vewybad gave you this scroll to thank you for your favours.",ep:2,g:26,l:20,wd:false,fm:true,w:0,p:1};
I.u[8588] = {n:"Rogue Emblem",nn:"ROGUE EMBLEM",t:24,d:"This emblem, given by a rogue, might be the key to enter their den.",ep:2,g:86,l:1,wd:true,fm:true,w:0,p:1};
I.u[8590] = {n:"Contract between Vil Smisse and Yse Vewybad",nn:"CONTRACT BETWEEN VIL SMISSE AND YSE VEWYBAD",t:24,d:"This contract proves Yse Vewybad\'s treachery. All you have to do now is to bring it to the right person.",ep:1,g:26,l:1,wd:true,fm:true,w:0,p:1};
I.u[8595] = {n:"Obese Tofu Egg",nn:"OBESE TOFU EGG",t:24,d:"This egg contains an obese Tofu... a creature that may defeat the Pwince of Wa in the future.",ep:1,g:122,l:1,wd:true,fm:false,w:0,p:1};
I.u[8596] = {n:"Crimson Claw",nn:"CRIMSON CLAW",t:6,d:"This sharp sword gained this elegant crimson colour after gutting thousands of Bworks. Before, it was just purplish.",ep:22,g:72,l:145,wd:true,fm:true,w:15,an:14,tw:true,e:[8,6,1,1,45,100,false,false],c:"CS>350&CV>400",p:52000};
I.u[8597] = {n:"Force-feeding Potion",nn:"FORCE-FEEDING POTION",t:24,d:"This potion will permit Vil Smisse to force-feed the obese Tofu that you brought him.",ep:3,g:2,l:1,wd:true,fm:true,w:0,p:1};
I.u[8598] = {n:"Emment Daggers",nn:"EMMENT DAGGERS",t:5,d:"These blades have holes all over them so that they make a nice little whistling noise when handled quickly. The holes are also extremely useful when it comes to gutting your enemies.",ep:22,g:61,l:67,wd:true,fm:true,w:10,an:13,tw:true,e:[10,3,1,1,30,50,false,true],c:"CA>100",p:10654};
I.u[8599] = {n:"Clawettes",nn:"CLAWETTES",t:5,d:"These daggers were forged for a manicure-addict Ecaflipette. She didn\'t like those heavy swords that didn\'t match her perfectly varnished claws.",ep:22,g:62,l:69,wd:true,fm:true,w:10,an:13,e:[15,3,1,1,30,30,false,true],c:"CA>100&CS>100",p:11254};
I.u[8600] = {n:"Too Lazy To Go Upstairs",nn:"TOO LAZY TO GO UPSTAIRS",t:30,d:"No wonder you\'re exhausted, you\'ve been shouting at the top of your voice all this time. Wait 6 minutes or so... Maybe then you won\'t be so lazy.",ep:1,g:1,l:1,wd:false,fm:true,w:0,m:true,p:1};
I.u[8603] = {n:"Wabbit Prince\'s Servant\'s Livery",nn:"WABBIT PRINCE\'S SERVANT\'S LIVERY",t:24,d:"With this livery, the Wabbit Prince\'s servant will be very trendy.",ep:2,g:715,l:1,wd:true,fm:true,w:0,p:1};
I.u[8604] = {n:"Stalk Sword",nn:"STALK SWORD",t:6,d:"This shiny sword is perfect for trimming flowers and trees, even if the plants fight back.",ep:22,g:73,l:100,wd:true,fm:true,w:10,an:14,e:[15,8,1,1,40,50,false,false],c:"CS>250&CI<1",p:25654};
I.u[8605] = {n:"Rubber Sword",nn:"RUBBER SWORD",t:6,d:"This blade has been lubricated to slip into its sheath more easily. However, it cannot be cleaned after use and must be thrown away, especially if it\'s covered in blood. Bear in mind that its main use is to protect you.",ep:22,g:74,l:73,wd:true,fm:true,w:15,an:14,e:[5,5,1,1,40,50,false,true],c:"CS>80&CI>80&CA>80",p:14000};
I.u[8606] = {n:"Wedding Dress",nn:"WEDDING DRESS",t:24,d:"The bride\'s wedding dress has been carefully wrapped in this gorgeous package. It just needs to be brought to the steward San Lavalss.",ep:2,g:715,l:1,wd:true,fm:true,w:0,p:1};
I.u[8607] = {n:"Studded Staff",nn:"STUDDED STAFF",t:4,d:"It\'s a big staff with nails knocked into it. Do not hope for something else in this staff, it\'s simple and loosens your enemies\' teeth nicely.",ep:22,g:72,l:107,wd:true,fm:true,w:15,an:12,tw:true,e:[7,4,1,1,30,50,false,true],c:"CA>150&CI>150",p:29856};
I.u[8608] = {n:"Scrap Staff",nn:"SCRAP STAFF",t:4,d:"Rumour has it that a Sadida named Fraye used this staff to protect his wife and son from an enraged flock of Gobballs. What a pity that he also mistook his family for the monsters during the fight.",ep:22,g:73,l:85,wd:true,fm:true,w:10,an:12,tw:true,e:[10,4,1,1,35,40,false,true],c:"CW>150&CV>200",p:20000};
I.u[8609] = {n:"Bushi\'Bow",nn:"BUSHI\'BOW",t:2,d:"This bow is light, delicate, elegant and smells of cherry tree flowers. But watch out, if you use it properly it can also be lethal.",ep:22,g:56,l:81,wd:true,fm:true,w:15,an:10,tw:true,e:[5,4,3,7,30,50,false,true],c:"CA>200",p:16900};
I.u[8610] = {n:"Bow Liwood",nn:"BOW LIWOOD",t:2,d:"If you hold this bow high and joyfully say " + "\"" + "Pampulilu!" + "\"" + ", you\'ll look absolutely ridiculous.",ep:22,g:57,l:79,wd:true,fm:true,w:10,an:10,tw:true,e:[7,5,4,7,30,50,false,true],c:"CI>150&CC>150",p:15320};
I.u[8611] = {n:"Wand Else?",nn:"WAND ELSE?",t:3,d:"This wand belongs to a famous Grinegau who makes excellent coffee, but who just can\'t get the ladies.",ep:22,g:61,l:72,wd:true,fm:true,w:15,an:11,e:[5,3,2,4,15,50,false,true],c:"CI>100&CC>100",p:16500};
I.u[8612] = {n:"The Bidjiz",nn:"THE BIDJIZ",t:3,d:"With the Bidjiz, you\'ll stay alive probably because your opponents will start dancing when they see the disco ball at the end of this wand.",ep:22,g:62,l:79,wd:true,fm:true,w:10,an:11,tw:true,e:[10,4,2,5,40,50,false,true],c:"CA>150",p:18000};
I.u[8613] = {n:"Shovel Minster",nn:"SHOVEL MINSTER",t:8,d:"Created like a Smithmagic masterpiece, this shovel digs really bad, except when it\'s your opponents\' heads.",ep:22,g:46,l:70,wd:true,fm:true,w:10,an:16,tw:true,e:[5,4,1,1,30,50,false,false],c:"CC>100",p:13540};
I.u[8614] = {n:"RIP Shovel",nn:"RIP SHOVEL",t:8,d:"This shovel is a great invention. It can dig up a grave and be left on top as a cross, provided only one adventure fellow dies at the time... Or else you\'ll have to dig the other graves with your hands.",ep:22,g:47,l:66,wd:true,fm:true,w:1,an:16,tw:true,e:[5,4,1,1,20,30,false,false],c:"CI>100&CC>50",p:1};
I.u[8615] = {n:"Red Hammer",nn:"RED HAMMER",t:7,d:"This Hammer easily crushes skulls to pulp... which is exactly why it\'s tinted with a nice red tomato colour.",ep:22,g:61,l:83,wd:true,fm:true,w:10,an:15,e:[10,4,1,1,40,50,false,false],c:"CI>100&CV>100",p:17560};
I.u[8616] = {n:"Stephammer",nn:"STEPHAMMER",t:7,d:"This hammer was created during the reign of King Stephan, a terrible tyrant who terrorised trillions of people.",ep:22,g:62,l:178,wd:true,fm:true,w:10,an:15,tw:true,e:[5,5,1,1,40,50,false,true],c:"CI>350&CW>180",p:10000};
I.u[8617] = {n:"Purrin Axe",nn:"PURRIN AXE",t:19,d:"Your pet, our passion.",ep:22,g:45,l:62,wd:true,fm:true,w:10,an:17,e:[5,4,1,1,20,30,false,false],c:"CV>150",p:10200};
I.u[8618] = {n:"Koss Axe",nn:"KOSS AXE",t:19,d:"This strange axe is designed to easily behead Bworks. The ease with which it can also behead Astrub\'s inhabitants clearly indicates that there\'s a connection between these two populations. But no one has discovered just what it is yet.",ep:22,g:46,l:80,wd:true,fm:true,w:10,an:17,tw:true,e:[10,4,1,1,50,50,false,true],c:"CC>120&CA>60",p:16700};
I.u[8619] = {n:"Hat Ariutokinabot",nn:"HAT ARIUTOKINABOT",t:16,d:"Thanks to this warrior hat, Iops really cut a fine figure. The only problem is that it hides some of their field of vision and prevents them from hearing their opponents\' words. This hat is designed for those who like to strike before they think.",ep:22,g:175,l:40,wd:true,fm:false,w:1,s:81,p:10000};
I.u[8620] = {n:"",nn:"",t:28,d:"",ep:0,g:2,l:1,wd:false,fm:true,w:0,m:true,p:1};
I.u[8621] = {n:"Cuwsed Cawwot",nn:"CUWSED CAWWOT",t:94,d:"This appetizing cawwot looks pwetty owdinawy but it stinks of evil fwom 50 paces. Will you wesist the temptation to devouw it?",ep:8,g:6,l:21,wd:true,fm:true,w:1,u:true,ut:true,p:100};
I.u[8624] = {n:"Magic Blue Balloon",nn:"MAGIC BLUE BALLOON",t:94,d:"Unlike the fake ones you\'ll find at the Trool fair, this balloon can take off with grace and ease. A beauty to behold, it\'s handmade with Strawberry Jelly eyelid, according to the traditions of the great magus Betsy Krokerr.",ep:1,g:7,l:1,wd:false,fm:true,w:1,u:true,p:1};
I.u[8625] = {n:"Magic Green Balloon",nn:"MAGIC GREEN BALLOON",t:94,d:"This green balloon is a delicacy made by force-feeding a Luidegit mushroom a green larva. The eyes of the larva roll out in the process and are sold to Bworks as appetizers. This balloon inspired Betsy Krokerr in some of her most famous recipes.",ep:1,g:8,l:1,wd:false,fm:true,w:1,u:true,p:1};
I.u[8626] = {n:"Cirsid Hizilnit",nn:"CIRSID HIZILNIT",t:94,d:"Tis ippitiziknik hizilnik sims ti bik kitik nirmik bik is inik ficik ixtrimilik ivik. Wilik yik risiksitik tik timptitinik ti nibblik itik ik?",ep:6,g:9,l:21,wd:false,fm:true,w:1,u:true,ut:true,p:100};
I.u[8627] = {n:"Sarr Ys\' Flute",nn:"SARR YS\' FLUTE",t:3,d:"Colleague and friend of the famous Guy-Guy (close parent of the Soft Oak), Sarr Ys is a minstrel whose enchanting melodies have been engraved in Amakna\'s collective unconscious. Unfortunately, this brilliant artist had a tragic Quaquack accident after a particularly humid new year\'s eve celebration.",ep:1,g:63,l:1,wd:false,fm:true,w:10,an:11,e:[10,6,2,3,100,30,false,true],p:1};
I.u[8628] = {n:"Ukando Hat",nn:"UKANDO HAT",t:16,d:"This headgear bestows self confidence and temerity on whoever wears it. Thanks to it you\'ll always keep going thinking you can actually make exploits. Remember to take it off from time to time or it will take you into inextricable situations!",ep:22,g:185,l:40,wd:true,fm:false,w:1,s:88,p:10000};
I.u[8629] = {n:"Hat Tsokey",nn:"HAT TSOKEY",t:16,d:"Every time an opponent comes to face this headgear, he turns as silent as the grave. No one dares ask what it is supposed to be. But that\'s ok, Srams aren\'t known for their sensitivity.",ep:22,g:179,l:40,wd:true,fm:false,w:1,s:89,p:10000};
I.u[8630] = {n:"Hatter Lily",nn:"HATTER LILY",t:16,d:"This hat looks like the Sulik and the Paperb\' Hat. But since it\'s very comfy and Xelors never take it off - even when they sleep, it is commonly called Hatter Lily. It gives off a nice pleasant white water lily smell which helps you get to sleep. However, it also provokes snores as unbearable as the Toads\' song during the mating season.",ep:22,g:177,l:40,wd:true,fm:false,w:1,s:86,p:10000};
I.u[8631] = {n:"Hat Wisholdoo",nn:"HAT WISHOLDOO",t:16,d:"Thanks to this hat, Eniripsas always know how to deal with emergencies. Indeed, all the first aid tips are discreetly written on the lining. It makes them look as if they just graduated even though no one would normally wear such a ridiculous hat even for a school ceremony!",ep:22,g:184,l:40,wd:true,fm:false,w:1,s:83,p:10000};
I.u[8632] = {n:"Cap Ricott",nn:"CAP RICOTT",t:16,d:"Given its name, you\'d think this cap would be a nice orange colour. Well, not at all! Moreover, this fashion accessory is more of a mask than a hat. So why did its creators name it this way? To make curious people talk, of course!",ep:22,g:178,l:40,wd:true,fm:false,w:1,s:90,p:10000};
I.u[8633] = {n:"Cap Rayer",nn:"CAP RAYER",t:16,d:"The sharpened points coming out of this cap not only enable you to hurt your enemy but also increase your chances of being struck on the head by lightning during a storm. Indeed, Sacriers are a class who love to hurt others as much as themselves. A nice smell of grilled meat: nothing\'s better to start your day!",ep:22,g:180,l:40,wd:true,fm:false,w:1,s:87,p:10000};
I.u[8634] = {n:"Cool Hood",nn:"COOL HOOD",t:16,d:"Thanks to this headgear, you\'ll really look like a horny beast. People will turn round to admire the huge pair on your head. This roomy hood can also keep food fresh like hay for Gobballs or bread for Tofus.",ep:22,g:181,l:40,wd:true,fm:false,w:1,s:84,p:10000};
I.u[8635] = {n:"Helmet Hicc",nn:"HELMET HICC",t:16,d:"This helmet can make wearers make promises they instantly forget. Is it because it exerts pressure on the part of the brain which deals with memory or language? No one truly knows. What matters is to only take into account the less extravagant words.",ep:22,g:176,l:40,wd:true,fm:false,w:1,s:92,p:10000};
I.u[8636] = {n:"Robbie Hoodie Cap",nn:"ROBBIE HOODIE CAP",t:16,d:"Robbie Hood was a famous Cra who lived in the woods. This explains the camouflage green colour of the cap. He wanted to take the law into his own hands and spent all his time stealing money from the rich for a good cause: himself!",ep:22,g:186,l:40,wd:true,fm:false,w:1,s:82,p:10000};
I.u[8637] = {n:"Zinda Hood",nn:"ZINDA HOOD",t:16,d:"With this hood on, Fecas increase their chance as well as that of their allies. If you\'ve hired a Feca who\'s wearing the Zinda Hood, you\'re sure to arrive safe and sound.",ep:22,g:182,l:40,wd:true,fm:false,w:1,s:91,p:10000};
I.u[8638] = {n:"Sthood",nn:"STHOOD",t:16,d:"This hood can be used to fend off the bad weather, but it\'s also great for cooking. It allows you to regulate the strength of a flame with a tip of the head. Now you know why Enutrofs have such muscular necks.",ep:22,g:183,l:40,wd:true,fm:false,w:1,s:85,p:10000};
I.u[8639] = {n:"Cloak Orporal",nn:"CLOAK ORPORAL",t:17,d:"The floating cape will give you the aura of a warrior, especially when you turn your back to your enemy. Some think it\'s reinforced by a metal sheet capable of staving off the slyest attacks.",ep:22,g:131,l:80,wd:true,fm:false,w:1,s:81,p:10000};
I.u[8640] = {n:"Cape of Spades",nn:"CAPE OF SPADES",t:17,d:"Cut from a soft and strong fabric, this cape gives the Ecaflips the impression that they\'re always the best, no matter what happens. When they win, they love to wear this piece of clothing and claim their reward: " + "\"" + "And now, gimme the cash!" + "\"" + "",ep:22,g:133,l:80,wd:true,fm:false,w:1,s:88,p:10000};
I.u[8641] = {n:"F-Ha(r)t",nn:"F-HA(R)T",t:17,d:"This cape is so light and thin that the slightest wind, wind of any sort, will blow it off... It\'s anything but discreet especially after a meal of veggies and fermented drinks.",ep:22,g:140,l:80,wd:true,fm:false,w:1,s:89,p:10000};
I.u[8642] = {n:"Cap Cape",nn:"CAP CAPE",t:17,d:"The dark colour of this cape makes it easy to match any other cloth, of any colour. Since it\'s a very wide cape, it can also protect your head and nose.",ep:22,g:141,l:80,wd:true,fm:false,w:1,s:86,p:10000};
I.u[8643] = {n:"Cape Sulitis",nn:"CAPE SULITIS",t:17,d:"Place the cape lightly on your shoulders to prevent stiff joints, stiff upper lips and other stiffness-related problems.",ep:22,g:134,l:80,wd:true,fm:false,w:1,s:83,p:10000};
I.u[8644] = {n:"Cape o\'Tbelly",nn:"CAPE O\'TBELLY",t:17,d:"This item is used to hide the Sadidas\' weight problem. Because they spend most of their time sleeping instead of practicing sports, the characters of this class tend to become rather stout. If Sadidettes actually have anti-starvation supplies flatteringly placed in the eyes of men, Sadidas usually use this cape to hide theirs.",ep:22,g:139,l:80,wd:true,fm:false,w:1,s:90,p:10000};
I.u[8645] = {n:"Cape Ytal",nn:"CAPE YTAL",t:17,d:"This piece of clothing plays an essential part in the Sacriers\' style. They use it to bandage their wounds, to clean off blood, to warm their feet during the cold seasons. We cannot say it enough - this class is terrified of frozen ground!",ep:22,g:138,l:80,wd:true,fm:false,w:1,s:87,p:10000};
I.u[8646] = {n:"Cape Anama",nn:"CAPE ANAMA",t:17,d:"This soft cape is sometimes used by the Osamodettes as a wide-brimmed hat. Male Osamodas only use it as a cape. Thanks to the drawing which decorates this piece of clothing, this character class has inherited the reputation of being frank... they can\'t laugh up their sleeves since the laugh has actually been sewn onto the cape!",ep:22,g:137,l:80,wd:true,fm:false,w:1,s:84,p:10000};
I.u[8647] = {n:"Cape Wuera",nn:"CAPE WUERA",t:17,d:"Pandawas use this cape as protection against bad weather and as a dance accessory. They put it on the floor, in the middle of a circle of tipsy dancers. Then they start their improvised choreography during which their feet can\'t leave the cloth.",ep:22,g:142,l:80,wd:true,fm:false,w:1,s:92,p:10000};
I.u[8648] = {n:"Cape Hulco",nn:"CAPE HULCO",t:17,d:"Let me tell you about Hulco, a very strange Cra. He couldn\'t find his way to his own bathroom because everything he saw looked like a tropical beach at sunset. At least he never had to pay for a sun holiday.",ep:22,g:132,l:80,wd:true,fm:false,w:1,s:82,p:10000};
I.u[8649] = {n:"Cape Adossia",nn:"CAPE ADOSSIA",t:17,d:"This cape\'s warm and comforting colours take its wearer on an imaginary journey to sunny lands, full of canyons carved by the elements, and cave paintings hidden amongst wind-blasted rocks and weathered cliffs. This is extremely useful way of forgetting the cold, grey, rainy weather that normally blights Amakna. At least it never snows, though - well, only at Kwismas. Funny, that.",ep:22,g:136,l:80,wd:true,fm:false,w:1,s:91,p:10000};
I.u[8650] = {n:"Cape Hernaum",nn:"CAPE HERNAUM",t:17,d:"Enutrofs have the habit of drawing a map on their cape. They live in such a mess that they can barely find the toilets and this is quite a problem, especially when nature calls urgently.",ep:22,g:135,l:80,wd:true,fm:false,w:1,s:85,p:10000};
I.u[8651] = {n:"Bloody Belt",nn:"BLOODY BELT",t:10,d:"This beautiful belt adds to the prestige of the Iops\' appearance. Some say that the central cross can become a detachable weapon when their swords aren\'t within reach. Obviously this is a legend. Only Srams are deceitful enough to have such things.",ep:22,g:116,l:130,wd:true,fm:false,w:1,s:81,p:10000};
I.u[8652] = {n:"Belt Sterous",nn:"BELT STEROUS",t:10,d:"When an Ecaflip is bored, he usually takes his cards from his belt and starts a game of solitaire. In fight, he can also use the cards as projectiles. He draws a card faster than his shadow and once victory is his, there\'s always time for a little game!",ep:22,g:117,l:130,wd:true,fm:false,w:1,s:88,p:10000};
I.u[8653] = {n:"Clinkin Belt",nn:"CLINKIN BELT",t:10,d:"The name of this belt comes from the particular noise it makes. The bones decorating the belt bang together, preventing the Srams, even those who are invisible, from going unnoticed. Then, you could easily hear such a dialogue: " + "\"" + "Surprising someone wearing the Clinkin Belt? You\'ve got a hope!" + "\"" + "",ep:22,g:118,l:130,wd:true,fm:false,w:1,s:89,p:10000};
I.u[8654] = {n:"Sleepless Belt",nn:"SLEEPLESS BELT",t:10,d:"The incessant ticking sound of the clock system of this belt prevents Xelors from sleeping when fights last forever. It is strongly advised that you take this belt off before going to bed.",ep:22,g:119,l:130,wd:true,fm:false,w:1,s:86,p:10000};
I.u[8655] = {n:"Blub Belt",nn:"BLUB BELT",t:10,d:"Though it won\'t stop the sobbing, this belt will hold lots of medical supplies. This is especially important for Eniripsas... when they have a sore throat, they can\'t cast their spells properly. And when they\'re voiceless ... well, they\'re as useful as a bear skin jacket in a heat wave.",ep:22,g:120,l:130,wd:true,fm:false,w:1,s:83,p:10000};
I.u[8656] = {n:"Belt Atio",nn:"BELT ATIO",t:10,d:"The reason the designers of this fashion accessory put the doll head next to the crotch is still unknown.",ep:22,g:121,l:130,wd:true,fm:false,w:1,s:90,p:10000};
I.u[8657] = {n:"Strap Pado",nn:"STRAP PADO",t:10,d:"Warriors of an ancient tribe renowned for their love for blood, the Pados may have been the first worshippers of the goddess Sacrier. Despite its harmless appearance, this belt is a real sophisticated instrument of torture.",ep:22,g:122,l:130,wd:true,fm:false,w:1,s:87,p:10000};
I.u[8658] = {n:"Diezzle Belt",nn:"DIEZZLE BELT",t:10,d:"Osamodas are known to be quite impulsive. They tend to be quite touchy. The numerous buckles on this belt are meant to help them put a cap on their spontaneity. " + "\"" + "Before you light up your anger and inject your eyes with blood, touch each and every horn on your fantastic Diezzle Belt!" + "\"" + "",ep:22,g:123,l:130,wd:true,fm:false,w:1,s:84,p:10000};
I.u[8659] = {n:"Sticky Strap",nn:"STICKY STRAP",t:10,d:"This belt helps Pandawas stand up straight (or almost) when they\'re drunk which they are, most of the time. In these moments, they tend to feel like they\'re stuck to the ground though gravity has no link whatsoever to this sensation. Gravity is way too busy knocking out scientists with apples.",ep:22,g:124,l:130,wd:true,fm:false,w:1,s:92,p:10000};
I.u[8660] = {n:"Targ Belt",nn:"TARG BELT",t:10,d:"This belt enables you to keep up baggy clothes (so as not to scare away sensitive souls) and it also offers a target to apprentice Cras. Indeed, they\'re taught to shoot right in the centre but never below the belt.",ep:22,g:125,l:130,wd:true,fm:false,w:1,s:82,p:10000};
I.u[8661] = {n:"Girdle Belt",nn:"GIRDLE BELT",t:10,d:"Provident, Fecas keep their survival supplies in a small bag, which mainly include Crackrock bread. That\'s why this food can last for so long. The Sekuritee Belt firmly secures the bag around their waist, because there are still way too many accidents linked to this bag falling, either due to negligence or just plain stupidity.",ep:22,g:126,l:130,wd:true,fm:false,w:1,s:91,p:10000};
I.u[8662] = {n:"Belt Urgid",nn:"BELT URGID",t:10,d:"This belt is a very handy fashion accessory used by male Enutrofs. Thanks to the map sewn onto it, they can hide their little accidents when they got lost looking for the toilets.",ep:22,g:127,l:130,wd:true,fm:false,w:1,s:85,p:10000};
I.u[8663] = {n:"Boot-a-Hoop",nn:"BOOT-A-HOOP",t:11,d:"With these boots on, the Iops are always the first to get to the battlefield and start livening the place up. It\'s common knowledge that this class love to amuse the crowd with their warrior exploits. But they\'re never taken seriously because of their intelligence, which is inversely proportional to their strength.",ep:22,g:100,l:100,wd:true,fm:false,w:1,s:81,p:10000};
I.u[8664] = {n:"Sleephairs",nn:"SLEEPHAIRS",t:11,d:"It is said that Sadidas never feel lonely, since they can summon lots of dolls to keep them company. Thanks to these shoes, you too can say goodbye to loneliness! Not only will you be able to talk to them, but they\'ll be great for hiding aces when you\'re playing poker. Once you\'re wearing them, you\'ll feel really good, even if you are a little ashamed to actually go out with them on.",ep:22,g:105,l:100,wd:true,fm:false,w:1,s:88,p:10000};
I.u[8665] = {n:"Boots Hox",nn:"BOOTS HOX",t:11,d:"Impregnated with a revolutionary firming agent, these boots will perfect the curvature of your legs and fight the good fight against your death-like Sram complexion. This cream has spectacular effects on Sramettes but seems to be absolutely useless on Srams. Could it be a hormonal problem?",ep:22,g:102,l:100,wd:true,fm:false,w:1,s:89,p:10000};
I.u[8666] = {n:"Shal\'Hal Boots",nn:"SHAL\'HAL BOOTS",t:11,d:"Created by a Xelor artist, these boots should enable him to paint more quickly. And well, it does! However, they tend to distort the vision, meaning the artist was incapable of drawing anything but curvy females.",ep:22,g:103,l:100,wd:true,fm:false,w:1,s:86,p:10000};
I.u[8667] = {n:"Boots Ulism",nn:"BOOTS ULISM",t:11,d:"Thanks to these boots, you\'ll always be protected from the toxins that come from the ground. You can never be suspicious enough of microbes and other germs that hang about. Some fine observers say that these shoes have more of a slipper than a boot look. But well, that\'s what the Eniripsas call them. Is it because of their small size that they see everything from a different point of view?",ep:22,g:104,l:100,wd:true,fm:false,w:1,s:83,p:10000};
I.u[8668] = {n:"Boots Hanik",nn:"BOOTS HANIK",t:11,d:"Thanks to their recognition system, these boots prevent Sadidas from crushing rare plants. However they won\'t prevent you from stepping in various sort of excrements if you don\'t watch where you\'re going.",ep:22,g:101,l:100,wd:true,fm:false,w:1,s:90,p:10000};
I.u[8669] = {n:"Nailed Thongs",nn:"NAILED THONGS",t:11,d:"Unlike the other classes, Sacriers love to live with a thorn in their foot and do not wish to have it taken out. The pain they feel reminds them that they\'re alive and give them the raging energy they need to slaughter their enemies. Besides, the drops of blood they leave behind mean they always find their way back home.",ep:22,g:106,l:100,wd:true,fm:false,w:1,s:87,p:10000};
I.u[8670] = {n:"Wawka Boots",nn:"WAWKA BOOTS",t:11,d:"Crocodyl Dandy lost many a good opportunity and many a wife through his undying urge to wander round the outback. His fame grew fast and he was even offered a sponsorship deal by the famed shoemakers of Aerdala, but when he saw what they had on offer, he threw them back and shouted " + "\"" + "That\'s not a pair of boots... THIS is a pair of boots." + "\"" + "",ep:22,g:107,l:100,wd:true,fm:false,w:1,s:84,p:10000};
I.u[8671] = {n:"Great Coralator Relic",nn:"GREAT CORALATOR RELIC",t:24,d:"This impressive relic might make you forget all of your worries.",ep:22,g:152,l:1,wd:true,fm:true,w:0,p:1};
I.u[8672] = {n:"Gourlo the Terrible Relic",nn:"GOURLO THE TERRIBLE RELIC",t:24,d:"This impressive relic might make you forget all of your worries.",ep:22,g:153,l:31,wd:true,fm:true,w:0,p:1};
I.u[8673] = {n:"Greater Bherb Relic",nn:"GREATER BHERB RELIC",t:24,d:"This impressive relic might make you forget all of your worries.",ep:22,g:154,l:61,wd:true,fm:true,w:0,p:1};
I.u[8674] = {n:"Tynril Relic",nn:"TYNRIL RELIC",t:24,d:"This impressive relic might make you forget all of your worries.",ep:22,g:155,l:101,wd:true,fm:true,w:0,p:1};
I.u[8675] = {n:"Kimbo Relic",nn:"KIMBO RELIC",t:24,d:"This impressive relic might make you forget all of your worries.",ep:22,g:156,l:141,wd:true,fm:true,w:0,p:1};
I.u[8676] = {n:"Wicked Sword",nn:"WICKED SWORD",t:6,d:"Vlad fell in love with this wonderful sword. He entered the most terrifying of all Dragokart races to get enough money to buy it. Unfortunately, he didn\'t know that the sword was cursed...",ep:1,g:75,l:95,wd:true,fm:true,w:1,an:14,tw:true,e:[10,4,1,1,40,50,false,false],c:"CV>50&CA>50&CS>50",p:9500};
I.u[8677] = {n:"Ross",nn:"ROSS",t:18,d:"This cute Coney is the favourite pet of the famous Master Nero Jack. Do not be fooled by appearances, he can be really fearsome in battle. This pet only eats Cawwots and other plants.",ep:3,g:44,l:1,wd:true,fm:true,w:10,c:"PZ=1",p:1};
I.u[8678] = {n:"Satanass Hat",nn:"SATANASS HAT",t:16,d:"Long ago, Satanass was a figure skater slicing up Holyball on Ice on the rinks of the Second Zone. While recovering from an injury to the coccyx he had a life-changing revelation: his destiny was to be the most talented Dragokart driver of his generation! A little bump can sometimes leave a hefty lump.",ep:1,g:187,l:36,wd:true,fm:true,w:20,p:3550};
I.u[8679] = {n:"Ross Ghost",nn:"ROSS GHOST",t:90,d:"This Ghost hasn\'t left the living world yet. Will you find a way to resurrect it or cowardly abandon it once and for all?",ep:3,g:46,l:1,wd:true,fm:true,w:1,p:1};
I.u[8680] = {n:"Slimy Scale",nn:"SLIMY SCALE",t:59,d:"This slimy, slippery scale allowed its owner to escape capture by strange adventurers who fished with their teeth. Unfortunately, it was no use against the well-placed swipe of a rusty sword.",ep:1,g:626,l:10,wd:true,fm:true,w:1,p:1};
I.u[8681] = {n:"Raul Mops\' Skin",nn:"RAUL MOPS\' SKIN",t:59,d:"Raul Mops has a real weakness. He loves to smear himself with water, wine and vinegar. What he loves most of all is rolling  peanuts along the road with his nose. Dry roasted, by the way, just in case you ever feel like doing him a favour.",ep:1,g:627,l:40,wd:true,fm:true,w:1,p:1};
I.u[8682] = {n:"Sponge Mob Skin",nn:"SPONGE MOB SKIN",t:59,d:"Either side of this skin can be used for your DIY projects. The soft side is for cleaning delicate surfaces, and the rough side for scouring all your previously inaccessible body parts.",ep:1,g:628,l:10,wd:true,fm:true,w:1,p:1};
I.u[8683] = {n:"Gift: Pingoku",nn:"GIFT: PINGOKU",t:89,d:"If you shake this object vigorously and hold your ear to it, you\'ll hear a strange noise... like a tiny animal weeping... yeah, actually maybe you don\'t want to shake it... Ahhh, just open the damn thing. You\'ll find a wonderful pet inside it, as well as some food and healing Eniripsa powders.",ep:1,g:42,l:1,wd:false,fm:true,w:1,u:true,p:1};
I.u[8684] = {n:"Bow Kennel Certificate: Ross",nn:"BOW KENNEL CERTIFICATE: ROSS",t:77,d:"This certificate describes the characteristics of the Ross you\'ve put in the Bow Kennels.",ep:3,g:45,l:1,wd:false,fm:true,w:1,p:100};
