I.u[8338] = {n:"Kwismas Sword",nn:"KWISMAS SWORD",t:6,d:"This strange sword sure won\'t make you invincible... In fact, people have died trying to kill snowmen with it.",ep:1,g:69,l:1,wd:false,fm:true,w:1,an:14,e:[1,1,1,1,2,2,false,true],p:1};
