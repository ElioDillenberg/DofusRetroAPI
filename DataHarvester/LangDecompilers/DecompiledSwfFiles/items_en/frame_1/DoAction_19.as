I.u[8352] = {n:"Black Dragoss Scale",nn:"BLACK DRAGOSS SCALE",t:59,d:"You could use this scale to make a very nice pair of boots or fighting underwear.",ep:11,g:606,l:50,wd:true,fm:true,w:1,p:10};
