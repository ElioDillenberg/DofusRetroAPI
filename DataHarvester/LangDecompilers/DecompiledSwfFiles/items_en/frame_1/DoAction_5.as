I.u[2019] = {n:"Golden Wheat Flour",nn:"GOLDEN WHEAT FLOUR",t:52,d:"This flour was made with an Ear of Golden Wheat. Awesome!",ep:1,g:251,l:1,wd:true,fm:true,w:2,p:0};
I.u[2020] = {n:"Golden Bread",nn:"GOLDEN BREAD",t:33,d:"This Bread has been kneaded with Magic Corn Flour. Selling it is a good way to get rich...",ep:1,g:69,l:10,wd:true,fm:true,w:2,c:"Cv<10",u:true,ut:true,p:1};
I.u[2021] = {n:"Bright Hop",nn:"BRIGHT HOP",t:34,d:"This type of hop has a bright and subdued aura revealing the magic Mother Nature has endowed it with.",ep:1,g:475,l:30,wd:true,fm:true,w:2,p:0};
I.u[2022] = {n:"Bright Hop Flour",nn:"BRIGHT HOP FLOUR",t:52,d:"This flour was made with Bright Hops. It contains magical energy for sure!",ep:1,g:253,l:1,wd:true,fm:true,w:2,p:0};
I.u[2024] = {n:"Briochette",nn:"BRIOCHETTE",t:33,d:"This small brioche is made from Hops. This explains the huge surge of energy which overcomes you when you eat it.",ep:1,g:8,l:20,wd:true,fm:true,w:1,u:true,ut:true,p:1};
I.u[2025] = {n:"Magic Briochette",nn:"MAGIC BRIOCHETTE",t:33,d:"This small brioche is made with Bright Hop Flour. This explains the huge surge of energy which overcomes you when you eat it.",ep:1,g:8,l:20,wd:true,fm:true,w:1,c:"Cv<20",u:true,p:1};
I.u[2026] = {n:"Storm Flax",nn:"STORM FLAX",t:34,d:"This Flax went through the most terrible storms and rose up again more than seven times. Pretty extraordinary for flax, you must admit. After getting wet every day for more than 148 years by the magic dew of the Great Pahti fields, its fibre might be useful for something.",ep:1,g:476,l:40,wd:true,fm:true,w:1,p:0};
I.u[2027] = {n:"Storm Flax Flour",nn:"STORM FLAX FLOUR",t:52,d:"This flour has been made with Storm Flax. It probably has the same properties of resistance.",ep:1,g:254,l:1,wd:true,fm:true,w:1,p:0};
I.u[2028] = {n:"Magic Solid Bread",nn:"MAGIC SOLID BREAD",t:33,d:"This type of Bread is made with Storm Flax Flour, which gives it a pleasant lightness and a great taste.",ep:1,g:12,l:1,wd:true,fm:true,w:1,c:"Cv<30",u:true,ut:true,p:1};
I.u[2029] = {n:"Resistant Rye",nn:"RESISTANT RYE",t:34,d:"This dark cereal can grow under the worst of conditions.",ep:1,g:124,l:50,wd:true,fm:true,w:2,p:0};
I.u[2030] = {n:"Resistant Rye Flour",nn:"RESISTANT RYE FLOUR",t:52,d:"This flour retains all the characteristics of Resisting Rye. Excessive consumption can therefore have chronic laxative effects.",ep:1,g:257,l:1,wd:true,fm:true,w:2,p:0};
I.u[2031] = {n:"Resistant Rye Bread",nn:"RESISTANT RYE BREAD",t:33,d:"This Bread is prepared with Magic Rye Flour, which gives it an awful flavour but lends it unique nutritional qualities.",ep:1,g:9,l:10,wd:true,fm:true,w:1,c:"Cv<40",u:true,ut:true,p:1};
I.u[2032] = {n:"Sweet Barley",nn:"SWEET BARLEY",t:34,d:"This rare type of barley absorbed sweet rainwater during its germination phase, which accounts for its incomparable magical powers.",ep:1,g:477,l:10,wd:true,fm:true,w:2,p:0};
I.u[2033] = {n:"Sweet Barley Flour",nn:"SWEET BARLEY FLOUR",t:52,d:"Sweet Barley flour produces very light bread, so light in fact that it could be blown away by a gust of wind. That\'s no excuse to pig out on it though, OK?",ep:1,g:256,l:1,wd:true,fm:true,w:1,p:0};
I.u[2034] = {n:"Barley Sugar",nn:"BARLEY SUGAR",t:42,d:"By extracting sugar from sweet barley flour, we obtain this unique magic-flavoured sweetie.",ep:1,g:128,l:1,wd:true,fm:true,w:1,c:"Cv<50",u:true,p:50000};
I.u[2035] = {n:"Euphoric Hemp",nn:"EUPHORIC HEMP",t:34,d:"This Hemp Stem seems to instantly destroy concentration and coordination, causing your enemy to hum ceaselessly, rifle through his sack for anything remotely edible and giggle uncontrollably. Handled by a specialist, it can prove very useful...",ep:1,g:478,l:70,wd:true,fm:true,w:1,p:0};
I.u[2036] = {n:"Gold-Bearing Oats",nn:"GOLD-BEARING OATS",t:34,d:"When we look at this kind of oats we get the impression that the grains are covered with gold, the sign of the great magic power that is absorbed during sprouting.",ep:1,g:551,l:20,wd:true,fm:true,w:2,p:0};
I.u[2037] = {n:"Gold-Bearing Oats Flour",nn:"GOLD-BEARING OATS FLOUR",t:52,d:"This flour was obtained using Gold-bearing Oats which impart their soft golden colour and considerable magical powers. They won\'t make a baker-wizard of you, though.",ep:1,g:250,l:1,wd:true,fm:true,w:1,p:0};
I.u[2038] = {n:"Gold-Bearing Rolled Oat Bread",nn:"GOLD-BEARING ROLLED OAT BREAD",t:33,d:"This bread with an uber-soft centre is just perfect.",ep:1,g:5,l:10,wd:true,fm:true,w:2,c:"Cv<70",u:true,ut:true,p:1};
I.u[2039] = {n:"Hunter Test Mark",nn:"HUNTER TEST MARK",t:24,d:"You tried to take the test to become a hunter.",ep:1,g:55,l:1,wd:true,fm:true,w:0,p:1};
I.u[2040] = {n:"Tsog\'s Hidden Chest",nn:"TSOG\'S HIDDEN CHEST",t:24,d:"You have found Tsog\'s Chest in a small box. It\'s strange but this box has the same smell as Tsog... yes, that same indescribably horrible smell.",ep:2,g:56,l:1,wd:true,fm:true,w:0,p:1};
I.u[2041] = {n:"Smelly Gobball Leg",nn:"SMELLY GOBBALL LEG",t:24,d:"This leg reeks of death... I assure you it was cleared of all involvement in the murder of its owner... Just smells like... Death.",ep:1,g:64,l:20,wd:true,fm:true,w:0,p:1};
I.u[2042] = {n:"Mak Gahan\'s Bone",nn:"MAK GAHAN\'S BONE",t:24,d:"You took this bone with you after you scattered Mak Gahan\'s body parts all over the room.",ep:1,g:57,l:1,wd:true,fm:true,w:0,p:1};
I.u[2043] = {n:"Scribbed Name of the Assassin",nn:"SCRIBBED NAME OF THE ASSASSIN",t:24,d:"The name of the Burnt Cat innkeeper\'s murderer is inscribed on this slimy, smelly piece of paper.",ep:2,g:58,l:1,wd:true,fm:true,w:0,p:1};
I.u[2044] = {n:"Burnt Cat Inn Beer",nn:"BURNT CAT INN BEER",t:37,d:"This beer looks too much like cat pee... Or maybe not enough.",ep:16,g:4,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:6};
I.u[2045] = {n:"Receipt for the Box",nn:"RECEIPT FOR THE BOX",t:24,d:"This document proves that you\'ve brought back a crate.",ep:2,g:50,l:1,wd:true,fm:true,w:0,p:1};
I.u[2046] = {n:"Brakmar Key",nn:"BRAKMAR KEY",t:24,d:"This key was stolen by a squirrel for mysterious reasons.",ep:2,g:61,l:1,wd:true,fm:true,w:0,p:1};
I.u[2047] = {n:"List of soldiers taking a break",nn:"LIST OF SOLDIERS TAKING A BREAK",t:24,d:"You got this list from the militiaman at the Inn. You can read the following names on it : \nBrank Thoof, Haco Norss, Jack Rottance, Kad Maxy.",ep:2,g:55,l:1,wd:true,fm:true,w:0,p:1};
I.u[2048] = {n:"List of soldiers taking a break",nn:"LIST OF SOLDIERS TAKING A BREAK",t:24,d:"You got this list from the militiaman at the Inn. You can read the following names on it : \nHaco Norss, Tink Berval, Brank Thoof, Kad Maxy, Seta Bnamron.",ep:2,g:58,l:1,wd:true,fm:true,w:0,p:1};
I.u[2049] = {n:"Warrior Xale Dugros\' Confession",nn:"WARRIOR XALE DUGROS\' CONFESSION",t:24,d:"Xale DuGros confesses that he never goes to the Inn. But he regularly has a break.",ep:2,g:55,l:1,wd:true,fm:true,w:0,p:1};
I.u[2050] = {n:"Warrior Nager Nacmeil\'s Confession",nn:"WARRIOR NAGER NACMEIL\'S CONFESSION",t:24,d:"The warrior Nager Nacmeil confesses that he no longer takes breaks. He\'s been a bit under the weather lately.",ep:2,g:55,l:1,wd:true,fm:true,w:0,p:1};
I.u[2051] = {n:"Bishom Pot\'s Confession",nn:"BISHOM POT\'S CONFESSION",t:24,d:"Bishom Pot confesses everything. His behaviour is unforgivable.",ep:2,g:55,l:1,wd:true,fm:true,w:0,p:1};
I.u[2052] = {n:"Signed and Dated Leave",nn:"SIGNED AND DATED LEAVE",t:24,d:"This document indicates you\'ve got a day off.",ep:2,g:50,l:1,wd:true,fm:true,w:0,p:1};
I.u[2053] = {n:"Militiaman Allen Tokar\'s Badge",nn:"MILITIAMAN ALLEN TOKAR\'S BADGE",t:24,d:"The Badge of the militiaman Allen Tokar.",ep:2,g:49,l:1,wd:true,fm:true,w:0,p:1};
I.u[2054] = {n:"Militiaman Yook Aput\'s Badge",nn:"MILITIAMAN YOOK APUT\'S BADGE",t:24,d:"The Badge of the militiaman Yook Aput.",ep:2,g:49,l:1,wd:true,fm:true,w:0,p:1};
