I.u[2056] = {n:"Crow Eye",nn:"CROW EYE",t:109,d:"A Crow eye is always useful... so long as you know how to avoid the evil eye.",ep:1,g:326,l:20,wd:true,fm:true,w:1,p:100};
I.u[2058] = {n:"Brandon\'s Heart",nn:"BRANDON\'S HEART",t:15,d:"This human heart surely comes from the renowned Lhi, who had a passion for crows.",ep:1,g:328,l:20,wd:true,fm:true,w:5,p:500};
I.u[2059] = {n:"Dead Crow",nn:"DEAD CROW",t:15,d:"You did good picking it up, it\'s intact. Dead but intact.",ep:1,g:329,l:20,wd:true,fm:true,w:10,p:100};
I.u[2060] = {n:"Crow Foot",nn:"CROW FOOT",t:103,d:"No need to break the foot of a crow. Especially as you know that, contrary to the Wabbit leg, crow foot brings bad luck.",ep:1,g:330,l:20,wd:true,fm:true,w:1,p:10};
I.u[2061] = {n:"Crow Cloak",nn:"CROW CLOAK",t:17,d:"This cloak, not really nice at first sight, is nevertheless really efficient. Once you get used to the smell of rotting Crow.",ep:1,g:15,l:45,wd:true,fm:true,w:5,p:5600};
I.u[2062] = {n:"Camouflaging",nn:"CAMOUFLAGING",t:31,d:"You\'ll be nicely hidden until your next fight.",ep:1,g:1,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2063] = {n:"Winged Boots",nn:"WINGED BOOTS",t:11,d:"These boots are made from different parts of Crow carcasses. The wearer has a strange feeling of nimbleness comparable to that given by the traditional flying potion made with grilled boletus.",ep:1,g:17,l:41,wd:true,fm:true,w:15,p:4500};
I.u[2064] = {n:"Camouflaging Scroll",nn:"CAMOUFLAGING SCROLL",t:14,d:"Thanks to this scroll you\'ll be able to camouflage yourself in the surrounding environment until your next fight. Avoid moving and making strange noises if you don\'t want to be found.",ep:16,g:1,l:10,wd:true,fm:true,w:1,c:"Pg=10&PM=B",u:true,p:250};
I.u[2065] = {n:"Elite Chafer Helmet",nn:"ELITE CHAFER HELMET",t:24,d:"This Chafer helmet is damaged, but it\'s still effective if you want to be part of the elite.",ep:2,g:59,l:1,wd:false,fm:true,w:0,p:1};
