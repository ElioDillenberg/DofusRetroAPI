I.u[2118] = {n:"Brakmarian Cloak",nn:"BRAKMARIAN CLOAK",t:17,d:"This cloak allows you to remain incognito in a cemetery, or to transform yourself at will into a female Chafer. Both these things are entirely useless to anyone but the most criminally perverse among us.",ep:1,g:20,l:1,wd:true,fm:true,w:7,c:"Ps=2",p:800};
I.u[2119] = {n:"Bontarian Cloak",nn:"BONTARIAN CLOAK",t:17,d:"This cloak gives you wings of a sort. The smoke it gives off when you burn it... you\'ll be high as a kite.",ep:5,g:34,l:1,wd:true,fm:true,w:3,c:"Ps=1",p:800};
I.u[2120] = {n:"Words of Ixa Mustam",nn:"WORDS OF IXA MUSTAM",t:24,d:"" + "\"" + "My will is near the Lighthouse of Amakna, get yourself a Small Twiggy Shovel to dig it up. And give my son a smack as you go by." + "\"" + "",ep:2,g:50,l:1,wd:true,fm:true,w:0,p:1};
I.u[2121] = {n:"Words of Tsog",nn:"WORDS OF TSOG",t:24,d:"" + "\"" + "I\'ve hidden a chest in the East Coast of Sufokia, inside a tunnel, under barrels of wine. A bit of advice before you go, get yourself a Small Twiggy Shovel to dig it up." + "\"" + "",ep:2,g:50,l:1,wd:true,fm:true,w:0,p:1};
