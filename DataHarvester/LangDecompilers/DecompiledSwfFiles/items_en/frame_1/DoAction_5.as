I.u[2157] = {n:"Clint",nn:"CLINT",t:16,d:"The Clint is a simple but classy hat. The latest version of this hat strangely suits both delicate female adventurers and the more virile male sort. A sure sign of quality, that.",ep:1,g:38,l:50,wd:true,fm:true,w:10,c:"Ps=3",p:1};
I.u[2158] = {n:"Letter from Oto Mustam to Sbab",nn:"LETTER FROM OTO MUSTAM TO SBAB",t:24,d:"The aim of this fully ironic letter is to denigrate Sbab.",ep:2,g:55,l:1,wd:true,fm:true,w:0,p:1};
I.u[2159] = {n:"Scroll of Bravery",nn:"SCROLL OF BRAVERY",t:14,d:"This parchment increases your Strength. It can be useful when a furious Kaniger charges you.",ep:1,g:2,l:1,wd:true,fm:true,w:1,ut:true,p:200};
I.u[2160] = {n:"Disheartening Scroll",nn:"DISHEARTENING SCROLL",t:14,d:"With this scroll you can reduce the Strength of a single target. This can be useful when a furious Kaniger is charging your enemy.",ep:1,g:4,l:1,wd:true,fm:true,w:1,ut:true,p:300};
I.u[2161] = {n:"Scroll of Suppleness",nn:"SCROLL OF SUPPLENESS",t:14,d:"This parchment increases your Agility. It can be useful for dodging Moowolves.",ep:1,g:6,l:1,wd:true,fm:true,w:1,ut:true,p:200};
I.u[2162] = {n:"Scroll of Stiffness",nn:"SCROLL OF STIFFNESS",t:14,d:"This parchment reduces the Agility of a single target. Use it to prevent your enemy from escaping a Moowolf attack.",ep:16,g:5,l:1,wd:true,fm:true,w:1,ut:true,p:300};
I.u[2163] = {n:"Truce",nn:"TRUCE",t:31,d:"You can\'t be attacked until your next fight.",ep:1,g:3,l:1,wd:true,fm:true,w:0,m:true,c:"Ps=0",p:1};
I.u[2164] = {n:"Lava Sample",nn:"LAVA SAMPLE",t:24,d:"This sample has been found East of the mine.",ep:2,g:37,l:1,wd:true,fm:true,w:0,p:1};
I.u[2165] = {n:"Lava Sample",nn:"LAVA SAMPLE",t:24,d:"This sample has been found West of the mine.",ep:2,g:37,l:1,wd:true,fm:true,w:0,p:1};
I.u[2166] = {n:"Lava Sample",nn:"LAVA SAMPLE",t:24,d:"This sample was found close to the mine.",ep:2,g:37,l:1,wd:true,fm:true,w:0,p:1};
I.u[2167] = {n:"Lava Sample",nn:"LAVA SAMPLE",t:24,d:"This sample has been found South of the mine.",ep:2,g:37,l:1,wd:true,fm:true,w:0,p:1};
I.u[2168] = {n:"Start of Report",nn:"START OF REPORT",t:24,d:"This report starts to outline the worrying changes in the Brakmarian magmatic mass.",ep:2,g:55,l:1,wd:true,fm:true,w:0,p:1};
I.u[2169] = {n:"Raaga",nn:"RAAGA",t:32,d:"Bring Raaga to a magmatic cave.",ep:14,g:4,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2170] = {n:"Mercenary\'s Sword",nn:"MERCENARY\'S SWORD",t:6,d:"This long sword is heavy and difficult to handle, but it cuts like any sword you can name...",ep:1,g:25,l:1,wd:true,fm:true,w:1,an:14,tw:true,e:[30,5,1,1,25,50,false,false],p:1};
I.u[2171] = {n:"Raaga\'s Report",nn:"RAAGA\'S REPORT",t:24,d:"This report outlines the chaotic changes in volcanic activities around Brakmar.",ep:2,g:55,l:1,wd:true,fm:true,w:0,p:1};
I.u[2172] = {n:"List of Samples to find",nn:"LIST OF SAMPLES TO FIND",t:24,d:"Raaga roughly wrote down the information you need to find good lava samples:\n\'Search within a 5 miles radius. Look for small peaks.\'",ep:2,g:41,l:1,wd:true,fm:true,w:0,p:1};
I.u[2173] = {n:"Secrets of the Squirrel Language",nn:"SECRETS OF THE SQUIRREL LANGUAGE",t:25,d:"This dictionary will teach you how to talk with squirrels, or at least how to try to...",ep:2,g:6,l:1,wd:true,fm:true,w:1,u:true,p:50};
I.u[2174] = {n:"Interrogation Room Pass",nn:"INTERROGATION ROOM PASS",t:24,d:"With this piece of paper, you can freely enter the prison\'s small Interrogation Rooms",ep:2,g:26,l:1,wd:true,fm:true,w:0,p:1};
I.u[2175] = {n:"Danathor\'s Threat",nn:"DANATHOR\'S THREAT",t:24,d:"This is a threat that Danathor wrote not to forget he needed something to fall back on in any circumstances.",ep:2,g:50,l:1,wd:true,fm:true,w:0,p:1};
I.u[2176] = {n:"Rimaraf\'s Address",nn:"RIMARAF\'S ADDRESS",t:24,d:"Rimaraf lives in a red brick house at 13 Carving Alley, in the Butcher\'s quarter.",ep:2,g:50,l:1,wd:false,fm:true,w:0,p:1};
I.u[2177] = {n:"Danathor\'s Brooch",nn:"DANATHOR\'S BROOCH",t:24,d:"Danathor gave you this small brooch.",ep:2,g:69,l:1,wd:true,fm:true,w:0,p:1};
I.u[2178] = {n:"Investigation Form 36",nn:"INVESTIGATION FORM 36",t:80,d:"This form is a good way to identify potential suspects of criminal actions. \n(Click on it, then on someone else to use it)",ep:16,g:6,l:1,wd:true,fm:true,w:1,c:"PO!2181&PO!2177&Ps=1&Pa=27",ut:true,p:100};
I.u[2179] = {n:"Dragoturkey Tooth",nn:"DRAGOTURKEY TOOTH",t:47,d:"This tooth seems to be very sharp. It could become a fearsome weapon in the wrong hands. Fortunately, the Dragoturkey only uses it to chew salad.",ep:2,g:419,l:20,wd:true,fm:true,w:2,p:1};
I.u[2180] = {n:"Statement",nn:"STATEMENT",t:24,d:"This person\'s alibi seems to hold up perfectly.",ep:2,g:55,l:1,wd:true,fm:true,w:0,c:"PO!2177",p:1};
I.u[2181] = {n:"Statement",nn:"STATEMENT",t:24,d:"This person is a suspect who\'s alibi doesn\'t stand up.",ep:2,g:55,l:1,wd:true,fm:true,w:0,c:"PO=2177",p:1};
I.u[2182] = {n:"Investigating Officer\'s Card",nn:"INVESTIGATING OFFICER\'S CARD",t:24,d:"A Militia Officer is keeping a close eye on you. You are a suspect in the killing of Rimaraf.  The officer left his card after questioning you...",ep:2,g:71,l:1,wd:true,fm:true,w:0,c:"PO=2177",p:1};
I.u[2183] = {n:"Unlearning Potion for Profession: Fisherman",nn:"UNLEARNING POTION FOR PROFESSION: FISHERMAN",t:44,d:"With this potion you will unlearn the profession of Fisherman.",ep:1,g:23,l:1,wd:true,fm:true,w:1,u:true,p:10};
I.u[2184] = {n:"Unlearning Potion for Profession: Fishmonger",nn:"UNLEARNING POTION FOR PROFESSION: FISHMONGER",t:44,d:"With this potion you will unlearn the profession of Fishmonger.",ep:1,g:18,l:1,wd:true,fm:true,w:1,u:true,p:10};
I.u[2185] = {n:"Unlearning Potion for Profession: Hunter",nn:"UNLEARNING POTION FOR PROFESSION: HUNTER",t:44,d:"With this potion you\'ll unlearn the profession of Hunter.",ep:1,g:19,l:1,wd:true,fm:true,w:1,u:true,p:10};
I.u[2186] = {n:"Unlearning Potion for Profession: Butcher",nn:"UNLEARNING POTION FOR PROFESSION: BUTCHER",t:44,d:"With this potion you will unlearn the profession of Butcher.",ep:1,g:20,l:1,wd:true,fm:true,w:1,u:true,p:10};
I.u[2187] = {n:"Contest Snapper",nn:"CONTEST SNAPPER",t:41,d:"This fish can only be caught in the Sufokia Schwimming Pool, it is a cross breed of many diverse fish.",ep:7,g:400,l:1,wd:true,fm:true,w:20,p:1};
I.u[2188] = {n:"Fishing Rod for Snappers",nn:"FISHING ROD FOR SNAPPERS",t:20,d:"This fishing rod is the only way to catch snappers in the Sufokia Schwimming Pool.",ep:7,g:8,l:1,wd:true,fm:true,w:2,an:18,e:[1,5,1,5,10,10,false,true],p:50};
I.u[2189] = {n:"Good Vote Certificate",nn:"GOOD VOTE CERTIFICATE",t:24,d:"This small piece of paper signed by Satirev Refibrom proves you voted on a Judgement... and you voted well.",ep:2,g:26,l:1,wd:true,fm:true,w:0,p:1};
I.u[2190] = {n:"Investigation Form 33",nn:"INVESTIGATION FORM 33",t:80,d:"This form is usually used to make suspects confess their criminal acts. All you have to do now is to fill in a fake interview of Oni Tnec. \n(Use it on yourself to use it)",ep:16,g:6,l:1,wd:true,fm:false,w:1,c:"PO=2177&Ps=1&Pa=27",u:true,p:100};
I.u[2191] = {n:"Wrong Vote Certificate",nn:"WRONG VOTE CERTIFICATE",t:24,d:"This small piece of paper signed by Satirev Refibrom proves you voted on a Judgement... and you lied.",ep:2,g:26,l:1,wd:true,fm:true,w:0,p:1};
I.u[2192] = {n:"Stolen Documents",nn:"STOLEN DOCUMENTS",t:24,d:"These top secret documents are sealed with a death glyph. Even if you break the seal, you will never be able to read them.",ep:2,g:55,l:1,wd:true,fm:true,w:0,p:1};
I.u[2193] = {n:"Tuft of Puja\'s Hair",nn:"TUFT OF PUJA\'S HAIR",t:24,d:"You have given Oto Mustam\'s son a good hiding. Neither of them will appreciate it!",ep:2,g:30,l:1,wd:true,fm:true,w:0,p:1};
I.u[2194] = {n:"Reminder",nn:"REMINDER",t:24,d:"Bring back 10 Barks of Treechnid.",ep:2,g:50,l:1,wd:true,fm:true,w:0,p:1};
I.u[2195] = {n:"Tooth",nn:"TOOTH",t:24,d:"This tooth must be yours judging by the hiding Oto Mustam gave you. \nWisdom would suggest you give it a while before going back to him... unless you want to be picking the rest of your teeth up with your other broken arm.",ep:1,g:35,l:1,wd:true,fm:true,w:0,p:1};
I.u[2196] = {n:"Knowledge",nn:"KNOWLEDGE",t:29,d:"This blessing will make you gain Intelligence for your next few fights.",ep:18,g:3,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2197] = {n:"Divorce Force",nn:"DIVORCE FORCE",t:14,d:"These rings can break the sacred bond of marriage.",ep:1,g:13,l:1,wd:true,fm:true,w:0,ut:true,p:1};
I.u[2198] = {n:"Scroll of Knowledge",nn:"SCROLL OF KNOWLEDGE",t:14,d:"This parchment increases your Intelligence. It will help you keep your cool when faced with multiple enemies.",ep:1,g:7,l:1,wd:true,fm:true,w:1,ut:true,p:1};
I.u[2199] = {n:"Scroll of Stupidity",nn:"SCROLL OF STUPIDITY",t:14,d:"This parchment reduces the Intelligence of a single target. It can be useful to prevent your enemy from moving when faced with a mob of angry Bworks.",ep:1,g:8,l:1,wd:true,fm:true,w:1,ut:true,p:1};
I.u[2200] = {n:"Satirev Refibrom\'s Threat",nn:"SATIREV REFIBROM\'S THREAT",t:24,d:"The Great Judge of Brakmar doesn\'t want to see you again. Apparently he\'s not overly fond of you... don\'t worry, he has a heart of stone anyway.",ep:2,g:50,l:1,wd:true,fm:true,w:0,p:1};
I.u[2201] = {n:"Serenity",nn:"SERENITY",t:29,d:"This blessing will increase Wisdom for the next few battles.",ep:1,g:4,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2202] = {n:"Senility",nn:"SENILITY",t:30,d:"This curse will reduce your Wisdom in your next fight.",ep:1,g:4,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2203] = {n:"Scroll of Restraint",nn:"SCROLL OF RESTRAINT",t:14,d:"This scroll increases your Wisdom, which is essential when confronting Dark Vlad.",ep:1,g:9,l:1,wd:true,fm:true,w:2,ut:true,p:250};
I.u[2204] = {n:"Scroll of Senility",nn:"SCROLL OF SENILITY",t:14,d:"This parchment reduces the Wisdom of a single target. It\'s necessary when your enemy tries to equal Dark Vlad.",ep:1,g:10,l:1,wd:true,fm:true,w:2,ut:true,p:500};
I.u[2205] = {n:"Change of Order",nn:"CHANGE OF ORDER",t:24,d:"This paper indicates you\'ve already changed your order.",ep:2,g:50,l:1,wd:true,fm:true,w:0,p:1};
I.u[2206] = {n:"Godsend",nn:"GODSEND",t:29,d:"This blessing will increase Luck for your next few fights.",ep:18,g:5,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2207] = {n:"Detention Paper",nn:"DETENTION PAPER",t:24,d:"You can read about all the crimes you\'ve committed on this paper as well as the date you were imprisoned... Great reading for those long trips.",ep:2,g:73,l:1,wd:true,fm:true,w:0,p:1};
I.u[2208] = {n:"Rotten Luck",nn:"ROTTEN LUCK",t:30,d:"This curse will reduce your luck in your next fight.",ep:1,g:5,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2209] = {n:"Scroll of Godsend",nn:"SCROLL OF GODSEND",t:14,d:"This parchment increases your Luck. Will you be lucky enough to find a Tofu in a haystack?",ep:1,g:11,l:1,wd:true,fm:true,w:2,ut:true,p:250};
I.u[2210] = {n:"Scroll of Rotten Luck",nn:"SCROLL OF ROTTEN LUCK",t:14,d:"This parchment can reduce the Chance of a single target, which is useful when, for example, your opponent\'s from the Emerald Isle.",ep:1,g:12,l:1,wd:true,fm:true,w:2,ut:true,p:500};
I.u[2211] = {n:"Iopish Gear",nn:"IOPISH GEAR",t:14,d:"You will be a success in all the best balls of the world with this gear. They\'ll have no respect for you though. Hey, it depends what you\'re looking for.",ep:1,g:14,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:100};
I.u[2212] = {n:"Xelorish Gear",nn:"XELORISH GEAR",t:14,d:"With this hat and a few well-chosen weapons, you\'ll pass for a Xelor.",ep:1,g:15,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:100};
I.u[2213] = {n:"Sadidish Gear",nn:"SADIDISH GEAR",t:14,d:"It\'s not hard to sleep like a Sadida. A bedside rug, darkness... your head trapped in your rectum. Success guaranteed!",ep:1,g:16,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:100};
I.u[2214] = {n:"Iop Attitude",nn:"IOP ATTITUDE",t:31,d:"You\'ll have the Iop Attitude until your next fight.",ep:1,g:4,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2215] = {n:"Xelor Attitude",nn:"XELOR ATTITUDE",t:31,d:"You\'ll have the Xelor Attitude until your next fight.",ep:1,g:5,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2216] = {n:"Sadida Attitude",nn:"SADIDA ATTITUDE",t:31,d:"You\'ll have the Sadida Attitude until your next fight.",ep:1,g:6,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2217] = {n:"Feca-style Wig",nn:"FECA-STYLE WIG",t:14,d:"You took this wig out of an old cupboard, didn\'t you? It might have been fashionable in your grandmother\'s day, but not anymore, buddy. It makes you look like a Feca on a Saturday night. Awful.",ep:1,g:17,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:100};
I.u[2218] = {n:"Feca Attitude",nn:"FECA ATTITUDE",t:31,d:"You\'ll have the Feca Attitude until your next fight.",ep:1,g:7,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2219] = {n:"Eniripsish Gear",nn:"ENIRIPSISH GEAR",t:14,d:"These Eniripsa\'s wings won\'t make you fly. So don\'t try to show this gear off or you\'re sure to make a complete idiot of yourself.",ep:1,g:18,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:100};
I.u[2220] = {n:"Eniripsa Attitude",nn:"ENIRIPSA ATTITUDE",t:31,d:"You\'ll have the Eniripsa Attitude until your next fight.",ep:1,g:8,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2221] = {n:"Enutrofish Gear",nn:"ENUTROFISH GEAR",t:14,d:"A fake beard which makes you look like an Enutrof and which also gives you the breath of an Enutrof on a Monday morning. You don\'t need me to tell you that it must be used in extreme moderation.",ep:1,g:19,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:100};
I.u[2222] = {n:"Enutrof Attitude",nn:"ENUTROF ATTITUDE",t:31,d:"You\'ll have the Enutrof Attitude until your next fight.",ep:1,g:9,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2223] = {n:"Sacrish Gear",nn:"SACRISH GEAR",t:14,d:"A little Tofu blood, a false scar, the eyes of an epileptic pirate, and there you go! You can now go to Sacrier funerals unnoticed! That can be useful, you know.",ep:1,g:20,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:1};
I.u[2224] = {n:"Sacrier Attitude",nn:"SACRIER ATTITUDE",t:31,d:"You\'ll have the Sacrier Attitude until your next fight.",ep:1,g:10,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2225] = {n:"Sramish Gear",nn:"SRAMISH GEAR",t:14,d:"A Skully, a cape on your head and the illusion is perfect. If you wear this outfit, you will look like a hideous character coming from the depths of a cemetery... yes, you\'ll look like a Sram!",ep:1,g:21,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:1};
I.u[2226] = {n:"Sram Attitude",nn:"SRAM ATTITUDE",t:31,d:"You\'ll have the Sram Attitude until your next fight.",ep:1,g:11,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2227] = {n:"Osamodish Gear",nn:"OSAMODISH GEAR",t:14,d:"Whether you like it or not, if you wear these horns and forked tail you\'ll find yourself invited to numerous secret Osamodas meetings. They\'re so secret almost nobody ever turns up... That\'s the one good thing about secret events... You always have a good excuse not to go.",ep:1,g:22,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:1};
I.u[2228] = {n:"Osamodas Attitude",nn:"OSAMODAS ATTITUDE",t:31,d:"You\'ll have the Osamodas Attitude until your next fight.",ep:1,g:12,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2229] = {n:"Ecaflipish Gear",nn:"ECAFLIPISH GEAR",t:14,d:"You will find silky Wabbit ears, a tabby tail and a box of restless fleas. If you find yourself in a popular restaurant and you need a table, just release the little devils, you\'ll have no problem. Well... the chef will leave too, I suppose. And the staff. Come to think of it, you probably wouldn\'t want to stay there yourself. I\'ll get back to you on that one.",ep:1,g:23,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:1};
I.u[2230] = {n:"Ecaflip Attitude",nn:"ECAFLIP ATTITUDE",t:31,d:"You\'ll have the Ecaflip Attitude until your next fight.",ep:1,g:13,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2231] = {n:"Crahish Gear",nn:"CRAHISH GEAR",t:14,d:"As you know, a Cra never leaves home without a quiver. No wonder people think you\'re a Cra when you walk around with yours.",ep:1,g:24,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:1};
I.u[2232] = {n:"Cra Attitude",nn:"CRA ATTITUDE",t:31,d:"You\'ll have the Cra Attitude until your next fight.",ep:1,g:14,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2233] = {n:"The Cruncher",nn:"THE CRUNCHER",t:10,d:"Xof the Fox created this belt after he inexplicably decided to gather up all the teeth he could find on a Wabbit battlefield. It has now become a lifelong passion of his.",ep:8,g:20,l:28,wd:true,fm:true,w:20,c:"CS<40&CI>80",p:2400};
I.u[2234] = {n:"Bonta Attitude",nn:"BONTA ATTITUDE",t:31,d:"You\'ll have the Bonta Attitude until your next fight.",ep:1,g:15,l:1,wd:true,fm:true,w:0,m:true,u:true,p:1};
I.u[2235] = {n:"Brakmar Attitude",nn:"BRAKMAR ATTITUDE",t:31,d:"You have the Brakmar Attitude until your next fight.",ep:1,g:16,l:1,wd:true,fm:true,w:0,m:true,u:true,p:1};
I.u[2236] = {n:"Bontarian Costume",nn:"BONTARIAN COSTUME",t:14,d:"With this costume on, you won\'t be able take two steps without being asked for directions.",ep:1,g:25,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:1};
I.u[2237] = {n:"Brakmarish Gear",nn:"BRAKMARISH GEAR",t:14,d:"You\'ll be dreaded by all \'n\' sundry with these things on.",ep:1,g:26,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:1};
I.u[2239] = {n:"Eniripsa Powder",nn:"ENIRIPSA POWDER",t:48,d:"This powder is only made in small quantities from Eniripsas\' wings. Pets all over the world say yes to its amazing healing powers.",ep:3,g:260,l:1,wd:true,fm:true,w:1,p:1000};
I.u[2240] = {n:"Small Red Fairywork",nn:"SMALL RED FAIRYWORK",t:74,d:"Ideal at small weddings, perfect for the annual Captain Chafer Panty Party.",ep:16,g:28,l:1,wd:true,fm:true,w:1,ut:true,p:350};
I.u[2241] = {n:"Royal Mint Jelly",nn:"ROYAL MINT JELLY",t:110,d:"This Royal Jelly used to strike fear into the hearts of all but the craziest warriors. Be careful not to eat it... it\'s no plain Mint Jelly.",ep:7,g:408,l:100,wd:true,fm:true,w:1,p:100};
I.u[2242] = {n:"Royal Strawberry Jelly",nn:"ROYAL STRAWBERRY JELLY",t:110,d:"This concentrate of Royal Strawberry Jelly can turn a pacifist Tabi into a bloodthirsty killing-machine. An excellent reason to avoid giving it to your friends as a starter.",ep:7,g:409,l:150,wd:true,fm:true,w:1,p:100};
I.u[2245] = {n:"Scurvion Pincer",nn:"SCURVION PINCER",t:47,d:"Once used as a backscratcher (back when a back was a back and a man was a man), it\'s now used to hang coats. I think that sums up the whole sorry history of the World of Twelve.",ep:14,g:411,l:36,wd:true,fm:true,w:1,p:1};
I.u[2246] = {n:"Broken Royal Tofu Crown",nn:"BROKEN ROYAL TOFU CROWN",t:15,d:"This crown was either broken by a powerful magic attack or by that last headbutt the Royal Tofu tried to give you.",ep:3,g:412,l:100,wd:true,fm:true,w:10,p:250};
I.u[2247] = {n:"Royal Tofu Feather",nn:"ROYAL TOFU FEATHER",t:53,d:"You\'d have to be a lop not to realize that this Feather comes from the backside of a Royal Tofu. You\'d have to be an Osamodas to want to touch it.",ep:3,g:413,l:100,wd:true,fm:true,w:11,p:250};
I.u[2248] = {n:"Ouginak Hair",nn:"OUGINAK HAIR",t:54,d:"This kind of hair can keep you warm since you are all sweaty after holding it for quite some time. Unless you are sweaty because you are afraid of meeting another Ouginak who might see you holding an Ouginak hair and therefore want to avenge his friend.",ep:14,g:414,l:51,wd:true,fm:true,w:1,p:1};
I.u[2249] = {n:"Trunknid Leaf",nn:"TRUNKNID LEAF",t:36,d:"This leaf must supply the Trunknid with strength to fight. But for this, one should know how to use it properly. Indeed, the first thing you\'d think about is chopping and adding it to your beans. So delicious!",ep:2,g:415,l:35,wd:true,fm:true,w:1,p:1};
I.u[2250] = {n:"Trunknid Wood",nn:"TRUNKNID WOOD",t:38,d:"This piece of wood sounds hollow. The green sap coming out of it proves that it is the nose of the Trunknid.",ep:9,g:416,l:35,wd:true,fm:true,w:1,p:100};
I.u[2251] = {n:"Megacrackler Megastone",nn:"MEGACRACKLER MEGASTONE",t:51,d:"Thou shalt not cast the first Megastone!",ep:5,g:417,l:45,wd:true,fm:true,w:15,p:150};
I.u[2252] = {n:"Gold Crackler Tooth",nn:"GOLD CRACKLER TOOTH",t:47,d:"You\'ve just broken the tooth of a Crackler. What a pity it was his only tooth! A denture could easily have covered the monthly cost of the Zaap.",ep:5,g:418,l:31,wd:true,fm:true,w:5,p:1};
I.u[2253] = {n:"Greedovore Flower",nn:"GREEDOVORE FLOWER",t:35,d:"This Flower facilitates digestion. Now you can stay as long as you want at the local free buffet.",ep:12,g:420,l:42,wd:true,fm:true,w:1,p:100};
I.u[2254] = {n:"Ambusher Leaf",nn:"AMBUSHER LEAF",t:36,d:"Ambusher leaf has a very unusual taste, somewhere between hazelnut and slightly limp salad. If you dare to try the meals you make, you will soon realise they taste a little like treachery...",ep:2,g:421,l:22,wd:true,fm:true,w:1,p:1};
I.u[2255] = {n:"Spy Report",nn:"SPY REPORT",t:24,d:"No recent dangerous activity has been reported. The Militiamen Organization is in turmoil since the murder of one of their people. The culprit is apparently a Bontarian.",ep:2,g:55,l:1,wd:true,fm:true,w:0,p:1};
I.u[2256] = {n:"Sewer Rat Carcass",nn:"SEWER RAT CARCASS",t:24,d:"This rat is now dead meat... one less scum-rodent to crowd out the sewer entrance.",ep:2,g:121,l:1,wd:true,fm:true,w:0,p:1};
I.u[2257] = {n:"A piece of the Sewer Wall",nn:"A PIECE OF THE SEWER WALL",t:24,d:"This piece of Sewer Wall was found in the stomach of a thingamajig.",ep:2,g:55,l:1,wd:true,fm:true,w:0,p:1};
I.u[2259] = {n:"Karne Tooth",nn:"KARNE TOOTH",t:47,d:"What\'s in your hand is the most ancient game in Amakna. The rules are simple: throw a tooth and try to catch it with the same hand while you pick up another Karne Tooth on the ground.",ep:4,g:13,l:16,wd:true,fm:true,w:1,p:20};
I.u[2260] = {n:"A Brakmarian\'s Card",nn:"A BRAKMARIAN\'S CARD",t:24,d:"Someone, apparently a Brakmarian, slipped this card into your bag. It says that Warriors are interested in your talents as an assassin and they suggest you join the mad Brakmarian adventure.",ep:2,g:72,l:1,wd:true,fm:true,w:0,c:"PO=2177",p:1};
I.u[2261] = {n:"Sniffling Spirit",nn:"SNIFFLING SPIRIT",t:30,d:"You\'re possessed by a Sniffling Spirit. Your odour and vitality are its bread and butter.",ep:1,g:7,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2262] = {n:"Assassin\'s Description",nn:"ASSASSIN\'S DESCRIPTION",t:24,d:"You found a Bontarian who stank of blood. All you know about this fellow is written on this piece of paper.",ep:2,g:55,l:1,wd:true,fm:true,w:0,c:"PO=2177",p:1};
I.u[2263] = {n:"Blood Sniffling Staff",nn:"BLOOD SNIFFLING STAFF",t:80,d:"This staff is possessed by a sniffling spirit. This imp-like being haunts items and living things alike with a special smell before going back to the Land of the not-quite-living-but-far-from-dead.",ep:16,g:25,l:1,wd:true,fm:true,w:10,ut:true,p:1};
I.u[2264] = {n:"Goblin Skin",nn:"GOBLIN SKIN",t:59,d:"Goblins often make fun of people, which can be quite irritating. Slicing up those bobbling, wobbling Goblins should shut them up, however.",ep:4,g:422,l:16,wd:true,fm:true,w:1,p:1};
I.u[2265] = {n:"Description of a Bontarian",nn:"DESCRIPTION OF A BONTARIAN",t:24,d:"You\'ve freed a sniffling spirit but he didn\'t anchor on the targeted person. You have written some information about the person on this piece of paper.",ep:2,g:55,l:1,wd:true,fm:true,w:0,c:"PO!2177",p:1};
I.u[2266] = {n:"Piglet Milk",nn:"PIGLET MILK",t:15,d:"This Pig Milk comes from a Piglet. Remember this just in case...",ep:1,g:423,l:18,wd:true,fm:true,w:1,p:1};
I.u[2267] = {n:"Dragomilk",nn:"DRAGOMILK",t:15,d:"How many farmhands does it take to milk a Dragon Pig? Seven. One to milk it and the other six to carry his coffin. \nBOOOOO! GET OFF THE STAGE!",ep:2,g:424,l:70,wd:true,fm:true,w:2,p:1};
I.u[2268] = {n:"Bwork Archer Scalp",nn:"BWORK ARCHER SCALP",t:15,d:"Here is a scalp which could start a new fashion in the Enutrof world: the Toopish. A little bit of anti-lice potion and you\'ll be ready to participate in the " + "\"" + "World\'s Classiest Enutrof" + "\"" + " Contest.",ep:4,g:425,l:24,wd:true,fm:true,w:1,p:1};
I.u[2269] = {n:"Blue Larva Eye",nn:"BLUE LARVA EYE",t:24,d:"This Eye is intact. Scrambled eyes with a pinch of oregano and some salty tears really gets you going in the morning. At the same time having it in your hands will prove to Oto that you\'ve killed a Blue Larva.",ep:1,g:70,l:1,wd:true,fm:true,w:0,p:1};
I.u[2270] = {n:"Bunch of Larva Eggs",nn:"BUNCH OF LARVA EGGS",t:24,d:"This bunch of eggs looks like a big bunch of grapes. Don\'t even think about popping them in your mouth though.",ep:1,g:83,l:1,wd:true,fm:true,w:0,p:1};
I.u[2271] = {n:"Purple Bwork Leather",nn:"PURPLE BWORK LEATHER",t:56,d:"Bwork handicraft remains very mysterious. Some adventurers think the purplish colour comes from Goblins. But the origins of the leather are still unknown. Very strange.",ep:4,g:426,l:31,wd:true,fm:true,w:1,p:1};
I.u[2272] = {n:"Golden Larva",nn:"GOLDEN LARVA",t:32,d:"This larva has just hatched. As its very first image of the world was your face, it considers you as its mother.",ep:1,g:5,l:1,wd:true,fm:true,w:10,m:true,p:1};
I.u[2273] = {n:"Purple Fabric",nn:"PURPLE FABRIC",t:55,d:"The main characteristics of this strange Purple Fabric are that it is light and soft to the touch.  The tag says " + "\"" + "made in Tie One On" + "\"" + ".",ep:2,g:427,l:23,wd:true,fm:true,w:1,p:1};
I.u[2274] = {n:"Dark Miner\'s Candle",nn:"DARK MINER\'S CANDLE",t:15,d:"Take an Enutrof. When it\'s dark in the mines, no one can see him. The dark Miner you took this candle off on the other hand... well, he can\'t be seen either since you now have his candle.",ep:1,g:428,l:24,wd:true,fm:true,w:10,p:10000};
I.u[2275] = {n:"Lousy Pig Leather",nn:"LOUSY PIG LEATHER",t:56,d:"The Lousy Pig nearly disappeared from the food chain. People had to mate Boars with Piglets to fill Cania Plains with Lousy Pigs once more. The Lousy Pig is mad at the entire world ever since.",ep:2,g:429,l:40,wd:true,fm:true,w:10,p:1};
I.u[2277] = {n:"Shroud",nn:"SHROUD",t:55,d:"To gain credibility, Ghosts roll themselves up in a Shroud. That\'s one way of frightening lost adventurers.",ep:14,g:430,l:65,wd:true,fm:true,w:1,p:1000};
I.u[2278] = {n:"Invisible Fabric",nn:"INVISIBLE FABRIC",t:55,d:"This fabric once belonged to Mill Burray, hunter of gophers. He spread it out to trap the burrowing creatures and prevent them from tunnelling all over his golf course. The charismatic Captain Chafer took hold of the fabric before disappearing in mysterious circumstances...",ep:1,g:431,l:23,wd:true,fm:true,w:1,p:1};
I.u[2279] = {n:"Pointed Tooth",nn:"POINTED TOOTH",t:47,d:"This tooth once belonged to a Vampire. This can\'t be any other way. We can see that from the very sharp tip of the tooth.",ep:3,g:432,l:15,wd:true,fm:true,w:1,p:1};
I.u[2280] = {n:"Straw",nn:"STRAW",t:15,d:"Never underestimate a Vampire, especially when he is holding a straw; he could suck your blood till his thirst is quenched.",ep:3,g:433,l:22,wd:true,fm:true,w:1,p:1};
I.u[2281] = {n:"Blood Fabric",nn:"BLOOD FABRIC",t:55,d:"This tissue looks like it is alive. Vampires often use it to make beautiful costumes for fancy dress balls.",ep:3,g:434,l:15,wd:true,fm:true,w:1,p:1};
I.u[2282] = {n:"Tuft of White Hair",nn:"TUFT OF WHITE HAIR",t:15,d:"Finally, conclusive evidence that even monsters get stressed out.",ep:1,g:435,l:24,wd:true,fm:true,w:1,p:1};
I.u[2283] = {n:"Kwoan Eye",nn:"KWOAN EYE",t:109,d:"This Kwoan Eye has surprising hypnotic effects. Try to keep a clear head while using it.",ep:3,g:436,l:8,wd:true,fm:true,w:1,p:1};
I.u[2284] = {n:"Harry Stottel\'s Note",nn:"HARRY STOTTEL\'S NOTE",t:24,d:"The entry of the Legendary Golden Larvae Cave is situated a little west of Amakna. The latent powers of your golden larva will allow you to enter the cave. But make sure it doesn\'t get hurt or else it will run to its daddy\'s arms.",ep:2,g:55,l:1,wd:true,fm:true,w:0,p:1};
I.u[2285] = {n:"Dark Tissue",nn:"DARK TISSUE",t:55,d:"One absolute certainty about this fabric is that it\'s itchy. You\'ll understand when I tell you that an Ouginak used to use it as a handkerchief.",ep:3,g:437,l:50,wd:true,fm:true,w:1,p:1};
I.u[2286] = {n:"Bandages",nn:"BANDAGES",t:15,d:"These bandages are revolutionary: triple thickness, ultra-resistant and violet-scented. Shaped for confidence, shaped for comfort.",ep:2,g:438,l:51,wd:true,fm:true,w:1,p:1};
I.u[2287] = {n:"Black Leather",nn:"BLACK LEATHER",t:56,d:"This leather comes ready-treated. Its origin remains unknown but the colour probably comes from a special dye.",ep:1,g:439,l:74,wd:true,fm:true,w:1,p:1};
I.u[2288] = {n:"Wabbit\'s Paw",nn:"WABBIT\'S PAW",t:103,d:"Many tourists come to Wabbit Island especially to find a Wabbit\'s Paw which is supposed to be a lucky charm. It\'s not true unfortunately, but keep it to yourself otherwise tourism could suffer from it.",ep:8,g:440,l:18,wd:true,fm:true,w:1,p:1};
I.u[2290] = {n:"White Scaraleaf Shell",nn:"WHITE SCARALEAF SHELL",t:107,d:"You want a lampshade? You got it! You don\'t want a lampshade? You got nothing.",ep:3,g:441,l:32,wd:true,fm:true,w:1,p:1};
I.u[2291] = {n:"Blue Scaraleaf Shell",nn:"BLUE SCARALEAF SHELL",t:107,d:"You want a lampshade? You got it! You don\'t want a lampshade? You got nothing.",ep:3,g:442,l:32,wd:true,fm:true,w:1,p:1};
I.u[2292] = {n:"Red Scaraleaf Shell",nn:"RED SCARALEAF SHELL",t:107,d:"You want a lampshade? You got it! You don\'t want a lampshade? You got nothing.",ep:3,g:443,l:32,wd:true,fm:true,w:1,p:1};
I.u[2293] = {n:"Green Scaraleaf Shell",nn:"GREEN SCARALEAF SHELL",t:107,d:"You want a lampshade? You got it! You don\'t want a lampshade? You got nothing.",ep:3,g:444,l:32,wd:true,fm:true,w:1,p:1};
I.u[2294] = {n:"Scaraleaf Entrails",nn:"SCARALEAF ENTRAILS",t:15,d:"At first sight these entrails do not look particularly mouth-watering. However, if you chop them finely with some sun-dried herbs, some wild mint and then braise them in a rusty cauldron, they\'ll make a satisfying meal for discerning Chafers.",ep:3,g:445,l:32,wd:true,fm:true,w:1,p:1};
I.u[2295] = {n:"Reptilian Sword Fragment",nn:"REPTILIAN SWORD FRAGMENT",t:15,d:"With this Fragment of Sword hung above the fireplace, you have at last a good reason to show off. You have defeated the Crocodyl after all, just as the eccentric Mikool J., also known as Crocodyl Dandi, did before you.",ep:4,g:446,l:69,wd:true,fm:true,w:10,p:1};
I.u[2296] = {n:"Reptilian Shield",nn:"REPTILIAN SHIELD",t:15,d:"How can you use such a beautiful, jewel-decorated shield? While you search for the answer, you can use it as a tea tray...",ep:4,g:447,l:69,wd:true,fm:true,w:15,p:1};
I.u[2297] = {n:"Crossbow Bolt",nn:"CROSSBOW BOLT",t:15,d:"This Bolt can be used to make a short but sharp weapon. It can also serve as a toothpick. It all depends where your priorities lie.",ep:1,g:125,l:34,wd:true,fm:true,w:1,p:1};
I.u[2299] = {n:"Dark Vlad\'s Epaulette",nn:"DARK VLAD\'S EPAULETTE",t:15,d:"This epaulette has survived many long battles... It\'s stronger than it looks. It\'ll make you think twice about hitting Dark Vlad on the shoulder if you\'re attached to your weapon at the time.",ep:2,g:449,l:120,wd:true,fm:true,w:5,p:80};
I.u[2300] = {n:"Bone Fragment",nn:"BONE FRAGMENT",t:47,d:"This fragment of bone has either come from the remains of a Chafer, or from the leftovers of a Moowolf meal. There\'s also a large chance that it hasn\'t got anything to do with either one.",ep:9,g:450,l:43,wd:true,fm:true,w:1,p:1};
I.u[2301] = {n:"Kolerat Whiskers",nn:"KOLERAT WHISKERS",t:15,d:"Not very resistant, these whiskers will enable you to show off in front of your friends...by imitating Salv and Torh Bali,  Great Brakmarian Painters, for example.",ep:2,g:451,l:9,wd:true,fm:true,w:1,p:1};
I.u[2302] = {n:"Crab Tongue",nn:"CRAB TONGUE",t:15,d:"Several illustrious Amakna think tanks are solely occupied with finding out what use the crab could possibly make of its little, crabby tongue. They would be shocked if they knew...",ep:1,g:452,l:4,wd:true,fm:true,w:1,p:1};
I.u[2303] = {n:"Empty Crab Shell",nn:"EMPTY CRAB SHELL",t:107,d:"Once emptied, this crab shell can hold a good many items. The problem is you\'ll never be able to get rid of the crabby smell. People have lost friends over much less...",ep:1,g:453,l:4,wd:true,fm:true,w:2,p:1};
I.u[2304] = {n:"Polished Stone Fragment",nn:"POLISHED STONE FRAGMENT",t:51,d:"This fragment of polished stone comes from the left buttock of a Plain Crackler. For squashing Arachnees, there\'s nothing finer, except for the Crackler itself, perhaps.",ep:5,g:454,l:45,wd:true,fm:true,w:5,p:1};
I.u[2305] = {n:"Pointed Stone Fragment",nn:"POINTED STONE FRAGMENT",t:51,d:"This fragment of pointed stone comes from the right shoulder blade of a Plain Crackler. Or maybe it\'s a part of the lower abdomen. Anyway, just be careful where you put it.",ep:5,g:455,l:45,wd:true,fm:true,w:5,p:1};
I.u[2306] = {n:"Crackler Heart",nn:"CRACKLER HEART",t:51,d:"Heart of stone, don\'t you see the love I feel for you? But if you go on, I\'ll leave you all alone, I won\'t let myself be tired out by you.",ep:5,g:456,l:45,wd:true,fm:true,w:5,p:1};
I.u[2307] = {n:"Chafer Lancer\'s Shield",nn:"CHAFER LANCER\'S SHIELD",t:24,d:"...",ep:2,g:75,l:1,wd:true,fm:true,w:0,p:1};
I.u[2308] = {n:"Brakmarian Badge",nn:"BRAKMARIAN BADGE",t:24,d:"This badge allows warriors who have fallen in battle to be identified. Apparently some people collect them in Bonta.",ep:2,g:76,l:1,wd:true,fm:true,w:0,p:1};
I.u[2309] = {n:"Red Candle",nn:"RED CANDLE",t:24,d:"This red candle is unlike any other.",ep:1,g:77,l:1,wd:true,fm:true,w:0,p:1};
I.u[2310] = {n:"Green Candle",nn:"GREEN CANDLE",t:24,d:"...",ep:2,g:78,l:1,wd:true,fm:true,w:0,p:1};
I.u[2311] = {n:"Blue Candle",nn:"BLUE CANDLE",t:24,d:"...",ep:2,g:79,l:1,wd:true,fm:true,w:0,p:1};
I.u[2312] = {n:"Blood Powder",nn:"BLOOD POWDER",t:24,d:"...",ep:2,g:80,l:1,wd:true,fm:true,w:0,p:1};
I.u[2313] = {n:"Aquatic Respirator",nn:"AQUATIC RESPIRATOR",t:24,d:"...",ep:2,g:81,l:1,wd:true,fm:true,w:0,p:1};
I.u[2314] = {n:"Jiva\'s Flask",nn:"JIVA\'S FLASK",t:24,d:"...",ep:2,g:82,l:1,wd:true,fm:true,w:0,p:1};
I.u[2315] = {n:"Whitish Fang Fur",nn:"WHITISH FANG FUR",t:54,d:"While the tiny, but beautiful, Midgette Bardot ruled the land, a strict ban was imposed on the hunting of Whitish Fangs. Now she\'s in exile, and it\'s hunting season all year round! The tailors are crying out for skins... Make sure you slaughter at least one Whitish Fang - before they disappear forever.",ep:1,g:196,l:19,wd:true,fm:true,w:1,p:1};
I.u[2316] = {n:"Yellow Fang",nn:"YELLOW FANG",t:47,d:"The Yellow Fang of the Whitish Fang has sent many an adventurer fleeing off in terror, don\'t underestimate this tooth.",ep:1,g:458,l:19,wd:true,fm:true,w:1,p:1};
I.u[2317] = {n:"Wolf Ears",nn:"WOLF EARS",t:106,d:"When you see what kind of filth and bugs the Whitish Fang has in his ears, you wonder how it hears a thing at all.",ep:1,g:459,l:19,wd:true,fm:true,w:1,p:1};
I.u[2318] = {n:"Bear Paw",nn:"BEAR PAW",t:103,d:"Some Goblins wear a Bear paw around their neck. Nobody recalls the origin or meaning of this superstition.",ep:1,g:460,l:12,wd:true,fm:true,w:1,p:1};
I.u[2320] = {n:"Ouginak Collar",nn:"OUGINAK COLLAR",t:15,d:"A message is inscribed on this Ouginak collar: " + "\"" + "If lost, please return me to my Brakmarian master on Sidimote Moor, who will reward you with a punch in the face." + "\"" + "",ep:14,g:462,l:51,wd:true,fm:true,w:5,p:1};
I.u[2321] = {n:"Ouginak Chops",nn:"OUGINAK CHOPS",t:15,d:"Did it cross your mind that these Ouginak Chops could slobber for all eternity? Well, the scary part is they probably can - Ouginaks are well-known for their everlasting gluttony.",ep:14,g:463,l:51,wd:true,fm:true,w:1,p:1};
I.u[2322] = {n:"Rat Fang",nn:"RAT FANG",t:47,d:"Don\'t be fooled, these small toothy-pegs are actually real sharp, stinky, used fangs. Sometimes unscrupulous Enutrof dentists replace teeth with them.",ep:1,g:464,l:50,wd:true,fm:true,w:1,p:1};
I.u[2323] = {n:"Spinal Cord",nn:"SPINAL CORD",t:47,d:"This is some good quality cord. Heavy though... You\'d be amazed how many people put their backs out just picking it up!",ep:1,g:465,l:34,wd:true,fm:true,w:2,p:1};
I.u[2324] = {n:"Watermelon",nn:"WATERMELON",t:46,d:"Unlike the Lolwut Pear, the Lol Wutermelon is made of fail.",ep:1,g:466,l:1,wd:true,fm:true,w:2,p:1};
I.u[2325] = {n:"Banana",nn:"BANANA",t:46,d:"The famous minstrel Jon Lemon once composed an ode to this fruit: " + "\"" + "Happiness is a warm Banana" + "\"" + ".",ep:1,g:467,l:1,wd:true,fm:true,w:1,p:1};
I.u[2326] = {n:"Orange",nn:"ORANGE",t:46,d:"They say that oranges are not the only fruit...",ep:1,g:468,l:1,wd:true,fm:true,w:1,p:1};
I.u[2327] = {n:"Apple",nn:"APPLE",t:46,d:"An apple a day keeps apple farmers in business.",ep:1,g:469,l:1,wd:true,fm:true,w:1,p:1};
I.u[2328] = {n:"Small Golden Pearl",nn:"SMALL GOLDEN PEARL",t:15,d:"This golden pearl is a gift from the small larva you have accompanied. Consequently, it has a great sentimental value... and some monetary value too. Its softness and colour make it a perfect gift for every person who has even the slightest feminine side.",ep:5,g:483,l:1,wd:true,fm:true,w:1,p:1};
I.u[2329] = {n:"Pear",nn:"PEAR",t:46,d:"This juicy Lolwut pear is made of win.",ep:1,g:470,l:1,wd:true,fm:true,w:1,p:1};
