I.u[1877] = {n:"Tattered Hood",nn:"TATTERED HOOD",t:24,d:"The former owner of this hat probably took a few blows too many to the head, which tends to happen when you rebel against Bonta\'s authority.",ep:1,g:42,l:1,wd:true,fm:true,w:0,p:1};
I.u[1878] = {n:"Special Playing Card",nn:"SPECIAL PLAYING CARD",t:24,d:"This playing card has a particular appearance close to perfection. It is apparently a collector\'s card.",ep:2,g:43,l:1,wd:true,fm:true,w:0,p:1};
I.u[1879] = {n:"Crate of Edged Weapons",nn:"CRATE OF EDGED WEAPONS",t:24,d:"A crate of edged weapons, full of all kinds of blunt instruments.",ep:2,g:44,l:1,wd:true,fm:true,w:500,p:1};
I.u[1880] = {n:"Kittycare Tin",nn:"KITTYCARE TIN",t:24,d:"Yum! Kittycare! Your Kaniger will leap over the moon with delight.",ep:2,g:45,l:1,wd:true,fm:true,w:0,p:1};
I.u[1881] = {n:"Mud",nn:"MUD",t:24,d:"That\'s that taken care of.",ep:2,g:46,l:1,wd:true,fm:true,w:2,p:1};
I.u[1882] = {n:"Crushed Blop Flowers",nn:"CRUSHED BLOP FLOWERS",t:24,d:"You have picked up a very appetizing Pulpy substance.",ep:2,g:47,l:1,wd:true,fm:true,w:0,p:1};
I.u[1883] = {n:"Repaired Necklace",nn:"REPAIRED NECKLACE",t:24,d:"The necklace the Mayor\'s daughter lost, as good as new...",ep:2,g:48,l:1,wd:true,fm:true,w:0,p:1};
I.u[1889] = {n:"Crow Feather",nn:"CROW FEATHER",t:53,d:"Sharpened into quill pens for years, Crow feathers are now used to adorn the rich and famous. No wonder crows are nervous creatures - anyone who knows the value of these feathers will go a long way to get their hands on them.",ep:1,g:327,l:20,wd:true,fm:true,w:1,p:1};
I.u[1890] = {n:"Kaniger Hair",nn:"KANIGER HAIR",t:54,d:"The hair of the Kaniger reflects the sun in a strange way, as if it was absorbing some of the rays.",ep:13,g:359,l:70,wd:true,fm:true,w:1,p:1};
I.u[1891] = {n:"Plisskenskin",nn:"PLISSKENSKIN",t:59,d:"Have you ever dreamt of a magnificent Plissken leather bag? Never? Good on you! It\'s not that resistant anyway!",ep:2,g:361,l:10,wd:true,fm:true,w:1,p:1};
I.u[1892] = {n:"Lord Crow Feather",nn:"LORD CROW FEATHER",t:53,d:"Panic-stricken, Lord Crow spread its wings to fly off and let this feather fall. Nothing special there and yet...",ep:13,g:362,l:160,wd:true,fm:true,w:1,p:1};
I.u[1893] = {n:"Scurvion Tail",nn:"SCURVION TAIL",t:65,d:"A fantastic trophy, it\'s a pity it oozes smelly fluid.",ep:14,g:363,l:36,wd:true,fm:true,w:1,p:1};
I.u[1894] = {n:"Plain Boar Hair",nn:"PLAIN BOAR HAIR",t:54,d:"This type of hair is really disgusting. If you have a closer look at it, you\'ll discover a great microcosm of different parasites living on it... How lucky you are !",ep:13,g:364,l:44,wd:true,fm:true,w:1,p:1};
I.u[1895] = {n:"Feubuk Beer",nn:"FEUBUK BEER",t:37,d:"You\'ll recover a few Health Points with this highly alcoholic beer.",ep:16,g:4,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:4};
I.u[1896] = {n:"Bird Meat",nn:"BIRD MEAT",t:63,d:"This Tofu leg is proof that hunters can show, although very infrequently, a tiny sunbeam of sensitivity.",ep:1,g:348,l:1,wd:true,fm:true,w:2,p:1};
I.u[1897] = {n:"Bird Meat **",nn:"BIRD MEAT **",t:63,d:"Tiny Kwak legs for maximum Kwak-taste.",ep:1,g:349,l:40,wd:true,fm:true,w:2,p:1};
I.u[1898] = {n:"Larva Flesh",nn:"LARVA FLESH",t:63,d:"This yellow and putrid heap represents the true nature of the Larva. Now you understand why their skin is so tough.",ep:1,g:354,l:1,wd:true,fm:true,w:2,p:1};
I.u[1899] = {n:"Larva Flesh **",nn:"LARVA FLESH **",t:63,d:"This yellow and putrid heap represents the true nature of the Larva. Now you understand why their skin is so tough.",ep:1,g:354,l:9,wd:true,fm:true,w:2,p:1};
I.u[1900] = {n:"Larva Flesh ***",nn:"LARVA FLESH ***",t:63,d:"This yellow and putrid heap represents the true nature of the Larva. Now you understand why their skin is so tough.",ep:1,g:354,l:10,wd:true,fm:true,w:2,p:1};
I.u[1901] = {n:"Wabbit Leg ****",nn:"WABBIT LEG ****",t:63,d:"This leg is simply perfect. You just can\'t help eyeing it up greedily for a while before eating it.",ep:2,g:368,l:80,wd:true,fm:true,w:2,p:1};
I.u[1902] = {n:"Wabbit Leg",nn:"WABBIT LEG",t:63,d:"This cawwot-flavoured Wabbit leg is ideal for stew.",ep:8,g:365,l:20,wd:true,fm:true,w:2,p:1};
I.u[1903] = {n:"Wabbit Leg **",nn:"WABBIT LEG **",t:63,d:"A good muscular leg which will make a delightful meal.",ep:8,g:366,l:40,wd:true,fm:true,w:2,p:1};
I.u[1904] = {n:"White Dreggheadgear",nn:"WHITE DREGGHEADGEAR",t:16,d:"A White Dreggon Shell padded with leather and wool, this headgear will protect you against basic attacks and the lightning bolts of a stumbling, drunk Bwork Magus. You\'ll also look like you just came out of a Dreggon\'s backside.",ep:11,g:28,l:39,wd:true,fm:true,w:50,p:4200};
I.u[1905] = {n:"Wabbit Leg ***",nn:"WABBIT LEG ***",t:63,d:"A big leg which will fill you up!",ep:8,g:367,l:60,wd:true,fm:true,w:2,p:1};
I.u[1907] = {n:"Bunny Ears",nn:"BUNNY EARS",t:16,d:"These ears will accentuate your bright smile and get the opposite sex spreading rumours about you...",ep:1,g:30,l:30,wd:true,fm:true,w:9,p:2600};
I.u[1908] = {n:"Chafer Helmet",nn:"CHAFER HELMET",t:16,d:"Even Spartapine the Great Reaper doesn\'t have such a helmet, the precious stones and metals used to make it ensure your noggin will be kept safe and sound inside..",ep:13,g:31,l:50,wd:true,fm:true,w:18,p:6500};
I.u[1910] = {n:"Ouginak Cloak",nn:"OUGINAK CLOAK",t:17,d:"This cloak is made from the body parts of an Ouginak, it still has the smell of the creature\'s humid nose. Personally, its taste reminds me of a horny set of Gobball\'s udders... Well, someone told me that... damn...",ep:14,g:33,l:50,wd:true,fm:true,w:10,p:6650};
I.u[1911] = {n:"Gobbly Leg",nn:"GOBBLY LEG",t:63,d:"The smell off this small leg disappears when cooked.",ep:1,g:334,l:10,wd:true,fm:true,w:2,p:1};
I.u[1912] = {n:"Gobball Leg **",nn:"GOBBALL LEG **",t:63,d:"This leg reeks of death... I assure you it was cleared of all involvement in the murder of its owner... Just smells like... Death.",ep:1,g:335,l:20,wd:true,fm:true,w:2,p:1};
I.u[1913] = {n:"Gobball Leg ***",nn:"GOBBALL LEG ***",t:63,d:"This is the leg of a Gobball War Chief. As solid as a Crackler, it could be a very powerful bludgeon, if you can stand the smell...",ep:1,g:336,l:40,wd:true,fm:true,w:2,p:1};
I.u[1914] = {n:"Gobball Leg ****",nn:"GOBBALL LEG ****",t:63,d:"Tender but tough, not too fatty or too dry, this leg improves with age like a good wine.",ep:1,g:337,l:50,wd:true,fm:true,w:2,p:1};
I.u[1915] = {n:"Insect Flesh",nn:"INSECT FLESH",t:63,d:"Don\'t underestimate the value of this slimy and suffocating flesh... It\'s nutritional value, that is.",ep:1,g:370,l:1,wd:true,fm:true,w:2,p:1};
I.u[1916] = {n:"Insect Flesh **",nn:"INSECT FLESH **",t:63,d:"Similar to Moskito flesh except there is ten times more of it. Fortunately, some people like it.",ep:3,g:370,l:50,wd:true,fm:true,w:4,p:1};
I.u[1917] = {n:"Pork Loin",nn:"PORK LOIN",t:63,d:"This choice morsel of boar is quite bloody for the moment but it will make a great meal.",ep:1,g:331,l:30,wd:true,fm:true,w:3,p:1};
I.u[1918] = {n:"Pork Loin **",nn:"PORK LOIN **",t:63,d:"A bit bigger for a bit more happiness, this skilfully carved lump of Plain Boar will delight all kinds of carnivores.",ep:13,g:331,l:50,wd:true,fm:true,w:3,p:1};
I.u[1919] = {n:"Pork Loin ****",nn:"PORK LOIN ****",t:63,d:"That piece of Dragon Pig is so tender and soft that you could make a perfect cushion with it. But it\'s still nicer to eat it.",ep:2,g:331,l:80,wd:true,fm:true,w:4,p:1};
I.u[1920] = {n:"Bontarian Badge",nn:"BONTARIAN BADGE",t:24,d:"This badge is used to identify militiamen who have fallen on battlefields. Apparently some people collect them in Brakmar.",ep:2,g:49,l:1,wd:false,fm:true,w:0,p:1};
I.u[1921] = {n:"Pork Loin ***",nn:"PORK LOIN ***",t:63,d:"A deliciously tough loin of pork from an athletic pig.",ep:1,g:331,l:60,wd:true,fm:true,w:3,p:1};
I.u[1922] = {n:"Dragomeat",nn:"DRAGOMEAT",t:63,d:"Rare high-quality meat, it remains hot hours after roasting. It\'s very convenient for picnics.",ep:2,g:371,l:40,wd:true,fm:true,w:2,p:1};
I.u[1923] = {n:"Dragomeat **",nn:"DRAGOMEAT **",t:63,d:"This dragomeat, on top of remaining hot, has a  naturally smoky taste.",ep:2,g:371,l:60,wd:true,fm:true,w:2,p:1};
I.u[1924] = {n:"Dragomeat ***",nn:"DRAGOMEAT ***",t:63,d:"Dreggon Dragomeat is a bit hard to chew but is far more nourishing than standard Dragomeat. Some say it has magic properties.",ep:11,g:371,l:70,wd:true,fm:true,w:2,p:1};
I.u[1925] = {n:"A Day Leave",nn:"A DAY LEAVE",t:24,d:"This pass gives you a day leave.",ep:2,g:50,l:1,wd:true,fm:true,w:0,p:1};
I.u[1926] = {n:"Dragomeat ****",nn:"DRAGOMEAT ****",t:63,d:"The meat of the Dragon Pig\'s dragon parts (nobody knows their exact location, nor does anybody want to try) is a godsend. Which makes sense given that Dragon Pigs can fly.",ep:2,g:371,l:80,wd:true,fm:true,w:2,p:1};
I.u[1927] = {n:"Muzzle",nn:"MUZZLE",t:63,d:"This muzzle of Miliboowolf, with its small firm chops, will perfectly accompany all your salads.",ep:1,g:372,l:30,wd:true,fm:true,w:1,p:1};
I.u[1928] = {n:"Aggressive Boar Tusk",nn:"AGGRESSIVE BOAR TUSK",t:24,d:"The extraction of this Plain Boar tusk certainly required a great deal of know how.",ep:2,g:51,l:1,wd:true,fm:true,w:0,p:1};
I.u[1929] = {n:"Muzzle **",nn:"MUZZLE **",t:63,d:"This dainty muzzle is synonymous with a lack of will. Consume without moderation.",ep:1,g:373,l:50,wd:true,fm:true,w:2,p:1};
I.u[1930] = {n:"Muzzle ***",nn:"MUZZLE ***",t:63,d:"This tasty meat must be eaten in moderation. There\'s a reason the Moowolf has such terribly bad breath!",ep:2,g:374,l:70,wd:true,fm:true,w:2,p:1};
I.u[1931] = {n:"Nicked Katana",nn:"NICKED KATANA",t:24,d:"It\'s Amayiro\'s weapon. You can easily handle it but it seems to be slightly nicked.",ep:2,g:52,l:1,wd:true,fm:true,w:0,p:1};
I.u[1932] = {n:"Repaired Katana",nn:"REPAIRED KATANA",t:24,d:"This weapon, which used to be nicked, is now ready to cut whatever you want. Amayiro won\'t be disappointed.",ep:2,g:53,l:1,wd:true,fm:true,w:0,p:1};
I.u[1933] = {n:"Bird Meat ****",nn:"BIRD MEAT ****",t:63,d:"This small piece of crow is impregnated with powerful magic. Lord Crow himself is responsible for its power.",ep:13,g:350,l:30,wd:true,fm:true,w:2,p:1};
I.u[1934] = {n:"Hunting Knife",nn:"HUNTING KNIFE",t:20,d:"This knife is the typical hallmark of the hunter. The brighter, sharper and bigger the blades are, the better-looking the hunters are... according to themselves at least...",ep:1,g:14,l:1,wd:true,fm:true,w:2,an:13,e:[10,3,1,1,25,30,false,false],c:"PJ>41,0",p:1};
I.u[1935] = {n:"Cheque for 100,000 Kamas",nn:"CHEQUE FOR 100,000 KAMAS",t:24,d:"Whoever pays his debts grows rich ok, but this is quite a sum! It would be better to avoid such debt, your children will be grateful...",ep:1,g:54,l:1,wd:true,fm:true,w:0,p:1};
I.u[1936] = {n:"Hunting Axe",nn:"HUNTING AXE",t:20,d:"This axe is especially designed to quickly cut off any part of an animal, living or dead.",ep:1,g:15,l:1,wd:true,fm:true,w:5,an:17,e:[10,4,1,1,30,40,false,false],c:"PJ>41,9",p:1000};
I.u[1937] = {n:"Spying Report",nn:"SPYING REPORT",t:24,d:"This report written by Bishom Pot must contain lots of information about the daily activities of the Brakmarians.",ep:2,g:55,l:1,wd:true,fm:true,w:0,p:1};
I.u[1938] = {n:"Hunting Blade",nn:"HUNTING BLADE",t:20,d:"This blade has been designed for hunting purposes only. It is quite delicate to handle. Nevertheless, it can be of a great help to skilled hunters.",ep:3,g:16,l:1,wd:true,fm:true,w:2,an:14,e:[5,4,1,1,40,40,false,false],c:"PJ>41,29",p:5000};
I.u[1939] = {n:"Hunting Spear",nn:"HUNTING SPEAR",t:20,d:"This spear is really useful when you fight groups of monsters. Nevertheless, it requires some talent to be handled with dexterity.",ep:3,g:17,l:1,wd:true,fm:true,w:2,an:12,e:[7,5,1,1,30,50,false,false],c:"PJ>41,49",p:10000};
I.u[1940] = {n:"Hunting Hammer",nn:"HUNTING HAMMER",t:20,d:"This hammer was designed by the very best hammer engineers so that it would knock its victims out instead of killing them. It\'s great for hunting, but not at all for staying alive.",ep:1,g:18,l:1,wd:true,fm:true,w:2,an:15,e:[5,6,1,1,50,50,false,false],c:"PJ>41,69",p:20000};
I.u[1941] = {n:"Hunting Shovel",nn:"HUNTING SHOVEL",t:20,d:"This shovel is particularly recommended for killing monsters without damaging them. A tool for experienced hunters only.",ep:3,g:9,l:1,wd:true,fm:true,w:2,an:16,e:[7,4,1,1,30,50,false,false],c:"PJ>41,29",p:5000};
I.u[1942] = {n:"Hunting Bow",nn:"HUNTING BOW",t:20,d:"The hunting bow is considered as the safest hunting weapon. A maximum of damage for a minimum of marks, without exposing yourself.",ep:3,g:10,l:1,wd:true,fm:true,w:2,an:10,tw:true,e:[7,5,1,6,35,40,false,false],c:"PJ>41,29",p:5000};
I.u[1943] = {n:"Hunting Wand",nn:"HUNTING WAND",t:20,d:"This wand will burn the animal in question perfectly... The meat is preserved and pre-cooked. Essential to warriors with an aggressive appetite.",ep:3,g:11,l:1,wd:true,fm:true,w:2,an:11,e:[5,3,2,4,30,50,false,false],c:"PJ>41,29",p:5000};
I.u[1944] = {n:"Pellet Sling",nn:"PELLET SLING",t:20,d:"This weapon will launch a small copper ball in any direction you want. Its efficiency is frightening but its noise limits its usefulness to hunting only.",ep:1,g:21,l:1,wd:false,fm:true,w:2,an:10,e:[5,3,1,10,40,40,false,false],c:"PJ>41,99",p:50000};
I.u[1945] = {n:"Butcher Cleaver",nn:"BUTCHER CLEAVER",t:20,d:"This big knife can carve any type of meat.",ep:1,g:12,l:1,wd:true,fm:true,w:2,an:14,e:[10,4,1,1,25,50,false,false],p:200};
I.u[1946] = {n:"Fishmonger Cleaver",nn:"FISHMONGER CLEAVER",t:20,d:"This huge knife can cut up any type of fish.",ep:1,g:13,l:1,wd:true,fm:true,w:2,an:14,e:[10,4,1,1,25,50,false,false],p:200};
