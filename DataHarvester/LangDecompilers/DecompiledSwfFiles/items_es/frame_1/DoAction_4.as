I.u[1728] = {n:"Miaumiau",nn:"MIAUMIAU",t:18,d:"Esta pequeña bola de pelo bribona y cariñosa ha engatusado a más de una chica sin distinción de clases. Es por lo tanto un temible animal.",ep:1,g:1,l:1,wd:true,fm:true,w:10,c:"PZ=1",p:1};
I.u[1729] = {n:"Anillo Mer",nn:"ANILLO MER",t:9,d:"Este anillo pertenecía a Mer, el más vago de toda la comarca de Amakna. Este rendía culto a los dunoths, pero también a las costilletas de ribs.",ep:7,g:28,l:25,wd:true,fm:true,w:25,c:"CI<100&CC<100",p:1900};
I.u[1730] = {n:"Medida de sal",nn:"MEDIDA DE SAL",t:48,d:"Esta medida de sal es la dosis ideal para salar cantidad de platos sabrosos. Es indispensable para hacer una buena comida.",ep:1,g:261,l:1,wd:true,fm:true,w:1,p:1};
I.u[1731] = {n:"Dosis de zumo sabroso",nn:"DOSIS DE ZUMO SABROSO",t:15,d:"Este zumo te proporcionará los sabores elementales para conseguir un plato perfumado.",ep:1,g:262,l:1,wd:true,fm:true,w:1,p:4};
I.u[1732] = {n:"Bol",nn:"BOL",t:15,d:"¡El no va más de la tecnología! ¡¡Totalmente a prueba de líquidos como la sopa, el agua, o incluso la leleche. Su exclusiva función multiuso permite convertirlo en casco* dándole la vuelta!! \n*Función totalmente decorativa, el artesano no se responsabiliza de posibles daños físicos en caso de mala utilización.",ep:16,g:263,l:1,wd:true,fm:true,w:1,p:6};
I.u[1733] = {n:"Tarro",nn:"TARRO",t:15,d:"" + "\"" + "El tarro está lleno de carne triturada, rico en proteínas, en grasas y en colesterol. ¡Te vas a chupar los dedos!" + "\"" + " (según el Himno del Carnicero, estrofa 4).",ep:16,g:264,l:1,wd:true,fm:true,w:1,p:6};
I.u[1734] = {n:"Cereza",nn:"CEREZA",t:46,d:"Esta fruta se parece mucho a un par de bolas rebotadoras.",ep:1,g:265,l:1,wd:true,fm:true,w:1,p:1};
I.u[1735] = {n:"Certificado de admisión en la petrera: miaumiau",nn:"CERTIFICADO DE ADMISION EN LA PETRERA: MIAUMIAU",t:77,d:"Este certificado describe las características del miaumiau que has dejado en la petrera.",ep:3,g:24,l:1,wd:false,fm:true,w:1,p:100};
I.u[1736] = {n:"Limón",nn:"LIMON",t:46,d:"El zumo de esta fruta es perfecto para ocultar los olores a carne podrida. Perfecto para servirles restos de pescado caducado a tus invitados...",ep:1,g:266,l:1,wd:true,fm:true,w:1,p:2};
I.u[1737] = {n:"Pan de los Campos",nn:"PAN DE LOS CAMPOS",t:33,d:"Que se encuentre en el bolsillo o en el estómago, este pan de los campos es un pan muy pesado. Pero hay que confesar que su sabor tiene el toque rústico de un buen pan.",ep:1,g:68,l:40,wd:true,fm:true,w:3,u:true,ut:true,p:1};
I.u[1738] = {n:"Pan de las Urbes",nn:"PAN DE LAS URBES",t:33,d:"Este pan procura, bastante rápido, una agradable sensación de saciedad.",ep:1,g:69,l:10,wd:true,fm:true,w:2,u:true,ut:true,p:1};
I.u[1739] = {n:"Varita Fogosa",nn:"VARITA FOGOSA",t:3,d:"Esta varita tiene extrañas propiedades: cuando la tocas casi quema. Pero lo más enigmático reside en su olor a ceniza que deja entrever que su madera ha sobrevivido al incendio de algún bosque.",ep:13,g:24,l:80,wd:true,fm:true,w:1,an:11,e:[5,4,2,3,40,50,false,true],c:"CI>60&CV>40",p:1};
I.u[1740] = {n:"Transformación Cocobur",nn:"TRANSFORMACION COCOBUR",t:27,d:"El Cocobur encierra la esencia de Cocoburio y sólo puede ser robado a su amo quitándole la vida.",ep:4,g:2,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[1741] = {n:"Sangre del fénix",nn:"SANGRE DEL FENIX",t:14,d:"Esta sangre de fénix permite resucitar un fantasma y darle algunos puntos de energía.",ep:18,g:3,l:10,wd:true,fm:true,w:1,c:"Pg=23",ut:true,p:1000};
I.u[1742] = {n:"Sangre de fénix de gran cosecha",nn:"SANGRE DE FENIX DE GRAN COSECHA",t:14,d:"Esta sangre de fénix permite resucitar un fantasma y darle un gran número de puntos de energía.",ep:18,g:3,l:50,wd:true,fm:true,w:1,c:"Pg=23",ut:true,p:2500};
I.u[1744] = {n:"Bola de pelos de Torcha",nn:"BOLA DE PELOS DE TORCHA",t:24,d:"Esta bola de pelos demuestra tu victoria frente a Torche. Una hermosura.",ep:1,g:30,l:1,wd:true,fm:true,w:0,p:1};
I.u[1745] = {n:"Jeringuilla de Veterinario",nn:"JERINGUILLA DE VETERINARIO",t:24,d:"Una aguja puntiaguda, un depósito de líquidos, no hay duda alguna, esto es una jeringuilla... Quedas avisado.",ep:1,g:31,l:1,wd:true,fm:true,w:0,p:1};
I.u[1746] = {n:"Sangre de Wabbit GM",nn:"SANGRE DE WABBIT GM",t:24,d:"¿Quién hubiera imaginado llegar junto a los wabbits con una jeringuilla, para extraer sangre sobre la criatura más genéticamente modificada de la isla? Tú, claro.",ep:8,g:32,l:1,wd:true,fm:true,w:0,p:1};
I.u[1747] = {n:"Análisis de sangre de Wabbit GM",nn:"ANALISIS DE SANGRE DE WABBIT GM",t:24,d:"Los análisis son formales, la sangre identificada procede de un posterior de Wabbit GM. Las pruebas han sido tan profundizadas que el alquimista hasta supo decirte qué nalga se había pinchado, en este caso la derecha.",ep:5,g:33,l:1,wd:true,fm:true,w:0,p:1};
I.u[1748] = {n:"Wabbit",nn:"WABBIT",t:18,d:"Este pequeño Wabbit está domesticado, por lo cual no te morderá. En cambio los demás...",ep:8,g:7,l:1,wd:true,fm:true,w:10,c:"PZ=1",s:11,p:1};
I.u[1749] = {n:"Ficha de la Feria del Trool",nn:"FICHA DE LA FERIA DEL TROOL",t:15,d:"Has comprado esta ficha a Parissil Trool, un hombre que sueña con abrir un lugar de diversión para todos (Llanura de Cania).",ep:13,g:737,l:1,wd:true,fm:true,w:1,p:1};
I.u[1750] = {n:"Pescado empanado",nn:"PESCADO EMPANADO",t:41,d:"Este pescado cúbico es famoso porque siempre cuadra bien. Además es tan tonto que resulta fácil de pescar.",ep:2,g:269,l:1,wd:true,fm:true,w:3,p:10};
I.u[1751] = {n:"Pescado empanado destripado",nn:"PESCADO EMPANADO DESTRIPADO",t:62,d:"Este pescado empanado ahora se puede utilizar como ingrediente.",ep:2,g:270,l:1,wd:true,fm:true,w:2,p:1};
I.u[1752] = {n:"Pescado empanado frito **",nn:"PESCADO EMPANADO FRITO **",t:49,d:"Este sabroso rectángulo se compone con las mejores y más ligeras partes de varios antiguos pescados cúbicos. A los niños les encanta su sabor, pero sus valores nutritivos no son muy altos.",ep:2,g:71,l:1,wd:true,fm:true,w:3,u:true,ut:true,p:60};
I.u[1753] = {n:"Pescado empanado frito ***",nn:"PESCADO EMPANADO FRITO ***",t:49,d:"Este sabroso rectángulo se compone con las mejores y más ligeras partes de varios antiguos pescados cúbicos. A los niños les encanta su sabor, pero sus valores nutritivos no son muy altos.",ep:2,g:72,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:100};
I.u[1754] = {n:"Pez iglú",nn:"PEZ IGLU",t:41,d:"Esta especie rara de pescado empanado suele vivir en las aguas heladas del Gran Norte, por eso se llama así. Gracias a su alimentación polar, su carne tiene muchas fibras.",ep:1,g:271,l:1,wd:true,fm:true,w:3,p:100};
I.u[1755] = {n:"Pez iglú destripado",nn:"PEZ IGLU DESTRIPADO",t:62,d:"Este pez iglú ahora se puede utilizar como ingrediente.",ep:1,g:272,l:1,wd:true,fm:true,w:2,p:100};
I.u[1756] = {n:"Pescado empanado frito ****",nn:"PESCADO EMPANADO FRITO ****",t:49,d:"Este pescado Igloo frito, además de tener buen sabor, posee increíbles valores energéticos.",ep:1,g:73,l:1,wd:true,fm:true,w:1,c:"Ci<10",u:true,ut:true,p:10000};
I.u[1757] = {n:"Cangrejo Surimi",nn:"CANGREJO SURIMI",t:41,d:"Este cangrejo, con su forma tan particular, es muy apreciado por la gran cantidad de carne que contiene. También es el único cangrejo que se puede pescar con una caña ya que instintivamente siempre intenta cortar los hilos con sus pinzas... que no cortan.",ep:2,g:273,l:1,wd:true,fm:true,w:3,p:1};
I.u[1758] = {n:"Cangrejo Surimi destripado",nn:"CANGREJO SURIMI DESTRIPADO",t:62,d:"Este cangrejo surimi ahora se puede utilizar como ingrediente.",ep:2,g:274,l:1,wd:true,fm:true,w:2,p:1};
I.u[1759] = {n:"Cangrejo Surimi exótico",nn:"CANGREJO SURIMI EXOTICO",t:41,d:"Primo del Cangrejo Surimi, éste es mucho más inteligente. Eso explica su rareza. Sin embargo, después de algunas fiestas alcoholizadas puede que su instinto vuelva a hacer hablar de él.",ep:1,g:275,l:1,wd:true,fm:true,w:3,p:20};
I.u[1760] = {n:"Cangrejo Surimi exótico destripado",nn:"CANGREJO SURIMI EXOTICO DESTRIPADO",t:62,d:"Este cangrejo exótico ahora se puede utilizar como ingrediente.",ep:1,g:276,l:1,wd:true,fm:true,w:2,p:20};
