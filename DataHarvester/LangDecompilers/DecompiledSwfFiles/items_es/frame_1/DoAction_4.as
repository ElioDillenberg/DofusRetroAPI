I.u[1559] = {n:"Abranillo Blando",nn:"ABRANILLO BLANDO",t:9,d:"Este anillo era en un principio una excrecencia del Roble Blando que se arrancó durante una tempestad. Ahora es tuyo, pero procura no acercarlo demasiado a su maestro, pues podría volverse contra ti.",ep:9,g:11,l:46,wd:true,fm:true,w:3,p:7775};
