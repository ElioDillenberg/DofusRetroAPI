I.u[1511] = {n:"Nueve Colas",nn:"NUEVE COLAS",t:6,d:"Cuenta la leyenda que un zurcarák utilizó, durante el dantesco combate que lo elevó al rango de dios, una arma hecha con sus propias colas, cada una correspondía a una de sus vidas.",ep:13,g:19,l:80,wd:true,fm:true,w:20,an:14,e:[9,9,1,1,20,20,false,false],c:"CS>99&CA>99&CV>99",p:20000};
