I.u[1764] = {n:"Barra de cangrejo **",nn:"BARRA DE CANGREJO **",t:49,d:"Se puede consumir en cualquier lugar con los dedos pero con moderación.",ep:2,g:74,l:1,wd:true,fm:true,w:3,u:true,ut:true,p:30};
