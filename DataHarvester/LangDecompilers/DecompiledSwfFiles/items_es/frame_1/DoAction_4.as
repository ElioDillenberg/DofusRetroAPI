I.u[1769] = {n:"Pez Tigre Ahumado ****",nn:"PEZ TIGRE AHUMADO ****",t:49,d:"Un poco más tierno que el pez gatito ahumado y sobre todo más refinado. Este pez tigre encantará a los aficionados a grandes manjares.",ep:1,g:77,l:1,wd:true,fm:true,w:1,c:"Cw<40",u:true,ut:true,p:40000};
I.u[1770] = {n:"Cacho de Blop coco",nn:"CACHO DE BLOP COCO",t:15,d:"Este extracto de blop desprende un olor muy fuerte a coco. ¿Cómo un residuo de monstruos puede oler tanto?",ep:5,g:281,l:34,wd:true,fm:true,w:1,p:1};
