I.u[1766] = {n:"Barra de cangrejo ****",nn:"BARRA DE CANGREJO ****",t:49,d:"Preparado con carne de cangrejo sumiri exótico, el sabor esta barra de cangrejo es sencillamente asqueroso. En cambio, alimenta excesivamente.",ep:1,g:76,l:1,wd:true,fm:true,w:1,c:"Ci<30",u:true,ut:true,p:30000};
I.u[1767] = {n:"Pez Gatito Ahumado **",nn:"PEZ GATITO AHUMADO **",t:49,d:"Ahumado con delicadeza, este pez gatito es un maravilloso almuerzo. Pero ojo, su olor se queda impregnado en tus prendas durante varias semanas.",ep:1,g:78,l:1,wd:true,fm:true,w:3,u:true,ut:true,p:60};
