I.u[1647] = {n:"Pócima de olvido de oficio: Escultomago de Arcos",nn:"POCIMA DE OLVIDO DE OFICIO: ESCULTOMAGO DE ARCOS",t:44,d:"Esta pócima te hará olvidar el oficio de Escultomago de Arcos.",ep:1,g:8,l:1,wd:true,fm:true,w:1,u:true,p:10};
I.u[1648] = {n:"Pócima de olvido de oficio: Escultomago de Varitas",nn:"POCIMA DE OLVIDO DE OFICIO: ESCULTOMAGO DE VARITAS",t:44,d:"Esta pócima te hará olvidar el oficio de Escultomago de Varitas.",ep:1,g:9,l:1,wd:true,fm:true,w:1,u:true,p:10};
I.u[1649] = {n:"Pócima de olvido de oficio: Escultomago de Bastones",nn:"POCIMA DE OLVIDO DE OFICIO: ESCULTOMAGO DE BASTONES",t:44,d:"Esta pócima te hará olvidar el oficio de Escultomago de Bastones.",ep:1,g:10,l:1,wd:true,fm:true,w:1,u:true,p:10};
I.u[1650] = {n:"El Gran Cutter",nn:"EL GRAN CUTTER",t:6,d:"Esta espada es perfecta para agredir a las buenas gentes. Rápida, manejable y sobre todo extremadamente cortante. De hecho está prohibida su venta desde hace tiempo. Pero hoy en día se tolera como defensa anti-bwork.",ep:14,g:23,l:58,wd:true,fm:true,w:12,an:14,e:[10,5,1,1,30,50,false,false],p:5200};
I.u[1651] = {n:"El Cutter Sangriento",nn:"EL CUTTER SANGRIENTO",t:6,d:"Esta espada es perfecta para agredir a las buenas gentes. Rápida, manejable y sobre todo extremadamente cortante. De hecho está prohibida su venta desde hace tiempo. Pero hoy en día se tolera como defensa anti-bwork.",ep:14,g:23,l:64,wd:true,fm:true,w:12,an:14,e:[10,5,1,1,30,50,false,false],p:5200};
I.u[1652] = {n:"Pata de arakna mayor",nn:"PATA DE ARAKNA MAYOR",t:103,d:"Una pata muy fina pero muy velluda. Se utilizaba antiguamente para hacer escobas o pelucas algo exóticas.",ep:1,g:246,l:39,wd:true,fm:true,w:1,p:1};
I.u[1653] = {n:"Fe de Don Huan",nn:"FE DE DON HUAN",t:24,d:"Este anillo simboliza el hecho que estés en busca del secreto de Don Huan. Por ahora has aprendido la valentía.",ep:2,g:27,l:1,wd:true,fm:true,w:0,p:1};
I.u[1654] = {n:"Fe de Don Huan",nn:"FE DE DON HUAN",t:24,d:"Este anillo simboliza el hecho que estés en busca del secreto de Don Huan. Por ahora has aprendido la valentía, la comunicación y ahora debes tener paciencia.",ep:2,g:27,l:1,wd:true,fm:true,w:0,p:1};
I.u[1655] = {n:"Fe de Don Huan",nn:"FE DE DON HUAN",t:24,d:"Este anillo simboliza el hecho que estés en busca del secreto de Don Huan.",ep:2,g:27,l:1,wd:true,fm:true,w:0,p:1};
I.u[1656] = {n:"Fecalizador del Mago",nn:"FECALIZADOR DEL MAGO",t:9,d:"Este anillo es capaz de proteger a su dueño contra pequeños ataques mágicos y además contra pequeños monstruos que recurren a la magia para hacerse los pillos y vacilar.",ep:1,g:40,l:26,wd:true,fm:true,w:2,p:2000};
I.u[1657] = {n:"Fecalizador del Guerrero",nn:"FECALIZADOR DEL GUERRERO",t:9,d:"Este anillo es capaz de proteger a su dueño contra pequeños ataques físicos y por añadidura, contra pequeños monstruos que se aprovechan de su físico para sorprenderte.",ep:1,g:41,l:26,wd:true,fm:true,w:2,p:2000};
I.u[1658] = {n:"Chuhula",nn:"CHUHULA",t:22,d:"Con esta Guadaña, tendrás una pinta muy " + "\"" + "chuhula" + "\"" + ", incluso puedes prenteder se el Campesino más " + "\"" + "chuhulo" + "\"" + " del mundo. Pero ten cuidado, los monstruos no se equivocarán y se reirán de ti si confundes " + "\"" + "chuhulo" + "\"" + " con " + "\"" + "cohoqueto" + "\"" + ".",ep:0,g:1,l:31,wd:true,fm:true,w:15,e:[10,5,1,1,50,45,false,false],c:"CA>40&CV>20",p:2670};
I.u[1659] = {n:"Guadaña de Mack Kormick",nn:"GUADANA DE MACK KORMICK",t:22,d:"Desde los albores, Mack Kormick el segador, siega campos enteros de cereales y todos sus monstruos con esta guadaña. Hoy te toca demostrar que eres digno de utilizar esta guadaña, o sea que ¡A trabajar sea dicho!",ep:0,g:1,l:56,wd:false,fm:true,w:15,an:12,e:[10,5,1,1,45,50,false,false],c:"CC>80&CA>40&CI>20",p:8345};
I.u[1660] = {n:"Ámbar de abráknido oscuro",nn:"AMBAR DE ABRAKNIDO OSCURO",t:50,d:"Esta magnífica piedra de ámbar contiene un desagradable bichito, prisionero de la savia de su amo. Una especie de sarcófago, que le permite al abráknido oscuro guardar buenos recuerdos de sus invocaciones, después de que se mueran.",ep:9,g:638,l:78,wd:true,fm:true,w:2,p:6000};
I.u[1661] = {n:"Ambarmuleto Moskito",nn:"AMBARMULETO MOSKITO",t:1,d:"Llevar alrededor del cuello un Moskito atrapado en ámbar, es un poco poner de manifiesto a tus adversarios, que eres un temible cazador... de Moskitos. Te ridiculizará si lo llevas fuera de la Cuna.",ep:1,g:17,l:40,wd:true,fm:true,w:5,p:4400};
I.u[1662] = {n:"Ambarmuleto Arakna",nn:"AMBARMULETO ARAKNA",t:1,d:"Bastará con llevar alrededor del cuello una Arakna atrapada en ámbar cuando estás atacando a una de sus hermanas para desorientarla. Pobre criatura, eso sí es irónico...",ep:1,g:18,l:34,wd:true,fm:true,w:5,p:3225};
I.u[1663] = {n:"Escama de cocodrail",nn:"ESCAMA DE COCODRAIL",t:59,d:"Esta escama de cocodrail constituye un pequeño trofeo para cualquier cazador novato. No dudes en mostrárselo a los borrachos de la taberna, estarán obviamente impresionados y te propondrán pagarte unas rondas.",ep:4,g:233,l:36,wd:true,fm:true,w:5,p:50};
I.u[1664] = {n:"Diente de cocodrail",nn:"DIENTE DE COCODRAIL",t:47,d:"Cuando un cocodrail quiere hincarte el diente, la mejor solución consiste en arrancársela con fuerza y determinación, tal y como lo haría un buen dentista.",ep:4,g:234,l:36,wd:true,fm:true,w:1,p:50};
I.u[1665] = {n:"Botas de Aprendizaje",nn:"BOTAS DE APRENDIZAJE",t:11,d:"Estas botas muy extrañas eran utilizadas para los discípulos de capacidades intelectuales diminutas, para permitirles recuperar su retraso. Son idóneas para los Yopuka.",ep:4,g:68,l:50,wd:true,fm:true,w:10,p:6675};
I.u[1666] = {n:"Botas del Invocador Aprendiz",nn:"BOTAS DEL INVOCADOR APRENDIZ",t:11,d:"Estas botas fueron especialmente diseñadas para los que prefieren dar rienda suelta a sus criaturas durante los combates.",ep:12,g:14,l:51,wd:true,fm:true,w:10,p:7000};
I.u[1667] = {n:"Cinturón Ropopompero",nn:"CINTURON ROPOPOMPERO",t:10,d:"Este cinturón lleva el ridículo nombre de su creador, Ropopompero, un zapatero cuya fama no alcanza niveles tan altos como la de Bowis, pero que sigue haciendo alarde de una gran creatividad a pesar de su avanzada edad.",ep:8,g:13,l:50,wd:true,fm:true,w:10,p:6700};
I.u[1668] = {n:"Cinturón del Temblador Regordete",nn:"CINTURON DEL TEMBLADOR REGORDETE",t:10,d:"El Temblador era un Sadida que se empeñaba en hacer temblar a sus enemigos, le encargó a Bowis este cinturón, combinado al color de sus pelos, porque no hay que olvidarlo, el Temblador además de ser un Temblador era también un seductor.",ep:4,g:14,l:55,wd:true,fm:true,w:8,p:8000};
I.u[1669] = {n:"Cinturón de los Vientos",nn:"CINTURON DE LOS VIENTOS",t:10,d:"Este cinturón ha sido creado para la más famosa Ocrita de Amakna. A ella le gustaba dedicar su tiempo libre para organizar múltiples fiestas y eventos fuera de lo común para divertir al pueblo entre colada y colada de los calzoncillos de cuero de Capitán Chafer.",ep:8,g:15,l:84,wd:true,fm:true,w:10,p:18300};
I.u[1670] = {n:"Pluma puntiaguda de kwak del viento",nn:"PLUMA PUNTIAGUDA DE KWAK DEL VIENTO",t:53,d:"A veces, un kwak del viento dispone de una pluma puntiaguda diferente de todas las demás, en general suele ser mágica y puedes utilizarla para disfrazarte de jefe indio.",ep:5,g:393,l:24,wd:true,fm:true,w:10,p:100};
I.u[1671] = {n:"Avena Mágica",nn:"AVENA MAGICA",t:34,d:"Esta pajita de Avena Mágica pertenecía a un humilde campesino hechizado por un aprendiz Bwork Mago que leía su libro al revés.",ep:1,g:550,l:20,wd:true,fm:true,w:10,p:0};
I.u[1672] = {n:"Pelos de Darit",nn:"PELOS DE DARIT",t:54,d:"Estos pelos es lo que queda de los restos de Darit, un joven mago muy prepotente, que se había olvidado de comprobar si el Prespic había lanzado Reenvío de Hechizo antes de lanzarle su hechizo Rayo Desplantador.",ep:1,g:555,l:18,wd:true,fm:true,w:1,p:100};
I.u[1673] = {n:"Pata de arakna mágica",nn:"PATA DE ARAKNA MAGICA",t:103,d:"Esta pata de Arakna, no es como las demás, se mueve completamente sola y parece estar llena de vida, incluso tras haber sido arrancada a su ama. Una nueva vida podría comenzar para esta pata a la que, según parece, le gusta la costura.",ep:1,g:392,l:2,wd:true,fm:true,w:10,p:100};
I.u[1674] = {n:"Champiñón Luidchi",nn:"CHAMPINON LUIDCHI",t:15,d:"Este héroe ha sido hechizado por el Roble Blando, pero como la magia de este último es un poco aleatoria, acabo transformado en una seta. Los champos, que son bastante idiotas, lo veneran y protegen hasta la muerte.",ep:1,g:390,l:46,wd:true,fm:true,w:10,p:100};
I.u[1675] = {n:"Hueso Sorpresa del chafer Infante",nn:"HUESO SORPRESA DEL CHAFER INFANTE",t:47,d:"Este Hueso mágico les permite a la infantería chafer realizar cosas sorprendentes como quitarse sus calzoncillos y ponérselos en la cabeza sin las manos, en pleno combate y consiguiendo esquivar los golpes. Uno nunca se cansa verlos hacer eso...",ep:3,g:15,l:35,wd:true,fm:true,w:1,p:100};
I.u[1676] = {n:"Mimbre hechizado",nn:"MIMBRE HECHIZADO",t:15,d:"Este trozo de mimbre hechizado debería servir para fabricar una magnífica cesta para frutas mágicas, una mezcla de elegancia y de poderío, todo lo contrario de una malvada Lich.",ep:2,g:123,l:69,wd:true,fm:true,w:10,p:100};
I.u[1677] = {n:"Pócima Dantegulesca",nn:"POCIMA DANTEGULESCA",t:12,d:"Esta pócima mágica es la única que puede darle magia a los calzoncillos de cuero de Capitán Chafer trenzados entre ellos, eso lo dice todo acerca del poder de esta pócima.",ep:2,g:139,l:50,wd:true,fm:true,w:10,p:1000};
I.u[1678] = {n:"Nuez de kokoko traumatizada",nn:"NUEZ DE KOKOKO TRAUMATIZADA",t:46,d:"Esta nuez de kokoko ha sido traumatizada durante su niñez, ha acumulado un odio increíble durante todos estos años, por eso este objeto es muy poderoso.",ep:12,g:552,l:42,wd:true,fm:true,w:2,p:100};
I.u[1679] = {n:"Piedra de Cristalobola",nn:"PIEDRA DE CRISTALOBOLA",t:50,d:"Con una pizca de patiencia y de esfuerzo, esta piedra preciosa cogerá una forma redonda y brillará como un pompón de sombrero mágico. Su forma le permitirá además colarse casi por cualquier parte.",ep:4,g:240,l:16,wd:true,fm:true,w:10,p:100};
I.u[1680] = {n:"Cuerno quemado del minotauroro",nn:"CUERNO QUEMADO DEL MINOTAURORO",t:47,d:"Este cuerno de minotauroro se quemó con una magia muy poderosa, lo que lo convierte en un artefácto mágico de primer orden. Por desgracia, como muchas cosas de esta vida, sólo funcionan correctamente por parejas...",ep:2,g:245,l:125,wd:true,fm:true,w:10,p:100};
I.u[1681] = {n:"Piel de larva verde mágica",nn:"PIEL DE LARVA VERDE MAGICA",t:59,d:"No te equivoques, detrás de su apariencia viscosa se esconde en realidad un artefacto que si se lleva cerca de la cabeza puede hacerte tan afortunado como un chafer nacido con una flor en el trasero...",ep:1,g:391,l:7,wd:true,fm:true,w:10,p:100};
I.u[1682] = {n:"Corteza de Liroy Merlin",nn:"CORTEZA DE LIROY MERLIN",t:96,d:"Este pedazo de corteza procede de un árbol del claro encantado de Liroy Merlin. Este trozo se despegó cuando Brus Bulguro el leñador rompió su Bulgurda mientras trataba de talar un árbol encantado.",ep:9,g:542,l:78,wd:true,fm:true,w:1,p:100};
I.u[1683] = {n:"Saco de patatas claro",nn:"SACO DE PATATAS CLARO",t:15,d:"Este saco mágico le permitía al sadida Salteadorillo transportar cantidades increíbles de patatas peladas para vendérselas a los prisioneros. Cuando el Salteadorillo se muere, su saco es legado a otro como una especie de ritual.",ep:1,g:243,l:12,wd:true,fm:true,w:10,p:100};
I.u[1684] = {n:"Saco de patatas oscuro",nn:"SACO DE PATATAS OSCURO",t:15,d:"Es más o menos el mismo saco que el saco de patatas claro, aunque éste ha estado a remojo en savia de abráknido oscuro, lo cual le confiere poderes adicionales.",ep:1,g:244,l:12,wd:true,fm:true,w:10,p:100};
I.u[1685] = {n:"Pluma del último Pohoyo",nn:"PLUMA DEL ULTIMO POHOYO",t:53,d:"Ojo, esta es la última pluma que se le haya arrancado del trasero al último Pohoyo de toda Amakna. Esta pluma constituirá un estupendo regalo para tus amigos sastres o para los fanáticos del Pohoyo, cada día más numerosos.",ep:3,g:247,l:32,wd:true,fm:true,w:1,p:100};
I.u[1686] = {n:"Tinte mágico rojizo",nn:"TINTE MAGICO ROJIZO",t:70,d:"Esta pequeña pócima mágica tiñe con un rojo magnífico los pelos sobre los que la aplicas. Opcionalmente, parece ser que puede proporcionar algunos efectos mágicos.",ep:7,g:63,l:10,wd:true,fm:true,w:1,p:500};
I.u[1687] = {n:"Tinte mágico verdoso",nn:"TINTE MAGICO VERDOSO",t:70,d:"Esta pequeña pócima mágica tiñe con un verde magnífico los pelos sobre los que la aplicas. Opcionalmente, parece ser que puede proporcionar algunos efectos mágicos.",ep:1,g:64,l:10,wd:true,fm:true,w:1,p:500};
I.u[1688] = {n:"Tinte mágico azulado",nn:"TINTE MAGICO AZULADO",t:70,d:"Esta pequeña pócima mágica tiñe con un azul magnífico los pelos sobre los que la aplicas. Opcionalmente, parece ser que puede proporcionar algunos efectos mágicos.",ep:5,g:65,l:10,wd:true,fm:true,w:1,p:500};
I.u[1689] = {n:"Tinte mágico naranja",nn:"TINTE MAGICO NARANJA",t:70,d:"Esta pequeña pócima mágica tiñe con un naranja magnífico los pelos sobre los que la aplicas. Además, parece ser que puede proporcionar algunos efectos mágicos.",ep:1,g:66,l:10,wd:true,fm:true,w:1,p:500};
I.u[1690] = {n:"Pelos de minilubo",nn:"PELOS DE MINILUBO",t:54,d:"Estos pelos se parecen a los de mediulubo aunque son más cortos, menos resistentes y más suaves. Desgraciadamente, el olor sigue siendo el mismo.",ep:1,g:554,l:22,wd:true,fm:true,w:1,p:1};
I.u[1691] = {n:"Pelos del maxilubo",nn:"PELOS DEL MAXILUBO",t:54,d:"Pocos son los aventureros que tuvieron la gran suerte de poder tocar estos pelos cuando el maxilubo estaba vivo todavía... pero no creas que por haberlo conseguido eres un héroe.",ep:2,g:553,l:198,wd:true,fm:true,w:10,p:200};
I.u[1692] = {n:"Tinte mágico oscuro",nn:"TINTE MAGICO OSCURO",t:70,d:"Esta pequeña pócima mágica tiñe con un negro magnífico los pelos sobre los que la aplicas. Opcionalmente, parece ser que puede proporcionar algunos efectos mágicos.",ep:1,g:67,l:10,wd:true,fm:true,w:1,p:500};
I.u[1693] = {n:"Capa de Mano Lito",nn:"CAPA DE MANO LITO",t:17,d:"Mano Lito era un famoso sastre conocido sobre todo por haber cambiado a su mujer por un dedal mágico. Esto demuestra que todavía existe gente con sentido del negocio.",ep:8,g:8,l:54,wd:true,fm:true,w:1,p:7800};
I.u[1694] = {n:"Lana del trool airado",nn:"LANA DEL TROOL AIRADO",t:57,d:"Este trozo de lana procede del trool más valiente de Amakna, que habría sido transformado en colcha por un sastre sin escrúpulos... el mismo que -según dicen- entregó a su mujer a cambio de un dedal mágico. El misterio sigue sin resolver.",ep:4,g:27,l:115,wd:true,fm:true,w:10,p:1};
I.u[1695] = {n:"Capa de Elya Wood",nn:"CAPA DE ELYA WOOD",t:17,d:"Se cuenta que Capitán Chafer intentó robar esta capa para remplazar su famosa ropa interior de cuero... pero no le favorecía mucho. Según decía, no encarnaba a la perfección la perversidad y la perfidia que le caracterizaban.",ep:8,g:9,l:80,wd:true,fm:true,w:10,s:39,p:16500};
I.u[1696] = {n:"Tejido del maxilubo",nn:"TEJIDO DEL MAXILUBO",t:55,d:"Hace años, unos sastres encontraron la manera de valorar un tejido. Cuanto más mal huela este, más poderoso es. Esta huele peor que un pediluvio de Anutrof en el cual un Chafer fue a lavar su calzoncillo...",ep:2,g:14,l:198,wd:true,fm:true,w:20,p:300};
I.u[1697] = {n:"Pequeña mochila en lana de jalatín",nn:"PEQUENA MOCHILA EN LANA DE JALATIN",t:81,d:"Esta pequeña mochila no te llevará a la fama, pero tendrá por lo menos el mérito de permitirte guardar algunos de tus bienes.",ep:1,g:25,l:1,wd:true,fm:true,w:5,p:100};
I.u[1698] = {n:"Mochila del pequeño recolector",nn:"MOCHILA DEL PEQUENO RECOLECTOR",t:81,d:"Esta mochila ha sido especialmente diseñada para almacenar algunos recursos que se pueden recolectar. Sólo verdaderos recolectores pueden ponérsela (requiere nivel 20 de recolector).",ep:1,g:26,l:5,wd:true,fm:true,w:5,c:"PJ>2,20|PJ>24,20|PJ>36,20|PJ>28,20|PJ>26,20|PJ>41,20",p:2000};
I.u[1699] = {n:"Mochila del Pequeño Aventurero",nn:"MOCHILA DEL PEQUENO AVENTURERO",t:81,d:"Esta mochila está diseñada para los que no se alejan demasiado de casa y necesitan llevarse cosas sin ninguna importancia, como Tablas de Fresno por ejemplo.",ep:1,g:27,l:10,wd:true,fm:true,w:5,p:3000};
I.u[1700] = {n:"Cinturón de Celebridad",nn:"CINTURON DE CELEBRIDAD",t:10,d:"Este cinturón aerodinámico diseñado por Tengra Reufrus te vuelve ágil como el aire y libre como un Tofu. ¿O será al contrario?",ep:4,g:17,l:27,wd:true,fm:true,w:15,p:2000};
I.u[1701] = {n:"Cinturón de Bolsillo",nn:"CINTURON DE BOLSILLO",t:10,d:"Este cinturón lleva pequeños bolsillos que te permiten guardar más objetos en tu inventario.",ep:1,g:16,l:20,wd:true,fm:true,w:10,p:1300};
I.u[1702] = {n:"Mochila del Recolector",nn:"MOCHILA DEL RECOLECTOR",t:81,d:"Esta mochila ha sido especialmente diseñada para almacenar recursos que se pueden colectar. Sólo verdaderos recolectores pueden ponérsela (requiere nivel 40 de recolector).",ep:8,g:28,l:15,wd:true,fm:true,w:10,c:"PJ>2,40|PJ>24,40|PJ>36,40|PJ>28,40|PJ>26,40|PJ>41,40",p:3000};
I.u[1703] = {n:"Mochila del Gran Recolector",nn:"MOCHILA DEL GRAN RECOLECTOR",t:81,d:"Esta mochila ha sido especialmente diseñada para almacenar múltiples recursos que se pueden colectar. Sólo verdaderos recolectores pueden ponérsela (requiere nivel 60 de recolector).",ep:8,g:29,l:20,wd:true,fm:true,w:10,c:"PJ>2,60|PJ>24,60|PJ>36,60|PJ>28,60|PJ>26,60|PJ>41,60",p:2000};
I.u[1704] = {n:"Mochila del Staca Novista",nn:"MOCHILA DEL STACA NOVISTA",t:81,d:"Cuentan que un minero famoso era capaz de excavar sin parar durante meses en 4 minas distintas a la vez. La verdad es que se trata de una leyenda inventada por el gremio Martillhoz para que los demás mineros se animen y sean más productivos.",ep:8,g:30,l:20,wd:true,fm:true,w:10,c:"PJ>2,90|PJ>24,90|PJ>36,90|PJ>28,90|PJ>26,90|PJ>41,90",p:10000};
I.u[1705] = {n:"Mochila del Aventurero",nn:"MOCHILA DEL AVENTURERO",t:81,d:"Esta mochila es perfecta para guardar todos tus panes y pócimas cuando te marchas para varios meses, al laberinto por ejemplo...",ep:8,g:31,l:40,wd:true,fm:true,w:4,p:4000};
I.u[1707] = {n:"Mochila del Gran Aventurero",nn:"MOCHILA DEL GRAN AVENTURERO",t:81,d:"Esta mochila es perfecta para guardar todos tus panes y pócimas cuando te marchas de viaje a sitios exóticos, como a la Isla de Moon.",ep:13,g:32,l:60,wd:true,fm:true,w:5,p:6000};
I.u[1709] = {n:"Huesito",nn:"HUESITO",t:47,d:"" + "\"" + "¿Y para quién es este huesito?" + "\"" + " Estas fueron las últimas palabras de una anutrof que confundió a un mediulubo con su animal de compañía.",ep:1,g:15,l:1,wd:true,fm:true,w:2,p:1};
I.u[1710] = {n:"Media Luna",nn:"MEDIA LUNA",t:1,d:"Los orígenes de este amuleto, que representa un gajo de luna, son totalmente desconocidos... o casi. ¿Cómo se podría entonces conocer los secretos que esta joya encierra...?",ep:1,g:19,l:1,wd:false,fm:true,w:10,p:1000};
I.u[1711] = {n:"Wauwau",nn:"WAUWAU",t:18,d:"Con este wauwau, podrás disfrutar de interminables paseos para que haga pis. Ha sido criado comiendo raíces salvajes, por lo tanto te desaconsejamos darle de comer... pues podrías perder una mano. (A ESTA MASCOTA NO LE GUSTA NINGÚN RECURSO).",ep:1,g:6,l:1,wd:true,fm:true,w:10,c:"PZ=1",p:1};
I.u[1712] = {n:"Pócima Pahoa Reid",nn:"POCIMA PAHOA REID",t:12,d:"Esta pócima permite recuperar unos cuantos puntos de energía.",ep:1,g:140,l:20,wd:true,fm:true,w:1,u:true,p:50};
I.u[1713] = {n:"Pócima Raid Bul",nn:"POCIMA RAID BUL",t:12,d:"Esta pócima permite recuperar unos cuantos puntos de energía.",ep:1,g:141,l:40,wd:true,fm:true,w:1,u:true,p:100};
I.u[1715] = {n:"El don de Nawidad",nn:"EL DON DE NAWIDAD",t:25,d:"Best seller del año 634, este libro ha de ser leído. ¡Es un clásico!",ep:2,g:19,l:1,wd:true,fm:true,w:1,u:true,p:50};
I.u[1716] = {n:"La familia Delcielo",nn:"LA FAMILIA DELCIELO",t:25,d:"Segunda mejor venta del invierno 634, este libro ya es una referencia.",ep:2,g:19,l:1,wd:true,fm:true,w:1,u:true,p:50};
I.u[1717] = {n:"Primeras Navidades",nn:"PRIMERAS NAVIDADES",t:25,d:"Outsider de principios del año 635, este libro vale la pena ser leído.",ep:2,g:19,l:1,wd:true,fm:true,w:1,u:true,p:50};
I.u[1718] = {n:"Fuerza de Cocoburio",nn:"FUERZA DE COCOBURIO",t:9,d:"Este anillo ha sido hechizado por el soplido negro de Grugalorasalar, él le confirió un inmenso poder que desafortunadamente hoy, ha decrecido. Esta reliquia fue creada para Cocoburio porque sólo él podía ponérselo sin sufrir su dominación.",ep:4,g:38,l:40,wd:true,fm:true,w:2,c:"CS>59",p:4000};
I.u[1719] = {n:"Espíritu de Cocoburio",nn:"ESPIRITU DE COCOBURIO",t:11,d:"Este par de botas pertenecía a Cocoburio. El puñado de personas aptas para calzarlas se siente de pronto sumergido por maquiavélicos e ingeniosos pensamientos. Cuenta la leyenda que este maleficio es debido a los miles de cráneos que las botas han aplastado.",ep:4,g:12,l:42,wd:true,fm:true,w:10,c:"CI>49",p:5000};
I.u[1720] = {n:"Poder de Cocoburio",nn:"PODER DE COCOBURIO",t:10,d:"A Grugalorasalar le gustaba guardar fragmentos de huesos de los más poderosos enemigos que había vencido. Su afecto por Cocoburio le incitó a utilizarlos para crear este superpotente cinturón.",ep:4,g:18,l:34,wd:true,fm:true,w:15,c:"CV>119",p:3000};
I.u[1721] = {n:"Pócima de olvido: " + "\"" + "Reloj de Pared" + "\"" + "",nn:"POCIMA DE OLVIDO: " + "\"" + "RELOJ DE PARED" + "\"" + "",t:43,d:"Esta pócima permite olvidar un nivel del hechizo de Xelor: " + "\"" + "Reloj de Pared" + "\"" + ".",ep:2,g:37,l:1,wd:true,fm:true,w:1,u:true,p:1000};
I.u[1722] = {n:"Transformación abráknida",nn:"TRANSFORMACION ABRAKNIDA",t:27,d:"La maldición del roble blando te transformó en abráknido.\nPara romper el hechizo, debes vencer a un aventurero perdido en el bosque de los abráknidos.",ep:1,g:1,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[1723] = {n:"Caramelo rojo superpoderoso",nn:"CARAMELO ROJO SUPERPODEROSO",t:28,d:"Este caramelo te boostea durante un determinado tiempo.",ep:1,g:1,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[1724] = {n:"Valentía",nn:"VALENTIA",t:29,d:"Esta bendición permite ganar Fuerza para los siguientes combates.",ep:18,g:1,l:1,wd:true,fm:true,w:0,m:true,p:1};
