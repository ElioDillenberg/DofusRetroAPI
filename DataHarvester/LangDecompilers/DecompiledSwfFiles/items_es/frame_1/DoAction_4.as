I.u[1455] = {n:"Ala de escarahoja azul",nn:"ALA DE ESCARAHOJA AZUL",t:104,d:"Estas alas de Escarahoja tienen propiedades mágicas y permiten fabricar pócimas elementales de agua.",ep:3,g:212,l:32,wd:true,fm:true,w:2,p:100};
I.u[1456] = {n:"Ala de escarahoja blanco",nn:"ALA DE ESCARAHOJA BLANCO",t:104,d:"Estas alas de Escarahoja tienen propiedades mágicas y permiten fabricar pócimas elementales de aire.",ep:3,g:213,l:32,wd:true,fm:true,w:2,p:100};
I.u[1457] = {n:"Ala de escarahoja rojo",nn:"ALA DE ESCARAHOJA ROJO",t:104,d:"Estas alas de Escarahoja tienen propiedades mágicas y permiten fabricar pócimas elementales de fuego.",ep:3,g:214,l:32,wd:true,fm:true,w:2,p:100};
I.u[1458] = {n:"Ala de escarahoja verde",nn:"ALA DE ESCARAHOJA VERDE",t:104,d:"Estas alas de Escarahoja tienen propiedades mágicas y permiten fabricar pócimas elementales de tierra.",ep:3,g:215,l:32,wd:true,fm:true,w:2,p:100};
I.u[1459] = {n:"Tubo largo",nn:"TUBO LARGO",t:71,d:"Este tubo ha sido especialmente diseñado para poder destaparse con una sola mano.",ep:1,g:55,l:1,wd:true,fm:true,w:1,p:5};
I.u[1460] = {n:"Gran frasco",nn:"GRAN FRASCO",t:71,d:"Este frasco ha sido especialmente diseñado para poder destaparse fácilmente incluso sin las manos.",ep:3,g:56,l:1,wd:true,fm:true,w:1,p:10};
I.u[1461] = {n:"Veneno azotador",nn:"VENENO AZOTADOR",t:15,d:"Este veneno a base de patas de araknas te hará perder la cabeza. En el propia sentido como en el figurado puesto que después de un período de locura, la caja craneal estalla con la presión sanguínea.",ep:1,g:189,l:20,wd:true,fm:true,w:1,p:100};
I.u[1462] = {n:"Líquido vampiresco",nn:"LIQUIDO VAMPIRESCO",t:15,d:"Este líquido hecho con un cruce de varias sangres tiene la mala costumbre de saltar al cuello.",ep:2,g:188,l:30,wd:true,fm:true,w:1,p:100};
I.u[1463] = {n:"Frasco de elemento",nn:"FRASCO DE ELEMENTO",t:71,d:"Naturalmente, este frasco está destinado a encerrar la esencia elemental.",ep:1,g:57,l:1,wd:true,fm:true,w:1,p:50};
I.u[1464] = {n:"Antena del escarahoja azul",nn:"ANTENA DEL ESCARAHOJA AZUL",t:15,d:"Nadie sabe para qué sirven las antenas de un Escarahoja. La única cosa que se sepa con seguridad es que no huelen muy bien.",ep:3,g:216,l:32,wd:true,fm:true,w:1,p:100};
I.u[1465] = {n:"Antena del escarahoja rojo",nn:"ANTENA DEL ESCARAHOJA ROJO",t:15,d:"Nadie sabe para qué sirven las antenas de un Escarahoja. La única cosa que se sepa con seguridad es que no huelen muy bien.",ep:3,g:217,l:32,wd:true,fm:true,w:1,p:1};
I.u[1466] = {n:"Antena del escarahoja verde",nn:"ANTENA DEL ESCARAHOJA VERDE",t:15,d:"Nadie sabe para qué sirven las antenas de un Escarahoja. La única cosa que se sepa con seguridad es que no huelen muy bien.",ep:3,g:218,l:32,wd:true,fm:true,w:1,p:100};
I.u[1467] = {n:"Antena del escarahoja blanco",nn:"ANTENA DEL ESCARAHOJA BLANCO",t:15,d:"Nadie sabe para qué sirven las antenas de un Escarahoja. La única cosa que se sepa con seguridad es que no huelen muy bien.",ep:3,g:219,l:32,wd:true,fm:true,w:1,p:100};
I.u[1468] = {n:"Bocal",nn:"BOCAL",t:71,d:"Este bocal puede servir para todo, desde la fabricación de pócimas hasta la caza de peces gordos.",ep:1,g:58,l:1,wd:true,fm:true,w:1,p:100};
I.u[1469] = {n:"Pergamino de Recomendación",nn:"PERGAMINO DE RECOMENDACION",t:24,d:"Este pergamino te ha sido entregado por Uba Teseo. Te recomienda con vehemencia para que te vuelvas alquimista.",ep:1,g:26,l:1,wd:true,fm:true,w:0,p:1};
I.u[1470] = {n:"Pergamino de Recomendación",nn:"PERGAMINO DE RECOMENDACION",t:24,d:"Este pergamino te ha sido entregado por Uba Teseo. Te recomienda con vehemencia para que te vuelvas alquimista. Una pequeña nota ha sido añadida por Boff Chalon.",ep:2,g:26,l:1,wd:true,fm:true,w:0,p:1};
I.u[1471] = {n:"Pergamino de Recomendación y Flor",nn:"PERGAMINO DE RECOMENDACION Y FLOR",t:24,d:"Este pergamino te ha sido entregado por Uba Teseo. Te recomienda con vehemencia para que te vuelvas alquimista. Una pequeña nota ha sido añadida por Boff Chalon. Tiene una flor pegada que demuestra que has aprendido la primera lección.",ep:3,g:26,l:1,wd:true,fm:true,w:0,p:1};
I.u[1472] = {n:"Pergamino de Inscripción para el Examen",nn:"PERGAMINO DE INSCRIPCION PARA EL EXAMEN",t:24,d:"Este pergamino indica que has acabado tu formación de alquimista y que eres apto para pasar el examen.",ep:3,g:26,l:1,wd:true,fm:true,w:0,p:1};
I.u[1473] = {n:"Manos verdes",nn:"MANOS VERDES",t:20,d:"Con estos guantes podrás arrancar fácilmente y sin riesgo cualquier planta o flor.",ep:1,g:5,l:1,wd:true,fm:true,w:5,e:[5,3,1,1,30,50,false,true],p:100};
I.u[1474] = {n:"Amuleto de Xelor",nn:"AMULETO DE XELOR",t:1,d:"Este poderosísimo amuleto es el fruto del trabajo de un viejo Xelor sobre el dominio del tiempo.",ep:1,g:28,l:52,wd:true,fm:true,w:10,p:7300};
I.u[1475] = {n:"La Bunita Hoja",nn:"LA BUNITA HOJA",t:6,d:"La Bunita Hoja ha sido crafteada por el célebre herrero Dave-LoL antes de morirse por la más bunita de las yopukitas del reino. Dicen que solamente la belleza de su portador puede igualar la eficiencia de esta espada.",ep:4,g:15,l:60,wd:true,fm:true,w:30,an:14,e:[10,4,1,1,30,50,false,false],c:"CS>100&CV>50",p:3000};
I.u[1476] = {n:"Amuleto del Crujidor",nn:"AMULETO DEL CRUJIDOR",t:1,d:"Este poderoso amuleto contiene toda la vida del crujidor, todos los objetos que ha reunido antes de morir bajo los golpes de aventureros sedientos de sus riquezas.",ep:2,g:25,l:47,wd:true,fm:true,w:5,p:5800};
I.u[1477] = {n:"Suerte de Amulata",nn:"SUERTE DE AMULATA",t:1,d:"Amulata era una pequeña Zurcarák que nunca tuvo suerte en su vida, hasta el día en que su abuela le regaló este amuleto.",ep:1,g:26,l:44,wd:true,fm:true,w:5,p:5000};
I.u[1478] = {n:"Armonía",nn:"ARMONIA",t:1,d:"Este colgante ayuda a su portador a encontrar la paz en el mundo que le rodea.",ep:1,g:122,l:47,wd:true,fm:true,w:1,p:6000};
I.u[1479] = {n:"El Twote de Cartón",nn:"EL TWOTE DE CARTON",t:7,d:"Este martillo legendario ya hizo sus pruebas en Amakna. Pertenecía al potente Twote de Cartón, que no dudó nunca en utilizarlo para convertir a sus adversarios en tortilla sin patatas.",ep:13,g:6,l:46,wd:true,fm:true,w:20,an:15,e:[5,4,1,1,50,30,false,false],c:"CI>50&CV>30",p:5600};
I.u[1480] = {n:"Acuerdo",nn:"ACUERDO",t:1,d:"Este colgante ayuda a su portador a encontrar la paz en el mundo que le rodea.",ep:1,g:121,l:38,wd:true,fm:true,w:1,p:4000};
I.u[1481] = {n:"Entendimiento",nn:"ENTENDIMIENTO",t:1,d:"Este colgante ayuda a su portador a encontrar la paz en el mundo que le rodea.",ep:1,g:11,l:26,wd:true,fm:true,w:1,p:2000};
I.u[1482] = {n:"Emblema Lívido",nn:"EMBLEMA LIVIDO",t:1,d:"Este amuleto es extremadamente útil, pero hace que su portador se vuelva blanco como un Jalató.",ep:1,g:32,l:41,wd:true,fm:true,w:2,p:4500};
I.u[1483] = {n:"Cábala",nn:"CABALA",t:1,d:"Se trata del símbolo de un misterioso organismo. De hecho no existe ¡Y jamás has oído hablar de él!",ep:11,g:13,l:29,wd:true,fm:true,w:2,p:2500};
I.u[1484] = {n:"Amuleto Blada modificado",nn:"AMULETO BLADA MODIFICADO",t:1,d:"Excelente trabajo del orfebre aprendiz Polma Kartnez, esta copia supera el original.",ep:1,g:31,l:17,wd:true,fm:true,w:5,p:1000};
I.u[1485] = {n:"La Ranilla",nn:"LA RANILLA",t:1,d:"Este amuleto es un perfeccionamiento de El Destino Dorado. Aunque parezca extraño, no tienen nada en común, excepto su aspecto bastante ridículo, hay que reconocerlo.",ep:1,g:27,l:22,wd:true,fm:true,w:5,c:"CC>10",p:1500};
I.u[1486] = {n:"Medalla Manduca",nn:"MEDALLA MANDUCA",t:1,d:"Manduca era una discípula de Yopuka que había prometido derrotar el mal dondequiera que esté. Mantenía una calma increíble, nunca se enfadaba aparte cuando la comparaban con una tal Lacena.",ep:1,g:23,l:31,wd:true,fm:true,w:3,p:2800};
I.u[1487] = {n:"El Adelus",nn:"EL ADELUS",t:10,d:"Este cinturón, además de favorecerte mucho, te proporcionará unos masajes muy agradables. Al reducir la ansiedad, te resultará más fácil sumirte en profundas reflexiones.",ep:1,g:12,l:48,wd:true,fm:true,w:30,p:6075};
I.u[1488] = {n:"Plimclik",nn:"PLIMCLIK",t:1,d:"Este nombre extraño designa un objeto igual de extraño nacido, no se sabe bien cómo, de anteriores amuletos. Las circunstancias de su descubrimiento siguen sin conocerse.",ep:11,g:123,l:130,wd:true,fm:true,w:24,p:1000};
I.u[1489] = {n:"El Broche Heta",nn:"EL BROCHE HETA",t:1,d:"Heta era uno de los más grandes guerreros de Amakna. Cuenta la leyenda que sacaba su fuerza del broche que llevaba siempre puesto.",ep:11,g:15,l:28,wd:true,fm:true,w:1,p:2300};
I.u[1490] = {n:"La Cadena Nhoro Kebriya",nn:"LA CADENA NHORO KEBRIYA",t:1,d:"Su creador, un tal Ken Aton, era muy conocido por sus extrañas costumbres. Se pasaba el día intentando cazar al Minotauroro para hacerse una moqueta pectoral.",ep:1,g:12,l:13,wd:true,fm:true,w:2,p:650};
I.u[1491] = {n:"Cadena del Lich",nn:"CADENA DEL LICH",t:1,d:"El Lich era una asquerosa criatura conocida por acoplarse a los grupos de valientes guerreros para robarles una parte del botín. Un día, su maniobra fue descubierta y tuvo que darse a la fuga.",ep:13,g:12,l:45,wd:true,fm:true,w:2,p:5400};
I.u[1492] = {n:"Kapa Tass",nn:"KAPA TASS",t:17,d:"La abuela de Tasselhoff cosió esta capa sin saber que este la usaría para realizar todas sus fantasías, las más sublimes y las más perversas.",ep:0,g:23,l:30,wd:true,fm:true,w:11,p:800};
I.u[1493] = {n:"Herida del Sacrógrito",nn:"HERIDA DEL SACROGRITO",t:9,d:"Dícese que este anillo nació fruto de una de las numerosas heridas sufridas por un Sacrógrito.",ep:1,g:13,l:15,wd:true,fm:true,w:2,p:800};
I.u[1494] = {n:"El Anillador",nn:"EL ANILLADOR",t:9,d:"Este hermoso anillo dio la posibilidad a los osamodas de reconocer a su tofu en cada combate.",ep:1,g:14,l:15,wd:true,fm:true,w:4,p:800};
I.u[1495] = {n:"Anillo Dhyno",nn:"ANILLO DHYNO",t:9,d:"Este pequeño anillo que no tiene nada del otro mundo lo creó Degun. Este joyero saltó a la fama por su mala follá con la gente que le rodeaba y por su consideración despectiva y cruel por el Lich y no por su creatividad en materia de orfebrería.",ep:1,g:37,l:36,wd:true,fm:true,w:3,p:3575};
I.u[1496] = {n:"Suerte de Zurcarák",nn:"SUERTE DE ZURCARAK",t:9,d:"Este anillo fue creado para aumentar la suerte de su dueño durante las partidas de cartas.",ep:1,g:32,l:20,wd:true,fm:true,w:4,p:1300};
I.u[1497] = {n:"Pasado de Xelor",nn:"PASADO DE XELOR",t:9,d:"Anillos engendrados durante los viajes de Xelor a través del tiempo.",ep:1,g:16,l:30,wd:true,fm:true,w:4,p:2600};
I.u[1498] = {n:"Recuerdo de Anutrof",nn:"RECUERDO DE ANUTROF",t:9,d:"Anillos creados con las lágrimas de un anutrof que perdió su pala. Lo que demuestra la desesperación en la que se encontraba sumido el pobre hombre.",ep:1,g:15,l:30,wd:true,fm:true,w:4,p:2600};
I.u[1499] = {n:"El Anutopía",nn:"EL ANUTOPIA",t:9,d:"Este anillo sólo lo pueden llevar los anutrof. Su dios lo ha hechizado para que solamente sus discípulos puedan ponérselo. La suerte de descubrir tesoros caerá del lado de los Anutrof de pura cepa.",ep:1,g:39,l:59,wd:true,fm:true,w:1,c:"PG=3",p:10000};
I.u[1500] = {n:"La Moketa",nn:"LA MOKETA",t:17,d:"Esta maravillosa capa, tejida con diversos elementos de fibra de todo tipo, es una pieza única por su color y sus reflejos.",ep:8,g:8,l:43,wd:true,fm:true,w:16,p:5000};
I.u[1501] = {n:"Alianza",nn:"ALIANZA",t:9,d:"Esta alianza te permitirá unirte al ser de tus sueños... Se acabaron los breves revolcones a escondidas detrás de los matorrales, esta vez es la buena, vais a poder mostraros sin escrúpulos.",ep:2,g:27,l:1,wd:true,fm:true,w:10,p:1};
I.u[1503] = {n:"Varita de  Fuego Fatuo",nn:"VARITA DE  FUEGO FATUO",t:3,d:"Para la fabricación de esta lúgubre varita, dicen que hay que meterla dentro de un cadáver fresco y enterrarlo poco después. Pero nadie osaría imaginar cómo hacer eso...",ep:3,g:14,l:20,wd:true,fm:true,w:15,an:11,e:[5,3,2,3,30,50,false,true],c:"CI>30",p:1000};
I.u[1504] = {n:"Daga Yumbo",nn:"DAGA YUMBO",t:5,d:"Yumbo era un Anutrof famoso porque nunca se cambiaba de ropa interior, su olor insoportable hizo que lo expulsaran de la mayor parte de las minas existentes en Amakna. Un día los del pueblo decidieron cogerlo por la fuerza y forzarlo a lavar sus calzones en el río bajo la amenaza de esta daga.",ep:1,g:1,l:30,wd:true,fm:true,w:10,an:13,e:[10,3,1,1,30,50,false,false],c:"CA>30",p:3000};
I.u[1505] = {n:"Flauta",nn:"FLAUTA",t:20,d:"Esta flauta muy rara, hecha con madera de abráknido, te permitirá divertir y conmover a tus amigos.",ep:1,g:6,l:1,wd:true,fm:true,w:1,an:4,e:[0,5,1,1,0,0,false,false],p:50000};
I.u[1506] = {n:"Símbolo de lo extraño",nn:"SIMBOLO DE LO EXTRANO",t:15,d:"Esta carta de jugar representa... algo. En realidad, no se sabe bien qué, pero todos están convencidos de que es importante.",ep:1,g:224,l:1,wd:true,fm:true,w:10,p:1};
