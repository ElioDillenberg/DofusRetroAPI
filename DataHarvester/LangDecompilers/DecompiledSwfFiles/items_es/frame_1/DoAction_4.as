I.u[1439] = {n:"Mal Pico",nn:"MAL PICO",t:21,d:"Este pico tiende a atraer el mal de ojo. Sin embargo, parece que falta no mucho para que se convierta en un gran pico.",ep:1,g:1,l:25,wd:false,fm:true,w:20,an:17,e:[4,6,1,1,50,10,false,true],p:250};
