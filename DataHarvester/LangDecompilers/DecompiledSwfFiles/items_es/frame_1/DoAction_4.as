I.u[1435] = {n:"La Pala Gorda",nn:"LA PALA GORDA",t:8,d:"Muy pesada, por lo tanto está reservada para los aventureros más gor... desarrollados.",ep:9,g:11,l:80,wd:true,fm:true,w:150,an:16,e:[10,6,1,1,50,50,false,true],c:"CS>100",p:40000};
I.u[1436] = {n:"Pala Kran",nn:"PALA KRAN",t:8,d:"Kran era la mascota de un aventurero muy famoso en Amakna llamado Shikteniku. Esta pala sirvió para el entierro de Kran, que había sido picado por un Moskito mientras que Shikteniku satisfacía una necesidad natural...",ep:7,g:8,l:50,wd:true,fm:true,w:50,an:16,e:[10,5,1,1,50,50,false,true],c:"CC>30&CA>10",p:7800};
