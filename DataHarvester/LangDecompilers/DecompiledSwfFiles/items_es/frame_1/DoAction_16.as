I.u[7226] = {n:"Sombrero Akwadala",nn:"SOMBRERO AKWADALA",t:16,d:"Con este sombrero, te parecerás desde lejos a un Pandawushuka del pueblo de Akwadala. Desde cerca, te parecerás a un peluche ganado en la feria.",ep:18,g:146,l:43,wd:true,fm:true,w:2,s:34,p:5100};
