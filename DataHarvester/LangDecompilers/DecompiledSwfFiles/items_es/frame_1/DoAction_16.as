I.u[7400] = {n:"Pergamino ligado",nn:"PERGAMINO LIGADO",t:87,d:"Este pergamino te permite localizar a tu blanco para cazarlo mejor.",ep:1,g:2,l:10,wd:false,fm:true,w:1,c:"Ps!0&PP>0",u:true,p:1};
I.u[7401] = {n:"Pergamino virgen",nn:"PERGAMINO VIRGEN",t:87,d:"Este pergamino permite " + "\"" + "marcar" + "\"" + " un blanco para poder cazarlo mejor después.",ep:16,g:1,l:10,wd:false,fm:true,w:1,c:"Ps!0&PP>0",u:true,ut:true,p:100};
I.u[7402] = {n:"Lote de diez pergaminos vírgenes",nn:"LOTE DE DIEZ PERGAMINOS VIRGENES",t:87,d:"Este lote permite \'marcar\' <b>un sólo blanco</b> para poder después cazarlo varias veces.",ep:16,g:3,l:10,wd:false,fm:true,w:10,c:"Ps!0&PP>0",u:true,ut:true,p:500};
I.u[7403] = {n:"Hueso de fantasma Leopardo",nn:"HUESO DE FANTASMA LEOPARDO",t:47,d:"Antaño, los Salteadorillos usaban este huesecillo para decorar sus narices. Era un modo elegante de ensalzar una nariz ganchuda.",ep:19,g:3,l:100,wd:true,fm:true,w:1,p:10};
I.u[7404] = {n:"Hueso de fantasma Maho Firefux",nn:"HUESO DE FANTASMA MAHO FIREFUX",t:47,d:"Este hueso todavía contiene una magia digamos... ¡Artesanal! Así que no te extrañe si un día cobra vida e intenta encontrar los otros restos de su dueño.",ep:19,g:8,l:100,wd:true,fm:true,w:1,p:10};
I.u[7405] = {n:"Hueso de fantasma Pandikaze",nn:"HUESO DE FANTASMA PANDIKAZE",t:47,d:"Este hueso es todo lo que queda del fantasma que acabas de exterminar. No tienes suerte, parece que no hay magia alguna que lo habite.",ep:19,g:4,l:100,wd:true,fm:true,w:1,p:10};
I.u[7406] = {n:"Hueso de fantasma Pandora",nn:"HUESO DE FANTASMA PANDORA",t:47,d:"Aunque te pueda parecer que sí, este hueso no sale de la caja de Pandora. ¡Ni siquiera de la caja torácica de Pandora!",ep:19,g:5,l:100,wd:true,fm:true,w:1,p:10};
I.u[7407] = {n:"Hueso de fantasma Pandulo",nn:"HUESO DE FANTASMA PANDULO",t:47,d:"Este hueso, correctamente tallado, puede servir de manecilla de un reloj gigante. Vamos, totalmente imprescindible.",ep:19,g:6,l:100,wd:true,fm:true,w:1,p:10};
I.u[7408] = {n:"Hueso de fantasma Soryo Firefux",nn:"HUESO DE FANTASMA SORYO FIREFUX",t:47,d:"La primera vez que Soryo Firefux se rompió un hueso, se intentó vengar en el primero que cruzara su camino. Al mismo tiempo, el trovador Badiz Mustabante se paseaba cantando por el bosque y se cruzó con Soryo Firefux. Nadie conoce el final de la historia, pero todo el mundo pudo comprobar que, a partir de aquel día, la voz del trovador fue mucho más aguda.",ep:19,g:7,l:100,wd:true,fm:true,w:1,p:10};
I.u[7410] = {n:"Hueso de fantasma Tanukui San",nn:"HUESO DE FANTASMA TANUKUI SAN",t:47,d:"A fuerza de soportar el peso inconmensurable de sus enormes testículos, los huesos del Tanukui San acabaron por volverse increíblemente resistentes.",ep:19,g:1,l:100,wd:true,fm:true,w:1,p:10};
I.u[7411] = {n:"Hueso de fantasma Yokai Firefux",nn:"HUESO DE FANTASMA YOKAI FIREFUX",t:47,d:"Este hueso habría podido servir  para abrir el cráneo a los aventureros insolentes, o incluso como martillo para plantar Anutrofs en el suelo como si fueran espárragos. Pero, desde que está entre tus manos, parece que su destino sea diferente...",ep:19,g:2,l:100,wd:true,fm:true,w:1,p:10};
I.u[7412] = {n:"Lote de cien pergaminos vírgenes",nn:"LOTE DE CIEN PERGAMINOS VIRGENES",t:87,d:"Este lote permite \'marcar\' <b>un sólo blanco</b> para poder después cazarlo varias veces.",ep:16,g:3,l:10,wd:false,fm:true,w:100,c:"Ps!0&PP>0",u:true,ut:true,p:3500};
I.u[7413] = {n:"Pergamino " + "\"" + "Señalar con el dedo" + "\"" + "",nn:"PERGAMINO " + "\"" + "SENALAR CON EL DEDO" + "\"" + "",t:25,d:"(Doble clic para aprender este emote) \nEste pergamino te permite aprender la actitud " + "\"" + "Señalar con el dedo" + "\"" + ", para indicar una dirección o para denunciar abiertamente a los tramposos.",ep:18,g:9,l:1,wd:false,fm:true,w:1,u:true,p:1};
I.u[7414] = {n:"Pequeño Miaumiau Blanco",nn:"PEQUENO MIAUMIAU BLANCO",t:18,d:"Esta pequeña bola de pelos araña, muerde y pellizca incluso la mano del que le da de comer. Por eso es una terrible criatura, pero no deja de ser irresistiblemente entrañable y fascinante. Cualquier semblante con personas del género femenino no sería puro azar. Ojo, esta criatura además de tener un carácter irascible es muy complicada a nivel alimentario, puesto que sólo come carne de criaturas que se han cazado.",ep:3,g:9,l:1,wd:true,fm:true,w:10,c:"PZ=1",p:1};
I.u[7415] = {n:"Pequeño Wauwau Negro",nn:"PEQUENO WAUWAU NEGRO",t:18,d:"Esta extraña pequeña criatura sólo se alimenta con pescados recién pescados. Pero no te preocupes, su compañía agradable compensa ampliamente su aliento de pescado deshidratado.",ep:3,g:10,l:1,wd:true,fm:true,w:10,c:"PZ=1",p:1};
