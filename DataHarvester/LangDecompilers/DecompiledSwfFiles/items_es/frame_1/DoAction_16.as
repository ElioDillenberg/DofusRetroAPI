I.u[7230] = {n:"Capa Akwadala",nn:"CAPA AKWADALA",t:17,d:"Esta capa proveniente del pueblo de Akwadala, puede utilizarse como felpudo o incluso como toalla si acabas de salir del agua.",ep:18,g:113,l:41,wd:true,fm:true,w:10,s:34,p:4550};
