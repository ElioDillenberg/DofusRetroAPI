I.u[965] = {n:"Piedra de dopeul",nn:"PIEDRA DE DOPEUL",t:50,d:"Esta piedra viene del mundo de los dopeuls. Tiene un enorme valor para algunos habitantes de Amakna.",ep:2,g:663,l:41,wd:true,fm:true,w:15,p:10000};
I.u[966] = {n:"Manual del Sastre",nn:"MANUAL DEL SASTRE",t:25,d:"(Doble clic para aprender el oficio de Sastre)\nEste manual de sastre te permite aprender los secretos del oficio. Para los que no tienen mucha imaginación, las primeras páginas proponen patrones sencillos que puedes recortar.",ep:1,g:13,l:1,wd:true,fm:true,w:1,u:true,p:100};
I.u[967] = {n:"Capa Fulgurante",nn:"CAPA FULGURANTE",t:17,d:"Esta magnífica capa aumenta los puntos de acción del que la lleve puesta. También aumenta su vitalidad y su sabiduría.",ep:13,g:78,l:55,wd:true,fm:true,w:20,p:100000};
I.u[968] = {n:"Cinturón Fulgurante",nn:"CINTURON FULGURANTE",t:10,d:"Este magnífico cinturón aumenta los puntos de acción de quien lo lleve puesto. También aumenta su vitalidad y su sabiduría.",ep:1,g:10,l:30,wd:true,fm:true,w:200,p:100000};
I.u[969] = {n:"Corona del Wey Wabbit",nn:"CORONA DEL WEY WABBIT",t:16,d:"Esta magnífica corona perteneció al tristemente célebre Wey Wabbit.",ep:8,g:21,l:30,wd:true,fm:true,w:30,s:40,p:3000};
I.u[970] = {n:"Bastón del Wey Wabbit",nn:"BASTON DEL WEY WABBIT",t:4,d:"Este bastón pertenecía al tristemente célebre Wey Wabbit.",ep:8,g:21,l:25,wd:true,fm:true,w:20,an:12,tw:true,e:[5,5,1,1,30,20,false,false],s:40,p:2500};
I.u[971] = {n:"Capa del Wey Wabbit",nn:"CAPA DEL WEY WABBIT",t:17,d:"Esta capa pertenecía al tristemente célebre Wey Wabbit. Todavía está impregnada por su transpiración lo cual te permitirá recordar siempre ese magnífico combate en cuanto te la pongas.",ep:8,g:21,l:30,wd:true,fm:true,w:25,s:40,p:2525};
I.u[972] = {n:"Dofus Zanahowia",nn:"DOFUS ZANAHOWIA",t:23,d:"El Dofus Zanahowia aumenta la sabiduría de su portador.",ep:1,g:6,l:6,wd:true,fm:true,w:5,p:100000};
I.u[973] = {n:"Pata de araknela",nn:"PATA DE ARAKNELA",t:24,d:"Una pata muy fina, pero muy peluda. Los mejores Alquimistas consiguen crear potentes venenos con el jugo que extraen de estas patas.",ep:2,g:11,l:1,wd:true,fm:true,w:0,p:1};
I.u[974] = {n:"Cabeza del Campesino Desamparado",nn:"CABEZA DEL CAMPESINO DESAMPARADO",t:24,d:"Lo que tenía que ocurrir ha ocurrido, el Campesino Desamparado perdió finalmente la cabeza.",ep:1,g:12,l:1,wd:true,fm:true,w:0,p:1};
I.u[975] = {n:"Colmillo del Vampiro Cautivo",nn:"COLMILLO DEL VAMPIRO CAUTIVO",t:24,d:"Este diente muy puntiagudo, es el de un Vampiro. Poseerlo en tu inventario o llevarlo alrededor del cuello te permitirá impresionar a tus amigos haciéndoles creer que eres un cazador de Vampiros.",ep:1,g:13,l:1,wd:true,fm:true,w:0,p:1};
I.u[976] = {n:"Calawaza",nn:"CALAWAZA",t:16,d:"La calawaza tiene la particularidad de evitar la dispersión de partículas de magia, esto hace de ella un buen aislante mágico. Por eso algunos guerreros utilizan esta legumbre como sombrero.",ep:4,g:24,l:20,wd:true,fm:true,w:10,p:500};
I.u[989] = {n:"Shigekax limón",nn:"SHIGEKAX LIMON",t:42,d:"Vuelve a disfrutar el sabor de las golosinas de antaño... con este caramelo de limón preparado de forma tradicional.",ep:1,g:23,l:1,wd:true,fm:true,w:1,u:true,p:200};
I.u[990] = {n:"Shigekax fresa",nn:"SHIGEKAX FRESA",t:42,d:"Vuelve a disfrutar el sabor de las golosinas de antaño... con este caramelo de fresa preparado de forma tradicional.",ep:13,g:25,l:1,wd:true,fm:true,w:1,u:true,p:140};
I.u[991] = {n:"Shigekax menta",nn:"SHIGEKAX MENTA",t:42,d:"Vuelve a disfrutar el sabor de las golosinas de antaño... con este caramelo de menta preparado de forma tradicional.",ep:2,g:24,l:1,wd:true,fm:true,w:1,u:true,p:400};
I.u[992] = {n:"Shigekax cereza",nn:"SHIGEKAX CEREZA",t:42,d:"Vuelve a disfrutar el sabor de las golosinas de antaño... con este caramelo de cereza preparado de forma tradicional.",ep:2,g:19,l:1,wd:true,fm:true,w:1,u:true,p:100};
I.u[993] = {n:"Verdely",nn:"VERDELY",t:42,d:"Está preparado con gelatina de menta, aunque no haya conquistado a todos los vegetarianos, no deja de ser una especialidad de Amakna.",ep:5,g:21,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:1};
I.u[994] = {n:"Rogely",nn:"ROGELY",t:42,d:"Está preparado con gelatina de fresa, aunque no haya conquistado a todos los vegetarianos, no deja de ser una especialidad de Amakna.",ep:5,g:20,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:1};
I.u[995] = {n:"Blugely",nn:"BLUGELY",t:42,d:"Preparado con gelatina azul, aunque haya disgustado a más de un vegetariano, no deja de ser una de las especialidades de Amakna.",ep:5,g:22,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:1};
I.u[996] = {n:"Multygely",nn:"MULTYGELY",t:42,d:"La multygely ha sido prohibida a la venta porque provocó la desaparición de muchos niños. Parece ser que envía a la persona que se la come dentro de sus peores pesadillas.",ep:5,g:26,l:1,wd:false,fm:true,w:1,c:"Sc!1056",u:true,p:100};
I.u[997] = {n:"Nuez de kokoko",nn:"NUEZ DE KOKOKO",t:46,d:"Tan redonda como una bola de cañón, esta nuez posee además 3 agujeros muy prácticos para levantarla y lanzarla a lo lejos.",ep:12,g:159,l:42,wd:true,fm:true,w:2,p:1};
I.u[998] = {n:"Calzón mono del Capitán Pirata",nn:"CALZON MONO DEL CAPITAN PIRATA",t:15,d:"El calzón del capitán Pirata demuestra que el ridículo puede matar de vez en cuando. Pero según parece, trae suerte a pesar de todo.",ep:12,g:163,l:82,wd:true,fm:true,w:2,p:1};
I.u[999] = {n:"Tela poco apetitosa",nn:"TELA POCO APETITOSA",t:55,d:"Pegajosa y apestosa, esta amplia tela no deja de ser sin embargo de menor calidad. Una pena que esté agujereada.",ep:12,g:164,l:1,wd:true,fm:true,w:5,p:1};
I.u[1000] = {n:"Barril vacío",nn:"BARRIL VACIO",t:15,d:"Este gran barril está vacío desde hace mucho tiempo, a pesar de ello podemos adivinar lo que contenía: ron, kamas, piratas borrachos... un poco de todo, pero ciertamente nunca llevó agua.",ep:12,g:165,l:1,wd:true,fm:true,w:5,p:1};
I.u[1001] = {n:"Tabla de madera de kokoko",nn:"TABLA DE MADERA DE KOKOKO",t:95,d:"¿Será un futuro fragmento de barco? ¿Un futuro suelo de casa? En cualquier caso, por ahora sólo es una tabla como cualquier otra.",ep:12,g:166,l:42,wd:true,fm:true,w:10,p:10};
I.u[1002] = {n:"Tronco de kokoko",nn:"TRONCO DE KOKOKO",t:38,d:"Como todos los troncos, el del Kokotero es derecho y sólido, pero un poco más delgado que la media.",ep:12,g:167,l:42,wd:true,fm:true,w:1,p:1};
I.u[1003] = {n:"Viga de Madera de kokoko",nn:"VIGA DE MADERA DE KOKOKO",t:38,d:"Una viga de madera sólida. Ten cuidado al manipularla, vete a saber por qué, parece que este tipo de material pesado siempre se siente atraído por el dedo gordo del pie...",ep:12,g:168,l:42,wd:true,fm:true,w:1,p:1};
I.u[1004] = {n:"Indispensable Cuerda",nn:"INDISPENSABLE CUERDA",t:15,d:"Todos los aventureros saben que siempre hay que tener una cuerda como esta... Sin embargo, nadie la lleva nunca consigo.",ep:12,g:170,l:1,wd:true,fm:true,w:1,p:1};
I.u[1005] = {n:"Recibo para una balsa",nn:"RECIBO PARA UNA BALSA",t:24,d:"Un recibo firmado por la mano de Kib Roca y que indica que tu balsa estará lista dentro de poco.",ep:1,g:176,l:1,wd:true,fm:true,w:0,p:1};
I.u[1006] = {n:"Sombrero Pirata",nn:"SOMBRERO PIRATA",t:24,d:"Un sombrero con formas imponentes, una insignia de muerte bordada en la parte delantera, un forro para hacer bonito, este sombrero de pirata es un clásico entre los capitanes.",ep:12,g:14,l:1,wd:true,fm:true,w:0,p:1};
I.u[1007] = {n:"Garfio",nn:"GARFIO",t:24,d:"Accesorio indispensable para los piratas desafortunados, este garfio reemplaza ventajosamente una mano. Su color le da además un aspecto bastante elegante.",ep:12,g:15,l:1,wd:true,fm:true,w:0,p:1};
I.u[1008] = {n:"Mandíbula colgante",nn:"MANDIBULA COLGANTE",t:24,d:"Esta mandíbula en forma de collar proviene, seguramente, del enfado que un capitán tuvo con un marinero que se había partido de risa al verle los calzoncillos a su jefe.",ep:12,g:16,l:1,wd:true,fm:true,w:0,p:1};
I.u[1009] = {n:"Calavera de Pirata",nn:"CALAVERA DE PIRATA",t:24,d:"Una calavera con un paño demuestra que un pirata, incluso muerto, todavía puede ocultar su calvicie.",ep:12,g:17,l:1,wd:true,fm:true,w:0,p:1};
I.u[1010] = {n:"Carapazón verde",nn:"CARAPAZON VERDE",t:24,d:"Este es el caparazón de una tortuga loca que creía se ninja.",ep:12,g:18,l:1,wd:true,fm:true,w:0,p:1};
I.u[1011] = {n:"Carapazón amarillo",nn:"CARAPAZON AMARILLO",t:24,d:"Este es el caparazón de una tortuga loca que sin duda abusó de substancias peligrosas.",ep:12,g:19,l:1,wd:true,fm:true,w:0,p:1};
I.u[1012] = {n:"Carapazón rojo",nn:"CARAPAZON ROJO",t:24,d:"Este es el caparazón de una tortuga loca que se comía tomates y queso sobre pan caliente.",ep:12,g:20,l:1,wd:true,fm:true,w:0,p:1};
I.u[1013] = {n:"Carapazón azul",nn:"CARAPAZON AZUL",t:24,d:"Este es el caparazón de una tortuga loca que quería imitar a Zowo, el Zollo enmascarado.",ep:12,g:21,l:1,wd:true,fm:true,w:0,p:1};
I.u[1014] = {n:"Máscara verde",nn:"MASCARA VERDE",t:24,d:"Una máscara de Kaníbola Reidor, a la efigie de la Verdura, condimento imprescindible para la carne.",ep:12,g:22,l:1,wd:true,fm:true,w:0,p:1};
I.u[1015] = {n:"Máscara Amarilla",nn:"MASCARA AMARILLA",t:24,d:"Una máscara de Kaníbola Llorador, a la efigie del Rayo, elemento que asusta a menudo a la carne joven.",ep:12,g:23,l:1,wd:true,fm:true,w:0,p:1};
I.u[1016] = {n:"Máscara roja",nn:"MASCARA ROJA",t:24,d:"Una máscara de Kaníbola Colérico, a la efigie del Fuego, elemento imprescindible para la asar la carne.",ep:12,g:24,l:1,wd:true,fm:true,w:0,p:1};
I.u[1017] = {n:"Máscara Azul",nn:"MASCARA AZUL",t:24,d:"Una máscara de Kaníbola Sorprendido, a la efigie del Agua, elemento desalterante entre dos bocados de carne.",ep:12,g:25,l:1,wd:true,fm:true,w:0,p:1};
I.u[1018] = {n:"Kola",nn:"KOLA",t:15,d:"Este tarro de gelatina rosa con buen olor no debe en ningún caso servir de alimento. De lo contrario no podrás volver a hablar jamás dado lo pegajosa que es.",ep:12,g:171,l:1,wd:true,fm:true,w:1,p:1};
I.u[1019] = {n:"Casco",nn:"CASCO",t:16,d:"Llevar este casco significa adoptar un look único... A medio camino entre lo cómico y lo ridículo.",ep:1,g:25,l:20,wd:true,fm:true,w:10,p:200};
I.u[1020] = {n:"Casco roto",nn:"CASCO ROTO",t:15,d:"Este casco te ha salvado la vida, pero él no consiguió salvar la suya.",ep:2,g:173,l:1,wd:true,fm:true,w:5,p:1};
I.u[1021] = {n:"Alas de madera",nn:"ALAS DE MADERA",t:17,d:"Quienes hayan soñado desde siempre con poder volar como un Kwak, pueden regresar a sus quehaceres porque estas alas solamente les convertirán en un peligro público y encima estúpido.",ep:1,g:22,l:20,wd:true,fm:true,w:20,p:1200};
I.u[1022] = {n:"Alas rotas",nn:"ALAS ROTAS",t:104,d:"Estas alas no dejan de recordarte que hoy hubieras tenido que quedarte en cama.",ep:2,g:174,l:1,wd:true,fm:true,w:10,p:1};
I.u[1023] = {n:"Botella de ron",nn:"BOTELLA DE RON",t:15,d:"La etiqueta porne " + "\"" + "ron añejo - 10 años de edad" + "\"" + ", pero resulta imposible verificarlo. Una nota precisa también que no hay que abusar de él por los caminos so pena de mareo.",ep:12,g:177,l:1,wd:true,fm:true,w:2,p:1};
I.u[1026] = {n:"Cuchillito de Stek",nn:"CUCHILLITO DE STEK",t:5,d:"El ilustre loco Stek Rokefor encargó hacer estos extraños y temibles cuchillos. Nadie sabía para qué servían hasta que intentó regalar comida a su dios.",ep:1,g:13,l:32,wd:true,fm:true,w:10,an:13,e:[15,3,1,1,30,50,false,false],c:"CA>32&CS>10",p:3000};
I.u[1027] = {n:"Cuchillo de Stek",nn:"CUCHILLO DE STEK",t:5,d:"El ilustre loco Stek Rokefor encargó hacer estos extraños y temibles cuchillos. Nadie sabía para qué servían hasta que intentó regalar comida a su dios.",ep:1,g:13,l:35,wd:true,fm:true,w:10,an:13,e:[15,3,1,1,30,50,false,false],c:"CA>35&CS>12",p:3400};
