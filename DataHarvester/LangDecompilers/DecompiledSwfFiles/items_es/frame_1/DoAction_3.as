I.u[1059] = {n:"Pala de los Campos",nn:"PALA DE LOS CAMPOS",t:8,d:"Esta pala es la preferida de los agricultores, sobre todo de los cosechadores de pwatatas.  \nAunque nada te impida usarla para desnucar a tus enemigos.",ep:1,g:17,l:15,wd:true,fm:true,w:35,an:16,e:[5,4,1,1,40,40,false,false],c:"CC>15",p:750};
