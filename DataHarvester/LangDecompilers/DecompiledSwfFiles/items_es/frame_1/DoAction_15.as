I.u[6642] = {n:"Poderosa Espada de Urandal",nn:"PODEROSA ESPADA DE URANDAL",t:6,d:"Esta espada es un regalo honorífico que se le hace únicamente a los mercenarios endurecidos. Pero también corta muy bien.",ep:1,g:25,l:50,wd:false,fm:true,w:20,an:14,tw:true,e:[10,4,1,1,40,40,false,false],c:"Ps=3",p:1};
I.u[6643] = {n:"Helado Doloden",nn:"HELADO DOLODEN",t:42,d:"Este delicioso helado ha sido fabricado artesanalmente por la pequeña granja Doloden que intenta imponer el Dolo en todos postres congelados.",ep:13,g:143,l:1,wd:false,fm:true,w:1,u:true,ut:true,p:150};
I.u[6644] = {n:"Cono Weal",nn:"CONO WEAL",t:42,d:"Este helado está inspirado de la parte superior del famoso sombrero del Wa y hay que decir que eso produce su efecto.",ep:13,g:144,l:1,wd:false,fm:true,w:1,u:true,ut:true,p:120};
I.u[6646] = {n:"Helado bontariano",nn:"HELADO BONTARIANO",t:42,d:"Este helado, una sútil mezcla de dos perfumes de fruta muestra " + "\"" + "toda la experiencia de los ángeles" + "\"" + " ...bueno, eso es lo que se puede leer sobre la caja.",ep:13,g:145,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:90};
I.u[6647] = {n:"Magma Brakmariano",nn:"MAGMA BRAKMARIANO",t:42,d:"Este hielo, de crujiente suavidad, demuestra " + "\"" + "una vez más la superioridad de Brakmar" + "\"" + " ...o al menos eso pone en la caja.",ep:13,g:146,l:1,wd:false,fm:true,w:1,u:true,ut:true,p:90};
