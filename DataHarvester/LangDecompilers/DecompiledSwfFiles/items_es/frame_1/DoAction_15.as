I.u[6658] = {n:"Peluche del mediulubo",nn:"PELUCHE DEL MEDIULUBO",t:61,d:"Este magnífico peluche hecho con verdaderos pelos de mediulubo te abrigará durante la noche. También desprenderá un agradable olor a sudor de mediulubo muerto en la habitación.",ep:13,g:699,l:1,wd:true,fm:true,w:10,p:1};
I.u[6659] = {n:"Falso kuakuá",nn:"FALSO KUAKUA",t:15,d:"Desconfía, este kuakuá es falso. Dáselo a Tío Gilipato que está al lado del puesto de los patos para recibir una recompensa.",ep:1,g:700,l:1,wd:true,fm:true,w:1,p:1};
I.u[6660] = {n:"Sombrero de novio",nn:"SOMBRERO DE NOVIO",t:16,d:"Con parecido sombrero, ¡¿Quién podría decirte No?!\n¡Con este sombrero, Gloria y conquistas serán tuyas!",ep:2,g:53,l:1,wd:true,fm:true,w:5,c:"PS=0",p:1000};
I.u[6661] = {n:"Caña para kuakuás",nn:"CANA PARA KUAKUAS",t:20,d:"Esta caña te permite pescar un kuakuá en el puesto de la Feria del Trool.",ep:13,g:8,l:1,wd:true,fm:true,w:10,an:18,e:[0,10,1,1,50,2,false,false],p:1};
I.u[6662] = {n:"Sombrero de novia",nn:"SOMBRERO DE NOVIA",t:16,d:"Haz que se derrita como una gelatina de menta al sol con esta maravillosa cofia de seda de larva del sur, se lava con agua dulce.",ep:2,g:54,l:1,wd:true,fm:true,w:5,c:"PS=1",p:1000};
I.u[6663] = {n:"Ramo de rosas demoníacas",nn:"RAMO DE ROSAS DEMONIACAS",t:6,d:"Con este magnífico ramo de pétalos de rosas demoníacas, le demostrarás a tu media naranja que estás dispuesto(a) a arriesgarte en 1000 y 1 aventuras peligrosas por sus hermosos ojos. Y eso sin el más menor rasguño, por un precio interesante, adaptado a los pequeños presupuestos.",ep:2,g:47,l:1,wd:false,fm:true,w:3,an:14,e:[1,4,1,1,50,50,false,true],p:70};
I.u[6664] = {n:"Pergamino del hechizo especial Feliación",nn:"PERGAMINO DEL HECHIZO ESPECIAL FELIACION",t:75,d:"(Doble clic para aprender el hechizo) \nEste pergamino te permite aprender el hechizo Feliación... Sólo para Zurcaráks.",ep:2,g:5,l:1,wd:true,fm:true,w:1,c:"PG=6",u:true,p:1};
I.u[6666] = {n:"Piruleta de tofu",nn:"PIRULETA DE TOFU",t:42,d:"Esta deliciosa piruleta está aromatizada con extractos de tofus. " + "\"" + "Un gustazo para tu paladar, un cataclismo para tu aliento" + "\"" + ".",ep:13,g:147,l:1,wd:false,fm:true,w:1,u:true,ut:true,p:30};
I.u[6667] = {n:"Piruleta trool",nn:"PIRULETA TROOL",t:42,d:"Una piruleta que sabe muy bien y sobre todo que nunca se acaba. Para consumir entre amigos.",ep:13,g:148,l:1,wd:false,fm:true,w:1,u:true,ut:true,p:300};
I.u[6668] = {n:"Perrito Caliente de tofu",nn:"PERRITO CALIENTE DE TOFU",t:42,d:"Esta salchicha muy grasienta te permitirá recuperar fuerzas para digerir las demás salchichas que acabas de ingerir.",ep:13,g:150,l:1,wd:false,fm:true,w:1,u:true,ut:true,p:300};
I.u[6669] = {n:"Burger de Trool",nn:"BURGER DE TROOL",t:42,d:"Este impresionante burger podría alimentar a un ejército de Chafers hambrientos. Consúmase con moderación, el abuso de Trool es peligroso para la salud.",ep:13,g:149,l:1,wd:false,fm:true,w:1,u:true,ut:true,p:600};
I.u[6670] = {n:"Patatas fritas de girasol salvaje",nn:"PATATAS FRITAS DE GIRASOL SALVAJE",t:42,d:"Estas patatas fritas han sido recortadas en los tallos de girasol salvaje y se han freído en un baño hirviente de aceite de Girasol Salvaje para que se sintieran un poco como en su casa.",ep:13,g:151,l:1,wd:false,fm:true,w:1,u:true,p:300};
I.u[6671] = {n:"Alubias",nn:"ALUBIAS",t:58,d:"...",ep:1,g:471,l:1,wd:true,fm:true,w:1,p:1};
I.u[6672] = {n:"Harina de Javier el Panadero",nn:"HARINA DE JAVIER EL PANADERO",t:52,d:"Esta harina es extraordinariamente ligera y fina y es tan suave que se usa para secar las nalgas de los jóvenes Yopukas. Luego la reutilizan para darle más sabor a los panes.",ep:1,g:250,l:100,wd:true,fm:true,w:1,p:0};
I.u[6673] = {n:"Pluma de tofu negro",nn:"PLUMA DE TOFU NEGRO",t:24,d:"Una pluma negra de tofu negro...",ep:1,g:96,l:1,wd:false,fm:true,w:0,p:1};
I.u[6676] = {n:"Espada de Urandal",nn:"ESPADA DE URANDAL",t:6,d:"Esta espada es un regalo honorífico que se le hace únicamente a los mercenarios endurecidos. Pero también corta muy bien.",ep:1,g:25,l:25,wd:false,fm:true,w:20,an:14,tw:true,e:[10,4,1,1,40,40,false,false],c:"Ps=3",p:1};
