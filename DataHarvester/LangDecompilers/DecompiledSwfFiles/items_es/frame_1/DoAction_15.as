I.u[6884] = {n:"Llave de la Mazmorra del Bworker",nn:"LLAVE DE LA MAZMORRA DEL BWORKER",t:84,d:"Esta llave permite entrar en la mazmorra del Bworker. Ojo, esta llave se autodestruirá después de haber sido utilizada.",ep:4,g:713,l:30,wd:true,fm:true,w:1,p:100};
I.u[6885] = {n:"Ficha del Bworker",nn:"FICHA DEL BWORKER",t:15,d:"Perfecta para jugar a cara o cruz o para matar a tus enemigos con un lanzamiento de moneda, esta ficha en oro masivo hará de ti un jugador feliz, pero tal vez desafortunado.",ep:13,g:90,l:100,wd:true,fm:true,w:1,p:100};
