I.u[11781] = {n:"Palador",nn:"PALADOR",t:18,d:"Una mascotilla movida por nobles valores. ¿Seguro que eres una persona digna de algo así?... ¡Pues claro!",ep:3,g:77,l:1,wd:true,fm:true,w:10,c:"PZ=1",p:1};
