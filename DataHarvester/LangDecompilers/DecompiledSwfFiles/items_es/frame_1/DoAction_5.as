I.u[2314] = {n:"Frasco de Jiva",nn:"FRASCO DE JIVA",t:24,d:"...",ep:2,g:82,l:1,wd:true,fm:true,w:0,p:1};
I.u[2315] = {n:"Pelos de Colmillo Blando",nn:"PELOS DE COLMILLO BLANDO",t:54,d:"Antiguamente, Brujit Bardo hizo que se prohibiera la caza de los jóvenes Colmillos Blandos. Desde su exilio, la caza se volvió a permitir y los sastres vuelven a llenarse las alforjas.",ep:1,g:196,l:19,wd:true,fm:true,w:1,p:1};
I.u[2316] = {n:"Colmillo Amarillo",nn:"COLMILLO AMARILLO",t:47,d:"El Colmillo Amarillo de Colmillo Blando asusta a todos aquellos que subestiman su poder.",ep:1,g:458,l:19,wd:true,fm:true,w:1,p:1};
I.u[2317] = {n:"Orejas de lobo",nn:"OREJAS DE LOBO",t:106,d:"Al examinarlas desde más cerca uno puede preguntarse como hace un colmillo blando para oír bien con todos esos bichos y otras porquerías que lleva dentro de las orejas.",ep:1,g:459,l:19,wd:true,fm:true,w:1,p:1};
I.u[2318] = {n:"Pata de oso",nn:"PATA DE OSO",t:103,d:"Algunos goblins llevan una pata de oso alrededor del cuello. Una superstición cuyo origen y significado han sido completamente olvidados.",ep:1,g:460,l:12,wd:true,fm:true,w:1,p:1};
I.u[2319] = {n:"Cola Tupida Blanca",nn:"COLA TUPIDA BLANCA",t:65,d:"Por una vez esta cola no tiene nada de otro mundo excepto que nadie sabe de donde puede venir. Tal vez sea de algún monstruo con una Cola Tupida Blanca pero ni los especialistas se atreven a afirmarlo.",ep:0,g:461,l:40,wd:true,fm:true,w:1,p:1};
I.u[2320] = {n:"Collar Roto de Uginak",nn:"COLLAR ROTO DE UGINAK",t:15,d:"Como cualquier buen Uginak, su collar proporciona informaciones. Viene escrito en él: " + "\"" + "Estoy perdido, mi barrio está por el camino de las Landas de Sidimote, por favor lléveme hasta mis dueños Brakmarianos, se lo agradecerán a palos" + "\"" + ".",ep:14,g:462,l:51,wd:true,fm:true,w:5,p:1};
I.u[2321] = {n:"Morros de Uginak",nn:"MORROS DE UGINAK",t:15,d:"Tienes la impresión de que estos Morros de Uginak podrían babear indefinidamente. Tal vez sea porque este animal tiene fama de goloso eterno.",ep:14,g:463,l:51,wd:true,fm:true,w:1,p:1};
I.u[2322] = {n:"Colmillos de ratas",nn:"COLMILLOS DE RATAS",t:47,d:"No te equivoques, estos dientecitos son en realidad verdaderos colmillos muy bien afilados, malolientes y algo viejos. A veces sirven para reemplazar los dientes de oro de los Anutrofs más tacaños.",ep:1,g:464,l:50,wd:true,fm:true,w:1,p:1};
I.u[2323] = {n:"Columna vertebral",nn:"COLUMNA VERTEBRAL",t:47,d:"Aquí tienes una columna vertebral en buen estado. No es tampoco un motivo para romper la tuya... No es intercambiable.",ep:1,g:465,l:34,wd:true,fm:true,w:2,p:1};
I.u[2324] = {n:"Sandía",nn:"SANDIA",t:46,d:"...",ep:1,g:466,l:1,wd:true,fm:true,w:2,p:1};
I.u[2325] = {n:"Plátano",nn:"PLATANO",t:46,d:"Una famosa adivinanza inventada por el pueblo de los anutrofs dice: " + "\"" + "Oro parece, plata no es. El que no lo adivine, buen yopuka es..." + "\"" + "",ep:1,g:467,l:1,wd:true,fm:true,w:1,p:1};
I.u[2326] = {n:"Naranja",nn:"NARANJA",t:46,d:"...",ep:1,g:468,l:1,wd:true,fm:true,w:1,p:1};
I.u[2327] = {n:"Manzana",nn:"MANZANA",t:46,d:"...",ep:1,g:469,l:1,wd:true,fm:true,w:1,p:1};
I.u[2328] = {n:"Pequeña perla dorada",nn:"PEQUENA PERLA DORADA",t:15,d:"Esta perla dorada es un regalo de la pequeña larva que acompañaste. Su valor sentimental es inmenso. Su suavidad y su color lo convierten en un regalo idóneo para una persona algo feminina.",ep:5,g:483,l:1,wd:true,fm:true,w:1,p:1};
I.u[2329] = {n:"Pera",nn:"PERA",t:46,d:"...",ep:1,g:470,l:1,wd:true,fm:true,w:1,p:1};
I.u[2330] = {n:"Cápsula explosiva",nn:"CAPSULA EXPLOSIVA",t:15,d:"Esta cápsula contiene extractos de agrios mágicos. Se aconseja agitarla mucho antes de usarla para que la pulpa no se queda abajo.",ep:4,g:711,l:23,wd:true,fm:true,w:1,p:1};
I.u[2331] = {n:"Berenjena",nn:"BERENJENA",t:46,d:"No es realmente nutritiva, algunos acompañan la berenjena con todo tipo de condimentos. Esta legumbre fruta tiene mala reputación aunque sea realmente benéfica para la salud. Excepto para la de los Bworks, pero nadie sabe por qué...",ep:1,g:472,l:1,wd:true,fm:true,w:1,p:10};
I.u[2332] = {n:"Caramelo de consolación",nn:"CARAMELO DE CONSOLACION",t:42,d:"Este caramelo ha evitado que te pusieras a llorar.",ep:13,g:135,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:1};
I.u[2333] = {n:"Caramelo rojo superpoderoso",nn:"CARAMELO ROJO SUPERPODEROSO",t:42,d:"Este caramelo, aumentará tu atención durante los combates, por desgracia, tiene un sabor realmente asqueroso. (DOBLE CLIC PARA UTILIZAR, bonus de 20 en sabiduría y de 10 en Prospección para los 30 próximos combates).",ep:1,g:136,l:1,wd:true,fm:false,w:1,c:"PO!1723",u:true,p:1};
I.u[2336] = {n:"Cráneo de chafer",nn:"CRANEO DE CHAFER",t:47,d:"Este cráneo ya no hará más ruido... Aunque nunca se sabe.",ep:1,g:480,l:16,wd:true,fm:true,w:1,p:1};
I.u[2337] = {n:"Libro mágico",nn:"LIBRO MAGICO",t:25,d:"Parece que este libro es mágico. Muy extraño.",ep:1,g:7,l:1,wd:true,fm:true,w:1,p:50};
I.u[2338] = {n:"Viejo Libro",nn:"VIEJO LIBRO",t:25,d:"...",ep:1,g:8,l:1,wd:true,fm:true,w:1,p:100};
I.u[2339] = {n:"Lista de equipos estándar",nn:"LISTA DE EQUIPOS ESTANDAR",t:24,d:"Un guerrero de valor mediano debe llevar puesto otra cosa que andrajos o, al contrario, vestidos chillones. Por eso tendrá que encontrar: \n-Un camafeo al dedo mayor \n-Un pañuelo y un cinturón de vitalidad \n-Botas de Kluh \n-Una capa brakmariana",ep:2,g:41,l:1,wd:true,fm:true,w:0,p:1};
I.u[2340] = {n:"Calzones de miliciano",nn:"CALZONES DE MILICIANO",t:24,d:"Estos son unos verdaderos calzones. Debes de ser uno increíble fetichista para poseer esto. O bien alguien se quiere reír de ti.",ep:2,g:84,l:1,wd:true,fm:true,w:0,p:1};
I.u[2341] = {n:"Arco de Mech Riorraw",nn:"ARCO DE MECH RIORRAW",t:2,d:"Este arco ha sido diseñado por Mech Riorraw, esta calibrado para poder atravesar el río de lava y llegar a lo alto de las murallas de Brakmar, ni más, ni menos.",ep:16,g:8,l:10,wd:true,fm:true,w:100,an:10,tw:true,e:[0,6,20,21,0,30,false,true],c:"Ps=1",p:0};
I.u[2342] = {n:"Anillo de Juramento",nn:"ANILLO DE JURAMENTO",t:24,d:"Este anillo te ha sido entregado por Domen. Devolvérsela significaría que estás a favor de la destrucción de todo. Lo cual te cataloga como uno de los admiradores de Djaul.",ep:2,g:86,l:1,wd:true,fm:true,w:0,p:1};
I.u[2343] = {n:"Pulsera de Djaul",nn:"PULSERA DE DJAUL",t:24,d:"Esta pulsera te ha sido entregada por Djaul. Llevarla puesta significa que estás de acuerdo con sus propósitos.",ep:2,g:85,l:1,wd:true,fm:true,w:0,p:1};
I.u[2344] = {n:"Carta para Oto",nn:"CARTA PARA OTO",t:24,d:"Esta carta te ha sido entregada por Domen para Oto.",ep:2,g:65,l:1,wd:true,fm:true,w:0,p:1};
I.u[2345] = {n:"Ojo de tortuga",nn:"OJO DE TORTUGA",t:24,d:"Ahora esta tortuga ya no se hará la listilla.",ep:2,g:70,l:1,wd:true,fm:true,w:0,p:1};
I.u[2346] = {n:"Traje de Tymador",nn:"TRAJE DE TYMADOR",t:14,d:"No, no se trata del famoso traje del dragovampi, el famoso murciélago volador. Esto es la vestimenta de un habitante del cementerio, o sea un Tymador.",ep:1,g:27,l:1,wd:true,fm:true,w:10,p:1};
I.u[2347] = {n:"Informe de misión",nn:"INFORME DE MISION",t:24,d:"Este informe explica cómo se llevó el asalto de Brakmar por la muralla Este.",ep:2,g:55,l:1,wd:true,fm:true,w:0,p:1};
I.u[2348] = {n:"Tymador Attitude",nn:"TYMADOR ATTITUDE",t:31,d:"Tienes la Tymador attitude hasta que empieces un combate.",ep:1,g:17,l:1,wd:true,fm:true,w:0,m:true,u:true,p:1};
I.u[2349] = {n:"Carta para Oto",nn:"CARTA PARA OTO",t:24,d:"Esta carta te ha sido entregada por Domen para Oto. Pero te has olvidado de dársela.",ep:2,g:65,l:1,wd:true,fm:true,w:0,p:1};
I.u[2350] = {n:"Bola de pelos",nn:"BOLA DE PELOS",t:24,d:"Parece que esta bola de pelos perteneció a uno de tus enemigos.",ep:2,g:30,l:1,wd:true,fm:true,w:0,p:1};
I.u[2351] = {n:"Pan del Pankemante",nn:"PAN DEL PANKEMANTE",t:33,d:"En este pan, hay de comer y de beber, escurriéndolo mucho, es posible llenar una jarrita de cerveza.",ep:16,g:13,l:1,wd:true,fm:true,w:2,u:true,ut:true,p:1};
