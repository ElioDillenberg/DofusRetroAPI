I.u[2118] = {n:"Capa Brakmariana",nn:"CAPA BRAKMARIANA",t:17,d:"Esta capa permite desplazarse por un cementerio sin ser reconocido, o disfrazarse en Chafosa cuando la extrema perversidad se hace realmente necesaria.",ep:1,g:20,l:1,wd:true,fm:true,w:7,c:"Ps=2",p:800};
I.u[2119] = {n:"Capa bontariana",nn:"CAPA BONTARIANA",t:17,d:"Esta capa te da alas. Incluso el humo que desprende cuando se quema hará que te sientas ligero, muy ligero...",ep:5,g:34,l:1,wd:true,fm:true,w:3,c:"Ps=1",p:800};
I.u[2120] = {n:"Palabras de Ixa Mustam",nn:"PALABRAS DE IXA MUSTAM",t:24,d:"" + "\"" + "Mi testamento se encuentra en el faro de Amakna, consíguete una Palita de Maderucha para desenterrarlo. ¡Y dale un sopapo de mi parte a mi hijo!" + "\"" + "",ep:2,g:50,l:1,wd:true,fm:true,w:0,p:1};
