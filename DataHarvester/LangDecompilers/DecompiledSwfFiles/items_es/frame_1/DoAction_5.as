I.u[2208] = {n:"Negra",nn:"NEGRA",t:30,d:"Esta maldición te quita Suerte para el próximo combate.",ep:1,g:5,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2209] = {n:"Pergamino de la Suerte",nn:"PERGAMINO DE LA SUERTE",t:14,d:"Este pergamino te permite mejorar tu Suerte, eso te puede servir para encontrar más fácilmente algún tofu perdido.",ep:1,g:11,l:1,wd:true,fm:true,w:2,ut:true,p:250};
I.u[2210] = {n:"Pergamino de la Negra",nn:"PERGAMINO DE LA NEGRA",t:14,d:"Este pergamino te permite reducir la suerte de un determinado blanco, eso te puede servir cuando un adversario quiera volverse rico a tus despensas.",ep:1,g:12,l:1,wd:true,fm:true,w:2,ut:true,p:500};
I.u[2211] = {n:"Traje yopuquesco",nn:"TRAJE YOPUQUESCO",t:14,d:"Gracias a este disfraz vas a brillar en los bailes mundanos. El único problema es que te dirán a menudo que eres listo como un yopuka.",ep:1,g:14,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:100};
I.u[2212] = {n:"Traje xeloresco",nn:"TRAJE XELORESCO",t:14,d:"Con este sombrero y los utensilios que le acompañan, la semejanza engañará a cualquiera. Eres un xelor, de eso no cabe duda.",ep:1,g:15,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:100};
I.u[2213] = {n:"Traje sadidesco",nn:"TRAJE SADIDESCO",t:14,d:"No hace falta gran cosa para parecerse a un sadida: una alfombrilla, tener la mirada nublada y la cabeza atrancada en el recto. Efecto garantizado.",ep:1,g:16,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:100};
I.u[2214] = {n:"Yopuka Attitude",nn:"YOPUKA ATTITUDE",t:31,d:"Tienes la Yopuka attitude hasta que empieces un combate.",ep:1,g:4,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2215] = {n:"Xelor Attitude",nn:"XELOR ATTITUDE",t:31,d:"Tienes la Xelor attitude hasta que empieces un combate.",ep:1,g:5,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2216] = {n:"Sadida Attitude",nn:"SADIDA ATTITUDE",t:31,d:"Tienes la Sadida attitude hasta que empieces un combate.",ep:1,g:6,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2217] = {n:"Traje fequesco",nn:"TRAJE FEQUESCO",t:14,d:"Este pelucón sale de un viejo armario, eso se nota en el aspecto un tanto " + "\"" + "retro" + "\"" + " de esta cofia. En cambio, es increíble cómo te pareces a un Feca poniéndotelo en la cabeza (porque tú lo velas).",ep:1,g:17,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:100};
I.u[2218] = {n:"Feca Attitude",nn:"FECA ATTITUDE",t:31,d:"Tienes la Feca attitude hasta que empieces un combate.",ep:1,g:7,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2219] = {n:"Traje aniripesco",nn:"TRAJE ANIRIPESCO",t:14,d:"Estas alas de aniripsa no te permitirán volar. O sea que no te hagas el fanfarrón delante de tus amigos con esta indumentaria, puede que hagas el ridículo al momento de aterrizar.",ep:1,g:18,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:100};
I.u[2220] = {n:"Aniripsa Attitude",nn:"ANIRIPSA ATTITUDE",t:31,d:"Tienes las Aniripsa attitude hasta que empieces un combate.",ep:1,g:8,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2221] = {n:"Traje anutrofesco",nn:"TRAJE ANUTROFESCO",t:14,d:"Esta falsa barba te hace parecer a un Anutrof pero también te pega su haliento resacoso dominical. Todas caerán...",ep:1,g:19,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:100};
I.u[2222] = {n:"Anutrof Attitude",nn:"ANUTROF ATTITUDE",t:31,d:"Tienes la Anutrof attitude hasta que empieces un combate.",ep:1,g:9,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2223] = {n:"Traje sacrigriesco",nn:"TRAJE SACRIGRIESCO",t:14,d:"Un poco de sangre de tofu y unas cuantas falsas cicatrices, una mirada de pirata psicótica y hecho está. Podrás participar en las reuniones de los sacrógritos haciéndote pasar por uno de ellos sin levantar sospechas.",ep:1,g:20,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:1};
I.u[2224] = {n:"Sacrógrito Attitude",nn:"SACROGRITO ATTITUDE",t:31,d:"Tienes la Sacrógrito attitude hasta que empieces un combate.",ep:1,g:10,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2225] = {n:"Traje sramesco",nn:"TRAJE SRAMESCO",t:14,d:"Un kwan, una capa puesta sobre la cabeza y la ilusión es perfecta. Si te pones este traje, te parecerás a un horrible y abyecto personaje que sale de un  cementerio: En dos palabras: Un sram.",ep:1,g:21,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:1};
I.u[2226] = {n:"Sram Attitude",nn:"SRAM ATTITUDE",t:31,d:"Tienes la Sram attitude hasta que empieces un combate.",ep:1,g:11,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2227] = {n:"Traje osamodesco",nn:"TRAJE OSAMODESCO",t:14,d:"Tras equiparte con esta cola y estos cuernos, este traje te vendrá como anillo al dedo para invitarte a las famosas fiestas VIP (Violencias, Insultos y Porrazos) que organizan los osamodas a escondidas.",ep:1,g:22,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:1};
I.u[2228] = {n:"Osamodas Attitude",nn:"OSAMODAS ATTITUDE",t:31,d:"Tienes la Osamodas attitude hasta que empieces un combate.",ep:1,g:12,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2229] = {n:"Traje zurcaresco",nn:"TRAJE ZURCARESCO",t:14,d:"Aquí tienes una orejas de pelo de wabbit muy suave, una cola atigrada y una caja llena de pulgas. No hay duda, cuando les pasarás estos bichejos a tus vecinos, nadie podrá dudar sobre tus talentos de transformista.",ep:1,g:23,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:1};
I.u[2230] = {n:"Zurcarák Attitude",nn:"ZURCARAK ATTITUDE",t:31,d:"Tienes la Zurcarák attitude hasta que empieces un combate.",ep:1,g:13,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2231] = {n:"Traje ocraesco",nn:"TRAJE OCRAESCO",t:14,d:"Ya lo habrás notado, el ocra se pasea con su carcaj. No te extrañes si, llevando tú uno, te toman por uno de los suyos. Pero no es más que un farol, qué bueno resulta engañarlos a todos.",ep:1,g:24,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:1};
I.u[2232] = {n:"Ocra Attitude",nn:"OCRA ATTITUDE",t:31,d:"Tienes la Ocra attitude hasta que empieces un combate.",ep:1,g:14,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2233] = {n:"La Mordisca",nn:"LA MORDISCA",t:10,d:"Este cinturón fue creado por el Zollo Tsar. Tuvo la fabulosa idea de recoger todos los dientes que se podían encontrar en un campo de batalla Wabbit. Desde aquel día, ese gesto se ha convertido en una costumbre.",ep:8,g:20,l:28,wd:true,fm:true,w:20,c:"CS<40&CI>80",p:2400};
I.u[2234] = {n:"Bonta Attitude",nn:"BONTA ATTITUDE",t:31,d:"Tienes la Bonta attitude hasta que empieces un combate.",ep:1,g:15,l:1,wd:true,fm:true,w:0,m:true,u:true,p:1};
I.u[2235] = {n:"Brakmar Attitude",nn:"BRAKMAR ATTITUDE",t:31,d:"Tienes la Brakmar attitude hasta que empieces un combate.",ep:1,g:16,l:1,wd:true,fm:true,w:0,m:true,u:true,p:1};
I.u[2236] = {n:"Disfraz bontesco",nn:"DISFRAZ BONTESCO",t:14,d:"Con esta pinta, muchos viajeros te saludarán y te preguntarán el camino que deben tomar.",ep:1,g:25,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:1};
I.u[2237] = {n:"Traje brakmaresco",nn:"TRAJE BRAKMARESCO",t:14,d:"Con esta indumentaria, todos te temerán.",ep:1,g:26,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:1};
I.u[2239] = {n:"Pólvora de Aniripsa",nn:"POLVORA DE ANIRIPSA",t:48,d:"Esta pólvora es producida en pequeñas cantidades por las alas de los aniripsas. Parece tener virtudes curativas cuando se echan sobre las mascotas.",ep:3,g:260,l:1,wd:true,fm:true,w:1,p:1000};
I.u[2240] = {n:"Hadita Artificial Roja",nn:"HADITA ARTIFICIAL ROJA",t:74,d:"Ideal para festejar bodas, perfecto para poner ambiente en las fiestas de las Bragas Rotas.",ep:16,g:28,l:1,wd:true,fm:true,w:1,ut:true,p:350};
I.u[2241] = {n:"Gelatina menta real",nn:"GELATINA MENTA REAL",t:110,d:"La gelatina real ha despojado a más de un aventurero demasiado atrevido, pero acabó aniquilada. Ojo, no te la comas como si fuese una simple Jalea Menta.",ep:7,g:408,l:100,wd:true,fm:true,w:1,p:100};
I.u[2242] = {n:"Gelatina fresa real",nn:"GELATINA FRESA REAL",t:110,d:"Un concentrado de gelatina de fresa real puede transformar cualquier tabi pacifista en máquina de guerra sedienta de sangre. Cuidado, no lo sirvas como aperitivo a tus amigos.",ep:7,g:409,l:150,wd:true,fm:true,w:1,p:100};
I.u[2245] = {n:"Pinza de scorbuto",nn:"PINZA DE SCORBUTO",t:47,d:"Antiguamente se usaba para rascarse el trasero, también resulta ser un colgador de capas muy práctico si se clava en una pared. También puedes clavártela en el trasero aunque sea un poco más doloroso...",ep:14,g:411,l:36,wd:true,fm:true,w:1,p:1};
I.u[2246] = {n:"Corona rota del Tofu Real",nn:"CORONA ROTA DEL TOFU REAL",t:15,d:"Esta corona se rompió tras sufrir potentes ataques mágicos o por el último cabezazo que el Tofu Real intentó darte.",ep:3,g:412,l:100,wd:true,fm:true,w:10,p:250};
I.u[2247] = {n:"Pluma de Tofu Real",nn:"PLUMA DE TOFU REAL",t:53,d:"Sólo los Yopukas serían incapaces de darse cuenta de que esta pluma viene de la grupa de un Tofu Real. Y eso que se nota mucho ya que la pluma es más gruesa.",ep:3,g:413,l:100,wd:true,fm:true,w:11,p:250};
I.u[2248] = {n:"Pelo de Uginak",nn:"PELO DE UGINAK",t:54,d:"Este pelo da calor, ya ves que tu mano se ha puesto a sudar desde que lo observas. A no ser que sea por miedo a las represalias del próximo uginak que te vea con ese pelo en el bolsillo...",ep:14,g:414,l:51,wd:true,fm:true,w:1,p:1};
I.u[2249] = {n:"Hoja de Tronkónido",nn:"HOJA DE TRONKONIDO",t:36,d:"Esta hoja seguro que le da la fuerza necesaria al tronkónido para que pueda luchar. Ahora bien, será mejor saber como se utiliza. Lo primero que a uno se le ocurre es picarla e incorporarla a un plato de alubias. Una delicia.",ep:2,g:415,l:35,wd:true,fm:true,w:1,p:1};
I.u[2250] = {n:"Madera de tronkónido",nn:"MADERA DE TRONKONIDO",t:38,d:"Este cacho de madera suena hueco. La savia verde que de él se escapa indica que esta parte era la nariz del Tronkónido.",ep:9,g:416,l:35,wd:true,fm:true,w:1,p:100};
I.u[2251] = {n:"Mega piedra del crujidor",nn:"MEGA PIEDRA DEL CRUJIDOR",t:51,d:"¡No te tiro la megapiedra ni escondo la mano!",ep:5,g:417,l:45,wd:true,fm:true,w:15,p:150};
I.u[2252] = {n:"Diente de oro del crujidor",nn:"DIENTE DE ORO DEL CRUJIDOR",t:47,d:"Acabas de romperle los dientes descaradamente a un crujidor que pasaba por ahí. Que lástima que tuviera sólo uno, una dentadura hubiera cubierto de sobra los gastos de Zaap durante un mes.",ep:5,g:418,l:31,wd:true,fm:true,w:5,p:1};
I.u[2253] = {n:"Flor de zampávoro",nn:"FLOR DE ZAMPAVORO",t:35,d:"Esta flor facilita el tránsito intestinal. Se recomienda consumirla después de haber pasado unos días en casa de su cocinero preferido.",ep:12,g:420,l:42,wd:true,fm:true,w:1,p:100};
I.u[2254] = {n:"Hoja de mazorral",nn:"HOJA DE MAZORRAL",t:36,d:"La hoja de Mazorral tiene un sabor muy particular, entre la avellana y la enchalada poco fresca. En cualquier caso tus platos tendrán un pequeño perfume a mazorraleza si realmente deseas probarlos.",ep:2,g:421,l:22,wd:true,fm:true,w:1,p:1};
I.u[2255] = {n:"Informe del espía",nn:"INFORME DEL ESPIA",t:24,d:"Ninguna actividad peligrosa en estos momentos. La organización de los milicianos se ve afectada por el asesinato de uno de los suyos. Parece que fue un bontariano el que se ocupó del crimen.",ep:2,g:55,l:1,wd:true,fm:true,w:0,p:1};
I.u[2256] = {n:"Carcasa de rata de alcantarillas",nn:"CARCASA DE RATA DE ALCANTARILLAS",t:24,d:"Esta rata ya no es más que carne fría. Una menos que se pasea por la entrada de las alcantarillas.",ep:2,g:121,l:1,wd:true,fm:true,w:0,p:1};
I.u[2257] = {n:"Cacho de pared de alcantarilla",nn:"CACHO DE PARED DE ALCANTARILLA",t:24,d:"Este cacho de pared de alcantarilla ha sido encontrado dentro del estómago de un chisme.",ep:2,g:55,l:1,wd:true,fm:true,w:0,p:1};
I.u[2259] = {n:"Diente de Karne",nn:"DIENTE DE KARNE",t:47,d:"Tienes en tus manos al juego más antiguo de la comarca de Amakna. No hay reglas más sencillas:tira un Diente y trata de recogerlo con la misma mano, procurando al mismo tiempo recuperar otro diente de Karne en el suelo.",ep:4,g:13,l:16,wd:true,fm:true,w:1,p:20};
I.u[2260] = {n:"Tarjeta de visita de un brakmariano",nn:"TARJETA DE VISITA DE UN BRAKMARIANO",t:24,d:"Alguien ha metido esta tarjeta de visita dentro de tu equipo. Parece que ha sido un brakmariano. Esta tarjeta te anuncia que los Guerreros están interesados por tu talento de asesino y te proponen unirte a la loca aventura brakmariana.",ep:2,g:72,l:1,wd:true,fm:true,w:0,c:"PO=2177",p:1};
I.u[2261] = {n:"Espíritu husmeador",nn:"ESPIRITU HUSMEADOR",t:30,d:"Un espíritu husmeador te ha endemoniado. Se nutre con tu olor y con tu vitalidad.",ep:1,g:7,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2262] = {n:"Descripción del asesino",nn:"DESCRIPCION DEL ASESINO",t:24,d:"Has encontrado a un Bontariano que apestaba a sangre. Todas las informaciones que has podido juntar sobre este personaje están escritas sobre esta hoja de papel.",ep:2,g:55,l:1,wd:true,fm:true,w:0,c:"PO=2177",p:1};
I.u[2263] = {n:"Bastón Husmeador de Sangre",nn:"BASTON HUSMEADOR DE SANGRE",t:80,d:"Este bastón está poseído por un espíritu husmeador. Se puede liberar este espíritu que saldrá entonces disparado en línea recta hasta cruzarse con un objeto que tenga el olor que le gusta. Atormentará entonces a cualquier persona durante un tiempo antes de volver al mundo de los espíritus.",ep:16,g:25,l:1,wd:true,fm:true,w:10,ut:true,p:1};
I.u[2264] = {n:"Piel de goblin",nn:"PIEL DE GOBLIN",t:59,d:"Un Goblin suele ser burlón y travieso, pero lo haces pedazos, seguro que no se atreverá tanto.",ep:4,g:422,l:16,wd:true,fm:true,w:1,p:1};
I.u[2265] = {n:"Descripción de un Bontariano",nn:"DESCRIPCION DE UN BONTARIANO",t:24,d:"Has liberado a un espíritu husmeador pero no se ha pegado sobre la persona correcta. Has anotado algunas informaciones de la persona sobre este cacho de papel para poder recordarlas.",ep:2,g:55,l:1,wd:true,fm:true,w:0,c:"PO!2177",p:1};
I.u[2266] = {n:"Leche del Cochinillo",nn:"LECHE DEL COCHINILLO",t:15,d:"Esta leche es la de un Cochinillo. Nunca se sabe... Puede que pensaras haberlo sacado de un tofu, de eso nada.",ep:1,g:423,l:18,wd:true,fm:true,w:1,p:1};
I.u[2267] = {n:"Dragoleche",nn:"DRAGOLECHE",t:15,d:"Esta dragoleche es un alimento muy difícil de conseguir ya que nadie ha conseguido todavía ordeñar a un Dragocerdo. Podemos imaginar fácilmente que por lo menos 7 personas son necesarias para sujetar al bicho mientras que la octava lo ordeña muy rápidamente.",ep:2,g:424,l:70,wd:true,fm:true,w:2,p:1};
I.u[2268] = {n:"Cabellera de Bwork Arquero",nn:"CABELLERA DE BWORK ARQUERO",t:15,d:"Aquí tienes una cabellera que podría estar de moda entre los Anutrofs:el piostizo. Una pizca de producto antipiojo y estarás listo para participar en el concurso del Anutrof con más caché.",ep:4,g:425,l:24,wd:true,fm:true,w:1,p:1};
I.u[2269] = {n:"Ojo de larva azul",nn:"OJO DE LARVA AZUL",t:24,d:"Acabas de recuperar este ojo intacto. Bien preparados, los ojos revueltos son ideales para un desayuno equilibrado. Este ojo de larva le demostrará a Oto que realmente has matado a una larva azul.",ep:1,g:70,l:1,wd:true,fm:true,w:0,p:1};
I.u[2270] = {n:"Racimo de huevos de larva",nn:"RACIMO DE HUEVOS DE LARVA",t:24,d:"Este racimo de huevos se parece a un enorme racimo de uvas. Pero será mejor que no te lo comas...",ep:1,g:83,l:1,wd:true,fm:true,w:0,p:1};
I.u[2271] = {n:"Cuero lila de bwork",nn:"CUERO LILA DE BWORK",t:56,d:"Abundan los misterios acerca de esta artesanía Bwork. Algunos aventureros creen que la tinta violácea procedería de los Goblines, pero se desconoce el origen del cuero. Qué raro.",ep:4,g:426,l:31,wd:true,fm:true,w:1,p:1};
I.u[2272] = {n:"Larva dorada",nn:"LARVA DORADA",t:32,d:"Esta larva acaba de nacer de un huevo. Su primera mirada que se posó sobre ti, te considera como su propia madre.",ep:1,g:5,l:1,wd:true,fm:true,w:10,m:true,p:1};
I.u[2273] = {n:"Tejido púrpura",nn:"TEJIDO PURPURA",t:55,d:"Ligero y suave, esas son las principales características de este extraño Tejido Púrpura. En su interior verás una etiqueta en la que pone " + "\"" + "made in talla one" + "\"" + "",ep:2,g:427,l:23,wd:true,fm:true,w:1,p:1};
I.u[2274] = {n:"Vela del minero oscuro",nn:"VELA DEL MINERO OSCURO",t:15,d:"Fíjate en los anutrofs. Cuando las minas son oscuras ya no lo ves. El minero oscuro, cuando está en la oscuridad, tampoco lo ves ya que ahora tienes su vela.",ep:1,g:428,l:24,wd:true,fm:true,w:10,p:10000};
I.u[2275] = {n:"Cuero de puerkazo",nn:"CUERO DE PUERKAZO",t:56,d:"El Puerkazo estuvo a punto de desaparecer de la cadena alimentaria. Pues hizo falta aparear Jabalíes con Cochinillos para volver a encontrar las Llanuras de Cania llenas de Puerkazos. Desde entonces, el Puerkazo odia al mundo entero...",ep:2,g:429,l:40,wd:true,fm:true,w:10,p:1};
I.u[2277] = {n:"Mortaja",nn:"MORTAJA",t:55,d:"Para obtener más credibilidad, los fantasmas se visten con una mortaja que les permite aterrorizar a los aventureros perdidos.",ep:14,g:430,l:65,wd:true,fm:true,w:1,p:1000};
I.u[2278] = {n:"Tejido invisible",nn:"TEJIDO INVISIBLE",t:55,d:"Este tejido perteneció a Harry Toper, cazador de topos. Se enrollaba en él para matar a los topos que destrozaban su jardín. El carismático Capitán Chafer se apoderó más tarde de este tejido, antes de desaparecer en circunstancias un tanto extrañas...",ep:1,g:431,l:23,wd:true,fm:true,w:1,p:1};
I.u[2279] = {n:"Diente puntiagudo",nn:"DIENTE PUNTIAGUDO",t:47,d:"Este diente pertenecía a un Vampiro, no cabe duda. Eso se puede comprobar mirándolo desde muy cerca, verás que no existe diente más puntiagudo.",ep:3,g:432,l:15,wd:true,fm:true,w:1,p:1};
I.u[2280] = {n:"Pajita Vampiresca",nn:"PAJITA VAMPIRESCA",t:15,d:"Nunca te fíes de un Vampiro con una pajita en la mano, podría chuparte la sangre hasta la última gota.",ep:3,g:433,l:22,wd:true,fm:true,w:1,p:1};
I.u[2281] = {n:"Tejido sanguíneo",nn:"TEJIDO SANGUINEO",t:55,d:"Parece que este tejido está vivo, los Vampiros lo utilizan a menudo para disfrazarse durante los carnavales...",ep:3,g:434,l:15,wd:true,fm:true,w:1,p:1};
I.u[2282] = {n:"Mechón de pelo canoso",nn:"MECHON DE PELO CANOSO",t:15,d:"Aquí tienes la prueba que los monstruos también envejecen.",ep:1,g:435,l:24,wd:true,fm:true,w:1,p:1};
I.u[2283] = {n:"Ojo de kwoan",nn:"OJO DE KWOAN",t:109,d:"Este ojo de kwoan tiene efectos hipnóticos sorprendentes, así que no le saques el ojo de encima cuando lo estés manipulando.",ep:3,g:436,l:8,wd:true,fm:true,w:1,p:1};
I.u[2284] = {n:"Nota de Harry Stóteles",nn:"NOTA DE HARRY STOTELES",t:24,d:"La entrada de la legendaria gruta de las larvas doradas se encuentra un poco al Oeste de Amakna. Los poderes latentes de la larva dorada te permitirán entrar. Cuidado, si tu pequeña larva recibe daños se precipitará en los brazos de su padre.",ep:2,g:55,l:1,wd:true,fm:true,w:0,p:1};
I.u[2285] = {n:"Tejido oscuro",nn:"TEJIDO OSCURO",t:55,d:"La particularidad de este Tejido es que irrita la piel si lo tocas. Será porque algún Uginak se ha sonado los mocos con él durante muchos años...",ep:3,g:437,l:50,wd:true,fm:true,w:1,p:1};
I.u[2286] = {n:"Vendas",nn:"VENDAS",t:15,d:"Estas vendas son toda una revolución: triple espesor, perfumadas con lavanda, ultra resistentes. Un verdadero producto de calidad.",ep:2,g:438,l:51,wd:true,fm:true,w:1,p:1};
I.u[2287] = {n:"Cuero negro",nn:"CUERO NEGRO",t:56,d:"Este cuero ya ha sido tratado, su proveniencia no es muy segura pero el color demuestra que sin duda se ha utilizado un tinte especial.",ep:1,g:439,l:74,wd:true,fm:true,w:1,p:1};
I.u[2288] = {n:"Pata de wabbit",nn:"PATA DE WABBIT",t:103,d:"Muchos turistas van a la isla de Wabbits ante todo para hacerse con una Pata de Wabbit que hace las veces de talismán. Por desgracia, su efecto no es más que un espejismo, pero mejor callarse,la buena marcha del turismo podría acabar pagándolo.",ep:8,g:440,l:18,wd:true,fm:true,w:1,p:1};
I.u[2290] = {n:"Caparazón de escarahoja blanco",nn:"CAPARAZON DE ESCARAHOJA BLANCO",t:107,d:"No hace falta gran cosa para conseguir una bonita pantalla para lámpara. Aunque este caparazón ha de tener otra utilidad. Vamos, esperemos que sí la tenga...",ep:3,g:441,l:32,wd:true,fm:true,w:1,p:1};
I.u[2291] = {n:"Caparazón de escarahoja azul",nn:"CAPARAZON DE ESCARAHOJA AZUL",t:107,d:"No hace falta gran cosa para conseguir una bonita pantalla para lámpara. Aunque este caparazón ha de tener otra utilidad. Vamos, esperemos que sí la tenga...",ep:3,g:442,l:32,wd:true,fm:true,w:1,p:1};
I.u[2292] = {n:"Caparazón de escarahoja rojo",nn:"CAPARAZON DE ESCARAHOJA ROJO",t:107,d:"No hace falta gran cosa para conseguir una bonita pantalla para lámpara. Aunque este caparazón ha de tener otra utilidad. Vamos, esperemos que sí la tenga...",ep:3,g:443,l:32,wd:true,fm:true,w:1,p:1};
I.u[2293] = {n:"Caparazón de escarahoja verde",nn:"CAPARAZON DE ESCARAHOJA VERDE",t:107,d:"No hace falta gran cosa para conseguir una bonita pantalla para lámpara. Aunque este caparazón ha de tener otra utilidad. Vamos, esperemos que sí la tenga...",ep:3,g:444,l:32,wd:true,fm:true,w:1,p:1};
I.u[2294] = {n:"Vísceras de escarahoja",nn:"VISCERAS DE ESCARAHOJA",t:15,d:"A simple vista, estas vísceras no tienen un aspecto muy apetitoso. Sin embargo, picadas con especias secadas al sol y con un poco de menta salvaje y estofadas en un viejo calderón oxidado, estas vísceras colmarán de felicidad al más gourmet de los chafers.",ep:3,g:445,l:32,wd:true,fm:true,w:1,p:1};
I.u[2295] = {n:"Fragmento de Espada Reptiliana",nn:"FRAGMENTO DE ESPADA REPTILIANA",t:15,d:"Con este fragmento de Espada colgado en tu chimenea, podrás impresionar a tus invitados contándoles que eres domador de Cocodrails, como el excéntrico Mikul, más famoso bajo el mote de Cocodrail Dandi.",ep:4,g:446,l:69,wd:true,fm:true,w:10,p:1};
I.u[2296] = {n:"Escudo Reptiliano",nn:"ESCUDO REPTILIANO",t:15,d:"¿Para qué demonios puede servirte este magnífico escudo adornado con piedras preciosa? Mientras que te lo piensas, podrá servir de bandeja para traerles el té a tus amigos.",ep:4,g:447,l:69,wd:true,fm:true,w:15,p:1};
I.u[2297] = {n:"Flecha de ballesta",nn:"FLECHA DE BALLESTA",t:15,d:"Esta flecha puede servir para confeccionar un arma recortada pero muy cortante. También la puedes usar como palillo, tú verás.",ep:1,g:125,l:34,wd:true,fm:true,w:1,p:1};
I.u[2298] = {n:"Cola de Demonio",nn:"COLA DE DEMONIO",t:65,d:"Acabas de coger el Demonio por la Cola lo cual debe ser bastante desagradable.",ep:0,g:448,l:1,wd:true,fm:true,w:1,p:1};
I.u[2299] = {n:"Hombrera de Dark Vlad",nn:"HOMBRERA DE DARK VLAD",t:15,d:"Esta Hombrera parece haber sufrido más de una herida. Aunque parezca débil, más vale no golpear a Dark Vlad en el hombro si quieres quedarte con tu arma.",ep:2,g:449,l:120,wd:true,fm:true,w:5,p:80};
I.u[2300] = {n:"Fragmento de hueso",nn:"FRAGMENTO DE HUESO",t:47,d:"Este fragmento de hueso procede de un resto de chafer que data del neolítico o de un almuerzo de un mediulubo. Pero existen grandes probabilidades de que su origen sea de distinta naturaleza.",ep:9,g:450,l:43,wd:true,fm:true,w:1,p:1};
I.u[2301] = {n:"Mostacho de Kólerat",nn:"MOSTACHO DE KOLERAT",t:15,d:"Poco resistente, el mostacho podrá servirte para hacer el gandul delante de tus amigos. Gracias a él podrás disfrazarte en Sava Torh Balí, uno de los mayores pintores Brakmarianos.",ep:2,g:451,l:9,wd:true,fm:true,w:1,p:1};
I.u[2302] = {n:"Lengua de cangrejo",nn:"LENGUA DE CANGREJO",t:15,d:"Los sabios de Amakna aún siguen preguntándose para qué le sirve esta lengua al cangrejo. Se llevarían un buen disgusto si lo supieran.",ep:1,g:452,l:4,wd:true,fm:true,w:1,p:1};
I.u[2303] = {n:"Caparazón de cangrejo",nn:"CAPARAZON DE CANGREJO",t:107,d:"Correctamente vaciado, este caparazón de cangrejo puede contener diversos objetos. No obstante, los objetos en cuestión conservarán eternamente un olor a cangrejo más o menos desestabilizante.",ep:1,g:453,l:4,wd:true,fm:true,w:2,p:1};
I.u[2304] = {n:"Fragmento de piedra pulida",nn:"FRAGMENTO DE PIEDRA PULIDA",t:51,d:"Este fragmento procede de la parte izquierda del trasero de un Crujidor de las Llanuras. De lo mejorcito que hay para machacar a una Arakna, sin hablar del Crujidor en persona.",ep:5,g:454,l:45,wd:true,fm:true,w:5,p:1};
I.u[2305] = {n:"Fragmento de piedra afilada",nn:"FRAGMENTO DE PIEDRA AFILADA",t:51,d:"Este fragmento procede del omóplato derecho de un Crujidor de las Llanuras. A no ser que se trate de una parte del pubis. En todo caso, no coloques esta piedra en cualquier lugar.",ep:5,g:455,l:45,wd:true,fm:true,w:5,p:1};
I.u[2306] = {n:"Corazón de Crujidor",nn:"CORAZON DE CRUJIDOR",t:51,d:"Corazón de piedra, fíjate en el amor que siento por ti. Si te empeñas en seguir así, te dejo por tierra, no quiero que sigas agobiándome.",ep:5,g:456,l:45,wd:true,fm:true,w:5,p:1};
I.u[2307] = {n:"Escudo del Chafer Lancero",nn:"ESCUDO DEL CHAFER LANCERO",t:24,d:"...",ep:2,g:75,l:1,wd:true,fm:true,w:0,p:1};
I.u[2308] = {n:"Insignia Brakmariana",nn:"INSIGNIA BRAKMARIANA",t:24,d:"Esta insignia permite identificar al guerrero cuando fallece en el campo de batalla. Parece ser que algunos Bontarianos las coleccionan.",ep:2,g:76,l:1,wd:true,fm:true,w:0,p:1};
I.u[2309] = {n:"Vela roja",nn:"VELA ROJA",t:24,d:"No es una vela del montón...",ep:1,g:77,l:1,wd:true,fm:true,w:0,p:1};
I.u[2310] = {n:"Vela verde",nn:"VELA VERDE",t:24,d:"...",ep:2,g:78,l:1,wd:true,fm:true,w:0,p:1};
