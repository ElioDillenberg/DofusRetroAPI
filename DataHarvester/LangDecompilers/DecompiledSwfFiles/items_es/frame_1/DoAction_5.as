I.u[2078] = {n:"Testamento de Ixa Mustam",nn:"TESTAMENTO DE IXA MUSTAM",t:24,d:"El testamento del padre de Oto Mustam, está escrito en antiguo brakmariano, una lástima para ti, tu dominio de esta lengua se puede resumir con la palabra: " + "\"" + "Guo\' rhalys!" + "\"" + " .",ep:2,g:26,l:1,wd:true,fm:true,w:0,p:1};
I.u[2079] = {n:"Vino de Zampávoro",nn:"VINO DE ZAMPAVORO",t:24,d:"Parece ser que 20 años son necesarios para conseguir destilar zumo de Zampávoro sin hacer que el vino se vuelva tóxico.",ep:2,g:62,l:1,wd:true,fm:true,w:1,p:1};
I.u[2080] = {n:"Zumo de Zampávoro",nn:"ZUMO DE ZAMPAVORO",t:24,d:"El zumo de Zampávoro es tan tóxico como el zumo de scorbuto. Pero bien destilado da un vino con un sabor incomparable. Dicho esto, pocos son los que saben destilar este zumo. Y la mayor parte del tiempo son piratas.",ep:2,g:63,l:1,wd:true,fm:true,w:1,p:1};
I.u[2081] = {n:"Informe de Finche Aticus",nn:"INFORME DE FINCHE ATICUS",t:24,d:"El informe del espía Finche Aticus indica una lista exhaustiva de las armas de Bonta y de sus efectivos.",ep:2,g:33,l:1,wd:true,fm:true,w:0,p:1};
I.u[2082] = {n:"Jabato",nn:"JABATO",t:32,d:"Cuando un joven jabato ve a sus padres morirse ante sus ojos, sigue el asesino para que este se ocupe de él.",ep:13,g:1,l:1,wd:true,fm:true,w:0,m:true,p:10};
I.u[2083] = {n:"Carta de Oto Mustam",nn:"CARTA DE OTO MUSTAM",t:24,d:"Una carta de tu amo dirigida a Vil Smis. Está escrita en un dialecto desconocido para evitar que los curiosos como tú le echen un vistazo.",ep:2,g:26,l:1,wd:true,fm:true,w:0,p:1};
I.u[2084] = {n:"Respuesta de Vil Smis",nn:"RESPUESTA DE VIL SMIS",t:24,d:"Una respuesta de Vil Smis a Oto Mustam, totalmente incomprensible para el común de los mortales.",ep:2,g:26,l:1,wd:true,fm:true,w:0,p:1};
I.u[2085] = {n:"Cerveza de La Paliza Feliz",nn:"CERVEZA DE LA PALIZA FELIZ",t:37,d:"No solamente no tiene gusto sino que además es carísima. Otro punto en común con las mujeres.",ep:16,g:4,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:5};
I.u[2086] = {n:"Cerveza de Almenú",nn:"CERVEZA DE ALMENU",t:37,d:"Esta cerveza aromatizada con Blop Guinda se puede llamar  también Manoca.",ep:16,g:4,l:1,wd:true,fm:true,w:1,u:true,p:3};
I.u[2087] = {n:"Cerveza de la Bagruta",nn:"CERVEZA DE LA BAGRUTA",t:37,d:"Esta cerveza vuelve más valiente durante un momento... pero ten cuidado con el bajón, porque el momento es muy breve.",ep:16,g:4,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:5};
I.u[2088] = {n:"Cerveza de la Jarradepalo",nn:"CERVEZA DE LA JARRADEPALO",t:37,d:"Una vulgar cerveza, pero la Jarra está maravillosamente esculpida. Por una vez el contenedor vale más que el contenido. Una característica compartida con el cráneo de los yopuka y la prensa del corazón.",ep:16,g:4,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:2};
I.u[2089] = {n:"Cerveza del Pankemante",nn:"CERVEZA DEL PANKEMANTE",t:37,d:"Esta cerveza está caliente, por lo cual es muy difícil beberla en verano. En cambio, desprende un agradable olor a pan tostado, por eso es una cerveza ideal para los desayunos sin pan.",ep:16,g:4,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:2};
I.u[2090] = {n:"Cerveza del Chatarrero",nn:"CERVEZA DEL CHATARRERO",t:37,d:"Esta cerveza tiene un gusto a chatarra muy desagradable, pero muy fortalecedor lo cual puede recordarte los besitos de la Momia Nova.",ep:16,g:4,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:7};
I.u[2091] = {n:"El Bwak: Criarlo bien",nn:"EL BWAK: CRIARLO BIEN",t:25,d:"Este libro explica cómo criar un Bwak.",ep:3,g:4,l:1,wd:true,fm:true,w:1,u:true,p:200};
I.u[2092] = {n:"El Miaumiau: Criarlo bien",nn:"EL MIAUMIAU: CRIARLO BIEN",t:25,d:"Este libro explica cómo criar un Miaumiau.",ep:3,g:3,l:1,wd:true,fm:true,w:1,u:true,p:200};
