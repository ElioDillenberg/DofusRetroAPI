I.u[2060] = {n:"Pata de cuerbok",nn:"PATA DE CUERBOK",t:103,d:"No mata ni espanta. Pero a la inversa de la pata de wabbit, es posible que la pata de cuerbok te traiga la mala suerte.",ep:1,g:330,l:20,wd:true,fm:true,w:1,p:10};
I.u[2061] = {n:"Capa del Cuerbok",nn:"CAPA DEL CUERBOK",t:17,d:"Aunque a primera vista no parezca muy bonita, esta capa es muy eficaz siempre y cuando uno se acostumbre a su olor de Cuerbok en descomposición.",ep:1,g:15,l:45,wd:true,fm:true,w:5,p:5600};
I.u[2062] = {n:"Camuflaje",nn:"CAMUFLAJE",t:31,d:"Estás camuflado dentro del decorado hasta que empieces un combate.",ep:1,g:1,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2063] = {n:"Botas Volátiles",nn:"BOTAS VOLATILES",t:11,d:"Estas botas confeccionadas a partir de numerosas partes del cadáver de Cuerbok, proporcionan al que las lleva puestas una extraña sensación de ligereza, comparable a la que proporciona el consumo de Boletus tostados.",ep:1,g:17,l:41,wd:true,fm:true,w:15,p:4500};
I.u[2064] = {n:"Pergamino de Camuflaje",nn:"PERGAMINO DE CAMUFLAJE",t:14,d:"Este pergamino te permite camuflarte en el decorado, hasta que entres en algún combate. Procura evitar los movimientos bruscos y los ruidos sospechosos si no quieres que te descubran.",ep:16,g:1,l:10,wd:true,fm:true,w:1,c:"Pg=10&PM=B",u:true,p:250};
I.u[2065] = {n:"Casco de chafer de élite",nn:"CASCO DE CHAFER DE ELITE",t:24,d:"Este casco de chafer está estropeado, sin embargo no deja de ser eficaz cuando uno quiere formar parte de la élite.",ep:2,g:59,l:1,wd:false,fm:true,w:0,p:1};
