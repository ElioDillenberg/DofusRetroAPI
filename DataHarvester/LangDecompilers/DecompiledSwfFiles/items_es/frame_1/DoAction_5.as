I.u[2058] = {n:"Corazón de Brandon",nn:"CORAZON DE BRANDON",t:15,d:"Este corazón humano viene seguramente del famoso Lhi que era un apasionado de los Cuerboks.",ep:1,g:328,l:20,wd:true,fm:true,w:5,p:500};
