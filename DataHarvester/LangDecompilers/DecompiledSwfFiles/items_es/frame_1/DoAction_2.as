I.u[942] = {n:"Alisador",nn:"ALISADOR",t:16,d:"El Alisador te protege contra los daños mágicos.",ep:4,g:23,l:1,wd:true,fm:true,w:1,p:150};
I.u[943] = {n:"Pañuelito de vitalidad",nn:"PANUELITO DE VITALIDAD",t:16,d:"Este pañuelo permite aumentar la vitalidad de quien lo lleve puesto.",ep:1,g:20,l:1,wd:true,fm:true,w:1,p:1500};
