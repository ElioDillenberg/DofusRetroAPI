I.u[828] = {n:"Arco Silbante",nn:"ARCO SILBANTE",t:2,d:"Este arco dispara tan rápido sus flechas que se las oye desde muy lejos. Por lo cual piensa en equiparlo con un silenciador si necesitas ser discreto.",ep:1,g:15,l:11,wd:true,fm:true,w:10,an:10,tw:true,e:[5,3,2,6,30,40,false,true],c:"CA>11&CS>5",p:550};
I.u[829] = {n:"Arco Nada",nn:"ARCO NADA",t:2,d:"Por culpa de su escaso alcance, este arco no es muy eficaz aunque permita debilitar suficientemente al enemigo antes de rematarlo con otra arma o con la magia.",ep:1,g:1,l:8,wd:true,fm:true,w:10,an:10,tw:true,e:[5,4,2,5,30,50,false,true],c:"CA>8",p:400};
