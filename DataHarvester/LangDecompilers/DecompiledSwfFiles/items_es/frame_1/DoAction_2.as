I.u[945] = {n:"Pequeña Capa Oscura",nn:"PEQUENA CAPA OSCURA",t:17,d:"Esta capa aumenta la sabiduría del que la lleve puesta.",ep:1,g:15,l:8,wd:true,fm:true,w:5,p:375};
I.u[946] = {n:"Capa Oscura",nn:"CAPA OSCURA",t:17,d:"Esta capa aumenta la sabiduría del que la lleve puesta.",ep:1,g:15,l:12,wd:true,fm:true,w:5,p:600};
