I.u[847] = {n:"Huevo de dragohuevo blanco",nn:"HUEVO DE DRAGOHUEVO BLANCO",t:105,d:"Este extraño huevo dará nacimiento a un hermoso Dragohuevito una vez que este haya sido empollado por una Hembra Dragón Negro. Mientras tanto, procura no llevarlo en uno de tus calentitos bolsillos, nunca se sabe lo que podría ocurrir...",ep:10,g:207,l:74,wd:true,fm:true,w:1,p:100};
I.u[848] = {n:"Viejo Hueso de Dragohuevo",nn:"VIEJO HUESO DE DRAGOHUEVO",t:47,d:"Este viejo hueso es mágico, correctamente utilizado, debería permitirte hacer objetos muy poderosos.",ep:11,g:15,l:70,wd:true,fm:true,w:20,p:100};
I.u[849] = {n:"Anillo Forcesco",nn:"ANILLO FORCESCO",t:9,d:"Este pequeño anillo procura Fuerza a su portador.",ep:1,g:5,l:1,wd:true,fm:true,w:1,p:50};
I.u[850] = {n:"Anillo Agilesco",nn:"ANILLO AGILESCO",t:9,d:"Curiosamente, los coleccionistas Amaknienses se disputan este anillo a pesar de que no valga un churro.",ep:1,g:1,l:1,wd:true,fm:true,w:1,p:50};
I.u[851] = {n:"Anillo Vitalesco",nn:"ANILLO VITALESCO",t:9,d:"Este pequeño anillo aumenta la vida de su portador.",ep:1,g:2,l:1,wd:true,fm:true,w:1,p:50};
I.u[852] = {n:"Pequeño Anillo Mágico",nn:"PEQUENO ANILLO MAGICO",t:9,d:"Este pequeño anillo aumenta la inteligencia de su portador.",ep:1,g:3,l:1,wd:true,fm:true,w:1,p:50};
I.u[853] = {n:"Pequeño Anillo del Afortunado",nn:"PEQUENO ANILLO DEL AFORTUNADO",t:9,d:"Este pequeño anillo aumenta la suerte de su portador.",ep:1,g:42,l:1,wd:true,fm:true,w:1,p:50};
I.u[854] = {n:"Amuleto del Osito",nn:"AMULETO DEL OSITO",t:1,d:"Este amuleto aumenta la fuerza de su portador.",ep:1,g:5,l:1,wd:true,fm:true,w:1,p:50};
I.u[855] = {n:"Amuleto del Lobezno",nn:"AMULETO DEL LOBEZNO",t:1,d:"Este amuleto aumenta la vida de su portador.",ep:0,g:6,l:1,wd:true,fm:true,w:1,p:50};
I.u[856] = {n:"Ranilla",nn:"RANILLA",t:1,d:"Este amuleto aumenta la agilidad de su portador.",ep:1,g:2,l:1,wd:true,fm:true,w:1,p:50};
I.u[859] = {n:"Cinturón de Agilidad",nn:"CINTURON DE AGILIDAD",t:10,d:"Este modesto cinturón aumenta la agilidad de quien lo lleve puesto.",ep:1,g:8,l:1,wd:true,fm:true,w:1,p:50};
I.u[860] = {n:"Cinturón de Suerte",nn:"CINTURON DE SUERTE",t:10,d:"Este modesto cinturón aumenta la suerte de quien lo lleve puesto.",ep:1,g:3,l:1,wd:true,fm:true,w:1,p:50};
I.u[861] = {n:"Pequeñas Botas de Fuerza",nn:"PEQUENAS BOTAS DE FUERZA",t:11,d:"Esta botas aumentan la fuerza del que las lleva en detrimento de su agilidad, lo cual tiende a diferenciarlo de un Tofu sin que sea tampoco menos idiota.",ep:1,g:3,l:2,wd:true,fm:true,w:1,p:200};
I.u[862] = {n:"Sandalias de Inteligencia",nn:"SANDALIAS DE INTELIGENCIA",t:11,d:"Estas botas permiten aumentar la inteligencia del que las lleva al mismo tiempo que le confiere un estilo muy a la moda gracias a su suela de madera de nogal que ofrece una adherencia increíble.",ep:1,g:4,l:8,wd:true,fm:true,w:1,p:400};
I.u[863] = {n:"Botas Agilescas",nn:"BOTAS AGILESCAS",t:11,d:"Estas botas permiten aumentar la agilidad del que las lleva. De esta manera podrá imitar a un Tofu cuando le entren ganas de hacer el imbécil.",ep:1,g:5,l:8,wd:true,fm:true,w:1,p:400};
I.u[864] = {n:"Botas de Suerte",nn:"BOTAS DE SUERTE",t:11,d:"Estas botas aumentan la suerte del que las lleva para ayudarle a encontrar objetos muy poderosos, como por ejemplo la baba de Jalató.",ep:1,g:1,l:8,wd:true,fm:true,w:1,p:100};
I.u[867] = {n:"Dragolira",nn:"DRAGOLIRA",t:1,d:"Este espléndido amuleto confeccionado a partir de restos de Dragohuevo, es lo suficientemente impresionante como para alejar a la mayoría de los enemigos normalmente constituidos.",ep:11,g:9,l:32,wd:true,fm:true,w:2,p:4700};
I.u[878] = {n:"Pergamino de panadero",nn:"PERGAMINO DE PANADERO",t:13,d:"(Haz doble clic para utilizarlo) \nEste pergamino permite ganar puntos de experiencia en el oficio de panadero.",ep:1,g:33,l:1,wd:true,fm:true,w:1,u:true,p:1000};
I.u[879] = {n:"Pergamino de zapatero",nn:"PERGAMINO DE ZAPATERO",t:13,d:"(Haz doble clic para utilizarlo) \nEste pergamino permite ganar puntos de experiencia en el oficio de zapatero.",ep:1,g:34,l:1,wd:true,fm:true,w:1,u:true,p:1000};
I.u[880] = {n:"Lana de jalató real",nn:"LANA DE JALATO REAL",t:57,d:"Esta lana es real, es suave y ligera, pero tiende a picar las pieles sensibles. Esta lana está en cambio muy gastada y por consiguiente, es muy frágil, dada la edad del jalató real.",ep:1,g:207,l:35,wd:true,fm:true,w:1,p:4000};
I.u[881] = {n:"Lana de jalatín blanco",nn:"LANA DE JALATIN BLANCO",t:57,d:"Esta lana es extremadamente suave, ya que procede del plumón de un jalató. Sin embargo ten cuidado si la llevas contigo, por que está todavía impregnada de un olor que podría incitar a la madre del jalató a lamerte.",ep:1,g:208,l:2,wd:true,fm:true,w:1,p:1};
I.u[882] = {n:"Lana del jefe de guerra del jalató",nn:"LANA DEL JEFE DE GUERRA DEL JALATO",t:57,d:"Esta lana huele muy mal, pero perteneció a un Jefe de Guerra Jalató, lo cual le confiere un gran valor sentimental para los belicosos.",ep:1,g:209,l:19,wd:true,fm:true,w:1,p:1};
I.u[883] = {n:"Cuero de jalatín blanco",nn:"CUERO DE JALATIN BLANCO",t:56,d:"Este cuero muy liso está especialmente adaptado para la elaboración de ropa interior de cuero, el Calzoncillo de Cuero del Chafer llega a ser un sorprendente ejemplo.",ep:1,g:534,l:2,wd:true,fm:true,w:1,p:1};
I.u[884] = {n:"Cuero de jalatín negro",nn:"CUERO DE JALATIN NEGRO",t:56,d:"Este cuero reluciente es muy apreciado por los Zapateros y sastres, para la elaboración de pequeñas botas o sombreros.",ep:1,g:16,l:2,wd:true,fm:true,w:1,p:1};
I.u[885] = {n:"Lana de jalatín negro",nn:"LANA DE JALATIN NEGRO",t:57,d:"Esta lana áspera no es muy utilizada por los sastres, es en cambio muy apreciada por los anutrofs, que la usan para ocultar su incipiente calvicie.",ep:1,g:210,l:2,wd:true,fm:true,w:1,p:1};
I.u[886] = {n:"Cuero de Jalató Real",nn:"CUERO DE JALATO REAL",t:56,d:"Este cuero es real, tiene una flexibilidad extraordinaria y las grietas que lleva son los testigos de la larga vida del Jalató Real. No existe mejor cuero en toda la provincia de Amakna.",ep:1,g:535,l:35,wd:true,fm:true,w:1,p:4000};
I.u[887] = {n:"Cuero del jefe de guerra jalató",nn:"CUERO DEL JEFE DE GUERRA JALATO",t:56,d:"" + "\"" + "Este cuero conoce cada guerra, cada herida, cada ser" + "\"" + " tales eran las palabras del famoso Trovador Badiz Mustabante, exactamente antes de morir asfixiado por el olor de un Jefe de Guerra Jalató.",ep:1,g:534,l:19,wd:true,fm:true,w:1,p:1};
I.u[888] = {n:"Grandes Botas de Concentración",nn:"GRANDES BOTAS DE CONCENTRACION",t:11,d:"Estas botas son tan cómodas que permiten que el héroe que las lleve aumente su capacidad para concentrarse.",ep:1,g:6,l:26,wd:true,fm:true,w:15,p:1950};
