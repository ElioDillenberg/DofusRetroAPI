I.u[718] = {n:"Pergamino del hechizo Llamilla",nn:"PERGAMINO DEL HECHIZO LLAMILLA",t:75,d:"(Doble clic para aprender el hechizo) \nEste pergamino te permitirá aprender el hechizo elemental Llamilla.",ep:1,g:1,l:1,wd:true,fm:true,w:1,u:true,p:10000};
I.u[719] = {n:"Pergamino del hechizo Liberación",nn:"PERGAMINO DEL HECHIZO LIBERACION",t:75,d:"(Doble clic para aprender el hechizo) \nEste pergamino te permitirá aprender el hechizo elemental Liberación.",ep:1,g:18,l:1,wd:true,fm:true,w:1,u:true,p:10000};
I.u[720] = {n:"Pergamino del hechizo Fulminación",nn:"PERGAMINO DEL HECHIZO FULMINACION",t:75,d:"(Doble clic para aprender el hechizo) \nEste pergamino te permitirá aprender el hechizo elemental Fulminación.",ep:1,g:19,l:1,wd:true,fm:true,w:1,u:true,p:1};
I.u[721] = {n:"Pergamino del hechizo Invocación de Arakna",nn:"PERGAMINO DEL HECHIZO INVOCACION DE ARAKNA",t:75,d:"(Doble clic para aprender el hechizo) \nEste pergamino te permitirá aprender el hechizo Invocación de Arakna.",ep:16,g:16,l:1,wd:true,fm:true,w:1,u:true,p:10000};
I.u[724] = {n:"Hechizo Dominio del Bastón",nn:"HECHIZO DOMINIO DEL BASTON",t:73,d:"(Doble clic para aprender el hechizo) \nEste pergamino te permitirá infligir más daños cuando utilices un bastón.",ep:1,g:2,l:20,wd:true,fm:true,w:1,u:true,p:10000};
I.u[725] = {n:"Hechizo Dominio de la Espada",nn:"HECHIZO DOMINIO DE LA ESPADA",t:73,d:"(Doble clic para aprender el hechizo) \nEste pergamino te permitirá infligir más daños cuando utilizas una espada.",ep:3,g:1,l:20,wd:true,fm:true,w:1,u:true,p:10000};
I.u[726] = {n:"Hechizo Dominio del Arco",nn:"HECHIZO DOMINIO DEL ARCO",t:73,d:"(Doble clic para aprender el hechizo) \nEste pergamino te permitirá infligir más daños cuando utilizas un arco.",ep:4,g:3,l:20,wd:true,fm:true,w:1,u:true,p:10000};
