I.u[822] = {n:"Espada del Caballero",nn:"ESPADA DEL CABALLERO",t:6,d:"La Espada del Caballero es una espada sólida y eficaz, si la usas de forma adecuada podrás causar enormes daños a tus adversarios.",ep:7,g:9,l:23,wd:true,fm:true,w:30,an:14,e:[4,5,1,1,30,30,false,false],c:"CS>46",p:1600};
