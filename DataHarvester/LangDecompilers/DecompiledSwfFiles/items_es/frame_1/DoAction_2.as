I.u[948] = {n:"Abracapa",nn:"ABRACAPA",t:17,d:"Esta magnífica capa es una sabia mezcla entre la esencia de abráknido y el buen jalató de siempre.",ep:1,g:16,l:10,wd:true,fm:true,w:5,p:10000};
I.u[949] = {n:"Gran Pañuelo de Vitalidad",nn:"GRAN PANUELO DE VITALIDAD",t:16,d:"Este Pañuelo permite aumentar la vitalidad de quien lo lleve puesto.",ep:1,g:20,l:3,wd:true,fm:true,w:2,p:4500};
I.u[951] = {n:"Agujas e hilo",nn:"AGUJAS E HILO",t:20,d:"Estas agujas y este hilo te permitirán ejercer el oficio de sastre.",ep:2,g:4,l:1,wd:true,fm:true,w:1,an:4,e:[1,4,1,1,100,100,false,false],p:100};
I.u[952] = {n:"Capa del Mediulubo Loco",nn:"CAPA DEL MEDIULUBO LOCO",t:17,d:"Esta capa ha sido creada con pelos y otros elementos orgánicos de varios mediulubos... Se puede decir que su olor es poderoso y duradero.",ep:1,g:3,l:33,wd:true,fm:true,w:5,p:3075};
I.u[953] = {n:"Capa del tofu loco",nn:"CAPA DEL TOFU LOCO",t:17,d:"Esta capa, cosida con plumas de tofus, permite que el héroe que la lleve puesta corra más rápido y se escape más fácilmente de las zonas de placaje.",ep:1,g:19,l:36,wd:true,fm:true,w:10,p:3075};
I.u[954] = {n:"La Samulay",nn:"LA SAMULAY",t:17,d:"La Samulay es una capa legendaria, bordada por los mejores sastres para los guerreros que prefieren los combates de cuerpo a cuerpo.",ep:7,g:2,l:22,wd:true,fm:true,w:10,p:1500};
I.u[955] = {n:"Capa Ylar",nn:"CAPA YLAR",t:17,d:"La capa Ylar es un tipo de capa muy pedida por los peluqueros. Les encanta revolcarse en ella. Hace que los pelos del que la lleve puesta se vuelvan más resistentes.",ep:8,g:5,l:34,wd:true,fm:true,w:10,p:3275};
I.u[956] = {n:"Capa Mariya",nn:"CAPA MARIYA",t:17,d:"Esta capa aumenta el alcance del que la lleve puesta, permitiéndole a su vez atinar los blancos muy alejados o impresionar a los aventureros un poco ingenuos.",ep:8,g:7,l:33,wd:true,fm:true,w:10,p:3000};
I.u[957] = {n:"La Jalatada",nn:"LA JALATADA",t:17,d:"La Jalatada aumenta los daños y la vitalidad del que la lleve puesta. Sin embrago, desprende un fuerte olor y puede ser que algún Jalató piense que eres uno de los suyos.",ep:5,g:12,l:23,wd:true,fm:true,w:15,p:1575};
I.u[958] = {n:"Dofustadora",nn:"DOFUSTADORA",t:17,d:"Esta capa es sin duda alguna una de las más poderosas del mundo. Aumenta todas las características del que la lleve puesta.",ep:1,g:8,l:6,wd:true,fm:true,w:15,p:100000};
I.u[959] = {n:"Mapa del Tesoro 5",nn:"MAPA DEL TESORO 5",t:24,d:"-- a faire --",ep:1,g:10,l:1,wd:true,fm:true,w:0,p:1};
I.u[960] = {n:"Pócima wabbit",nn:"POCIMA WABBIT",t:24,d:"Esta pócima es un remedio contra la mixomawosis. Un trago de este brebaje te permite visitar la isla de los wabbits sin temer por tu vida.",ep:5,g:2,l:1,wd:true,fm:true,w:0,p:1};
I.u[961] = {n:"Llave wabbit",nn:"LLAVE WABBIT",t:24,d:"Una de las tres llaves que permiten abrir la puerta del castillo wabbit.",ep:8,g:3,l:1,wd:true,fm:true,w:0,p:1};
I.u[962] = {n:"Llave wabbit",nn:"LLAVE WABBIT",t:24,d:"Una de las tres llaves que permiten abrir la puerta del castillo wabbit.",ep:8,g:4,l:1,wd:true,fm:true,w:0,p:1};
I.u[963] = {n:"Llave wabbit",nn:"LLAVE WABBIT",t:24,d:"Una de las tres llaves que permiten abrir la puerta del castillo wabbit.",ep:8,g:5,l:1,wd:true,fm:true,w:0,p:1};
