I.u[8341] = {n:"Botapiñas",nn:"BOTAPINAS",t:11,d:"Estas botas huelen a zumo de piña macerada en substrato de calcetín agujereado. La ventaja es que nadie te pedirá que te los quites cuando vayas a casa de un amigo.",ep:1,g:90,l:1,wd:true,fm:true,w:10,s:74,p:100};
I.u[8342] = {n:"Llave del Santuario de los Dragohuevos",nn:"LLAVE DEL SANTUARIO DE LOS DRAGOHUEVOS",t:84,d:"Esta llave hecha con cáscaras de Dragohuevo te permitirá acceder al terrible Santuario de los Dragohuevos, famoso por la escasa locuacidad de sus habitantes.",ep:10,g:18,l:1,wd:true,fm:true,w:1,p:100};
I.u[8343] = {n:"Llave de la Mazmorra de los Dragohuevos",nn:"LLAVE DE LA MAZMORRA DE LOS DRAGOHUEVOS",t:84,d:"Puede que al entrar en el Santuario de los Dragohuevos pensaras que sería el final de tus sufrimientos... Pues lamentablemente te has equivocado. Esta llave será tal vez la causa de tu muerte.",ep:10,g:17,l:1,wd:true,fm:true,w:1,p:100};
I.u[8344] = {n:"Cuerno de dragoss negro",nn:"CUERNO DE DRAGOSS NEGRO",t:47,d:"Es menos puntiagudo que una aguja de Xelor pero más resistente. Este cuerno les permite a los Dragoss dar cornadas al trasero de sus adversarios.",ep:11,g:648,l:50,wd:true,fm:true,w:1,p:10};
