I.u[8337] = {n:"Regalo: de Nawidad",nn:"REGALO: DE NAWIDAD",t:89,d:"El equipo Ankama te desea unas felices fiestas. Este Regalo: te colmará de felicidad.",ep:1,g:3,l:1,wd:false,fm:true,w:1,u:true,p:1};
I.u[8338] = {n:"Espada de Nawidad",nn:"ESPADA DE NAWIDAD",t:6,d:"Esta extraña espada no te volverá invencible, pero tal vez consigas utilizarla para matar bolas de nieve...",ep:1,g:69,l:1,wd:false,fm:true,w:1,an:14,e:[1,1,1,1,2,2,false,true],p:1};
I.u[8339] = {n:"Gran Regalo de Nawidad",nn:"GRAN REGALO DE NAWIDAD",t:89,d:"El equipo Ankama te desea unas felices y agradables fiestas. Este regalo te colmará de felicidad.",ep:1,g:3,l:1,wd:false,fm:true,w:1,u:true,p:1};
