I.u[8348] = {n:"Cáscara de dragoss negro",nn:"CASCARA DE DRAGOSS NEGRO",t:111,d:"Esta cáscara puede servir para proteger tus pertenencias más valiosas.",ep:11,g:738,l:50,wd:true,fm:true,w:1,p:10};
I.u[8349] = {n:"Cáscara de dragoss de zafiro",nn:"CASCARA DE DRAGOSS DE ZAFIRO",t:111,d:"Esta cáscara puede servir para proteger tus pertenencias más valiosas.",ep:11,g:739,l:50,wd:true,fm:true,w:1,p:10};
