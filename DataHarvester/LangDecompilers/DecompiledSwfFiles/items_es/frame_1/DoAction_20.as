I.u[9265] = {n:"Tierrapatas",nn:"TIERRAPATAS",t:24,d:"Las tierrapatas suelen encontrarse en la cabeza de los crujidores que no se lavan.",ep:5,g:727,l:1,wd:false,fm:true,w:0,p:1};
I.u[9266] = {n:"Grasa de gelatina real",nn:"GRASA DE GELATINA REAL",t:24,d:"La grasa de gelatina real es un ingrediente esencial en la composición de una pócima de parálisis.",ep:7,g:32,l:1,wd:false,fm:true,w:0,p:1};
I.u[9267] = {n:"Trompa de Trompseta",nn:"TROMPA DE TROMPSETA",t:15,d:"Todavía repleta del aliento pútrido de su antiguo dueño, podrás tocar esta trompa para asfixiar a tu enemigo al tiempo que lo embelesas con tu música.",ep:23,g:764,l:125,wd:true,fm:true,w:1,p:10};
I.u[9268] = {n:"Martillo de luz sin espíritu",nn:"MARTILLO DE LUZ SIN ESPIRITU",t:24,d:"Este martillo de luz parece haber perdido el espíritu que le confería su poder y ahora no parece gran cosa.",ep:3,g:8,l:1,wd:false,fm:true,w:0,p:1};
I.u[9269] = {n:"Lengua de Champidonte",nn:"LENGUA DE CHAMPIDONTE",t:15,d:"Para lamer las esporas de sus compañeros, el Champidonte dispone de una lengua ágil increíblemente resistente. Será la única cosa que le puedes envidiar al Champidonte.",ep:23,g:765,l:134,wd:true,fm:true,w:1,p:1};
I.u[9270] = {n:"Muñeca de repuesto",nn:"MUNECA DE REPUESTO",t:24,d:"Esta muñeca ha sido fabricada por las hábiles manos de Maskara para remplazar la que perdió el espíritu de la luz.",ep:1,g:680,l:1,wd:false,fm:true,w:0,p:1};
I.u[9271] = {n:"Promesa de Maskara",nn:"PROMESA DE MASKARA",t:24,d:"Este anillo simboliza simplemente la promesa que Maskara te hizo cuando aceptó jugar a las muñecas contigo (sobre todo haciendo ella de muñeca) cada vez que pueda. Éste es uno de los bienes más preciados del mundo.",ep:1,g:730,l:1,wd:false,fm:true,w:0,p:1};
I.u[9272] = {n:"Caja de Bonta",nn:"CAJA DE BONTA",t:24,d:"Has osado robar esta caja en tu propia Torre de las Órdenes para dársela a los tymadores sin tan siquiera saber lo que contiene. Actuar así es traicionar la confianza que tu maestro de orden ha puesto en ti.",ep:1,g:44,l:1,wd:false,fm:true,w:0,p:1};
