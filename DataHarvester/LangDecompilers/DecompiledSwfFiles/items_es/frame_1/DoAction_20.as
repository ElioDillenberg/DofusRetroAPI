I.u[9227] = {n:"Lista matrimonial de Novini",nn:"LISTA MATRIMONIAL DE NOVINI",t:24,d:"Satirev Novini te ha dado esta lista para que intercedas en su favor ante las siguientes mujeres de Astrub: Nepra Lido, Eníleda, Gwenolaz Barnamuza, Maskara y Chronokwartz.",ep:16,g:26,l:1,wd:false,fm:true,w:0,p:1};
