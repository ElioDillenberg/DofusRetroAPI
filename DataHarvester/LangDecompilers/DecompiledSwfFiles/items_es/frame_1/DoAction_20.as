I.u[9044] = {n:"Pócima de disfraz de yopuka",nn:"POCIMA DE DISFRAZ DE YOPUKA",t:14,d:"Con esta pócima podrás tomar la apariencia que quieras, ni siquiera tu madre te reconocerá.",ep:0,g:43,l:1,wd:false,fm:true,w:1,c:"Pg>10,3",u:true,p:10};
