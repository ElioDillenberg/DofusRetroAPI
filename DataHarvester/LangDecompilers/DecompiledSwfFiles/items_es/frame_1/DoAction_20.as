I.u[9157] = {n:"Amublop Multicolor Real",nn:"AMUBLOP MULTICOLOR REAL",t:1,d:"Sin aditivos ni colorantes, este amuleto sería transparente. Puedes estar tranquilo, nadie te va a pedir que te lo comas.",ep:5,g:136,l:90,wd:true,fm:true,w:10,s:106,p:21000};
