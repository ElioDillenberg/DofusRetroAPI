I.u[9255] = {n:"Amuleto Objevivo",nn:"AMULETO OBJEVIVO",t:113,d:"Este objeto polimorfo mantiene una relación de parásito o de predador-simbionte con los amuletos inanimados. (Tienes que alimentarlo dandole de comer otros amuletos. Puedes alimentar tu Amuleto Objevivo cada doce horas)",ep:1,g:139,l:1,wd:true,fm:false,w:1,p:1};
I.u[9256] = {n:"Anillo Objevivo",nn:"ANILLO OBJEVIVO",t:113,d:"Este objeto polimorfo mantiene una relación de parásito o de predador-simbionte con los anillos inanimados. (Tienes que alimentarlo dándole de comer otros anillos. Puedes alimentar tu Anillo Objevivo cada doce horas)",ep:1,g:161,l:1,wd:true,fm:false,w:1,p:1};
I.u[9257] = {n:"Soporífero",nn:"SOPORIFERO",t:24,d:"Esta poción tiene una propiedad interesante: una vez que se añade a un alimento, cualquiera que lo coma cae rápidamente en un sueño profundo. No tiene nada que ver con las pociones que has conocido hasta el momento.",ep:16,g:500,l:1,wd:false,fm:true,w:0,p:1};
