I.u[9059] = {n:"Pata de Jalabarro",nn:"PATA DE JALABARRO",t:24,d:"Una pata de esta patética bestia.",ep:1,g:11,l:1,wd:false,fm:true,w:0,p:1};
I.u[9060] = {n:"Pedazo de papel roto",nn:"PEDAZO DE PAPEL ROTO",t:24,d:"Es una parte de una carta que alguien ha roto. Todavía se puede leer: " + "\"" + "...al y como convenimos, me las arreglaré para que la investigación sobre ese mercader no dé ningún resultado. No olvide depositarme la suma en el lugar habitual, allí donde las mujeres son una deliciosa compañía" + "\"" + ". \nLa carta está firmada con una X.",ep:16,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9061] = {n:"Exoram",nn:"EXORAM",t:32,d:"Debes sacar a Exoram del campamento de los goblins y escoltarlo hasta el templo Sram.",ep:1,g:9,l:1,wd:false,fm:true,w:0,m:true,p:1};
I.u[9062] = {n:"Documento sobre Xéphirex",nn:"DOCUMENTO SOBRE XEPHIREX",t:24,d:"Este documento contiene mucha información interesante sobre un individuo llamado Xéphirex. Entre otras cosas podemos aprender que pasa mucho tiempo en la Taberna del Pandawa Borracho",ep:2,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9063] = {n:"Lista de Milone",nn:"LISTA DE MILONE",t:24,d:"Milone, el joyero, te ha dado esta interminable lista. En ella están escritos los objetos necesarios para confeccionar una suntuosa joya.",ep:16,g:176,l:1,wd:false,fm:true,w:0,p:1};
I.u[9064] = {n:"Baba del Jalató Real",nn:"BABA DEL JALATO REAL",t:24,d:"¿Existe alguna diferencia entre ésta y una baba común? A simple vista parece que no...",ep:1,g:32,l:1,wd:false,fm:true,w:0,p:1};
I.u[9065] = {n:"Carta para Milone",nn:"CARTA PARA MILONE",t:24,d:"Esta carta, escrita rápidamente, explica a Milone que la joya está en peligro. De hecho, ¿por qué pierdes el tiempo leyéndola en lugar de ir corriendo a su joyería?",ep:16,g:176,l:1,wd:false,fm:true,w:0,p:1};
I.u[9066] = {n:"Carta del jefe de los ladrones",nn:"CARTA DEL JEFE DE LOS LADRONES",t:24,d:"Uno de los ladrones que derrotaste en la joyería de Milone tenía esta carta:\nMilone Corichel, el joyero, acaba de crear una joya de un gran valor. Róbasela cueste lo que te cueste.\nParece que los ladrones fracasaron y encima al dorso del mensaje está escrita la ubicación de su guarida. Se impone una visita de cortesía, pero más vale ir acompañado.",ep:16,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9067] = {n:"Carta para Xéphirex",nn:"CARTA PARA XEPHIREX",t:24,d:"Esta carta de Oto Mustam explica detalladamente el plan previsto para Xéphirex.",ep:1,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9068] = {n:"Joya de Milone",nn:"JOYA DE MILONE",t:24,d:"Esta joya es el resultado de tus esfuerzos y de los de Milone, el joyero. Seguramente valga toda una fortuna... para aquellos a quienes les interesen las joyas.",ep:16,g:717,l:1,wd:false,fm:true,w:0,p:1};
I.u[9069] = {n:"Nueva carta para Xéphirex",nn:"NUEVA CARTA PARA XEPHIREX",t:24,d:"En esta carta, Oto ha escrito sus condiciones para corromper a Xéphirex.",ep:1,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9070] = {n:"Espada extraña",nn:"ESPADA EXTRANA",t:24,d:"Esta espada sí que es rara. Nunca has visto nada parecido en Bonta.",ep:1,g:718,l:1,wd:false,fm:true,w:0,p:1};
I.u[9071] = {n:"Notas del tabernero Feubuk",nn:"NOTAS DEL TABERNERO FEUBUK",t:24,d:"El tabernero Feubuk a recibido a unos zurcaráks sospechosos que pronunciaron el nombre de Loudac.",ep:16,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9072] = {n:"Notas del tabernero Pankemante",nn:"NOTAS DEL TABERNERO PANKEMANTE",t:24,d:"El tabernero Pankemante ha oído a unos guardias que hablaban de sus problemas con una banda de zurcaráks.",ep:16,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9073] = {n:"Insignia del bandido zurcarák",nn:"INSIGNIA DEL BANDIDO ZURCARAK",t:24,d:"Has recuperado esta insignia de uno de los bandidos zurcaráks de las llanuras de Cania.",ep:16,g:49,l:1,wd:false,fm:true,w:0,p:1};
I.u[9074] = {n:"Joya de Amayiro",nn:"JOYA DE AMAYIRO",t:24,d:"Esta joya servirá de señuelo para atraer al jefe de los bandidos y capturarlo.",ep:1,g:111,l:1,wd:false,fm:true,w:0,p:1};
I.u[9075] = {n:"Carta del xelor",nn:"CARTA DEL XELOR",t:24,d:"Esta carta te ha sido transmitida con la ayuda de Marge Gondersun, la joyera. \nLleva la joya a Ekusoyk Ubnan, a la Taberna de Jarradepalo. Será mucho más rápido para nosotros y mucho más lucrativo para ti.",ep:1,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9076] = {n:"Joya de Amayiro (falsa)",nn:"JOYA DE AMAYIRO (FALSA)",t:24,d:"El parecido de esta joya con la que Amayiro te envió a buscar, engañaría a cualquiera.\nDe hecho, más vale que también engañe a Amayiro, porque si no...",ep:1,g:110,l:1,wd:false,fm:true,w:0,p:1};
I.u[9077] = {n:"Cráneo de feca",nn:"CRANEO DE FECA",t:47,d:"El cráneo Feca es el sujetalibros ideal.",ep:1,g:34,l:1,wd:true,fm:false,w:1,p:1};
I.u[9078] = {n:"Cráneo de osamodas",nn:"CRANEO DE OSAMODAS",t:47,d:"Un tofu por aquí, un bwork por allá: este discípulo de Osamodas perdió la cabeza.",ep:1,g:33,l:1,wd:true,fm:false,w:2,p:1};
I.u[9079] = {n:"Cráneo de anutrof",nn:"CRANEO DE ANUTROF",t:47,d:"Un viejo trofeo, que ya era viejo antes de ser trofeo.",ep:1,g:32,l:1,wd:true,fm:false,w:2,p:1};
I.u[9080] = {n:"Cráneo de sram",nn:"CRANEO DE SRAM",t:47,d:"Un trofeo mórbido, que ya era mórbido antes de ser trofeo.",ep:1,g:31,l:1,wd:true,fm:false,w:2,p:1};
I.u[9081] = {n:"Cráneo de xelor",nn:"CRANEO DE XELOR",t:47,d:"Este cráneo a menudo se confunde con el de la famosa momia de Tuthan Kamha. Que no te engañen, sus bendajes no están hechos con aleación de kamas, sino con papel-maché.",ep:1,g:30,l:1,wd:true,fm:false,w:2,p:1};
I.u[9082] = {n:"Cráneo de zurcarák",nn:"CRANEO DE ZURCARAK",t:47,d:"Ya no podrá jugar a cara o cruz, pero quizás nos deje jugar a las tabas...",ep:1,g:29,l:1,wd:true,fm:false,w:2,p:1};
I.u[9083] = {n:"Cráneo de aniripsa",nn:"CRANEO DE ANIRIPSA",t:47,d:"¡No seas malo! Aunque este cráneo sea pequeño, no es una cabeza de mosquito",ep:1,g:28,l:1,wd:true,fm:false,w:2,p:1};
I.u[9084] = {n:"Cráneo de yopuka",nn:"CRANEO DE YOPUKA",t:47,d:"Un trofeo tan vacío como cuando estaba lleno.",ep:1,g:27,l:1,wd:true,fm:false,w:1,p:1};
I.u[9085] = {n:"Cráneo de ocra",nn:"CRANEO DE OCRA",t:47,d:"¡Hasta parece recién salido de una escavación arquelógica, Este cráneo de Ocra mañón!",ep:1,g:26,l:1,wd:true,fm:false,w:2,p:1};
I.u[9086] = {n:"Cráneo de sadida",nn:"CRANEO DE SADIDA",t:47,d:"¿Quién habría adivinado que había un cráneo bajo esa mata de pelo?",ep:1,g:25,l:1,wd:true,fm:false,w:2,p:1};
I.u[9087] = {n:"Cráneo de sacrógrito",nn:"CRANEO DE SACROGRITO",t:47,d:"Este sacrógrito se hace menos el chulo sin sus millares de puntos de vida.",ep:1,g:24,l:1,wd:true,fm:false,w:2,p:1};
I.u[9088] = {n:"Cráneo de pandawa",nn:"CRANEO DE PANDAWA",t:47,d:"Algunos pensarán que el consumo masivo de cerveza ha ayudado a conservar este trofeo.",ep:1,g:23,l:1,wd:true,fm:false,w:2,p:1};
I.u[9089] = {n:"Insignia de la patrulla de Bonta",nn:"INSIGNIA DE LA PATRULLA DE BONTA",t:24,d:"Has conseguido esta insignia durante la (muy fácil) escaramuza contra Bonta.",ep:16,g:49,l:1,wd:false,fm:true,w:0,p:1};
I.u[9090] = {n:"Insignia de la patrulla de Brakmar",nn:"INSIGNIA DE LA PATRULLA DE BRAKMAR",t:24,d:"Has conseguido esta insignia durante la ruda escaramuza contra Brakmar.",ep:16,g:76,l:1,wd:false,fm:true,w:0,p:1};
I.u[9092] = {n:"Anillo de Pomoly",nn:"ANILLO DE POMOLY",t:24,d:"Este anillo se lo arrancaste a Pomoly, la Sadida anti-Djaul que eliminaste a petición de Oto.",ep:16,g:86,l:1,wd:false,fm:true,w:0,p:1};
I.u[9094] = {n:"Veneno bwork",nn:"VENENO BWORK",t:24,d:"Se parece más a gelatina de menta que a un veneno. Los bworks no serán muy diestros con estas cosas, pero lo más inquietante es que tampoco parece que los espías brakmarianos lo sean.",ep:4,g:32,l:1,wd:false,fm:true,w:0,p:1};
I.u[9096] = {n:"Estratega de Bonta",nn:"ESTRATEGA DE BONTA",t:32,d:"El estratega ha aceptado seguirte hasta tu jefe.",ep:16,g:22,l:1,wd:false,fm:true,w:0,m:true,p:1};
I.u[9097] = {n:"Insignia del poderoso miliciano",nn:"INSIGNIA DEL PODEROSO MILICIANO",t:24,d:"Encontraste esta insignia al derrotar al temible miliciano que había vencido a varios guerreros de Brakmar. Su equipo era un tanto peculiar.",ep:16,g:49,l:1,wd:false,fm:true,w:0,p:1};
I.u[9098] = {n:"Martillo de Nara",nn:"MARTILLO DE NARA",t:24,d:"Este martillo servirá de prueba a Oto para saber que derrotaste al herrero mercenario de Bonta.",ep:16,g:8,l:1,wd:false,fm:true,w:0,p:1};
I.u[9099] = {n:"Petición de ayuda de Amayiro",nn:"PETICION DE AYUDA DE AMAYIRO",t:24,d:"Esta carta, firmada por Amayiro, es un petición oficial de ayuda.",ep:3,g:26,l:1,wd:false,fm:true,w:0,p:1};
I.u[9100] = {n:"Calzón de Lengland",nn:"CALZON DE LENGLAND",t:24,d:"Ahora fijo que Lenglad se pasará un buen rato sin dar motivos para que se hable de él.",ep:1,g:84,l:1,wd:false,fm:true,w:0,p:1};
I.u[9101] = {n:"Caja de herramientas de Nara",nn:"CAJA DE HERRAMIENTAS DE NARA",t:24,d:"Esta caja contiene las herramientas que Nara, el herrero, se había olvidado.",ep:3,g:44,l:1,wd:false,fm:true,w:0,p:1};
I.u[9103] = {n:"Carta brakmariana",nn:"CARTA BRAKMARIANA",t:24,d:"Esta carta, fechada hace una semana, explica en detalle el plan de ataque contra Bonta. No obstante hay que desconfiar, ya que una nota a pie de página indica que el plan aún no ha sido confirmado y que Kapi Ta\'han podría hacer algunas modificaciones en los próximos días. Por consiguiente, tienes que descubrir qué van a hacer al final.",ep:16,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9104] = {n:"Insignia de la patrulla del cementerio",nn:"INSIGNIA DE LA PATRULLA DEL CEMENTERIO",t:24,d:"Esta insignia la has recuperado de uno de los miembros de la patrulla brakmariana que atacaste en el Cementerio de los Torturados.",ep:16,g:76,l:1,wd:false,fm:true,w:0,p:1};
I.u[9105] = {n:"Carta de Danathor",nn:"CARTA DE DANATHOR",t:24,d:"Alguien ha metido esta carta en tu mochila mientras Amayiro te transmitía la misión: Recupera los documentos, pero tráemelos a mí en lugar de a Amayiro. Serás recompensado. Danathor.",ep:16,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9106] = {n:"Documentos secretos de Bonta",nn:"DOCUMENTOS SECRETOS DE BONTA",t:24,d:"Estos documentos contienen una gran cantidad de información sobre el sistema defensivo de Bonta. ¿A quién se lo vas a dar?",ep:16,g:26,l:1,wd:false,fm:true,w:0,p:1};
I.u[9107] = {n:"Órdenes para el espía de Bonta",nn:"ORDENES PARA EL ESPIA DE BONTA",t:24,d:"Penetra en la Torre de Brakmar a medianoche y recupera todos los documentos que encontrarás en el segundo piso. Luego vuelve a Bonta pasando por el este del camino.\nNo, no eres TÚ el que tenía que hacer esto. Estás eran las órdenes del espía al que debes salvar.",ep:16,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9108] = {n:"Documentos brakmarianos secretos",nn:"DOCUMENTOS BRAKMARIANOS SECRETOS",t:24,d:"Estos documentos contienen una gran cantidad de información sobre la defensa de Brakmar.",ep:14,g:26,l:1,wd:false,fm:true,w:0,p:1};
I.u[9109] = {n:"Bandana Mokete",nn:"BANDANA MOKETE",t:24,d:"Esta bandana está en mal estado y llena de mocos, ya que sirvió de pañuelo a varias generaciones de wabbits. Sin duda perteneció a Tugtora, prueba de ello, su olor a arenque podrido mezclado con el olor a zanahowia. El hecho de que siga oliendo tan mal indica que posee propiedades mágicas muy peculiares.",ep:8,g:748,l:1,wd:false,fm:true,w:0,p:1};
I.u[9110] = {n:"Cuerno mágico",nn:"CUERNO MAGICO",t:24,d:"Ya has intentado soplar en él, pero no ocurre nada especial...",ep:16,g:720,l:1,wd:false,fm:true,w:0,p:1};
I.u[9112] = {n:"Órdenes para el espía brakmariano",nn:"ORDENES PARA EL ESPIA BRAKMARIANO",t:24,d:"Penetra en la Torre de Bonta a medianoche, degüella a todo el que se cruce en tu camino y recupera todos los documentos que encontrarás en el primer piso. Luego vuelve a Brakmar pasando por el camino.\nNo, no eres TÚ el que tenía que hacer esto. Estás eran las órdenes dadas al espía que debes salvar.",ep:16,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9113] = {n:"Documentos secretos robados",nn:"DOCUMENTOS SECRETOS ROBADOS",t:24,d:"Los documentos que el espía herido robó de la Torre de Bonta.",ep:13,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9114] = {n:"Documentos secretos arrugados",nn:"DOCUMENTOS SECRETOS ARRUGADOS",t:24,d:"Los documentos que el espía herido robó de la torre de Bonta se dañaron un poco durante el combate contra las fuerzas de Bonta.",ep:16,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9115] = {n:"Enchalada rellena de polen",nn:"ENCHALADA RELLENA DE POLEN",t:24,d:"Esta enchalada está rellena de polen de blop y se preparó en el caldero especial que le confirió una serie de virtudes. Para saber cuáles son, habrá que probarla. Por desgracia, esta enchalada es para Tugtora. A veces la vida es injusta...",ep:12,g:719,l:1,wd:false,fm:true,w:0,p:1};
I.u[9116] = {n:"Caja de equipación",nn:"CAJA DE EQUIPACION",t:24,d:"Un arma, un casco, un escudo: esta caja contiene una equipación rudimentaria, pero eficaz, para los guerreros.",ep:16,g:44,l:1,wd:false,fm:true,w:0,p:1};
I.u[9117] = {n:"Ugartillo",nn:"UGARTILLO",t:7,d:"Ugah tiene la cabeza más dura que un arado, por eso un martillo hecho con su cabeza es ideal para aplastar la de los demás.",ep:23,g:67,l:180,wd:true,fm:true,w:15,an:15,e:[10,4,1,1,50,50,false,false],c:"CV>1200",s:118,p:50000};
