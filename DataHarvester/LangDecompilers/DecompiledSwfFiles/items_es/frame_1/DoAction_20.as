I.u[9233] = {n:"Capa Objeviva",nn:"CAPA OBJEVIVA",t:113,d:"Este objeto polimorfo mantiene una relación de parásito o de predador-simbionte con las capas inanimadas. (Tienes que alimentarla dandole de comer otras capas. Puedes alimentar tu Capa Objeviva cada doce horas)",ep:1,g:149,l:1,wd:true,fm:false,w:1,p:1};
I.u[9234] = {n:"Sombrero Objevivo",nn:"SOMBRERO OBJEVIVO",t:113,d:"Este objeto polimorfo mantiene una relación de parásito o de predador-simbionte con los " + "\"" + "cubre-cabezas" + "\"" + " inanimados. (Tienes que alimentarlo dandole de comer otros sombreros, gorritos, gorras... Puedes alimentar tu Sombrero Objevivo cada doce horas)",ep:1,g:211,l:1,wd:true,fm:false,w:1,p:1};
I.u[9235] = {n:"Testimonio sospechoso de Vasta Detontirías",nn:"TESTIMONIO SOSPECHOSO DE VASTA DETONTIRIAS",t:24,d:"En esta carte se recoge la versión poco fiable de Vasta Detontirías.",ep:16,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9236] = {n:"Testimonio creíble de Vasta Detontirías",nn:"TESTIMONIO CREIBLE DE VASTA DETONTIRIAS",t:24,d:"En esta carte se recoge la versión creíble de Vasta Detontirías.",ep:16,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9237] = {n:"Benoyce: testigo creíble",nn:"BENOYCE: TESTIGO CREIBLE",t:24,d:"En esta carta se indica que en tu opinión la testigo Benoyce es creíble, o quizas sea su forma de cantar lo que te ha embaucado.",ep:16,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9238] = {n:"Benoyce: testigo sospechoso",nn:"BENOYCE: TESTIGO SOSPECHOSO",t:24,d:"En esta carta se indica que en tu opinión la testigo Benoyce es sospechosa, o quizas sea porque no te gusta su forma de cantar.",ep:16,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9239] = {n:"Kerenah creíble",nn:"KERENAH CREIBLE",t:24,d:"En esta carta se indica que en tu opinión Kerenah no tuvo nada que ver.  A veces, oler a pescado tiene sus ventajas.",ep:16,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9240] = {n:"Kerenah cómplice",nn:"KERENAH COMPLICE",t:24,d:"En esta carta se indica que en tu opinión Kerenah metió el documento secreto en la bolsa de Vasta Detonterías, aunque puede que también quieras castigarle por la peste insoportable que despide.",ep:16,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9241] = {n:"Opinión desfavorable sobre Vasta Detontirías",nn:"OPINION DESFAVORABLE SOBRE VASTA DETONTIRIAS",t:24,d:"En esta carta se indica que en tu opinión Vasta Detontirías es culpable de traición.",ep:16,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9242] = {n:"Opinión favorable sobre Vasta Detontirías",nn:"OPINION FAVORABLE SOBRE VASTA DETONTIRIAS",t:24,d:"En esta carta se indica que en tu opinión Vasta Detontirías es inocente.",ep:16,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9243] = {n:"Vasta culpable",nn:"VASTA CULPABLE",t:24,d:"En esta carta se indica que has declarado contra Vasta Detontirías. Danathor está satisfecho de tu comportamiento.",ep:16,g:26,l:1,wd:false,fm:true,w:0,p:1};
I.u[9244] = {n:"Vasta inocente",nn:"VASTA INOCENTE",t:24,d:"En esta carta se indica que has declarado en favor de Vasta Detontirías. Amayiro está satisfecho de tu comportamiento.",ep:16,g:26,l:1,wd:false,fm:true,w:0,p:1};
I.u[9245] = {n:"Sobreseimiento",nn:"SOBRESEIMIENTO",t:24,d:"Esta carta indica que no has sido muy convincente en el proceso de Vasta Detontirías y que el caso de este último se ha sobreseído. No sirve de nada esperar el testimonio de Danathor o de Amayiro.",ep:16,g:26,l:1,wd:false,fm:true,w:0,p:1};
