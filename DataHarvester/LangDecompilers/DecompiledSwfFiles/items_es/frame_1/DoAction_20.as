I.u[9162] = {n:"Bloptas Guinda Reales",nn:"BLOPTAS GUINDA REALES",t:11,d:"Pronuncia lo más rápido que puedas la palabra " + "\"" + "Bloptas" + "\"" + " dieciocho veces seguidas. Ya esta, ahora sí que te pareces a un Yopuka.",ep:5,g:121,l:80,wd:true,fm:true,w:10,s:102,p:16000};
