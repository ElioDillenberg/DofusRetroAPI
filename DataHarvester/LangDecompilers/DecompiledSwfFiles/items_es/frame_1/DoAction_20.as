I.u[9277] = {n:"Ojo de setador",nn:"OJO DE SETADOR",t:109,d:"El setador suele rascarse con las puntas de su bastón. Cuando se rasca mucho la cara, puede perder un ojo. Cuando se rasca mucho el cuello, puede perder la cabeza.",ep:23,g:587,l:152,wd:true,fm:true,w:1,p:1};
I.u[9278] = {n:"Lamela de Champbidos",nn:"LAMELA DE CHAMPBIDOS",t:15,d:"El champbis tiene dos pies, dos brazos y dos cabezas. Lo que lo convierte en una criatura doblemente estúpida.",ep:23,g:766,l:144,wd:true,fm:true,w:1,p:1};
I.u[9279] = {n:"Bava de seta peleona",nn:"BAVA DE SETA PELEONA",t:15,d:"Viscosa pero apetitosa, esta baba se usa a veces para engrasar las armas, o para untar los cuerpos de los aventureros a los que les gusta combatir desnudos en las arenas.",ep:23,g:45,l:149,wd:true,fm:true,w:1,p:1};
I.u[9280] = {n:"Espina de Ugah",nn:"ESPINA DE UGAH",t:15,d:"Ugah sólo tiene cuatro espinas en su cabeza, pero son gordas.",ep:23,g:40,l:900,wd:true,fm:true,w:1,p:1};
I.u[9281] = {n:"Fragmento de Ugahlureta",nn:"FRAGMENTO DE UGAHLURETA",t:15,d:"Ugah posee un magnífico sombrero, y ahora tú tienes entre tus manos uno de los pedazos del mismo.",ep:23,g:41,l:900,wd:true,fm:true,w:1,p:1};
