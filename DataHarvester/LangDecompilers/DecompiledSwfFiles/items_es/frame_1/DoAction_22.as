I.u[10054] = {n:"Anillo de Vampyro",nn:"ANILLO DE VAMPYRO",t:9,d:"Este anillo ha pasado tres años metido en excrementos de murciélago... ¡pero anda que no es mágico!",ep:0,g:171,l:42,wd:true,fm:true,w:10,c:"PO!10119",s:130,p:4703};
I.u[10055] = {n:"Amuleto de Vampyro",nn:"AMULETO DE VAMPYRO",t:1,d:"Este amuleto previene a los vampyros, que se dispongan a morderte en el cuello, de que es inútil que intenten chuparte la sangre ya que tú eres otro vampyro.",ep:0,g:146,l:45,wd:true,fm:true,w:10,c:"PO!10119",s:130,p:5500};
I.u[10056] = {n:"Cinturón de Vampyro",nn:"CINTURON DE VAMPYRO",t:10,d:"El eslogan de este cinturón dice " + "\"" + "¡Y además brilla por la noche!" + "\"" + "...mmm sí, vale ¿además de qué?",ep:0,g:156,l:42,wd:true,fm:true,w:10,c:"PO!10119",s:130,p:4805};
I.u[10057] = {n:"Runa de caza",nn:"RUNA DE CAZA",t:78,d:"Acuérdate de lo que decía el rayante Crusy: " + "\"" + "El cazador tiene un arco, ve algo que se mueve y ¡dispara! Porque si hay que disparar se dispara, pero... pero disparar pa ná ¡es tontería!" + "\"" + " Gracias a esta runa, podrás comprobar el dicho con cualquiera de tus armas.",ep:0,g:60,l:10,wd:false,fm:true,w:1,p:10};
I.u[10058] = {n:"Capa de Vampyro",nn:"CAPA DE VAMPYRO",t:17,d:"Bram tenía razón... hay que desconfiar de las personas con dientes largos, sobre todo cuando llevan una capa de vampyro.",ep:0,g:168,l:41,wd:true,fm:true,w:5,c:"PO!10119",s:130,p:4550};
