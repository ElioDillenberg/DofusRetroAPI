I.u[9916] = {n:"Pergamino del hechizo especial Lazo Espiritual",nn:"PERGAMINO DEL HECHIZO ESPECIAL LAZO ESPIRITUAL",t:75,d:"(Doble clic para aprender el hechizo) \nEste pergamino te permite aprender el hechizo Lazo Espiritual... Sólo para Osamodas.",ep:1,g:6,l:1,wd:true,fm:true,w:1,c:"PG=2",u:true,p:1};
