I.u[10155] = {n:"Ugoleto",nn:"UGOLETO",t:1,d:"Este amuleto te protegerá de las decapitaciones aleatorias y de las teletransportaciones fortuitas..",ep:0,g:147,l:43,wd:true,fm:true,w:10,s:134,p:5030};
I.u[10156] = {n:"Ugotufla",nn:"UGOTUFLA",t:11,d:"Estas pantuflas son comodísimas, mucho más cómodas que las pantuflas de oferta, aunque no se sabe a ciencia cierta, puede que los millones de ácaros masajeadores que hay dentro tengan algo que ver...",ep:0,g:140,l:41,wd:true,fm:true,w:10,s:134,p:4575};
I.u[10157] = {n:"Ugorón",nn:"UGORON",t:10,d:"Este cinturon ha sido fabricado a base de hilos de lino. Te servirá para hacerle el nudo a las bolsas de basura.",ep:0,g:157,l:43,wd:true,fm:true,w:10,s:134,p:5025};
