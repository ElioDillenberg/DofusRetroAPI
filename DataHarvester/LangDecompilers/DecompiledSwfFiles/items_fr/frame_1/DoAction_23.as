I.u[10515] = {n:"Cadeau Couronne Koninginnedag",nn:"CADEAU COURONNE KONINGINNEDAG",t:89,d:"En secouant vigoureusement ce cadeau, vous comprenez qu\'il n\'est pas vide. Peut-être que pour une fois, les dieux ne se sont pas moqués de vous.",ep:0,g:42,l:1,wd:false,fm:true,w:1,u:true,p:1};
I.u[10516] = {n:"Transformation flammèche",nn:"TRANSFORMATION FLAMMECHE",t:27,d:"Voilà ce que c\'est que de vouloir jouer avec le feu. Allez, ce n\'est peut être pas si grave, prenez conseil auprès du Mage Ax.",ep:0,g:3,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[10518] = {n:"Recette de la Corde d\'escalade",nn:"RECETTE DE LA CORDE D\'ESCALADE",t:24,d:"Un bricoleur peut bricoler une Corde d\'escalade à l\'aide des ressources suivantes : 30 Chanvres, 5 Fils de Soie et 1 Fil Résistant.",ep:0,g:176,l:1,wd:false,fm:true,w:0,p:1};
I.u[10519] = {n:"Recette des Pierres de souillure impures",nn:"RECETTE DES PIERRES DE SOUILLURE IMPURES",t:24,d:"Un alchimiste peut fabriquer des Pierres de souillure impures de différentes couleurs avec les ingrédients suivants : \nBleue : 2 Gelées Bleutées, 1 Potion Ghetto Raide. \nJaune : 2 Gelées Citrons, 1 Potion Ghetto Raide. \nVerte : 2 Gelées à la Menthe, 1 Potion Ghetto Raide.",ep:0,g:176,l:1,wd:false,fm:true,w:0,p:1};
I.u[10520] = {n:"Recette des Pierres de souillure",nn:"RECETTE DES PIERRES DE SOUILLURE",t:24,d:"Un mineur peut polir des Pierres de souillure de différentes couleurs avec les ingrédients suivants : \nBleue : 10 Pierres de souillure bleues impures, 10 Eaux. \nJaune : 10 Pierres de souillure jaunes impures, 10 Eaux. \nVerte : 10 Pierres de souillure vertes impures, 10 Eaux.",ep:0,g:176,l:1,wd:false,fm:true,w:0,p:1};
I.u[10544] = {n:"Gelutin",nn:"GELUTIN",t:18,d:"Cette créature venue d\'ailleurs a été dressée par son ancien maître pour ne manger que les monstres issus de la famille des Blops. C\'est peut-être ce qui explique son apparence surprenante ou son haleine de Blop.",ep:0,g:55,l:1,wd:true,fm:true,w:10,p:1};
I.u[10552] = {n:"Yugonnet",nn:"YUGONNET",t:16,d:"Cette coiffe surprenante protégera efficacement vos oreilles du froid et des mauvaises blagues de vos amis.",ep:0,g:245,l:41,wd:true,fm:true,w:10,s:134,p:4525};
I.u[10553] = {n:"Chapeau de Tolot",nn:"CHAPEAU DE TOLOT",t:16,d:"Seul un boufbowler peut avoir l\'idée saugrenue de se déguiser en tofu pour impressionner son ennemi. A moins qu\'il ne s\'agisse de fendre l\'air comme personne grâce à la forme fuselée de ce chapeau. Quoiqu\'il en soit, vous ne passerez pas inaperçu.",ep:0,g:252,l:41,wd:false,fm:true,w:10,s:142,p:4550};
I.u[10554] = {n:"Chapeau de Beneji",nn:"CHAPEAU DE BENEJI",t:16,d:"Les lunettes intégrées dans ce casque vous permettront de voir rouge et de vous sentir plus boufton que jamais.",ep:0,g:253,l:41,wd:false,fm:true,w:10,s:143,p:4550};
I.u[10555] = {n:"Bouclier du Real Boitar",nn:"BOUCLIER DU REAL BOITAR",t:82,d:"Cet imposant bouclier porte les traces de nombreux matches. D\'apparence robuste, il n\'est pourtant d\'aucune utilité dans un vrai combat, si ce n\'est pour impressionner vos ennemis ou faire ami-ami avec des bouftous.",ep:0,g:54,l:1,wd:false,fm:true,w:10,c:"PP>0",s:142,p:1};
I.u[10556] = {n:"Bouclier des Bouftons Rouges",nn:"BOUCLIER DES BOUFTONS ROUGES",t:82,d:"Cet imposant bouclier illustre parfaitement la fougue et l\'insolence de la jeunesse. Tout dans l\'apparence et rien dans le ventre. C\'est le cas de le dire, il ne vous protégera de rien dans un vrai combat, sauf si votre adversaire vous attaque à coups de ballon.",ep:0,g:53,l:1,wd:false,fm:true,w:10,c:"PP>0",s:143,p:1};
I.u[10557] = {n:"Gantelet du Real Boitar",nn:"GANTELET DU REAL BOITAR",t:9,d:"Ce gantelet marqué par les années facilite la prise en main de ballon ou toute autre bestiole ronde recouverte de laine. Ce que vous en faites après ne regarde que vous.",ep:0,g:176,l:43,wd:true,fm:true,w:10,s:142,p:4500};
I.u[10558] = {n:"Gantelet des Bouftons Rouges",nn:"GANTELET DES BOUFTONS ROUGES",t:9,d:"Ce gantelet permettra au jeune Chienchien fou que vous êtes de jouer à la baballe comme un grand boufbowler.",ep:0,g:177,l:43,wd:true,fm:true,w:10,s:143,p:4500};
I.u[10559] = {n:"Ceinture de Tolot",nn:"CEINTURE DE TOLOT",t:10,d:"Initialement conçue pour absorber les chocs lors de matches de boufbowl musclés, cette ceinture pourra aussi vous servir de bouée si d\'aventure vous souhaitez prendre un bain. Quelle drôle d\'idée.",ep:0,g:159,l:41,wd:false,fm:true,w:10,s:142,p:4600};
I.u[10560] = {n:"Ceinture de Mounu",nn:"CEINTURE DE MOUNU",t:10,d:"Cette ceinture montre à quel point le boufbowl n\'est pas un sport de fillette, d\'où les protections judicieusement placées sous la ceinture pour éviter tout coup bas un peu trop bas. Idéal pour assurer l\'entraînement de petits Srams.",ep:0,g:160,l:41,wd:false,fm:true,w:10,s:143,p:4600};
I.u[10561] = {n:"Bottes du Real Boitar",nn:"BOTTES DU REAL BOITAR",t:11,d:"Ces bottes ont vécu, et ça se sent. Avec elles, vous pourrez vous faire passer pour un grand boufbowler, et planter vos crampons dans le postérieur de quelques bouftous devant vos fans.",ep:0,g:144,l:42,wd:false,fm:true,w:10,s:142,p:4825};
I.u[10562] = {n:"Bottes des Bouftons Rouges",nn:"BOTTES DES BOUFTONS ROUGES",t:11,d:"Ces bottes dans l\'air du temps feront fureur auprès de tous les jeunes aventuriers fascinés par le boufbowl. Elles facilitent les chutes pour pouvoir plus facilement faire semblant d\'avoir été poussé par un adversaire. Idéal pour apprendre les rudiments du sport.",ep:0,g:145,l:42,wd:false,fm:true,w:10,s:143,p:4825};
I.u[10563] = {n:"Orbe reconstituant",nn:"ORBE RECONSTITUANT",t:50,d:"Cet objet permet de répartir ses points de caractéristiques. Les maîtres des temples de classe savent comment l\'utiliser.",ep:0,g:460,l:1,wd:true,fm:true,w:100,p:1000};
I.u[10564] = {n:"Paquet orbe reconstituant",nn:"PAQUET ORBE RECONSTITUANT",t:89,d:"Ce paquet contient un orbe précieusement enveloppé.",ep:0,g:42,l:1,wd:true,fm:true,w:0,u:true,p:1};
I.u[10587] = {n:"Etoffe des Héros",nn:"ETOFFE DES HEROS",t:55,d:"On dit que cette étoffe fut fabriquée par le célèbre Féca XxRamboPLxX. Il s\'en servait comme bandeau qu\'il se passait autour du front, ce qui ne manquait pas d\'impressionner les jeunes fossoyeurs koalaks.",ep:0,g:430,l:100,wd:false,fm:true,w:1,p:1};
I.u[10597] = {n:"Fantôme de Gelutin",nn:"FANTOME DE GELUTIN",t:90,d:"Ce Gelutin n\'a pas encore quitté le monde des vivants, il ne tient qu\'à vous de trouver une façon de le ressusciter, ou de l\'abandonner lâchement une bonne fois pour toutes.",ep:0,g:55,l:1,wd:true,fm:true,w:1,p:1};
I.u[10598] = {n:"Cadeau Gelutin",nn:"CADEAU GELUTIN",t:89,d:"En posant votre oreille sur ce cadeau ou en le secouant vigoureusement, vous entendrez peut-être d\'étranges bruits qui vous révèleront la présence d\'une créature encore vivante. Ne perdez pas une seule seconde, ouvrez-le vite, vous y trouverez un superbe familier, des ressources pour le nourrir et des poudres d\'Eniripsa pour le soigner.",ep:0,g:42,l:1,wd:true,fm:true,w:1,u:true,p:1};
I.u[10599] = {n:"Certificat de mise en chanil : Gelutin",nn:"CERTIFICAT DE MISE EN CHANIL : GELUTIN",t:77,d:"Ce certificat décrit les caractéristiques du familier que vous avez placé au Chanil.",ep:0,g:67,l:1,wd:false,fm:true,w:1,p:100};
I.u[10601] = {n:"Certificat de reconstitution",nn:"CERTIFICAT DE RECONSTITUTION",t:24,d:"Ce document atteste de la remise à zéro de vos caractéristiques.",ep:0,g:10,l:1,wd:true,fm:true,w:0,p:0};
I.u[10602] = {n:"Parchemin de Sort apprivoisement de monture",nn:"PARCHEMIN DE SORT APPRIVOISEMENT DE MONTURE",t:75,d:"Ce parchemin permet d\'apprendre le sort apprivoisement de monture.",ep:0,g:3,l:1,wd:true,fm:true,w:1,u:true,p:0};
I.u[10603] = {n:"Parchemin de Sort  capture d\'âmes",nn:"PARCHEMIN DE SORT  CAPTURE D\'AMES",t:75,d:"Ce parchemin permet d\'apprendre le sort capture d\'âmes.",ep:0,g:3,l:1,wd:true,fm:true,w:1,u:true,p:0};
I.u[10604] = {n:"Parchemin de Sort marteau de Moon",nn:"PARCHEMIN DE SORT MARTEAU DE MOON",t:75,d:"Ce parchemin permet d\'apprendre le sort marteau de Moon.",ep:0,g:3,l:1,wd:true,fm:true,w:1,u:true,p:0};
I.u[10605] = {n:"Parchemin de Sort  boomerang perfide",nn:"PARCHEMIN DE SORT  BOOMERANG PERFIDE",t:75,d:"Ce parchemin permet d\'apprendre le sort boomerang perfide.",ep:0,g:3,l:1,wd:true,fm:true,w:1,u:true,p:0};
I.u[10606] = {n:"Gemme de Rat Blanc",nn:"GEMME DE RAT BLANC",t:85,d:"Gagnée en récompense de vos victoires en duel, cette gemme spirituelle referme l\'âme d\'un monstre. A conserver à l\'abri de la lumière dans un endroit sec. Secouer avant usage.",ep:0,g:2,l:1,wd:true,fm:true,w:1,u:true,p:0};
I.u[10608] = {n:"Potion vers Kralamoure Géant",nn:"POTION VERS KRALAMOURE GEANT",t:12,d:"Cette potion vous mènera directement vers le monstre indiqué.",ep:0,g:17,l:1,wd:false,fm:true,w:1,c:"MK<11095,30&Sc!1032",u:true,p:0};
I.u[10613] = {n:"Rune Pa Pi",nn:"RUNE PA PI",t:78,d:"Cette rune permet d\'augmenter la puissance de ses pièges.",ep:0,g:84,l:20,wd:true,fm:true,w:1,p:10};
I.u[10615] = {n:"Rune Pa Pi Per",nn:"RUNE PA PI PER",t:78,d:"Cette rune permet d\'augmenter la puissance de ses pièges.",ep:0,g:24,l:20,wd:true,fm:true,w:1,p:10};
I.u[10616] = {n:"Rune Ra Pi Per",nn:"RUNE RA PI PER",t:78,d:"Cette rune permet d\'augmenter la puissance de ses pièges.",ep:0,g:83,l:30,wd:true,fm:true,w:1,p:10};
I.u[10618] = {n:"Rune Pa Do Per",nn:"RUNE PA DO PER",t:78,d:"Cette rune permet d\'augmenter les dommages causés à ses adversaires.",ep:0,g:80,l:30,wd:true,fm:true,w:1,p:10};
I.u[10619] = {n:"Rune Ra Do Per",nn:"RUNE RA DO PER",t:78,d:"Cette rune permet d\'augmenter les dommages causés à ses adversaires.",ep:0,g:81,l:30,wd:true,fm:true,w:1,p:10};
I.u[10621] = {n:"Ordres des Sacre",nn:"ORDRES DES SACRE",t:24,d:"Ce document déchiré liste les consignes des Sacre. Simples, celles-ci consistent en la traque et l\'exécution d\'une cible. Et surtout, ne pas retourner les voir avant dix minutes et ce, quelque soit le dénouement du combat.",ep:0,g:176,l:1,wd:true,fm:true,w:0,p:0};
I.u[10622] = {n:"Epée de Grüt",nn:"EPEE DE GRUT",t:6,d:"Cette épée permettait à Grüt le Bwork de couper les noisettes de ses adversaires en deux, ce qui suffisait généralement pour les faire abandonner le moindre combat. Cette arme magique est encore imprégnée de la personnalité de Grüt. En l\'équipant, vous pourrez peut-être l\'incarner.",ep:0,g:94,l:41,wd:true,fm:false,w:10,an:0,e:[5,5,1,1,50,50,false,false],s:144,p:4600};
I.u[10623] = {n:"Amulette de Grüt",nn:"AMULETTE DE GRUT",t:1,d:"Grüt le Bwork n\'était pas très malin. Il était même complètement stupide. Heureusement, les méthodes d\'enseignement ancestrales fonctionnaient aussi bien sur les enfants que sur les Bworks. Quelques coups de bâton bien mérités ont fini par avoir raison de sa stupidité et au fil des années, Grüt est devenu juste un peu bête.",ep:0,g:153,l:42,wd:true,fm:true,w:10,s:144,p:4900};
I.u[10624] = {n:"Ceinture de Grüt",nn:"CEINTURE DE GRUT",t:10,d:"Ceux qui ont eu à affronter Grüt le Bwork vous le diront, " + "\"" + "ne visez pas la tête, elle est vide" + "\"" + ". Ce conseil est valable pour tous ceux qui affrontent les Iops, les gobelins ou les belles-mères.",ep:0,g:163,l:40,wd:true,fm:true,w:10,s:144,p:4300};
I.u[10625] = {n:"Bottes de Grüt",nn:"BOTTES DE GRUT",t:11,d:"Les bottes de Grüt dégagent une odeur tellement répugnante, qu\'elles peuvent étouffer vos adversaires d\'un simple coup de pied à la tête. Ces bottes ont été interdites en combat par la convention de Sufokia il y a très longtemps. Personne ne reviendra sur cette décision qui a permis d\'éviter de nombreuses morts atroces.",ep:0,g:150,l:44,wd:true,fm:true,w:10,s:144,p:5250};
I.u[10626] = {n:"Epée de Kocksis",nn:"EPEE DE KOCKSIS",t:6,d:"La vie d\'un Chafer Fantassin n\'est pas aussi facile que l\'on pourrait l\'imaginer. Mais comme personne n\'a jamais réussi à imaginer comment vit un Chafer Fantassin, tout le monde s\'en moque et s\'imagine que sa vie est facile. C\'est à cause de raisonnements aussi stupides que celui-ci, que le Chafer Fantassin est devenu un Chafer Fantassin. Cette arme magique est encore imprégnée de la personnalité de Kocksis. En l\'équipant, vous pourrez peut-être l\'incarner.",ep:0,g:95,l:41,wd:true,fm:false,w:10,an:0,e:[5,5,1,1,50,50,false,false],s:145,p:4600};
I.u[10627] = {n:"Anneau de Kocksis",nn:"ANNEAU DE KOCKSIS",t:9,d:"Kocksis le Chafer Fantassin avait de nombreux plaisirs plutôt malsains, comme manger des chips au vinaigre avec de la pâte à tartiner ou pire encore, manger des poils de Chacha. Nombreux sont ceux qui ont tenté de le ramener dans le droit chemin et se sont perdus sur le chemin du retour.",ep:0,g:179,l:41,wd:true,fm:true,w:10,s:145,p:4650};
I.u[10628] = {n:"Bottes de Kocksis",nn:"BOTTES DE KOCKSIS",t:11,d:"Rien de plus bruyant que le claquement des os sur un parquet en orme. Grâce à ces bottes, Kocksis le Chafer Fantassin pouvait danser l\'Ankarmagniole toute la nuit sans craindre de réveiller les voisins, les morts, ni même les voisins morts.",ep:0,g:149,l:42,wd:true,fm:true,w:10,s:145,p:4800};
I.u[10629] = {n:"Ceinture de Kocksis",nn:"CEINTURE DE KOCKSIS",t:10,d:"Kocksis le Chafer Fantassin avait deux rêves dans sa vie : gravir la montagne des Craqueleurs et devenir ménestrel. La vie fût très cruelle avec lui, puisqu\'il dû se contenter de faire les premières parties des concerts de Francisque Kabroule. Parfois le sort s\'acharne sur vous. Cette fois-ci il s\'est acharné sur celui de Kocksis.",ep:0,g:164,l:42,wd:true,fm:true,w:10,s:145,p:4900};
I.u[10630] = {n:"Bâton de Gobeuf",nn:"BATON DE GOBEUF",t:4,d:"Ce bâton ressemble étrangement à une lance, Gobeuf le Gobelin a fait monter une lame aiguisée au bout de ce bâton afin de pouvoir transpercer les œufs de Tofu à distance. Il est inutile de chercher une explication rationnelle derrière tout cela, les Gobelins ont tout simplement toujours eu des jeux étranges. Cette arme magique est encore imprégnée de la personnalité de Gobeuf. En l\'équipant, vous pourrez peut-être l\'incarner.",ep:0,g:90,l:43,wd:true,fm:false,w:10,an:0,e:[10,5,1,1,50,50,false,false],s:146,p:5040};
I.u[10631] = {n:"Amulette de Gobeuf",nn:"AMULETTE DE GOBEUF",t:1,d:"Voici une des devises les plus populaires chez les Gobelins : " + "\"" + "parfois, c\'est toi qui cognes le bar et parfois c\'est le bar qui te cogne" + "\"" + ". En regardant attentivement les Gobelins, on pourrait résumer cette devise à " + "\"" + "Parfois le bar te cogne" + "\"" + ".",ep:0,g:152,l:42,wd:true,fm:true,w:10,s:146,p:4837};
I.u[10632] = {n:"Ceinture de Gobeuf",nn:"CEINTURE DE GOBEUF",t:10,d:"Gobeuf le gobelin adorait gober les œufs en état de putréfaction avancée. Si son appareil digestif supportait sans problème cette fâcheuse habitude, les gens situés à proximité de son haleine putride tombaient comme des Mosquitos. Cette ceinture permettait à tous ses proches de se rattraper avant de s\'écrouler lamentablement au sol.",ep:0,g:165,l:42,wd:true,fm:true,w:10,s:146,p:4800};
I.u[10633] = {n:"Anneau de Gobeuf",nn:"ANNEAU DE GOBEUF",t:9,d:"Gobeuf le Gobelin était passionné par le jardinage, surtout lorsqu\'il s\'agissait de donner des coups de râteau sur les imprudents qui s\'approchaient de ses terres. Il a trouvé cet anneau en ratissant le doigt d\'un imprudent qui avait marché sur un de ses pieds de Salace.",ep:0,g:180,l:44,wd:true,fm:true,w:10,s:146,p:5250};
I.u[10634] = {n:"Arc de Laikteur",nn:"ARC DE LAIKTEUR",t:2,d:"Le Kanniboul Laikteur utilisait cette arme pour chasser ses proies et divertir ses enfants en leur envoyant quelques flèches dans les fesses lorsqu\'il était de bonne humeur. Cette arme magique est encore imprégnée de l\'âme du Kanniboul Laikteur. En l\'équipant, vous pourrez peut-être parvenir à l\'incarner.",ep:0,g:68,l:41,wd:true,fm:false,w:10,an:0,tw:true,e:[10,5,1,5,30,50,false,false],s:147,p:2000};
I.u[10635] = {n:"Amulette de Laikteur",nn:"AMULETTE DE LAIKTEUR",t:1,d:"Lorsque les deux petites boules accrochées à cette amulette s\'entrechoquent, elles produisent un léger bruit de claquement qui vous permet d\'être informé de l\'arrivée d\'un Kanniboul. Lorsque vous entendez ce bruit, il est généralement trop tard pour vous enfuir, mais il est encore temps de choisir à quelle sauce vous voulez être dévoré.",ep:0,g:151,l:41,wd:true,fm:true,w:10,s:147,p:4525};
I.u[10636] = {n:"Anneau de Laikteur",nn:"ANNEAU DE LAIKTEUR",t:9,d:"Cet anneau diffuse des épices afin d\'aromatiser facilement le bouillon lorsqu\'il est mélangé avec les mains. Ces épices sont si bonnes, qu\'une fois cet anneau équipé, vous risquez de vous en mordre les doigts.",ep:0,g:178,l:42,wd:true,fm:true,w:10,s:147,p:4837};
I.u[10637] = {n:"Bottes de Laikteur",nn:"BOTTES DE LAIKTEUR",t:11,d:"Le Kanniboul Laikteur a utilisé ces bottes pendant plusieurs années avant de les échanger contre un touriste de l\'île de Moon. La faim justifie les moyens.",ep:0,g:148,l:43,wd:true,fm:true,w:10,s:147,p:5050};
I.u[10638] = {n:"Epée de Rekto Topi",nn:"EPEE DE REKTO TOPI",t:6,d:"Le Pandit Rekto Topi utilisait cette épée en frappant ses adversaires avec le plat de la lame, afin de les assommer sans les taillader. Un seul coup bien placé suffisait généralement pour les envoyer directement au tapis. Cette arme magique est encore imprégnée de l\'âme du Pandit Rekto Topi. En l\'équipant, vous pourrez peut-être parvenir à l\'incarner.",ep:0,g:93,l:41,wd:true,fm:false,w:10,an:0,e:[3,5,1,1,30,50,false,false],s:148,p:4600};
I.u[10639] = {n:"Amulette de Rekto Topi",nn:"AMULETTE DE REKTO TOPI",t:1,d:"Rekto avait un frère, appelé Verso Litaire. Il aimait voyager seul et explorer les tunnels sombres qui ne sentent pas très bon.",ep:0,g:154,l:41,wd:true,fm:true,w:10,s:148,p:4525};
I.u[10640] = {n:"Ceinture de Rekto Topi",nn:"CEINTURE DE REKTO TOPI",t:10,d:"Cette ceinture est censée être réversible mais personne n\'a jamais osé utiliser l\'autre face de peur de découvrir qu\'elle n\'est en fait pas réversible.",ep:0,g:161,l:40,wd:true,fm:true,w:10,s:148,p:4475};
I.u[10641] = {n:"Bottes de Rekto Topi",nn:"BOTTES DE REKTO TOPI",t:11,d:"Rekto Topi a confectionné ces bottes lui-même, à partir de morceaux de bois et d\'un peu de tissu. Elles ne ressemblent pas du tout à des bottes, mais qu\'importe elles sont magiques tout de même.",ep:0,g:147,l:41,wd:true,fm:true,w:10,s:148,p:4550};
I.u[10642] = {n:"Pioche de Grizou",nn:"PIOCHE DE GRIZOU",t:21,d:"Grizou le Mineur Sombre a utilisé cette pioche pour creuser la célèbre mine sans fond de Kruup, dans laquelle le Gaddie a vécu plusieurs années. Cette arme est encore imprégnée de l\'âme de Grizou. En l\'équipant, vous pourrez peut-être l\'incarner.",ep:0,g:2,l:41,wd:true,fm:false,w:10,an:0,e:[5,5,1,1,50,50,false,false],s:149,p:4600};
I.u[10643] = {n:"Anneau de Grizou",nn:"ANNEAU DE GRIZOU",t:9,d:"Cet anneau change de couleur lorsqu\'il est trempé dans du jus de Chmoule bouillant. C\'est sûrement ce qui explique pourquoi Grizou le Mineur Sombre tenait autant à cet anneau.",ep:0,g:181,l:43,wd:true,fm:true,w:10,s:149,p:5107};
I.u[10644] = {n:"Bottes de Grizou",nn:"BOTTES DE GRIZOU",t:11,d:"La semelle magique de ces bottes permet de casser les blocs de charbon en envoyant les vibrations nécessaires à leur fragmentation. La notice indique qu\'elles sont même capables de vibrer toutes seules dès qu\'un son de Bouftonik est joué à proximité.",ep:0,g:146,l:41,wd:true,fm:true,w:10,s:149,p:4575};
I.u[10645] = {n:"Ceinture de Grizou",nn:"CEINTURE DE GRIZOU",t:10,d:"Cette ceinture renforcée à l\'aide de nombreux métaux était assez solide pour supporter le poids des énormes bourses de Grizou le Mineur Sombre. Elle devrait convenir pour les bourses plus légères.",ep:0,g:162,l:43,wd:true,fm:true,w:10,s:149,p:5050};
I.u[10647] = {n:"Cadeau Bottes du Real Boitar",nn:"CADEAU BOTTES DU REAL BOITAR",t:89,d:"En secouant vigoureusement ce cadeau, vous comprenez qu\'il n\'est pas vide. Peut-être que pour une fois, les dieux ne se sont pas moqués de vous.",ep:0,g:42,l:1,wd:true,fm:true,w:1,u:true,p:1};
