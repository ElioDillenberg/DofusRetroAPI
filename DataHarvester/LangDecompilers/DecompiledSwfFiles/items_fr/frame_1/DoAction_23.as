I.u[10414] = {n:"Couronne Koninginnedag",nn:"COURONNE KONINGINNEDAG",t:16,d:"Cette couronne est généralement portée le jour de la fête qui commémore l\'anniversaire d\'une illustre reine. Ses pouvoirs magiques sont très puissants, puisqu\'elle permet de changer les couleurs de son porteur pour le rendre intégralement orange et ainsi lui permettre de facilement se cacher parmi tous les gens déguisés de couleur orange.",ep:0,g:251,l:1,wd:false,fm:true,w:10,p:1};
I.u[10417] = {n:"Pierre d\'âme de gardien de donjon",nn:"PIERRE D\'AME DE GARDIEN DE DONJON",t:124,d:"Cette pierre contient les âmes d\'un groupe de monstres qui ne demandent qu\'à revenir à la vie pour se venger de vous. Elle n\'est cependant utilisable que dans les arènes prévues à cet effet, situées dans les grandes cités.",ep:0,g:4,l:1,wd:false,fm:true,w:1,u:true,p:10};
I.u[10418] = {n:"Pierre d\'âme d\'Archi-monstre",nn:"PIERRE D\'AME D\'ARCHI-MONSTRE",t:125,d:"Cette pierre contient les âmes d\'un groupe de monstres comprenant au moins un Archi-monstre qui ne demandent qu\'à revenir à la vie pour se venger de vous. Elle n\'est cependant utilisable que dans les arènes prévues à cet effet, situées dans les grandes cités.",ep:0,g:3,l:1,wd:false,fm:true,w:1,u:true,p:10};
