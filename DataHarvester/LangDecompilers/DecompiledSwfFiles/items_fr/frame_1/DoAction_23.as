I.u[10168] = {n:"Chispottes",nn:"CHISPOTTES",t:11,d:"Ponch cirait ses bottes tous les matins avant de partir en mission sur sa Dragodinde pour sauver la veuve et l\'orphelin, mais surtout la veuve... Un sacré charmeur ce Ponch.",ep:0,g:141,l:42,wd:true,fm:true,w:10,s:138,p:4850};
I.u[10169] = {n:"Chispouclier",nn:"CHISPOUCLIER",t:82,d:"Ponch portait fièrement ce bouclier sur sa dragodinde lorsqu\'il veillait à la sécurité des ruelles de Bonta.",ep:0,g:41,l:1,wd:true,fm:true,w:10,s:138,p:4800};
I.u[10170] = {n:"Chispocasque",nn:"CHISPOCASQUE",t:16,d:"Ce casque permet de se protéger très efficacement des chutes de dragodindes ou de passer pour un idiot sans en être un.",ep:0,g:246,l:40,wd:true,fm:true,w:10,s:138,p:4400};
I.u[10171] = {n:"Mitaines de Chisp",nn:"MITAINES DE CHISP",t:9,d:"Ces mitaines en cuir sont parfaites pour donner des fessées déculottées.",ep:0,g:173,l:44,wd:true,fm:true,w:10,s:138,p:4800};
I.u[10172] = {n:"Collertue",nn:"COLLERTUE",t:1,d:"Cette amulette peut nourrir une limace vorpale pendant deux jours, ou trois jours si elle l\'accompagne d\'une purée de votre tête.",ep:0,g:148,l:42,wd:true,fm:true,w:10,s:137,p:4900};
I.u[10173] = {n:"Libottes",nn:"LIBOTTES",t:11,d:"La semelle de ces bottes est recouverte d\'une bave collante et l\'intérieur est rempli d\'une bave glissante. A moins que ça ne soit l\'inverse.",ep:0,g:142,l:39,wd:true,fm:true,w:10,s:137,p:4105};
I.u[10174] = {n:"Limape",nn:"LIMAPE",t:17,d:"Une bave gluante sort des pores de cette cape. Prenez un peu d\'élan, allongez-vous sur le dos et laissez-vous glisser.",ep:0,g:169,l:42,wd:true,fm:true,w:10,s:137,p:4800};
I.u[10175] = {n:"Limasque",nn:"LIMASQUE",t:16,d:"Ce masque gluant et baveux alimentera votre cuir chevelu en gomina de façon continue. Les deux yeux accrochés au masque peuvent être connectés à votre crâne si vous acceptez d\'insérer les terminaisons nerveuses dans vos oreilles.",ep:0,g:247,l:42,wd:true,fm:true,w:10,s:137,p:4800};
I.u[10180] = {n:"L\'Esteban",nn:"L\'ESTEBAN",t:1,d:"Ce médaillon est censé servir de clé pour accéder à une mystérieuse cité oubliée.",ep:0,g:149,l:42,wd:true,fm:true,w:10,s:136,p:4800};
I.u[10181] = {n:"Anneau solaire",nn:"ANNEAU SOLAIRE",t:9,d:"Cet anneau a été retrouvé dans une mystérieuse cité dont personne ne soupçonnait réellement l\'existence jusqu\'à ce que l\'on commence à soupçonner son existence.",ep:0,g:174,l:41,wd:true,fm:true,w:10,s:136,p:4600};
I.u[10182] = {n:"Bouclier solaire",nn:"BOUCLIER SOLAIRE",t:82,d:"Ce bouclier absorbe les rayons du soleil et les renvoie directement dans le visage de vos adversaires. A défaut de les éblouir avec votre intelligence, vous les aveuglerez temporairement grâce à ce bouclier.",ep:0,g:42,l:1,wd:true,fm:true,w:10,s:136,p:4750};
I.u[10183] = {n:"Masque solaire",nn:"MASQUE SOLAIRE",t:16,d:"Ce masque captera les rayons de soleil et les concentrera sur votre visage pour que vous puissiez rester bronzé toute l\'année, même si vous habitez au nord d\'Amakna.",ep:0,g:248,l:42,wd:true,fm:true,w:10,s:136,p:4750};
I.u[10184] = {n:"Masque de Noké",nn:"MASQUE DE NOKE",t:16,d:"Noké aimait beaucoup revêtir ce masque et se cacher dans les bois pour effrayer les promeneurs égarés. Il est mort de peur il y a bien longtemps en se regardant dans une flaque d\'eau.",ep:0,g:249,l:42,wd:true,fm:true,w:10,s:135,p:4700};
I.u[10185] = {n:"Cape de Noké",nn:"CAPE DE NOKE",t:17,d:"Noké rêvait d\'être un grand méchant loup. Même avec cette cape sur les épaules, il ne ressemblait qu\'à un petit méchant.",ep:0,g:170,l:41,wd:true,fm:true,w:10,s:135,p:4600};
I.u[10186] = {n:"Collier de Noké",nn:"COLLIER DE NOKE",t:1,d:"Cette amulette en dents de sanglier effraiera les sangliers et par extension votre belle-mère.",ep:0,g:150,l:41,wd:true,fm:true,w:10,s:135,p:4525};
I.u[10187] = {n:"Dagues de Noké",nn:"DAGUES DE NOKE",t:5,d:"Ces dagues ont servi jadis à dépecer un sanglier belliqueux qui rêvait d\'être un cerf.",ep:0,g:76,l:41,wd:true,fm:true,w:10,an:13,tw:true,e:[12,3,1,1,30,45,false,false],c:"CS>50&CW>50&CV>80",s:135,p:4000};
I.u[10188] = {n:"Coiffe de la Denrée",nn:"COIFFE DE LA DENREE",t:16,d:"Cette coiffe fera de vous un excellent Wobot de protocole grâce à son dispositif unique qui permet de traduire à la volée les discussions des choux. C\'est également un excellent casque insonorisant.",ep:0,g:250,l:43,wd:true,fm:true,w:10,s:133,p:5075};
I.u[10189] = {n:"Oxano",nn:"OXANO",t:9,d:"Cet anneau appartenait à un sabotier connu sous le surnom de " + "\"" + "Le Glaude" + "\"" + ", dont il avait hérité après avoir fait un long voyage en trottant. Ses proches l\'avaient surnommé " + "\"" + "Le Glaude trotteur" + "\"" + ".",ep:0,g:175,l:37,wd:true,fm:true,w:10,s:133,p:3875};
I.u[10190] = {n:"Bottines de la Denrée",nn:"BOTTINES DE LA DENREE",t:11,d:"Si vous décidez de planter des choux, à la mode de chez nous, pensez à équiper ces bottines conçues pour ne pas écraser les choux. N\'essayez pas de les utiliser pour danser avec votre partenaire, elles ne permettent pas d\'éviter les écrasements de pieds.",ep:0,g:143,l:38,wd:true,fm:true,w:10,s:133,p:3925};
I.u[10191] = {n:"L\'oxolature",nn:"L\'OXOLATURE",t:10,d:"Cette ceinture permet de contenir vos ballonnements après une indigestion de soupe aux choux.",ep:0,g:158,l:37,wd:true,fm:true,w:10,s:133,p:373};
