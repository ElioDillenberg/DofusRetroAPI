I.u[10657] = {n:"Chercheur d\'Ogrines",nn:"CHERCHEUR D\'OGRINES",t:18,d:"Cet étrange familier est issu du croisement des meilleurs chercheurs de truffes de tout Amakna. Son énorme groin lui permet de repérer l\'Ogrine, même lorsqu\'elle est cachée sous l\'eau. Otomaï aurait mis plusieurs années à obtenir cette espèce et l\'on raconte qu\'il l\'utiliserait pour réparer une de ses erreurs, retrouver ce qu\'il aurait égaré ou laissé s\'enfuir. Personne ne sait... La recherche d\'Ogrine implique un processus physiologique si complexe et coûteux en énergie, que cette créature est obligée de consommer les âmes des monstres pour grandir. Elle a été élevée sur les plages de l\'île d\'Otomaï et ne dévore que les âmes des créatures qui y vivent.",ep:0,g:56,l:1,wd:true,fm:true,w:10,p:1};
I.u[10658] = {n:"Fantôme de Chercheur d\'Ogrines",nn:"FANTOME DE CHERCHEUR D\'OGRINES",t:90,d:"Ce Fantôme n\'a pas encore quitté le monde des vivants, il ne tient qu\'à vous de trouver une façon de le ressusciter, ou de l\'abandonner lâchement une bonne fois pour toutes.",ep:0,g:56,l:1,wd:true,fm:true,w:1,p:1};
I.u[10659] = {n:"Oeuf du Chercheur d\'Ogrines",nn:"OEUF DU CHERCHEUR D\'OGRINES",t:72,d:"Cet œuf étrange contient une petite créature magique. Gardez-le au chaud, il finira sûrement par éclore.",ep:0,g:206,l:1,wd:false,fm:true,w:0,u:true,p:1};
I.u[10660] = {n:"Cadeau Oeuf de Chercheur d\'Ogrines",nn:"CADEAU OEUF DE CHERCHEUR D\'OGRINES",t:89,d:"En secouant vigoureusement ce cadeau, vous comprenez qu\'il n\'est pas vide. Peut-être que pour une fois, les dieux ne se sont pas moqués de vous.",ep:0,g:42,l:1,wd:false,fm:true,w:1,u:true,p:1};
I.u[10661] = {n:"Certificat de mise en chanil : Chercheur d\'Ogrines",nn:"CERTIFICAT DE MISE EN CHANIL : CHERCHEUR D\'OGRINES",t:77,d:"Ce certificat décrit les caractéristiques du familier que vous avez placé au Chanil.",ep:0,g:68,l:1,wd:false,fm:true,w:1,p:100};
I.u[10662] = {n:"Rune Pa Prospe",nn:"RUNE PA PROSPE",t:78,d:"Cette rune permet d\'avoir plus de chance de trouver des objets rares.",ep:0,g:79,l:20,wd:true,fm:true,w:1,p:10};
I.u[10663] = {n:"Brioche à la viande de bouftou",nn:"BRIOCHE A LA VIANDE DE BOUFTOU",t:33,d:"Il faut avoir l\'estomac bien accroché pour avaler cette brioche. La viande de bouftou utilisée, pas toujours de première fraîcheur, donne une saveur proche des huîtres au chocolat. Un délice pour Bwork.",ep:0,g:71,l:20,wd:true,fm:true,w:1,c:"Sc=1",u:true,ut:true,p:1};
I.u[10664] = {n:"Abragland",nn:"ABRAGLAND",t:94,d:"Très délicat comme tous les glands, il suffit de le frotter légèrement pour qu\'il expulse l\'abraknyde emprisonné en son sein.",ep:0,g:15,l:1,wd:true,fm:true,w:2,c:"Sc=2&(PB=4|PB=22|PB=33|PB=169|PB=170|PB=486|PB=168)",u:true,p:0};
I.u[10665] = {n:"Coffre de Jorbak",nn:"COFFRE DE JORBAK",t:94,d:"Jorbak ayant perdu toute sa fortune, que peut bien contenir ce coffre étrange ? Ouvrez-le pour le découvrir.",ep:0,g:16,l:1,wd:true,fm:true,w:1,u:true,p:0};
I.u[10667] = {n:"Empreinte de Jorbak",nn:"EMPREINTE DE JORBAK",t:24,d:"Vous avez réussi à noter quand sera visible l\'esprit de Jorbak. Inutile de se précipiter.",ep:0,g:251,l:1,wd:true,fm:true,w:0,p:0};
I.u[10668] = {n:"Bonne fortune de Jorbak",nn:"BONNE FORTUNE DE JORBAK",t:28,d:"Pendant quelques temps, vous bénéficierez de la chance de Jorbak.",ep:0,g:16,l:1,wd:false,fm:true,w:0,m:true,p:1};
I.u[10669] = {n:"Potion de crapaud-mufle",nn:"POTION DE CRAPAUD-MUFLE",t:12,d:"Ça vous apprendra à boire n\'importe quoi !",ep:0,g:18,l:1,wd:true,fm:true,w:1,c:"Sc=4",u:true,p:0};
I.u[10670] = {n:"Parchemin de Persimol",nn:"PARCHEMIN DE PERSIMOL",t:94,d:"Simples à suivre comme une recette de cuisine Wabbit, les indications de ce parchemin révèlent un secret envoutant.",ep:0,g:17,l:1,wd:true,fm:true,w:1,c:"Sc=5",u:true,p:0};
