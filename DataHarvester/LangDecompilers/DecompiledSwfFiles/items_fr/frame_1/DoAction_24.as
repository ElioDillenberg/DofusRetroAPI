I.u[11049] = {n:"Coiffe de Grilliane",nn:"COIFFE DE GRILLIANE",t:16,d:"Grilliane était un très grand magicien, célèbre pour avoir réinventé la poudre d\'escampette, après avoir oublié comment la fabriquer.",ep:0,g:0,l:32,wd:false,fm:true,w:10,s:161,p:2900};
I.u[11050] = {n:"Cape de Grilliane",nn:"CAPE DE GRILLIANE",t:17,d:"Cette cape est censée protéger votre postérieur des morsures de monstroplantes.",ep:0,g:0,l:32,wd:false,fm:true,w:10,s:161,p:2850};
I.u[11051] = {n:"Amulette de Grilliane",nn:"AMULETTE DE GRILLIANE",t:1,d:"Grilliane portait cette amulette autour du coup lorsqu\'il... Lorsqu\'il... Quelle importance ? Il est mort et désormais cette amulette vous appartient.",ep:0,g:0,l:32,wd:false,fm:true,w:10,s:161,p:2925};
I.u[11052] = {n:"Bottes de Grilliane",nn:"BOTTES DE GRILLIANE",t:11,d:"Grilliane le magicien a échangé un anneau magique contre cette paire de bottes. Il semblerait qu\'il ait perdu au change puisqu\'elles ne sont même pas rembourrées.",ep:0,g:0,l:34,wd:false,fm:true,w:10,an:0,s:161,p:3205};
I.u[11055] = {n:"Chapeau de Gadjète",nn:"CHAPEAU DE GADJETE",t:16,d:"Gadjète était l\'un des bricoleurs les plus créatifs du Monde des 12. Malheureusement il était également l\'un des plus étourdis. On raconte qu\'il a confondu son enclume et la tête de sa femme plus d\'une fois et qu\'elle ne s\'en plaignait pas.",ep:0,g:0,l:41,wd:false,fm:true,w:10,an:0,s:162,p:4553};
I.u[11056] = {n:"Cape de Gadjète",nn:"CAPE DE GADJETE",t:17,d:"Gadjète a fabriqué cette cape en espérant qu\'elle puisse lui permettre de voler et ainsi échapper plus facilement aux punitions administrées quotidiennement par sa femme.",ep:0,g:0,l:42,wd:false,fm:true,w:10,an:0,s:162,p:4875};
I.u[11057] = {n:"Ceinture de Gadjète",nn:"CEINTURE DE GADJETE",t:10,d:"En mettant cette ceinture à l\'envers et en la serrant suffisamment fort, vous pourrez peut-être activer le module intégré de rétromagie. Il n\'a jamais fonctionné, mais vous pourrez l\'activer tout de même. Vous pourrez donc la revendre à une personne aussi naïve que vous.",ep:0,g:0,l:40,wd:false,fm:true,w:10,an:0,s:162,p:4305};
I.u[11058] = {n:"Anneau de Gadjète",nn:"ANNEAU DE GADJETE",t:9,d:"Porté par plusieurs générations de Bricoleurs, cet anneau protège une phalange de vos coups de marteau. Malheureusement, il n\'est jamais vendu par lot de cinq.",ep:0,g:0,l:43,wd:false,fm:true,w:10,an:0,s:162,p:5100};
