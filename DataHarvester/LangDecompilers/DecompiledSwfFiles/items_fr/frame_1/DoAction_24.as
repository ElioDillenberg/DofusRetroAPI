I.u[11525] = {n:"Lancé de dé à 5 faces",nn:"LANCE DE DE A 5 FACES",t:24,d:"",ep:35,g:0,l:1,wd:false,fm:false,w:0,p:1};
I.u[11526] = {n:"Plume nacrée d\'Ilyzaelle",nn:"PLUME NACREE D\'ILYZAELLE",t:53,d:"Cette plume brille d\'une lueur qui dépasse l\'entendement. Ilyzaelle s\'en sert pour voyager très rapidement à travers le grand continent. Bien entendu, ça va être compliqué désormais.",ep:35,g:646,l:500,wd:true,fm:true,w:2,p:1};
I.u[11527] = {n:"Cheveu d\'Ilyzaelle",nn:"CHEVEU D\'ILYZAELLE",t:15,d:"Ce cheveu dispose de propriétés qui rendrait fou n\'importe quel tailleur : il est incroyablement fin et difficile à manipuler. Il est cependant très soyeux et souple, idéal pour une personne de votre trempe. Une chose est sûre : il sent bon.",ep:35,g:776,l:500,wd:true,fm:true,w:1,p:1};
I.u[11528] = {n:"Arc brisé du Tireur d\'Élite",nn:"ARC BRISE DU TIREUR D\'ELITE",t:15,d:"Il ne reste plus grand chose de ce qu\'était ce bel arc, on distingue tout de même un magnifique ornement recouvrant le manche. Il y a également l\'insigne de la garde royale de Bonta fermement incrusté dans le bois.",ep:35,g:777,l:167,wd:true,fm:true,w:4,p:1};
I.u[11529] = {n:"Pierre Perchée du Craqueleur Céleste",nn:"PIERRE PERCHEE DU CRAQUELEUR CELESTE",t:51,d:"Les légendes racontent que seuls les plus téméraires peuvent trouver cette pierre sur la plus haute cime de la plus haute montagne de Cania. Elle attendrait le baiser d\'un preux chevalier, ce qui la changerait en princesse... La réalité est parfois bien moins romantique : vous allez plutôt vous prendre un mur.",ep:35,g:466,l:178,wd:true,fm:true,w:5,p:1};
I.u[11530] = {n:"Résidu de sort temporel",nn:"RESIDU DE SORT TEMPOREL",t:12,d:"Les puissants mages de l\'école de Bonta mettent ces sorts en bouteille afin de pouvoir les utiliser rapidement en combat. Il faut donc être prudent en les manipulant au risque de créer une faille dans l\'espace temps d\'une ampleur formidable. Tout le contraire de ce que vous pouvez faire donc.",ep:35,g:720,l:110,wd:true,fm:true,w:1,p:1};
I.u[11531] = {n:"Corne en Platine de Palidane",nn:"CORNE EN PLATINE DE PALIDANE",t:47,d:"A force de polir cette corne à longueur de journée, Palidane ne voit plus rien quand le soleil reflète dessus.",ep:35,g:801,l:194,wd:true,fm:true,w:1,p:1};
I.u[11532] = {n:"Défense Maléfique",nn:"DEFENSE MALEFIQUE",t:47,d:"Cette corne représente à elle seule tout ce qui caractérise l\'ex-lieutenant d\'Uk\'Not\'Allag : impressionnante, malodorante et très puissante.",ep:35,g:802,l:666,wd:true,fm:true,w:5,p:1};
I.u[11533] = {n:"Ongle Désincarné de Qu\'Tan",nn:"ONGLE DESINCARNE DE QU\'TAN",t:47,d:"Avec une douleur pareille, pas surprenant que Qu\'Tan soit aussi ronchon. D\'autant plus que Bworknroll lui a conseillé de le mettre dans de l\'eau tiède salée. Forcément, l\'eau étant une denrée rare à Sidimote, il dut se rabattre sur de la lave. On en aurait presque de la peine.",ep:35,g:803,l:666,wd:true,fm:true,w:1,p:1};
I.u[11534] = {n:"Canine de Démon",nn:"CANINE DE DEMON",t:47,d:"Aiguisée comme une lame de rasoir, cette dent peut arracher la chair d\'un aventurier trop imprudent en moins de temps qu\'il soit possible de dire « aïe ! ».",ep:35,g:805,l:167,wd:true,fm:true,w:1,p:1};
I.u[11535] = {n:"Peau Malodorante de Démon",nn:"PEAU MALODORANTE DE DEMON",t:59,d:"Cette peau à la texture écailleuse et froide au toucher apporte une protection supérieure si elle est portée. Une fois transformée en pièce d\'équipement, elle permettra également de repousser la gente féminine, ce qui, selon certains connaisseurs un peu vieux, justifie la contrainte de l\'odeur.",ep:35,g:630,l:172,wd:true,fm:true,w:2,p:1};
I.u[11536] = {n:"Os de Chafer Maudit",nn:"OS DE CHAFER MAUDIT",t:47,d:"A l\'instar de son cousin avec lequel il entretient des relations très cordiales (mais en toute amitié), le Chafer Maudit a les os solides. C\'est probablement dû a une alimentation pas trop grasse, trop sucrée ou trop salée.",ep:35,g:806,l:135,wd:true,fm:true,w:2,p:1};
I.u[11537] = {n:"Morceau de Bâton du Sorcier Démon",nn:"MORCEAU DE BATON DU SORCIER DEMON",t:15,d:"Contrairement à ce que l\'on pourrait penser, les Sorciers Démons invoqués par Qu\'Tan n\'utilisent pas un catalyseur ordinaire. Il s\'agit d\'une sorte de lance imprégnée de la magie noire de son maître. Décidément, quelle subtilité...",ep:35,g:778,l:172,wd:true,fm:true,w:2,p:1};
I.u[11538] = {n:"Flèche Ensorcelée",nn:"FLECHE ENSORCELEE",t:15,d:"Cette flèche est, par définition, spéciale. Elle le serait encore plus si vous n\'étiez pas déjà criblé d\'une dizaine d\'autres. Au moins, celle-ci est en bon état.",ep:35,g:779,l:132,wd:true,fm:true,w:1,p:1};
I.u[11539] = {n:"L\'Arc Adie",nn:"L\'ARC ADIE",t:2,d:"Cet arc a été assemblé avec des matériaux d\'une grande qualité. En le saisissant, un grand pouvoir s\'en dégage et cela se ressent également dans les flèches tirées. ",ep:35,g:69,l:195,wd:true,fm:true,w:20,an:10,tw:true,e:[7,5,2,7,50,50,false,true],c:"CS>274&CC>274",s:163,p:350};
I.u[11540] = {n:"Potion de protection enténébrée",nn:"POTION DE PROTECTION ENTENEBREE",t:12,d:"Cette potion vous permet de ne pas être affecté par l\'écrasante puissance d\'Ilyzaelle. Un objet indispensable, donc.",ep:35,g:721,l:200,wd:true,fm:false,w:1,p:1};
I.u[11541] = {n:"Potion de protection opalescente",nn:"POTION DE PROTECTION OPALESCENTE",t:12,d:"Cette potion vous permet de ne pas être affecté par l\'écrasante perfidie de Qu\'Tan. Un objet indispensable, donc.",ep:35,g:722,l:200,wd:true,fm:false,w:1,p:1};
I.u[11542] = {n:"Cape Ilyza\'aile",nn:"CAPE ILYZA\'AILE",t:17,d:"En revêtant cet accoutrement, attendez-vous à une augmentation non négligeable des traquenards, des embuscades et autres attaques en tout genre des assaillants de la cité noire. Si ça se trouve, au fond, vous cherchez peut-être volontairement toute cette attention.",ep:35,g:175,l:196,wd:true,fm:true,w:10,s:163,p:5800};
I.u[11543] = {n:"Ilyzanneau",nn:"ILYZANNEAU",t:9,d:"On vous a vendu cet anneau comme un objet unique en son genre, qui procurerait des pouvoirs convoités par des chevaucheurs de Dragodindes tout de noir vêtus. Il suffit d\'observer les autres aventuriers à Bonta pendant l\'heure de pointe pour constater que ce n\'est pas le cas.",ep:35,g:185,l:194,wd:true,fm:true,w:10,s:163,p:3150};
I.u[11544] = {n:"Qu\'Tanneau",nn:"QU\'TANNEAU",t:9,d:"A première vue, cet anneau a l\'air extrêment inconfortable voire même dangereux pour vos petits doigts. A bien y regarder de plus près : c\'est le cas.",ep:35,g:186,l:194,wd:true,fm:true,w:10,s:164,p:3150};
I.u[11545] = {n:"Ceinture à plumes",nn:"CEINTURE A PLUMES",t:10,d:"Cette ceinture vous permet, certes, de dissimuler efficacement votre postérieur; mais peut également être portée comme un couvre-chef pour vos soirées mondaines.",ep:35,g:170,l:193,wd:true,fm:true,w:5,s:163,p:3150};
I.u[11546] = {n:"Amulette de Qu\'Tan",nn:"AMULETTE DE QU\'TAN",t:1,d:"Sertie d\'un oeil de démon, cette amulette à la forme originale vous embrochera dès que vous vous mettrez à courir. Une sorte de vengence de la part du démon qui vous rappellera sans cesse que toute action engendre une réaction.",ep:35,g:159,l:195,wd:true,fm:true,w:1,s:164,p:3150};
I.u[11547] = {n:"Bottes Qu\'Tanées",nn:"BOTTES QU\'TANEES",t:11,d:"Ces bottes sentent le souffre et le fromage. En plus de vous procurer de la puissance, elles dissimuleront vos odeurs de pieds, c\'est tout bénef\' !",ep:35,g:153,l:194,wd:true,fm:true,w:10,s:164,p:3150};
I.u[11548] = {n:"Couvre-chef corné de Qu\'Tan",nn:"COUVRE-CHEF CORNE DE QU\'TAN",t:16,d:"Qu\'Tan se servait de ces cornes afin de punir ses sujets quand ils revenaient bredouille de la chasse, ou plutôt " + "\"" + "brocouille" + "\"" + " comme on dit dans le coin.",ep:35,g:282,l:196,wd:true,fm:true,w:5,s:164,p:3150};
I.u[11549] = {n:"Bourse de 30000 Kamas",nn:"BOURSE DE 30000 KAMAS",t:100,d:"Cette bourse est très lourde, elle contient sûrement une vraie fortune.",ep:35,g:56,l:1,wd:true,fm:true,w:10,u:true,p:0};
I.u[11550] = {n:"Potion de protection enténébrée",nn:"POTION DE PROTECTION ENTENEBREE",t:30,d:"Cette potion vous permet de ne pas être affecté par l\'écrasante puissance d\'Ilyzaelle. Un objet indispensable, donc.",ep:35,g:25,l:1,wd:true,fm:false,w:0,m:true,p:1};
I.u[11551] = {n:"Potion de protection opalescente",nn:"POTION DE PROTECTION OPALESCENTE",t:30,d:"Cette potion vous permet de ne pas être affecté par l\'écrasante perfidie de Qu\'Tan. Un objet indispensable, donc.",ep:35,g:26,l:1,wd:true,fm:false,w:0,m:true,p:1};
I.u[11552] = {n:"Fatigue",nn:"FATIGUE",t:30,d:"La salle du boss est enveloppée d\'une puissante aura qui vous affaiblit.",ep:35,g:1,l:1,wd:true,fm:false,w:0,m:true,p:1};
I.u[11553] = {n:"Parchemin Doré",nn:"PARCHEMIN DORE",t:13,d:"(Double clic pour gagner de l\'expérience) \nCe parchemin permet de gagner de l\'expérience sans prendre de risques.",ep:35,g:26,l:50,wd:true,fm:true,w:1,u:true,p:10};
I.u[11554] = {n:"Dragoune en Armure",nn:"DRAGOUNE EN ARMURE",t:18,d:"Inspirée par la légendaire Dragodinde en armure, ce spécimen rare de Dragoune en armure veut lui aussi entrer dans la légende... et peut-être même, voler la vedette à son aînée !",ep:35,g:63,l:1,wd:true,fm:true,w:10,c:"PZ=1",p:1};
I.u[11555] = {n:"Casque Dragarmure",nn:"CASQUE DRAGARMURE",t:16,d:"Impossible de passer inaperçu avec ce casque sur la tête : les vieux de la vieille, les vrais de vrais, reconnaîtront aussitôt votre valeur, votre goût incontestable et surtout l\'apparence de la célèbre Dragodinde en armure.",ep:35,g:283,l:1,wd:false,fm:false,w:20,ce:true,p:0};
I.u[11556] = {n:"Bouclier Dragarmure",nn:"BOUCLIER DRAGARMURE",t:82,d:"Forgé à partir de la même cuirasse que celle arborée par la célèbre Dragodinde en armure, ce bouclier freinera considérablement les attaques de votre adversaire, mais jamais votre ardeur sauvage.",ep:35,g:92,l:1,wd:false,fm:false,w:5,ce:true,p:0};
I.u[11557] = {n:"Cape Dragarmure",nn:"CAPE DRAGARMURE",t:17,d:"Elle flotte au vent, avec la même magnificence que la Dragodinde en armure qui l\'a inspirée. La cape Dragarmure vous donnera l\'impression de voler. Veillez toutefois à redescendre sur terre au moment de combattre...",ep:35,g:176,l:1,wd:false,fm:false,w:10,ce:true,p:0};
I.u[11558] = {n:"Fantôme de Dragoune en Armure",nn:"FANTOME DE DRAGOUNE EN ARMURE",t:90,d:"Ce Fantôme de Dragoune en Armure n\'a pas encore quitté le monde des vivants, il ne tient qu\'à vous de trouver une façon de le ressusciter, ou de l\'abandonner lâchement une bonne fois pour toutes.",ep:35,g:63,l:1,wd:true,fm:true,w:1,p:1};
I.u[11559] = {n:"Certificat de Mise en Chanil : Dragoune en Armure",nn:"CERTIFICAT DE MISE EN CHANIL : DRAGOUNE EN ARMURE",t:77,d:"Ce certificat décrit les caractéristiques du Dragoune en Armure que vous avez placé au chanil.",ep:35,g:75,l:1,wd:false,fm:true,w:1,p:100};
I.u[11567] = {n:"Gemme de Qu\'Tan",nn:"GEMME DE QU\'TAN",t:85,d:"Gagnée en récompense de vos victoires en duel, cette gemme spirituelle referme l\'âme d\'un monstre. A conserver à l\'abri de la lumière dans un endroit sec. Secouer avant usage.",ep:35,g:2,l:1,wd:true,fm:true,w:1,u:true,p:0};
I.u[11568] = {n:"Gemme d\'Ilyzaelle",nn:"GEMME D\'ILYZAELLE",t:85,d:"Gagnée en récompense de vos victoires en duel, cette gemme spirituelle referme l\'âme d\'un monstre. A conserver à l\'abri de la lumière dans un endroit sec. Secouer avant usage.",ep:35,g:2,l:1,wd:true,fm:true,w:1,u:true,p:0};
