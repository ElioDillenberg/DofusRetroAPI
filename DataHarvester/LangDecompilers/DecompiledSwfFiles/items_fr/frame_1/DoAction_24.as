I.u[10955] = {n:"Anneau du Corbeau Noir",nn:"ANNEAU DU CORBEAU NOIR",t:9,d:"Quoi de plus normal pour un volatile que d\'être bagué !",ep:0,g:187,l:33,wd:false,fm:true,w:10,s:155,p:3150};
I.u[10956] = {n:"Bottes du Corbeau Noir",nn:"BOTTES DU CORBEAU NOIR",t:11,d:"Cette paire de bottes, comme tout le reste de la panoplie du Corbeau Noir, est passée de pied en pied à travers les âges. Ce qui explique sa forte odeur de Clakoss...",ep:0,g:154,l:32,wd:false,fm:true,w:10,s:155,p:2850};
I.u[10957] = {n:"Cape du Corbeau Noir",nn:"CAPE DU CORBEAU NOIR",t:17,d:"Du fil, une aiguille, quelques plumes prélevées sur des dépouilles de Corbacs en décomposition, et vous voilà en possession de la plus chic des capes !",ep:0,g:179,l:35,wd:false,fm:true,w:10,s:155,p:3400};
I.u[10958] = {n:"Casque du Corbeau Noir",nn:"CASQUE DU CORBEAU NOIR",t:16,d:"Inutile de vous cacher ! On vous a démasqué, c\'est donc vous, le Corbeau Noir !",ep:0,g:287,l:32,wd:false,fm:true,w:10,s:155,p:2900};
I.u[10959] = {n:"Pik Epik du Corbeau Noir",nn:"PIK EPIK DU CORBEAU NOIR",t:6,d:"Vous y avez laissé des plumes, mais la voilà enfin entre vos mains ! L\'épée du Corbeau Noir...",ep:0,g:97,l:39,wd:false,fm:true,w:10,an:14,e:[13,5,1,1,40,50,false,false],c:"CA>60&CV>60",s:155,p:4400};
I.u[10960] = {n:"Cadeau Anneau du Corbeau Noir",nn:"CADEAU ANNEAU DU CORBEAU NOIR",t:89,d:"En secouant vigoureusement ce cadeau, vous comprenez qu\'il n\'est pas vide. Peut-être que pour une fois, les dieux ne se sont pas moqués de vous. \nAttention, ce cadeau ne peut-être ouvert que sur DOFUS 2.0.",ep:0,g:42,l:1,wd:false,fm:true,w:1,u:true,p:1};
