I.u[10899] = {n:"Bâton du Manitou Zoth",nn:"BATON DU MANITOU ZOTH",t:4,d:"Le Manitou Zoth était un ancien manutentionnaire, il déplaçait les problèmes au lieu de les résoudre. Mais qu\'importe, il était craint au sein de la communauté des Zoths et criait plus fort que tous les autres, ce qui suffit pour être chef chez les Zoths. Ce bâton est encore animé par l\'âme du Manitou Zoth. Vous pourrez peut-être réussir à l\'incarner en utilisant cette arme.",ep:0,g:91,l:50,wd:false,fm:false,w:10,an:12,e:[5,3,1,1,40,50,false,false],p:6000};
I.u[10900] = {n:"Eclat de coquille",nn:"ECLAT DE COQUILLE",t:24,d:"Vous avez en main tout ce qui reste de ces gigantesques coquilles aperçues sur la presqu\'île des dragoeufs. Ca ne peut même pas faire un bon cendrier.",ep:0,g:767,l:1,wd:false,fm:true,w:0,p:0};
I.u[10901] = {n:"Gigantesque Canine",nn:"GIGANTESQUE CANINE",t:24,d:"Grasmera et Grozilla se sont affrontés lors d\'un épique combat. Tout ce qu\'il reste est cette canine de taille démesurée. Qui croira que vous y étiez ?",ep:0,g:13,l:1,wd:false,fm:true,w:0,p:0};
I.u[10902] = {n:"Slip du Manitou Zoth",nn:"SLIP DU MANITOU ZOTH",t:24,d:"A présent, vous pouvez dire sans mentir que le Manitou Zoth est tout nu sous son pagne.",ep:0,g:621,l:1,wd:false,fm:true,w:0,p:1};
I.u[10903] = {n:"Clef de l\'antre du Manitou Zoth",nn:"CLEF DE L\'ANTRE DU MANITOU ZOTH",t:24,d:"Cette clef permet d\'aller combattre le Manitou Zoth.",ep:0,g:790,l:1,wd:false,fm:true,w:0,p:1};
I.u[10904] = {n:"Grozillamu",nn:"GROZILLAMU",t:1,d:"Cette amulette témoigne de votre incontestable capacité à casser les œufs sans faire d\'omelettes.",ep:0,g:157,l:51,wd:false,fm:true,w:10,p:7050};
I.u[10906] = {n:"Bouclier Draconique",nn:"BOUCLIER DRACONIQUE",t:82,d:"Façonné à partir d\'une écaille de Grozilla, ce lourd bouclier est d\'une prestance exceptionnelle. Seuls de rares et redoutables combattants arborent fièrement ce pavois.",ep:0,g:51,l:1,wd:false,fm:false,w:10,p:0};
I.u[10908] = {n:"Cadeau Donjons Beta-Test DOFUS 2.0",nn:"CADEAU DONJONS BETA-TEST DOFUS 2.0",t:89,d:"Ce cadeau vous a été offert car vous avez passé plus de temps dans les donjons du beta-test DOFUS 2.0 que le commun des mortels.",ep:0,g:42,l:1,wd:false,fm:true,w:0,u:true,p:1};
I.u[10909] = {n:"Phéromone de Tofu",nn:"PHEROMONE DE TOFU",t:48,d:"Cette poudre de phéromone séchée pourrait attirer un Tofu à des kilomètres si vos propres phéromones ne les asphyxiaient pas.",ep:0,g:241,l:1,wd:false,fm:true,w:1,p:1};
I.u[10910] = {n:"Cadeau Coffret Collector DOFUS 2.0",nn:"CADEAU COFFRET COLLECTOR DOFUS 2.0",t:89,d:"Cette panoplie fera de vous une lumière, à condition de vous taire. Dans le cas contraire, elle pourra toujours impressionner les habitants d\'Astrub.",ep:0,g:42,l:1,wd:false,fm:true,w:0,u:true,p:1};
I.u[10911] = {n:"Potion de Rappel de M. Scouty",nn:"POTION DE RAPPEL DE M. SCOUTY",t:12,d:"Deux fois plus crémeuse et deux fois plus onctueuse qu\'une potion de rappel classique, la « Potion de Rappel de M. Scouty » vous permettra de vous téléporter au zaap d\'Astrub depuis n\'importe quel endroit. Le double effet de cette potion vous assure un usage prolongé, qui dure longtemps, vraiment longtemps. \nLa « Potion de Rappel de M. Scouty » est une marque déposée des entreprises Keurk & Spouky.",ep:0,g:26,l:1,wd:false,fm:false,w:0,c:"Sc=666",u:true,p:0};
I.u[10912] = {n:"Cadeau de Nowel ridicule",nn:"CADEAU DE NOWEL RIDICULE",t:89,d:"Tous les cadeaux de Nowel n\'ont pas la même taille. Les aventuriers qui n\'ont pas été très courageux  gagnent des cadeaux ridicules ainsi que le droit de pleurer et d\'être moqués.",ep:0,g:2,l:6,wd:false,fm:true,w:10,u:true,p:1};
I.u[10913] = {n:"Cadeau de Nowel satisfaisant",nn:"CADEAU DE NOWEL SATISFAISANT",t:89,d:"Celui qui a fait ce cadeau est bien prétentieux, il était persuadé que son contenu serait satisfaisant.",ep:0,g:2,l:7,wd:false,fm:true,w:20,u:true,p:1};
I.u[10914] = {n:"Cadeau de Nowel potentiellement surpuissant",nn:"CADEAU DE NOWEL POTENTIELLEMENT SURPUISSANT",t:89,d:"Ce cadeau a du potentiel si l\'on fait confiance à ce qui est écrit sur le nœud en tissu. Cependant, tous les aventuriers expérimentés vous le diront : ne faîtes jamais confiance à ce qui est écrit sur les nœuds en tissu.",ep:0,g:2,l:8,wd:false,fm:true,w:30,u:true,p:1};
I.u[10915] = {n:"Ordre de mission de Félix Nowel",nn:"ORDRE DE MISSION DE FELIX NOWEL",t:24,d:"Félix Nowel vous a révélé l\'identité d\'un des cambrioleurs de l\'entrepôt du Père Nowel. Si vous ne parvenez pas à le vaincre d\'ici deux heures, vous pouvez, si vous le souhaitez,  retourner auprès de Félix Nowel pour obtenir un nouvel ordre de mission.",ep:0,g:33,l:1,wd:false,fm:true,w:0,p:1};
I.u[10916] = {n:"Félicitations de Félix Nowel",nn:"FELICITATIONS DE FELIX NOWEL",t:24,d:"Votre victoire a été très appréciée par Félix Nowel. Revenez le voir dans 24 heures, il aura encore besoin de vos services si les caisses n\'ont pas toutes été récupérées d\'ici là.",ep:0,g:176,l:1,wd:false,fm:true,w:0,p:1};
I.u[10917] = {n:"Petite caisse de cadeaux",nn:"PETITE CAISSE DE CADEAUX",t:24,d:"Cette petite caisse provient de l\'entrepôt du Père Nowel. La restituer à son propriétaire fera de vous un héros.",ep:0,g:44,l:1,wd:false,fm:true,w:0,p:1};
I.u[10918] = {n:"Caisse de cadeaux",nn:"CAISSE DE CADEAUX",t:24,d:"Cette caisse provient de l\'entrepôt du Père Nowel. La restituer à son propriétaire fera de vous un héros.",ep:0,g:44,l:1,wd:false,fm:true,w:0,p:1};
I.u[10919] = {n:"Grosse caisse de cadeaux",nn:"GROSSE CAISSE DE CADEAUX",t:24,d:"Cette grosse caisse provient de l\'entrepôt du Père Nowel. La restituer à son propriétaire fera de vous un héros.",ep:0,g:44,l:1,wd:false,fm:true,w:0,p:1};
I.u[10920] = {n:"Biscuits de Nowel",nn:"BISCUITS DE NOWEL",t:24,d:"Voici de délicieux biscuits cuisinés avec amour. Dommage que vous ne puissiez pas y goûter.",ep:0,g:245,l:1,wd:false,fm:true,w:0,p:1};
I.u[10921] = {n:"Cadeau scellé du Minotoboule",nn:"CADEAU SCELLE DU MINOTOBOULE",t:24,d:"Nul ne sait ce que ce cadeau contient, le Minotoboule ayant emporté ce secret dans sa tombe. Est-il nécessaire de rappeler que c\'est entièrement votre faute ? Vraiment, il y a de quoi avoir les boules.",ep:0,g:715,l:1,wd:false,fm:true,w:0,p:1};
