I.u[10966] = {n:"Transformation en larve",nn:"TRANSFORMATION EN LARVE",t:31,d:"Les mauvaises langues insinueraient que cela révèle votre nature profonde. Ont-elles vraiment tort ?",ep:0,g:19,l:1,wd:false,fm:true,w:0,m:true,c:"Sc=21",p:0};
I.u[10967] = {n:"Cervelle de larve",nn:"CERVELLE DE LARVE",t:69,d:"Il faut vraiment être désespéré pour manger ça. Non, ne faites pas ça !",ep:0,g:113,l:1,wd:false,fm:true,w:2,c:"Sc=21",u:true,ut:true,p:1};
I.u[10968] = {n:"Transformation en wabbit",nn:"TRANSFORMATION EN WABBIT",t:31,d:"Étonnamment, vous n\'avez pas envie de cawottes.",ep:0,g:19,l:1,wd:false,fm:true,w:0,m:true,c:"Sc=22",p:0};
I.u[10969] = {n:"Potion de Fwaîchew",nn:"POTION DE FWAICHEW",t:12,d:"La douceur d\'une cawotte, la fraîcheur d\'une dragodinde marathonienne. Rien de tel pour vous remettre d\'aplomb. Santé !",ep:0,g:141,l:20,wd:false,fm:true,w:1,c:"Sc=22",u:true,ut:true,p:1};
I.u[10970] = {n:"Sueur de dragodinde",nn:"SUEUR DE DRAGODINDE",t:15,d:"Mais comment avez vous fait pour récupérer ça ? Et pourquoi ? Vous êtes vraiment bizarre.",ep:0,g:773,l:1,wd:false,fm:true,w:1,p:1};
I.u[10971] = {n:"Sac ripant",nn:"SAC RIPANT",t:81,d:"Équipé de ce sac, vous pourrez faire les quatre cents coups. Et toujours avec le sourire, petit galopin va !",ep:0,g:25,l:20,wd:false,fm:true,w:10,c:"Sc=23",p:1};
I.u[10972] = {n:"Corbac suiveur",nn:"CORBAC SUIVEUR",t:31,d:"Bien que très affectueux, vous soupçonnez ce volatile de fuir à tire-d\'ailes au premier signe de danger.",ep:0,g:25,l:1,wd:false,fm:true,w:0,m:true,c:"Sc=24",p:0};
I.u[10973] = {n:"Potion de destination inconnue",nn:"POTION DE DESTINATION INCONNUE",t:12,d:"Votre instinct vous dicte de ne pas avaler le contenu de ce bocal. En fait, c\'est surtout votre nez : cette mixture empeste comme les aisselles d\'un Bwork centenaire.",ep:0,g:189,l:20,wd:false,fm:true,w:1,p:10};
I.u[10974] = {n:"Fée d\'artifice festive",nn:"FEE D\'ARTIFICE FESTIVE",t:74,d:"C\'est la fêêêêteuuuh, la fêêêêteuhh !",ep:0,g:32,l:1,wd:false,fm:true,w:1,c:"Sc=26",ut:true,p:1};
I.u[10985] = {n:"Chacha des Glaces",nn:"CHACHA DES GLACES",t:18,d:"Ce Chacha a connu l\'hiver du grand nord, même s\'il n\'a jamais vu le grand nord. Personne ne sait comment il a pu arriver en Amakna, les anciens prétendent qu\'il aurait été importé de l\'île de Frigost pour lutter contre la prolifération des Wabbits. Ce familier dévoreur d\'âmes ne mange que les Wabbits.",ep:0,g:62,l:1,wd:true,fm:true,w:1,c:"PZ=1",p:1};
I.u[10986] = {n:"Fantôme de Chacha des Glaces",nn:"FANTOME DE CHACHA DES GLACES",t:90,d:"Ce Fantôme n\'a pas encore quitté le monde des vivants, il ne tient qu\'à vous de trouver une façon de le ressusciter, ou de l\'abandonner lâchement une bonne fois pour toutes.",ep:0,g:62,l:1,wd:true,fm:true,w:1,p:1};
I.u[10991] = {n:"Cadeau Chacha des Glaces",nn:"CADEAU CHACHA DES GLACES",t:89,d:"En secouant vigoureusement ce cadeau, vous comprenez qu\'il n\'est pas vide. Peut-être que pour une fois, les dieux ne se sont pas moqués de vous. \nAttention, ce cadeau ne peut-être ouvert que sur DOFUS 2.0.",ep:0,g:42,l:1,wd:false,fm:true,w:1,c:"Sc=997",u:true,p:1};
I.u[11008] = {n:"Certificat de mise en Chanil : Chacha des Glaces",nn:"CERTIFICAT DE MISE EN CHANIL : CHACHA DES GLACES",t:77,d:"Ce certificat décrit les caractéristiques du familier que vous avez placé au Chanil.",ep:0,g:24,l:1,wd:false,fm:true,w:1,p:100};
I.u[11009] = {n:"Hormone de Chacha des Glaces",nn:"HORMONE DE CHACHA DES GLACES",t:48,d:"Cette hormone de croissance déshydratée peut être utilisée dans la confection d\'une puissante potion pour améliorer un familier.",ep:0,g:262,l:1,wd:true,fm:true,w:1,p:10};
I.u[11010] = {n:"Potion d\'amélioration de Chacha des Glaces",nn:"POTION D\'AMELIORATION DE CHACHA DES GLACES",t:116,d:"En utilisant cette puissante potion, vous augmenterez les capacités de votre familier de façon définitive. Attention, une seule dose sera prise en compte par votre familier, plusieurs tentatives provoqueront des résultats décevants.",ep:0,g:60,l:1,wd:true,fm:true,w:1,c:"PO=10985",u:true,p:10};
I.u[11011] = {n:"Tifoux Tigré",nn:"TIFOUX TIGRE",t:18,d:"Plus fou que le Tifoux commun mais surtout beaucoup moins roux, ce familier peut être trempé dans l\'eau sans qu\'il ne soit nécessaire de se boucher le nez. Ce familier se nourrit des créatures qui vivent dans l\'Arbre Hakam.",ep:0,g:70,l:1,wd:true,fm:true,w:10,c:"PZ=1",p:1};
