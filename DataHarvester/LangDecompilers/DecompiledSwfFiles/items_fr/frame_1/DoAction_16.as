I.u[6690] = {n:"Paire de ciseaux abîmée",nn:"PAIRE DE CISEAUX ABIMEE",t:80,d:"Une paire de ciseaux abîmée, tout ce qu\'il y a de plus classique.",ep:1,g:33,l:1,wd:true,fm:true,w:2,ut:true,p:1};
I.u[6706] = {n:"Bandelette de la Momie Nova",nn:"BANDELETTE DE LA MOMIE NOVA",t:15,d:"Ces bandelettes sont aussi vieilles que son propriétaire, lui-même aussi vieux que ses bandelettes. Au fil du temps, elles se sont imprégnées des pouvoirs magiques de la Momie Nova, mais également de son odeur de chaire en décomposition.",ep:1,g:712,l:150,wd:true,fm:true,w:1,p:1000};
I.u[6707] = {n:"Cape de la Momie Nova",nn:"CAPE DE LA MOMIE NOVA",t:17,d:"Cette cape malodorante est faîte à partir des bandelettes malodorantes de la Momie Nova. De quoi exciter tous les habitants des cimetières alentours.",ep:14,g:63,l:75,wd:false,fm:true,w:10,p:14600};
I.u[6708] = {n:"Certificat Dopeul",nn:"CERTIFICAT DOPEUL",t:24,d:"Ce certificat indique la date de votre dernière victoire sur un dopeul.",ep:1,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[6712] = {n:"Cape Hucine",nn:"CAPE HUCINE",t:17,d:"Cette magnifique cape sent la fleur des bois. Pas de quoi rendre folle de vous une Craêtte, mais suffisant pour couvrir les odeurs d\'un Sadida qui se néglige.",ep:1,g:82,l:71,wd:true,fm:true,w:10,p:13100};
I.u[6713] = {n:"Lorsotheuses",nn:"LORSOTHEUSES",t:5,d:"Imprégnées du sang du terrible Lordsoth, ces dagues lui ont permis de terrasser le roi des Chafers lors du célèbre concours de lancer de dagues de la foire du Trool. Le roi des Chafers n\'a plus jamais osé depuis ce jour, montrer le bout de son slip en cuir, et s\'est exilé avec l\'illustre Captain Chafer dans les environs du cimetière de Pandala.",ep:1,g:34,l:100,wd:false,fm:true,w:10,an:13,e:[10,3,1,1,30,30,false,false],c:"PN~Lordsoth",p:1};
I.u[6716] = {n:"Nomoon",nn:"NOMOON",t:18,d:"Ce petit singe chapardeur semble s\'être attaché à vous maintenant. Très docile, il est cependant très exigeant sur le plan alimentaire et n\'accepte que les végétaux qui poussent sur son île.",ep:3,g:11,l:1,wd:true,fm:true,w:10,c:"PZ=1",p:1};
I.u[6717] = {n:"Chienchien Noir",nn:"CHIENCHIEN NOIR",t:18,d:"Avec ce petit toutou, vous goûterez aux joies des petites promenades pipi. Fort de ses racines sauvages, il est déconseillé de lui donner à manger, sous peine de perdre une main.  \n(CE FAMILIER LEGENDAIRE N\'A PAS BESOIN DE MANGER)",ep:1,g:10,l:1,wd:true,fm:true,w:10,c:"PZ=1",p:1};
I.u[6718] = {n:"Chacha Blanc",nn:"CHACHA BLANC",t:18,d:"Cette petite boule de poil friponne et câline fait craquer la gent féminine toutes classes confondues. Un animal redoutable donc. \n(CE FAMILIER LÉGENDAIRE N\'A PAS BESOIN DE MANGER)",ep:1,g:9,l:1,wd:true,fm:true,w:10,c:"PZ=1",p:1};
I.u[6719] = {n:"Coiffe de l\'Homme Ours",nn:"COIFFE DE L\'HOMME OURS",t:16,d:"Cette étrange coiffe a appartenu au tristement célèbre Homme Ours. Cousue à partir des restes de la tête d\'un ours en décomposition, cette coiffe vous permettra de ne pas passer inaperçu, et surtout, de laisser grâce à son odeur originale, une trace de votre passage.",ep:1,g:64,l:20,wd:true,fm:true,w:10,s:12,p:1300};
I.u[6720] = {n:"Cape de l\'Homme Ours",nn:"CAPE DE L\'HOMME OURS",t:17,d:"Cette jolie cape a été portée pendant des années par le célèbre Homme Ours. Elle ne fera pas de vous un ours, mais vous pourrez rivaliser avec les plus grands.",ep:8,g:58,l:20,wd:true,fm:true,w:10,s:12,p:1300};
I.u[6721] = {n:"Bottes de l\'Homme Ours",nn:"BOTTES DE L\'HOMME OURS",t:11,d:"Ces bottes sont rembourrées avec de la fourrure d\'ours. De quoi garder les pieds au chaud même pour ceux qui ont le sang froid.",ep:2,g:51,l:20,wd:true,fm:true,w:10,s:12,p:1300};
I.u[6722] = {n:"Alliance de l\'Homme Ours",nn:"ALLIANCE DE L\'HOMME OURS",t:9,d:"L\'Homme Ours n\'a jamais quitté cette alliance, bien que sa femme fût dévorée sous ses yeux, par un des ours qu\'il venait d\'invoquer, et cela pour impressionner sa dulcinée. Elle a effectivement été très impressionnée.",ep:1,g:66,l:20,wd:true,fm:true,w:2,s:12,p:1300};
I.u[6723] = {n:"Amulette de l\'Homme Ours",nn:"AMULETTE DE L\'HOMME OURS",t:1,d:"L\'Homme Ours portait cette amulette autour du cou, lorsqu\'il a affronté les brigandins de la tanière de Bimdhoul. La légende raconte qu\'elle lui a sauvé la vie, mais quand on la regarde de près, on s\'aperçoit qu\'il s\'agit bien d\'une légende.",ep:1,g:74,l:20,wd:true,fm:true,w:2,s:12,p:1300};
I.u[6724] = {n:"Ceinture de l\'Homme Ours",nn:"CEINTURE DE L\'HOMME OURS",t:10,d:"Cette petite ceinture recouverte de poils d\'ours, est idéale pour cacher vos poignées d\'amour. Il est donc conseillé de ne jamais l\'enlever, pour ne pas décevoir l\'élu de votre coeur.",ep:1,g:66,l:20,wd:true,fm:true,w:10,s:12,p:1300};
I.u[6725] = {n:"Bâton de l\'Homme Ours",nn:"BATON DE L\'HOMME OURS",t:4,d:"L\'Homme Ours utilisait ce bâton pour dresser ses invocations d\'ours, et parfois pour frapper les doigts de ses enfants, lorsque ces derniers ne faisaient pas leurs devoirs en revenant de l\'école. Voilà un homme qui avait tout compris à l\'éducation des enfants.",ep:14,g:46,l:20,wd:true,fm:true,w:10,an:12,e:[5,4,1,1,50,50,false,false],c:"CA>50&CV>50",s:12,p:1300};
I.u[6731] = {n:"Amukwak de Glace",nn:"AMUKWAK DE GLACE",t:1,d:"Avec cette amulette autour du cou, vous pourriez tenter de faire croire à votre entourage que vous avez une haleine fraîche. Laissez tomber, c\'est peine perdue.",ep:1,g:71,l:42,wd:true,fm:true,w:3,s:13,p:4900};
I.u[6732] = {n:"Kwakanneau de Glace",nn:"KWAKANNEAU DE GLACE",t:9,d:"Ce petit anneau était jadis très convoité par les Enutrofettes qui préféraient s\'en servir pour décorer leurs orteils. Une excellente façon de cacher leurs verrues.",ep:1,g:63,l:42,wd:true,fm:true,w:2,s:13,p:4900};
I.u[6735] = {n:"Racine de Fourbasse",nn:"RACINE DE FOURBASSE",t:98,d:"Cette minuscule racine incarne parfaitement la fourberie. Personne ne sait pourquoi, mais tout le monde le sait.",ep:12,g:96,l:22,wd:true,fm:true,w:5,p:200};
I.u[6736] = {n:"Bourgeon de Fourbasse",nn:"BOURGEON DE FOURBASSE",t:108,d:"Ce petit bourgeon de Fourbasse contient une sève sirupeuse et collante. Si vous avez le malheur de marcher sur une vieille Fourbasse tapie dans les herbes hautes, vous risquez de restez accrochée à celle-ci jusqu\'à ce qu\'elle vous dévore entièrement vos bottes. En revanche, si vous arrivez à prélever un bourgeon de Fourbasse intact, vous pourrez faire taire le plus râleur des Fécas, en le lui offrant en guise de pâte à mâcher.",ep:12,g:97,l:22,wd:true,fm:true,w:5,p:1};
I.u[6737] = {n:"Ecorce de Fourbasse",nn:"ECORCE DE FOURBASSE",t:96,d:"La Fourbasse n\'a pas réellement de vraies branches comme tout arbre qui se respecte, elle a cependant une fine écorce assez résistante et recouverte d\'une multitude d\'aspérités. Très pratique pour créer des manches d\'outils qui ne glissent pas entre les mains, ou pour donner des coups de bâtons qui arrachent la peau du derrière des enfants les plus effrontés. Dans les deux cas, la satisfaction est maximale. Pour celui qui tient le manche.",ep:12,g:98,l:22,wd:true,fm:true,w:5,p:200};
I.u[6738] = {n:"Crâne de Chef Crocodaille",nn:"CRANE DE CHEF CROCODAILLE",t:47,d:"Cet énorme crâne témoigne de l\'hygiène dentaire déplorable du Chef Crocodaille. Chose amusante, il semble encore animé d\'une certaine magie, l\'âme du Chef Crocodaille l\'habitant encore. Il lui arrive même parfois de verser de grosses larmes de Crocodaille si vous êtes méchant avec lui.",ep:4,g:637,l:70,wd:true,fm:true,w:10,p:1000};
I.u[6739] = {n:"Peau de Crocodaille",nn:"PEAU DE CROCODAILLE",t:59,d:"Réussir à avoir un morceau complet de peau de Crocodaille est presque un miracle, bien souvent, les braconniers ne récupèrent que quelques écailles. Idéale pour faire un sac, cette vieille peau saura également contenter les tailleurs de sous-vêtements de combat.",ep:4,g:601,l:36,wd:true,fm:true,w:10,p:1000};
I.u[6740] = {n:"Peau de Chef Crocodaille",nn:"PEAU DE CHEF CROCODAILLE",t:59,d:"Cette peau constituée d\'une multitude d\'écailles, est en tout point supérieure à une classique peau de Crocodaille. En effet, la face extérieure de la peau du Chef Crocodaille est plus résistante et recouverte d\'écailles acérées très coupantes, alors que la face intérieure est particulièrement douce. On raconte que certains vieux tailleurs confondent les deux faces lors de la confection de sous-vêtements. L\'histoire ne raconte pas si cela leur apporte plus ou moins de clientèle...",ep:4,g:600,l:69,wd:true,fm:true,w:10,p:1000};
I.u[6742] = {n:"Fourbamulette",nn:"FOURBAMULETTE",t:1,d:"Cette amulette est constituée d\'un mélange de feuilles de Fourbasse et de détritus de l\'île de Moon. Elle ressemble plus à un collier de fleurs qu\'à une véritable amulette, mais elle vous permettra de séduire sans trop d\'efforts la faune locale de l\'île de Moon, composée presque exclusivement d\'abominables créatures. Ainsi, plus aucune Fourbasse ne saura vous résister, elles vous sauteront toutes au cou pour vous faire des suçons. Les plus malchanceux en amour sauront s\'en contenter.",ep:12,g:69,l:33,wd:true,fm:true,w:5,s:16,p:3000};
I.u[6743] = {n:"Fourballiance",nn:"FOURBALLIANCE",t:9,d:"Prenez des feuilles de Fourbasse, empilez-les, laissez-les macérer dans de l\'alcool éventé, perforez-les, buvez le reste d\'alcool, collez les feuilles autour d\'un morceau d\'écorce, fixez des morceaux de feuille de Kokoko avec une matière gluante, et voilà, vous avez une magnifique Fourballiance. Elle sent très mauvais, et vous sera particulièrement utile pour justifier et cacher l\'odeur de vos doigts surtout si vous avez tendance à les mettre n\'importe où. Surtout là où il ne faut pas.",ep:12,g:60,l:33,wd:true,fm:true,w:5,s:16,p:3000};
I.u[6744] = {n:"Fourbabottes",nn:"FOURBABOTTES",t:11,d:"Une fois que vous aurez essayé ces bottes, vous ne serez plus capable de vous en défaire. Non pas parce qu\'elles sont exceptionnelles, mais car elles sont incroyablement collantes. Une caractéristique trop souvent partagée avec les femelles Fourbasses.",ep:12,g:47,l:34,wd:true,fm:true,w:5,s:16,p:3200};
I.u[6745] = {n:"Ceinture Fourbissante",nn:"CEINTURE FOURBISSANTE",t:10,d:"Remarquable création artisanale à base de restes de Fourbasses et de détritus ramassés le long des plages de Moon, au milieu de l\'écume nauséabonde, cette ceinture si elle est portée toute seule, vous donnera une allure vraiment surprenante. De quoi décontenancer vos adversaires ou surprendre l\'être aimé, pour mieux tirer partie de cet instant où pour une fois c\'est vous qui aurez le dessus.",ep:12,g:62,l:33,wd:true,fm:true,w:5,s:16,p:3000};
I.u[6746] = {n:"Amukwak de Terre",nn:"AMUKWAK DE TERRE",t:1,d:"Jadis portée par Oto Mustam, chef des guerriers de Brâkmar, cette petite amulette ne lui servait qu\'à se faire beau lors des orgies organisées à Brâkmar. Tout un programme.",ep:5,g:72,l:42,wd:true,fm:true,w:3,s:14,p:4900};
I.u[6747] = {n:"Amukwak du Vent",nn:"AMUKWAK DU VENT",t:1,d:"Lorsque le vent souffle dans ses plumes, le Kwak du Vent se sent libre comme l\'air, et se laisse planer tel une vieille feuille morte emportée par une bourrasque. Contrairement aux idées reçues, c\'est assez impressionnant.",ep:5,g:70,l:42,wd:true,fm:true,w:3,s:15,p:4900};
I.u[6748] = {n:"Kwakanneau de Terre",nn:"KWAKANNEAU DE TERRE",t:9,d:"Judicieusement porté, cet anneau vous permettra d\'embellir vos doigts boudinés. Malheureusement pour certains, il n\'est efficace que sur les doigts.",ep:5,g:64,l:42,wd:true,fm:true,w:2,s:14,p:4900};
I.u[6749] = {n:"Kwakanneau de Vent",nn:"KWAKANNEAU DE VENT",t:9,d:"Cet anneau est si léger, qu\'il pourrait s\'envoler au premier coup de vent. Mais ne rêvez pas, si cela se produit, vous ne vous envolerez pas avec lui. On se moquera de vous dans le meilleur des cas.",ep:5,g:62,l:42,wd:true,fm:true,w:2,s:15,p:4900};
I.u[6750] = {n:"Kwaklame de Glace",nn:"KWAKLAME DE GLACE",t:6,d:"Lame froide et tranchante, elle réveillera les instincts les plus primitifs de ceux qui l\'utilisent.",ep:1,g:51,l:41,wd:true,fm:true,w:5,an:14,e:[5,5,1,1,50,50,false,false],c:"CC>50&CV>50",s:13,p:4600};
I.u[6751] = {n:"Kwaklame de Terre",nn:"KWAKLAME DE TERRE",t:6,d:"" + "\"" + "Cette lame aiguisée mais pas déguisée, conviendra parfaitement aux aventuriers déjà expérimentés en quête de nouvelles sensations" + "\"" + ". Voici ce que l\'on peut lire sur le manuel de cette épée. Les manuels ne sont plus ce qu\'ils étaient décidément.",ep:5,g:53,l:41,wd:true,fm:true,w:5,an:14,e:[5,5,1,1,50,50,false,false],c:"CS>50&CV>50",s:14,p:4600};
I.u[6752] = {n:"Kwaklame du Vent",nn:"KWAKLAME DU VENT",t:6,d:"Idéale pour couper les nuages, cette lame pourra également vous servir à faire du vent si vous l\'agitez comme un éventail. Une excellente technique pour décimer les Moskitos ou défigurer vos amis.",ep:5,g:50,l:41,wd:true,fm:true,w:5,an:14,e:[5,5,1,1,50,50,false,false],c:"CA>50&CV>50",s:15,p:4600};
I.u[6753] = {n:"Kwakobottes de Glace",nn:"KWAKOBOTTES DE GLACE",t:11,d:"Idéales comme patins à glace sur les lacs gelés, ces bottes sont également très utiles pour garder les pieds au frais, ou pour y stocker votre goûter durant les journées de forte chaleur. Il est cependant déconseillé de les faire sécher au coin du feu, sous peine de ne plus jamais les revoir.",ep:2,g:36,l:40,wd:true,fm:true,w:5,s:13,p:4400};
I.u[6754] = {n:"Kwakobottes de Terre",nn:"KWAKOBOTTES DE TERRE",t:11,d:"" + "\"" + "Rien de mieux que les Kwakobottes de Terre pour marcher dans la terre" + "\"" + ", telles étaient les paroles de Rola Ingalsse la célèbre laboureuse, juste avant de devenir aveugle, en recevant une motte de terre géante dans les yeux.",ep:5,g:49,l:40,wd:true,fm:true,w:5,s:14,p:4400};
I.u[6755] = {n:"Kwakobottes du Vent",nn:"KWAKOBOTTES DU VENT",t:11,d:"Rudimentaires mais efficaces, ces bottes d\'une légéreté surprenante sont parfois utilisées après avoir été retravaillées, comme manche à air pour indiquer la direction et la force du vent. Il est alors conseillé de se placer du bon côté de la manche à air, pour ne pas mourir asphyxié.",ep:5,g:48,l:40,wd:true,fm:true,w:5,s:15,p:4400};
I.u[6756] = {n:"Kwape de Terre",nn:"KWAPE DE TERRE",t:17,d:"Idéale pour se camoufler en motte de terre géante, cette cape vous permettra d\'attaquer par surprise les laboureurs et leur voler leurs richesses. Elle est donc avant tout réservée aux collectionneurs de silex.",ep:8,g:64,l:41,wd:true,fm:true,w:5,s:14,p:4600};
I.u[6757] = {n:"Kwape du Vent",nn:"KWAPE DU VENT",t:17,d:"Légère et raffinée, cette création peut accessoirement servir de rideau pour les plus dépourvus. Elle était jadis très convoitée par la Momie Nova en personne, qui désirait être correctement habillée pour sortir le soir. Un exemple à suivre pour certains.",ep:8,g:62,l:41,wd:true,fm:true,w:5,s:15,p:4600};
I.u[6758] = {n:"Kwakture de Terre",nn:"KWAKTURE DE TERRE",t:10,d:"Le Kwak de Terre n\'est pas très social et surtout très chassé pour ses plumes, c\'est bien connu. En portant cette ceinture autour des hanches, vous vous exposez donc à de multiples fourberies de la part des chasseurs de Kwaks de Terre, qui vous prendront de loin pour un vieux Kwak grabataire sur le déclin. Les apparences ne sont finalement pas toujours trompeuses.",ep:12,g:65,l:41,wd:true,fm:true,w:5,s:14,p:4600};
I.u[6759] = {n:"Kwakture de Vent",nn:"KWAKTURE DE VENT",t:10,d:"" + "\"" + "La seule façon d\'attraper un vieux mâle Kwak du Vent contrarié, c\'est de se déguiser en femelle Kwak du Vent contrariée. Dès lors, le mâle tentera de comprendre pourquoi la femelle est contrariée et tentera avec tous les moyens dont il dispose, de satisfaire la femelle. Une fois le mâle satisfait de sa prestation, il faut profiter de sa béatitude pour le plumer." + "\"" + " Voici ce que l\'on peut lire dans la section douze du tome trois sur la chasse aux Kwaks du Vent. Il y est en outre clairement indiqué, que cette ceinture est un élément indispensable pour se faire passer pour une femelle Kwak du Vent. Vous savez ce qu\'il vous reste à faire.",ep:12,g:63,l:41,wd:true,fm:true,w:5,s:15,p:4600};
I.u[6760] = {n:"Kwakoiffe de Terre",nn:"KWAKOIFFE DE TERRE",t:16,d:"Cette Kwakoiffe de Terre est tellement lourde qu\'elle vous permettra de garder en permanence les pieds sur terre, mais vous fera également parfois tomber par terre. On ne peut pas tout avoir.",ep:13,g:71,l:41,wd:true,fm:true,w:25,s:14,p:4600};
I.u[6761] = {n:"Kwakoiffe du Vent",nn:"KWAKOIFFE DU VENT",t:16,d:"Pour garder les cheveux au vent même une fois équipé d\'une coiffe, rien ne vaut cette sympathique Kwakoiffe du Vent, qui vous permettra sans aucun doute de laisser libre court à votre expression capillaire dans la plupart des circonstances. C\'est suffisant pour réussir à interloquer le plus blasé des Chafers.",ep:13,g:69,l:41,wd:true,fm:true,w:5,s:15,p:4600};
I.u[6762] = {n:"Fourbasse-Ton",nn:"FOURBASSE-TON",t:4,d:"" + "\"" + "Pour la baston, rien ne vaut un Fourbasse-Ton" + "\"" + ". Voici ce que l\'on pouvait lire sur le dépliant publicitaire de l\'Agence Touristes, qui proposait de reconstruire soi-même ce bâton à partir de diverses ressources cachées un peu partout sur l\'île de Moon. Une sorte de jeu de piste, mais avec aucune piste.",ep:12,g:44,l:32,wd:true,fm:true,w:5,e:[15,4,1,1,20,70,false,false],s:16,p:2900};
I.u[6763] = {n:"Fourbacapa",nn:"FOURBACAPA",t:17,d:"On raconte que les Fourbasses mortes sont très confortables une fois qu\'elles ont pris le temps de se décomposer au soleil. En effet, avec le temps elles deviennent beaucoup plus souples et douces. Beaucoup d\'aventuriers aimeraient pouvoir en dire autant de leur dulcinée.",ep:12,g:61,l:33,wd:true,fm:true,w:5,s:16,p:3000};
I.u[6764] = {n:"Fourbacoiffe",nn:"FOURBACOIFFE",t:16,d:"Avec cette coiffe sur la tête, vous n\'avez vraiment pas l\'air très futé. Mais plus que sans la coiffe, donc autant la garder.",ep:12,g:68,l:31,wd:true,fm:true,w:5,s:16,p:2800};
I.u[6765] = {n:"Lailait",nn:"LAILAIT",t:79,d:"Attention, il est bien précisé sur l\'emballage de ne pas donner ce lait à un jeune Chacha, il risquerait de faire quelques tâches dans votre demeure.",ep:1,g:1,l:1,wd:false,fm:true,w:1,p:4};
I.u[6766] = {n:"Epis de Farle",nn:"EPIS DE FARLE",t:1,d:"Cette amulette d\'apparence rudimentaire a été confectionnée à partir de trois épis de Blé magiques, recouverts de poussière de pierre précieuse, et fauchés dans les plaines de Snossios pendant la treizième moisson d\'Istrabraq. Elle confère un courage hors du commun à son porteur, désormais à même de faucher plaines et vallées sans jamais faillir.",ep:1,g:68,l:80,wd:true,fm:true,w:5,s:17,p:16500};
I.u[6767] = {n:"Bracelet Magique de Farle",nn:"BRACELET MAGIQUE DE FARLE",t:9,d:"Ce bracelet en pierres précieuses a été recouvert d\'une toile de Chanvre par Shika, pour éviter d\'attirer l\'attention des Corbacs, lorsque Farle semait son orge.",ep:13,g:61,l:80,wd:true,fm:true,w:5,s:17,p:16500};
I.u[6768] = {n:"Soki",nn:"SOKI",t:32,d:"Vous emmenez ce chacha malheureux afin de le distraire.",ep:1,g:6,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[6769] = {n:"Bâton de Farle",nn:"BATON DE FARLE",t:4,d:"Ce splendide bâton est muni de très belles dents à son extrémité, ce qui en fait un excellent fourcher. Farle s\'en servait pour créer d\'immenses meules de foin, plus hautes que le plus grand des Ormes qui poussait jadis en forêt de Litneg. On raconte que c\'est Brouce Boulgoure en personne qui aurait coupé le bois nécessaire à la fabrication de ce bâton, en échange des  monstrueuses rations quotidiennes de flocons d\'avoine offertes par Shika Ingalsse, qu\'il engouffrait avant de partir raser des forêts complètes.",ep:14,g:43,l:80,wd:true,fm:true,w:10,an:12,tw:true,e:[10,5,1,1,45,45,false,true],c:"CS>100&CV>100",s:17,p:16500};
I.u[6770] = {n:"Sousouris",nn:"SOUSOURIS",t:15,d:"Cette Sousouris a été dénichée à coup de balai, de la maison de Grand-mère.",ep:1,g:282,l:1,wd:true,fm:true,w:10,p:1};
I.u[6771] = {n:"Anneau Thé",nn:"ANNEAU THE",t:9,d:"Cet anneau n\'a pas encore fait ses preuves. Il s\'agit d\'une confection unique faite par un apprenti bijoutier à l\'occasion d\'un test important. Il est fabriqué à partir d\'un anneau de fer classique plongé dans une solution de Thé magique.",ep:1,g:1,l:4,wd:false,fm:true,w:1,p:50};
I.u[6773] = {n:"Amulette Tude",nn:"AMULETTE TUDE",t:1,d:"Cette amulette est construite de façon Tude, d\'où son nom. C\'est l\'une des premières méthodes de création, car l\'une des plus simples, que l\'on enseigne aux apprentis bijoutiers. Certes le produit n\'est pas puissant mais il ne faut pas en demander trop non plus.",ep:1,g:19,l:1,wd:false,fm:true,w:1,p:25};
I.u[6774] = {n:"Sabots de Farle",nn:"SABOTS DE FARLE",t:11,d:"Ces sabots ont été taillés dans des bois rares, renforcés avec des pièces de cuirs d\'une valeur inestimable, et rembourrés avec des céréales magiques. A la fois légère, élégante, et confortable, cette paire de sabots saura résister aux affres du temps. Beaucoup d\'aventuriers un peu trop misogynes aimeraient pouvoir en dire autant de leur dulcinée.",ep:9,g:46,l:80,wd:true,fm:true,w:10,s:17,p:16600};
I.u[6775] = {n:"Cape de Farle",nn:"CAPE DE FARLE",t:17,d:"Cette cape en tissu et en peaux permettait à Farle Ingalsse d\'ensemencer des champs entiers pendant une saison complète sans jamais devoir s\'arrêter. En effet, une fois repliée et utilisée comme une besace, sa capacité semble presque illimitée. Personne ne connaît réellement le tailleur à l\'origine de cette merveille, la légende raconte seulement, qu\'il s\'est retiré de la profession lorsque ses clients ont commencé à confondre ses capes avec des serpillères. Quel triste départ en retraite !",ep:13,g:70,l:80,wd:true,fm:true,w:10,s:17,p:16500};
I.u[6776] = {n:"Farlature",nn:"FARLATURE",t:10,d:"Cette ceinture servait également de tablier de boucher à Farle Ingalsse lorsqu\'il devait égorger ses troupeaux de Bouftous. Une tâche ingrate et cruelle, dont il s\'affranchissait pourtant avec un plaisir non dissimulé.",ep:4,g:61,l:80,wd:true,fm:true,w:10,s:17,p:16500};
I.u[6778] = {n:"Farlacoiffe",nn:"FARLACOIFFE",t:16,d:"Ce chapeau de paille a été confectionné à l\'aide d\'une multitude d\'épis magiques tressés entre eux, puis l\'intérieur a été rembourré avec divers poils et étoffes cousues à la main par Shika Ingalsse, femme de Farle. Plus classe que le Ploukosse, et plus stylé que le Kraignosse, ce chapeau vous protègera du cagnard comme il l\'a fait pour Farle pendant des centaines de moissons, tout en vous donnant une allure guillerette et vraiment " + "\"" + "cool" + "\"" + ". Suffisant pour s\'improviser roi ou reine des champs.",ep:13,g:67,l:80,wd:true,fm:true,w:10,s:17,p:16500};
I.u[6779] = {n:"Rapport de l\'espion Sram",nn:"RAPPORT DE L\'ESPION SRAM",t:24,d:"Rapport de l\'espion Ztiv Oztin, récupéré avec une certaine difficulté des mains de Joe Pierlivet.",ep:1,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[6780] = {n:"Épée de l\'initié",nn:"EPEE DE L\'INITIE",t:6,d:"Cette arme d\'apparence rudimentaire vous donnera l\'impression de savoir vous battre.",ep:1,g:96,l:1,wd:true,fm:true,w:10,an:14,e:[5,4,1,1,50,50,false,false],p:1};
I.u[6781] = {n:"Baguette de l\'Initié",nn:"BAGUETTE DE L\'INITIE",t:3,d:"Cette arme d\'apparence rudimentaire vous donnera l\'impression de savoir vous battre.",ep:1,g:8,l:1,wd:true,fm:true,w:10,an:11,e:[5,4,2,3,50,50,false,true],p:1};
I.u[6782] = {n:"Bâton de l\'initié",nn:"BATON DE L\'INITIE",t:4,d:"Cette arme d\'apparence rudimentaire vous donnera l\'impression de savoir vous battre.",ep:1,g:1,l:1,wd:true,fm:true,w:10,an:12,e:[5,4,1,1,50,50,false,false],p:1};
I.u[6783] = {n:"Arc de l\'initié",nn:"ARC DE L\'INITIE",t:2,d:"Cette arme d\'apparence rudimentaire vous donnera l\'impression de savoir vous battre.",ep:1,g:1,l:1,wd:true,fm:true,w:10,an:10,tw:true,e:[5,4,2,6,50,50,false,true],p:1};
I.u[6784] = {n:"Dagues de l\'initié",nn:"DAGUES DE L\'INITIE",t:5,d:"Cette arme d\'apparence rudimentaire vous donnera l\'impression de savoir vous battre.",ep:1,g:10,l:1,wd:true,fm:true,w:10,an:13,e:[5,3,1,1,30,50,false,false],p:1};
I.u[6785] = {n:"Marteau de l\'initié",nn:"MARTEAU DE L\'INITIE",t:7,d:"Cette arme d\'apparence rudimentaire vous donnera l\'impression de savoir vous battre.",ep:1,g:1,l:1,wd:true,fm:true,w:10,an:15,e:[5,4,1,1,50,50,false,false],p:1};
I.u[6786] = {n:"Pelle de l\'Initié",nn:"PELLE DE L\'INITIE",t:8,d:"Cette arme d\'apparence rudimentaire vous donnera l\'impression de savoir vous battre.",ep:1,g:1,l:1,wd:true,fm:true,w:10,an:16,e:[5,4,1,1,50,50,false,false],p:1};
I.u[6787] = {n:"Fiche remplie valide",nn:"FICHE REMPLIE VALIDE",t:80,d:"Cette petite fiche possède le nom d\'un potentiel candidat pour être livreur du boulanger",ep:1,g:6,l:1,wd:false,fm:true,w:0,c:"PW>1499&CA>49",p:0};
I.u[6788] = {n:"Gâteaux",nn:"GATEAUX",t:24,d:"Ces gâteaux viennent juste d\'être sortis du four. Il faut les livrer tant qu\'ils sont chauds. Pensez-vous vraiment que vous avez du temps à perdre pour les examiner ainsi ?",ep:1,g:245,l:1,wd:false,fm:true,w:0,p:1};
I.u[6789] = {n:"Amulette des Champs",nn:"AMULETTE DES CHAMPS",t:1,d:"Cette petite amulette fleurit au printemps, mais se dessèche en été, se fane en automne, et pourrit en hiver. \nL\'occasion rêvée pour changer de look au fil des saisons.",ep:1,g:73,l:10,wd:false,fm:true,w:5,s:18,p:500};
I.u[6790] = {n:"Fiche à remplir",nn:"FICHE A REMPLIR",t:80,d:"Cette fiche ressemble à une feuille toute simple, cependant au contact de la peau d\'un individu celle ci affiche tout de suite si la personne convient aux critères fournis dans l\'entête.",ep:1,g:6,l:1,wd:false,fm:true,w:0,ut:true,p:0};
I.u[6791] = {n:"Anneau Champêtre",nn:"ANNEAU CHAMPETRE",t:9,d:"Avec ce petit anneau autour du doigt, vous pourrez presque essayer de passer inaperçu au milieu des champs.",ep:1,g:65,l:10,wd:true,fm:true,w:5,s:18,p:500};
I.u[6792] = {n:"La Plantouze des Champs",nn:"LA PLANTOUZE DES CHAMPS",t:4,d:"Ce bâton en forme de fleur a été confectionné à partir des restes de fleurs champêtres ayant succombé durant d\'horribles et cruels combats. Toute la rage et la haine de ces pauvres plantes est contenue dans ce ridicule petit bâton. Cette arme est utilisée par les Srams les plus fourbes, qui tentent de persuader les ignorantes qu\'ils comptent leur offrir un bouquet de fleurs. Une fois à portée, l\'imprudente se retrouve souvent assenée de coups sur la tête bien mérités.",ep:1,g:45,l:10,wd:true,fm:true,w:10,an:12,e:[7,4,1,1,45,50,false,false],c:"CV>20",s:18,p:500};
I.u[6793] = {n:"Anneau Thé",nn:"ANNEAU THE",t:9,d:"Cet anneau a fait ses preuves, une petite marque à l\'intérieur le prouve. Il s\'agit d\'une confection unique faite par un apprenti bijoutier à l\'occasion d\'un test important. Il est fabriqué à partir d\'un anneau de fer classique plongé dans une solution de Thé magique.",ep:1,g:1,l:4,wd:true,fm:true,w:1,p:50};
I.u[6794] = {n:"Bottes Champêtres",nn:"BOTTES CHAMPETRES",t:11,d:"Rien de mieux que d\'utiliser des bottes confectionnées à partir de restes de fleurs champêtres, pour écraser ces misérables fleurs qui poussent dans vos champs. De plus, ces bottes permettront à vos pieds de dégager une délicieuse odeur de fleur. Dommage qu\'il n\'existe pas un équivalent à ces bottes pour les caleçons.",ep:1,g:50,l:10,wd:true,fm:true,w:10,s:18,p:500};
I.u[6795] = {n:"Capouze des Champs",nn:"CAPOUZE DES CHAMPS",t:17,d:"Si vous voulez être la plus belle ce soir pour aller danser, c\'est la cape qu\'il vous faut.",ep:1,g:65,l:10,wd:true,fm:true,w:10,s:18,p:500};
I.u[6796] = {n:"Ceinture Fleurie",nn:"CEINTURE FLEURIE",t:10,d:"Avec cette ceinture décorée de petits pétales autour de la taille, vous pourrez sans problème vous faire passer pour une danseuse étoile au milieu d\'un combat perdu d\'avance. De quoi décontenancer une armée de Chafers, pour retourner la situation à votre avantage.",ep:1,g:64,l:10,wd:true,fm:true,w:5,s:18,p:500};
I.u[6797] = {n:"Coiffe Champêtre",nn:"COIFFE CHAMPETRE",t:16,d:"Si vous avez toujours rêvé d\'être fleuriste et d\'avoir une haleine parfumée à l\'eau de rose, c\'est le moment d\'adopter cette étrange coiffe.",ep:1,g:70,l:10,wd:true,fm:true,w:10,s:18,p:500};
I.u[6798] = {n:"Fleur de Dzibi",nn:"FLEUR DE DZIBI",t:24,d:"Son parfum est léger, subtil, enivrant ... Mais seuls les Sadidas et les Eniripsas connaissent le secret de ces fleurs.",ep:1,g:117,l:1,wd:false,fm:true,w:0,p:1};
I.u[6799] = {n:"Lame du Chef Bwork",nn:"LAME DU CHEF BWORK",t:6,d:"Tranchante comme une lame de Bwork, cette lame du Chef Bwork a l\'avantage de peser beaucoup plus lourd. Elle est certes moins maniable, mais il suffit de la laisser tomber sur son adversaire pour le trancher en deux, telle une vulgaire tranche de Tronknyde. Si votre ennemi ne meurt pas de douleur, il mourra de honte, et vous de rire.",ep:1,g:48,l:100,wd:true,fm:true,w:100,an:14,tw:true,e:[10,5,1,1,40,50,false,false],c:"CS>200&CA>200&CV>200",s:19,p:25000};
I.u[6800] = {n:"Cape Banale",nn:"CAPE BANALE",t:17,d:"Cette cape est faîte de laine de Boufton. Elle ne vous sera guère utile si ce n\'est pour avoir l\'air classe... Et encore...",ep:1,g:15,l:1,wd:true,fm:true,w:1,p:1};
I.u[6801] = {n:"Cape Hivernale",nn:"CAPE HIVERNALE",t:17,d:"Une jolie doublure très chaude et très odorante a été admirablement cousue sur une cape banale. \nSur votre dos, ou comme peau de bête devant la cheminée, cette cape aura enfin une vraie utilité.",ep:1,g:18,l:1,wd:true,fm:true,w:2,p:1};
I.u[6802] = {n:"Cache Oreilles",nn:"CACHE OREILLES",t:16,d:"Ce cache oreille très doux tiendra vos feuilles de choux au chaud.",ep:1,g:73,l:1,wd:true,fm:true,w:10,p:1};
I.u[6803] = {n:"Extrait de fleur de Dzibi",nn:"EXTRAIT DE FLEUR DE DZIBI",t:24,d:"Eviter d\'en boire... simplement !",ep:1,g:82,l:1,wd:false,fm:true,w:0,p:1};
I.u[6804] = {n:"Amulette du Chef Bwork",nn:"AMULETTE DU CHEF BWORK",t:1,d:"D\'apparence rudimentaire, cette amulette en forme de silex taillé est pourtant dotée d\'une magie très puissante capable de transformer un Bwork en princesse, ou l\'inverse, personne ne sait.",ep:1,g:66,l:100,wd:true,fm:true,w:1,s:19,p:25600};
I.u[6805] = {n:"Bracelet du Chef Bwork",nn:"BRACELET DU CHEF BWORK",t:9,d:"Muni de ce petit anneau, vous pourrez faire croire à votre entourage que vous avez épousé un grand Chef Bwork et qu\'il vous a offert ce bracelet comme cadeau de mariage. De quoi vous rendre intéressant lors des soirées animées à la Taverne si vous n\'avez pas beaucoup de conversation.",ep:1,g:58,l:100,wd:true,fm:true,w:50,s:19,p:25600};
I.u[6806] = {n:"Fragment de bois",nn:"FRAGMENT DE BOIS",t:24,d:"Ce fragment de bois provient d\'une construction récente, vraisemblablement rouge.",ep:1,g:246,l:1,wd:false,fm:true,w:0,p:1};
I.u[6807] = {n:"Bottes du Chef Bwork",nn:"BOTTES DU CHEF BWORK",t:11,d:"De loin, ces bottes ressemblent à de classiques bottes de Bwork. De près aussi. Et pourtant, ce sont des bottes de Chef Bwork. Plus résistantes, plus puissantes, et plus malodorantes, elles sauront faire la différence dans un combat perdu d\'avance.",ep:1,g:44,l:100,wd:true,fm:true,w:50,s:19,p:25600};
I.u[6808] = {n:"Morceau de tissu arraché",nn:"MORCEAU DE TISSU ARRACHE",t:24,d:"Ce morceau de tissu blanc est à la fois très doux et très taché.",ep:1,g:247,l:1,wd:false,fm:true,w:0,p:1};
I.u[6809] = {n:"Description approximative",nn:"DESCRIPTION APPROXIMATIVE",t:24,d:"Cette description décrit une petite fille habillée d\'une couleur foncée et les cheveux blonds.",ep:1,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[6810] = {n:"Rapport d\'activité du quartier",nn:"RAPPORT D\'ACTIVITE DU QUARTIER",t:24,d:"Ce rapport stipule qu\'aucun incident n\'a été constaté dans le quartier le jour du vol de la maison. \nHormis la vision d\'un roublard dans la nuit de l\'incident.",ep:1,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[6811] = {n:"Cape du Chef Bwork",nn:"CAPE DU CHEF BWORK",t:17,d:"Cette cape déchirée et pleine de trous vous fera passer pour un très grand guerrier Bwork. Elle est parfaite si vous voulez terroriser les vieux du village. En revanche, pour séduire l\'âme sœur, cela risque d\'être un peu plus difficile, à moins de la porter entièrement nu, ce qui vous permettra en plus, de continuer à faire peur aux vieux du village.",ep:1,g:60,l:100,wd:true,fm:true,w:50,s:19,p:25600};
I.u[6812] = {n:"Slip du Chef Bwork",nn:"SLIP DU CHEF BWORK",t:10,d:"Ce slip en cuir violet se suffit à lui-même, vous pouvez vous contenter de ne porter que ça, vous aurez tout de même fière allure, à condition qu\'il soit à votre taille et qu\'il ne vous tombe pas sur les genoux lors d\'une partie de corde à sauter un peu trop mouvementée.",ep:1,g:59,l:100,wd:true,fm:true,w:50,s:19,p:25600};
I.u[6813] = {n:"Casque du Chef Bwork",nn:"CASQUE DU CHEF BWORK",t:16,d:"Si vous voulez réellement avoir une tête de gros débile, ce casque est fait pour vous. Avec un peu de chance, on ne verra même pas la différence lorsque vous l\'enlèverez.",ep:1,g:65,l:100,wd:true,fm:true,w:50,s:19,p:25600};
I.u[6814] = {n:"Ensemble de bouts de bois",nn:"ENSEMBLE DE BOUTS DE BOIS",t:24,d:"Cet ensemble de bouts de bois forme un petit lit douillet, idéal pour un Piou.",ep:1,g:248,l:1,wd:false,fm:true,w:0,p:1};
I.u[6815] = {n:"Seringue de Nibé Lulle",nn:"SERINGUE DE NIBE LULLE",t:24,d:"Nibé Lulle vous a donné cette seringue pour prélever un échantillon de sang de Radlu Minite le Mineur Sombre.",ep:1,g:31,l:1,wd:false,fm:true,w:0,p:1};
I.u[6816] = {n:"Echantillon de sang",nn:"ECHANTILLON DE SANG",t:24,d:"Vous avez enfin réussi à prélever un échantillon de sang sur Radlu Minite. Très appétissant.",ep:1,g:102,l:1,wd:false,fm:true,w:0,p:1};
I.u[6817] = {n:"Amulette du Chef Crocodaille",nn:"AMULETTE DU CHEF CROCODAILLE",t:1,d:"Cette surprenante amulette a en fait été confectionnée à partir d\'un Bouclier Reptilien dont on a détaché les multiples pierres précieuses. C\'est donc un bijou particulièrement lourd à porter mais vraiment très résistant. Votre cou sera parfaitement protégé des vampires, mais aussi des suçons des villageoises un peu trop entreprenantes.",ep:4,g:67,l:70,wd:true,fm:true,w:100,s:20,p:12700};
I.u[6818] = {n:"Rose démoniaque coupée",nn:"ROSE DEMONIAQUE COUPEE",t:35,d:"Cette fleur, autrefois démoniaque, est à présent un cadeau angélique pour une personne. Cela dit, si vous n\'obtenez en retour qu\'un regard neutre, vous pourrez toujours en faire de la confiture.",ep:1,g:118,l:1,wd:false,fm:true,w:1,p:1};
I.u[6819] = {n:"Bracelet du Chef Crocodaille",nn:"BRACELET DU CHEF CROCODAILLE",t:9,d:"Ce bracelet ressemble étrangement à un gant et pourtant ce n\'est pas un gant mais bien un bracelet. Les apparences sont parfois trompeuses.",ep:4,g:59,l:70,wd:true,fm:true,w:20,s:20,p:12800};
I.u[6820] = {n:"Herbe curative",nn:"HERBE CURATIVE",t:24,d:"Cette herbe est l\'ingrédient principal de vieux remèdes d\'alchimiste. Elle n\'est plus du tout utilisée de nos jours. On lui préfère le trèfle car ce dernier ne provoque pas de démangeaison faciale, ce qui, il faut l\'avouer, est un plus.",ep:1,g:103,l:1,wd:true,fm:true,w:0,p:1};
I.u[6821] = {n:"Lame du Chef Crocodaille",nn:"LAME DU CHEF CROCODAILLE",t:6,d:"Fabriquée à partir de débris trouvés sur un Chef Crocodaille, cette arme saura se révéler très efficace si vous êtes assez beau, habile, intelligent et fort pour la manier. Avec un peu de chance, vous finirez par trouver quelqu\'un qui acceptera de vous l\'échanger contre une épée de boisaille.",ep:4,g:49,l:70,wd:true,fm:true,w:100,an:14,e:[12,6,1,1,40,40,false,false],c:"CA>150&CC>150&CV>100",s:20,p:12800};
I.u[6822] = {n:"Piqûre de réveil",nn:"PIQURE DE REVEIL",t:24,d:"Cette piqûre contient un remède si puissant qu\'il pourrait réveiller un mort. D\'ailleurs il parait que l\'illustre mort-vivant Hyrkul s\'en injectait régulièrement. On ne peut trouver meilleure preuve de son efficacité.",ep:1,g:31,l:1,wd:false,fm:true,w:0,p:1};
I.u[6823] = {n:"Potion à base de plantes",nn:"POTION A BASE DE PLANTES",t:24,d:"Cette potion, à l\'odeur nauséabonde pourrait réveiller un sadida pendant sa sieste...Ou presque...Si le sadida est jeune et sur le point de se réveiller.",ep:1,g:500,l:1,wd:true,fm:true,w:0,p:1};
I.u[6824] = {n:"Pierre de Soin Fuyard",nn:"PIERRE DE SOIN FUYARD",t:24,d:"Cette pierre de soin perd de son pouvoir très rapidement, il faut l\'entourer d\'un métal qui absorbe certaines radiations, afin de confiner l\'effet de la pierre et ainsi de préserver son efficacité. On lui préfère des amulettes standards presque aussi efficaces et surtout bien moins chères.",ep:1,g:104,l:1,wd:false,fm:true,w:0,p:1};
I.u[6825] = {n:"Pantoufles Crochues du Chef Crocodaille",nn:"PANTOUFLES CROCHUES DU CHEF CROCODAILLE",t:11,d:"Ces magnifiques pantoufles en peau et en écailles de Chef Crocodaille sont munies de dents acérées à leurs extrémités. Méfiez-vous lorsque vous ferez du pied à votre dulcinée, vous pourriez lui déchirer ses petits bas en laine de Tabi.",ep:4,g:45,l:70,wd:true,fm:true,w:20,s:20,p:12700};
I.u[6826] = {n:"Pierre de soin sertie",nn:"PIERRE DE SOIN SERTIE",t:24,d:"Cette amulette est une pierre de soin fuyard joliement sertie par un morceau de métal rare. La tenir dans sa main procure une sensation de bien-être, la fatigue s\'en va et le cerveau est libéré. Et en plus on peut vous voir dans la nuit avec.",ep:1,g:105,l:1,wd:false,fm:true,w:0,p:1};
I.u[6827] = {n:"Lettre de Joe Pierlivet",nn:"LETTRE DE JOE PIERLIVET",t:24,d:"Joe t\'a confié cettre lettre afin que tu l\'amènes à un bon ami à lui, Howard le Simaisrien, un Iop euh ... comment dire ... un Iop quoi !",ep:1,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[6828] = {n:"Poignée d\'epée",nn:"POIGNEE D\'EPEE",t:24,d:"Cette poignée ne vous transformera pas en as du maniement d\'epée mais elle peut vous y aider.",ep:1,g:97,l:1,wd:false,fm:true,w:0,p:1};
I.u[6829] = {n:"Epée tout juste forgée",nn:"EPEE TOUT JUSTE FORGEE",t:24,d:"Cette épée vient tout juste d\'être forgée.",ep:1,g:98,l:1,wd:false,fm:true,w:0,p:1};
I.u[6830] = {n:"Petit sac d\'écolier du Chef Crocodaille",nn:"PETIT SAC D\'ECOLIER DU CHEF CROCODAILLE",t:81,d:"A Amakna aussi, la rentrée scolaire est une épreuve douloureuse pour les jeunes enfants. Surtout pour les jeunes Iops, pour qui la rentrée des classes est synonyme de véritable calvaire, parsemé d\'épreuves insurmontables. Réconfortez-les avec ce magnifique sac d\'écolier en peau et en écailles de Chef Crocodaille, et rembourré d\'une multitude de poils.",ep:14,g:58,l:70,wd:true,fm:true,w:20,s:20,p:12600};
I.u[6831] = {n:"Ceinture du Chef Crocodaille",nn:"CEINTURE DU CHEF CROCODAILLE",t:10,d:"Trop rigide pour servir de fouet, cette ceinture recouverte de minuscules pierres précisieuses peut néanmoins être utilisée de manière plus conventionnelle, pour éviter que vos guêtres ne tombent sur vos genoux par exemple.",ep:4,g:60,l:70,wd:true,fm:true,w:20,s:20,p:12700};
I.u[6832] = {n:"Livreur de Bière",nn:"LIVREUR DE BIERE",t:32,d:"Le livreur de bière est aussi important qu\'un convoyeur de banque. A ceci près qu\'il est bien plus hargneux.",ep:1,g:15,l:1,wd:false,fm:true,w:0,m:true,p:1};
I.u[6833] = {n:"Bourse de Kamas douteuse",nn:"BOURSE DE KAMAS DOUTEUSE",t:24,d:"Ces kamas semblent venir d\'un autre temps, il faudrait les fondre rapidement avant qu\'il ne décident de repartir d\'où ils sont venus.",ep:1,g:243,l:1,wd:false,fm:true,w:0,p:1};
I.u[6834] = {n:"Coiffe du Chef Crocodaille",nn:"COIFFE DU CHEF CROCODAILLE",t:16,d:"Si vous avez toujours rêvé de ressembler à un Crocodaille sans pour autant vous transformer en Crocodaille, cette coiffe est faîte pour vous. N\'espérez pas pour autant utiliser cette coiffe pour faire croire à vos amis que vous avez un jour réussi à tuer vous-même un Crocodaille. Personne n\'y croira, à part vous peut-être.",ep:4,g:66,l:70,wd:true,fm:true,w:20,s:20,p:12600};
I.u[6835] = {n:"Métal transmuté",nn:"METAL TRANSMUTE",t:24,d:"La bourse de kamas douteuse a été habilement transmutée par Nibé Lulle. Une épée est désormais prête à être créée avec ce métal ...",ep:1,g:241,l:1,wd:true,fm:true,w:0,p:1};
I.u[6836] = {n:"Epée grossière",nn:"EPEE GROSSIERE",t:24,d:"Voici une épée forgée des mains de Dedoiné, mais elle semble encore trop fragile pour vous. Nibé Lulle l\'Alchimiste devrait pouvoir y faire quelque chose",ep:1,g:98,l:1,wd:true,fm:true,w:0,p:1};
I.u[6837] = {n:"Epée bondissante",nn:"EPEE BONDISSANTE",t:24,d:"Une épée faîte pour vous, légère et redoutable. Mais il y manque une petite touche finale, quelque chose d\'Ecaflip ...",ep:1,g:98,l:1,wd:true,fm:true,w:0,p:1};
I.u[6838] = {n:"Epée de feu grossière",nn:"EPEE DE FEU GROSSIERE",t:24,d:"L\'eau de lune semble avoir fait son effet. Cette épée est d\'une solidité à toute épreuve, mais il y manque quelque chose d\'Ecaflip ...",ep:1,g:98,l:1,wd:true,fm:true,w:0,p:1};
I.u[6839] = {n:"Epée de Yingnitié",nn:"EPEE DE YINGNITIE",t:6,d:"Cette arme estampillée par Meow Meow pourrait faire de vous un Ecaflip de légende, éventuellement ...",ep:1,g:18,l:1,wd:true,fm:true,w:10,an:14,e:[5,4,1,1,50,50,false,false],c:"PG=6&PS=0",p:1};
I.u[6840] = {n:"Epée de Yanguru",nn:"EPEE DE YANGURU",t:6,d:"Cet arme estampillée par Meow Meow pourrait faire de vous une Ecaflip de légende, éventuellement ...",ep:1,g:18,l:1,wd:true,fm:true,w:10,an:14,e:[5,4,1,1,50,50,false,false],c:"PG=6&PS=1",p:1};
I.u[6841] = {n:"Etoffe d\'Ours",nn:"ETOFFE D\'OURS",t:55,d:"Cette étrange étoffe sent l\'Ours. De quoi faire craquer une partie non négligeable de la gente féminine.",ep:1,g:121,l:22,wd:true,fm:true,w:1,p:500};
I.u[6842] = {n:"Dent d\'Ours",nn:"DENT D\'OURS",t:47,d:"Cette étrange dent a appartenu à un vieil Ours. Ou plutôt, à une invocation d\'Ours, ce qui il faut bien le reconnaître, est totalement différent, puisque qu\'en réalité, vous avez entre vos mains, une dent invoquée. Très intéressant.",ep:1,g:432,l:22,wd:true,fm:true,w:1,p:500};
I.u[6843] = {n:"Os d\'Ours",nn:"OS D\'OURS",t:47,d:"Cet os pointu s\'est cassé pendant un terrible combat contre un des Ours invoqués de l\'Homme Ours. Il était trop tard pour mettre un plâtre sur la patte de ce pauvre Ours agonisant et gémissant de douleur à cause de vos attaques répétées. Vous avez préféré l\'achever lentement en l\'étouffant avec de la bave de Bouftou, le dépecer et ensuite lui arracher cet os alors qu\'il était encore conscient. Pas de pitié pour les invocations.",ep:1,g:48,l:22,wd:true,fm:true,w:1,p:500};
I.u[6844] = {n:"Poils d\'Ours",nn:"POILS D\'OURS",t:54,d:"Parfois, l\'Homme Ours se décide à épiler une de ses invocations d\'Ours pour s\'amuser ou pour passer le temps. Il récupère ensuite les poils pour en faire des sous-vêtements de grande qualité, très prisés des chasseurs solitaires attirés par les sensations et les odeurs fortes.",ep:1,g:195,l:22,wd:true,fm:true,w:1,p:500};
I.u[6845] = {n:"Peau d\'Ours",nn:"PEAU D\'OURS",t:59,d:"Cette peau rugueuse et dépourvue de poils est le fruit d\'un long travail d\'épilation intensive à l\'huile bouillante et au fer rouge. Ne vous étonnez donc pas si elle sent légèrement le roussi.",ep:1,g:35,l:22,wd:true,fm:true,w:1,p:500};
I.u[6846] = {n:"Fiole cristal de pâle Anthir",nn:"FIOLE CRISTAL DE PALE ANTHIR",t:24,d:"Fiole banale au premier regard. Elle possède cependant un pouvoir que seul les Xelors savent exploiter.",ep:1,g:99,l:1,wd:false,fm:true,w:0,p:1};
I.u[6847] = {n:"Sac de grains d\'Epyss",nn:"SAC DE GRAINS D\'EPYSS",t:24,d:"Grain assez rare, qui ne sert pas à grand chose. Qui à dit comme les Xélors ?",ep:1,g:243,l:1,wd:false,fm:true,w:0,p:1};
I.u[6848] = {n:"Bois fiorituré",nn:"BOIS FIORITURE",t:24,d:"Bois de moyenne facture.",ep:1,g:100,l:1,wd:false,fm:true,w:0,p:1};
I.u[6849] = {n:"Boîte de Suborhube",nn:"BOITE DE SUBORHUBE",t:24,d:"Les seuls à connaître le mode d\'emploi sont les Enutrofs. Peut-être, simplement, parce qu\'on ne préfère pas savoir trop de choses sur leur secrets... Et quels secrets !!!",ep:1,g:56,l:1,wd:false,fm:true,w:0,p:1};
I.u[6850] = {n:"Pizaille",nn:"PIZAILLE",t:24,d:"Une des spécialités culinaires d\'Astrub, la pizaille à l\'avantage de pouvoir être mangé n\'importe où et n\'importe quand, à l\'exception peut-être du quartier des bouchers car l\'odeur y est insupportable ...",ep:1,g:107,l:1,wd:false,fm:true,w:0,p:1};
I.u[6852] = {n:"Peluche de la foire",nn:"PELUCHE DE LA FOIRE",t:24,d:"La peluche que Pleur Nycheuz a gagnée à la foire du trool ...",ep:1,g:151,l:1,wd:true,fm:true,w:0,p:1};
I.u[6853] = {n:"Bois d\'Arash",nn:"BOIS D\'ARASH",t:24,d:"Ce bois a la particularité de pousser tout droit, comme un bâton. Pratique pour le stockage...",ep:1,g:100,l:1,wd:true,fm:true,w:0,p:1};
I.u[6854] = {n:"Peau de larve bleue",nn:"PEAU DE LARVE BLEUE",t:24,d:"Morceau de chair gluant arraché à une pauvre larve sans défense.",ep:1,g:101,l:1,wd:false,fm:true,w:0,p:1};
I.u[6855] = {n:"Petit arc d\'Enileda",nn:"PETIT ARC D\'ENILEDA",t:24,d:"Fruit du travail de la célèbre tailleuse d\'arc Enileda, ce petit arc n\'est pas un modèle en soit.",ep:1,g:244,l:1,wd:true,fm:true,w:0,p:1};
I.u[6856] = {n:"Ciseaux à rose démoniaque",nn:"CISEAUX A ROSE DEMONIAQUE",t:20,d:"La forme de ces ciseaux est spécialement étudiée pour couper la tige de n\'importe quelle rose démoniaque sans abîmer les pétales. Et ceci alors même qu\'elle vous mord le mollet.",ep:1,g:22,l:1,wd:true,fm:true,w:2,an:13,e:[5,3,1,1,20,20,false,false],p:1};
I.u[6857] = {n:"Bière d\'Astrub",nn:"BIERE D\'ASTRUB",t:37,d:"Une bière classique favorisant le développement de votre muscle abdominal, plus connu sous le nom de l\'Abdo kro.",ep:1,g:4,l:1,wd:false,fm:true,w:1,u:true,ut:true,p:4};
I.u[6858] = {n:"Fiche remplie mais non valide",nn:"FICHE REMPLIE MAIS NON VALIDE",t:80,d:"Cette petite fiche possède, hélas, le nom d\'un candidat non valide pour être livreur du boulanger. La petite tâche rouge en bas indiquant qu\'il ne possédait soit pas assez de force, soit pas assez d\'agilité pour ce métier.",ep:1,g:6,l:1,wd:false,fm:true,w:0,c:"PW<1500|CA<50",p:0};
I.u[6859] = {n:"Paire de ciseaux",nn:"PAIRE DE CISEAUX",t:24,d:"Une paire de ciseaux qui ne peigne pas de mine... elle aurait même plutôt tendance à couper qu\'à peigner, racontent certains coiffeurs...",ep:1,g:150,l:1,wd:true,fm:true,w:0,p:1};
I.u[6862] = {n:"Manuscrit de l\'Ours",nn:"MANUSCRIT DE L\'OURS",t:24,d:"Cet étrange parchemin explique comment l\'Homme Ours a appris a invoquer des Ours. Incroyable.",ep:1,g:95,l:1,wd:true,fm:true,w:0,p:1};
I.u[6863] = {n:"Casque des Mercenaires d\'Astrub",nn:"CASQUE DES MERCENAIRES D\'ASTRUB",t:16,d:"Casque des Mercenaires d\'Astrub.",ep:1,g:72,l:1,wd:false,fm:true,w:10,p:1};
I.u[6864] = {n:"Fleur de rose Obscure",nn:"FLEUR DE ROSE OBSCURE",t:24,d:"Une fleur de rose si sombre que son ombre parait étincelante en comparaison.",ep:2,g:106,l:23,wd:true,fm:true,w:0,p:1};
I.u[6865] = {n:"Note",nn:"NOTE",t:24,d:"" + "\"" + "Rendez-vous à l\'orée de la forêt pour la livraison" + "\"" + "",ep:1,g:50,l:1,wd:false,fm:true,w:0,p:1};
I.u[6866] = {n:"Morceau de cape",nn:"MORCEAU DE CAPE",t:24,d:"Un morceau de cape bleue de ceux qui vous ont agressé. Mine de rien vous les avez dérouillés.",ep:1,g:249,l:1,wd:false,fm:true,w:0,p:1};
I.u[6867] = {n:"Note de récolte",nn:"NOTE DE RECOLTE",t:24,d:"Pour ne pas oublier l\'heure à laquelle vous devez récolter le Vhan, vous l\'avez noté sur ce papier.",ep:1,g:50,l:1,wd:false,fm:true,w:0,p:1};
I.u[6868] = {n:"Planche en Châtaignier",nn:"PLANCHE EN CHATAIGNIER",t:95,d:"Cette planche en Châtaignier est parfois utilisée pour construire des habitations, mais le plus souvent on l\'utilise pour faire des pièges à mouche...",ep:1,g:1,l:10,wd:true,fm:true,w:10,p:20};
I.u[6869] = {n:"Pleur Nycheuz",nn:"PLEUR NYCHEUZ",t:32,d:"Pleur Nycheuz ne sait plus où se trouve sa maison, il faut donc la raccompagner ...",ep:1,g:14,l:1,wd:false,fm:true,w:0,m:true,p:1};
I.u[6870] = {n:"Padgref Demoël",nn:"PADGREF DEMOEL",t:32,d:"Padgref Demoël est maintenant sous votre contrôle. A vous de l\'amener en sécurité à la prison",ep:13,g:8,l:1,wd:false,fm:true,w:0,m:true,p:1};
