I.u[6642] = {n:"Puissante Epée d\'Urandal",nn:"PUISSANTE EPEE D\'URANDAL",t:6,d:"Cette épée est un cadeau honorifique que l\'on donne uniquement aux mercenaires accomplis. Mais elle coupe aussi très bien.",ep:1,g:25,l:50,wd:false,fm:true,w:20,an:14,tw:true,e:[10,4,1,1,40,40,false,false],c:"Ps=3",p:1};
I.u[6643] = {n:"Booden glace",nn:"BOODEN GLACE",t:42,d:"Cette délicieuse glace est fabriquée artisanalement par la petite ferme Booden qui essaye d\'imposer la Boo dans tous desserts glacés.",ep:13,g:143,l:1,wd:false,fm:true,w:1,u:true,ut:true,p:150};
I.u[6644] = {n:"Cône Wayal",nn:"CONE WAYAL",t:42,d:"Cette glace s\'inspire du très célèbre chapeau du Wa pour sa partie supérieure, ce qui donne un effet assez réussi",ep:13,g:144,l:1,wd:false,fm:true,w:1,u:true,ut:true,p:120};
I.u[6646] = {n:"Glace Bontarienne",nn:"GLACE BONTARIENNE",t:42,d:"Cette glace, subtil mélange de deux parfums fruités, montre " + "\"" + "tout le savoir faire des anges" + "\"" + "...enfin si l\'on en croit la boite",ep:13,g:145,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:90};
I.u[6647] = {n:"Magma Brâkmarien",nn:"MAGMA BRAKMARIEN",t:42,d:"Cette glace, associant le croquant à la douceur, montre " + "\"" + "encore une fois la supériorité de Brâkmar" + "\"" + "...enfin si l\'on en croit la boîte.",ep:13,g:146,l:1,wd:false,fm:true,w:1,u:true,ut:true,p:90};
I.u[6648] = {n:"Ballon rouge",nn:"BALLON ROUGE",t:15,d:"Ce ballon est un souvenir de la foire du trooll",ep:13,g:690,l:1,wd:true,fm:true,w:1,p:15};
