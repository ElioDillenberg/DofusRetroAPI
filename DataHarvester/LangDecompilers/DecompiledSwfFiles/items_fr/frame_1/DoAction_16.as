I.u[6906] = {n:"Mercenaire Neophyte",nn:"MERCENAIRE NEOPHYTE",t:14,d:"Ce parchemin est le formulaire d\'entrée parmi les Mercenaires. Celui à qui il est proposé sera inscrit dans les registres du clan, en tant que néophyte.",ep:1,g:5,l:1,wd:true,fm:true,w:0,c:"Ps=3&Pa>59",ut:true,p:1};
I.u[6907] = {n:"Neutraliseur",nn:"NEUTRALISEUR",t:14,d:"Permet de donner l\'alignement neutre au personnage ciblé.",ep:1,g:5,l:1,wd:true,fm:true,w:0,c:"Ps=3&Pa>79",ut:true,p:1};
I.u[6908] = {n:"Sanglature",nn:"SANGLATURE",t:10,d:"Poilue mais élégante, cette ceinture devrait parfois vous faire penser à votre dulcinée.",ep:1,g:75,l:18,wd:true,fm:true,w:2,s:21,p:1050};
I.u[6909] = {n:"Pieds du Sanglier",nn:"PIEDS DU SANGLIER",t:11,d:"Ces bottes sont parfaites pour marcher dans la boue et rayer les parquets. Elles sont généralement interdites sur toutes les pistes de danse.",ep:1,g:51,l:22,wd:true,fm:true,w:2,s:21,p:1525};
I.u[6910] = {n:"Anneau du Sanglier",nn:"ANNEAU DU SANGLIER",t:9,d:"Cet anneau était placé sur les pattes des sangliers en rut, pour que les éleveurs puissent facilement les reconnaître. Aujourd\'hui, c\'est vous qui portez cet anneau. Toutes les interprétations sont possibles.",ep:1,g:73,l:19,wd:true,fm:true,w:2,s:21,p:1200};
I.u[6911] = {n:"Araknoton",nn:"ARAKNOTON",t:9,d:"Ce ridicule petit anneau vous permettra de combattre plus facilement votre phobie des Araknes.",ep:1,g:76,l:5,wd:true,fm:true,w:1,s:22,p:275};
I.u[6912] = {n:"Araknoture",nn:"ARAKNOTURE",t:10,d:"Confectionnée à partir de vieux restes d\'Araknes en décomposition, cette ceinture a tout pour plaire. Elle reprendra parfois vie et prendra un malin plaisir à vous mordre les hanches. Sales bêtes !",ep:1,g:73,l:6,wd:true,fm:true,w:1,s:22,p:305};
I.u[6913] = {n:"L\'Araknacoiffe",nn:"L\'ARAKNACOIFFE",t:16,d:"Affublé de cette coiffe, vous ressemblerez à une Arakne sauvage. Dans le meilleur des cas, vous effraierez les Moskitos un peu naïfs.",ep:1,g:83,l:6,wd:true,fm:true,w:2,s:22,p:305};
I.u[6914] = {n:"La Trancheuse d\'Arakne",nn:"LA TRANCHEUSE D\'ARAKNE",t:19,d:"Fait très étrange, l\'Arakne, n\'a que quatre pattes velues, ce qui signifie qu\'avec seulement quatre coups de haches, vous pourrez la transformer en Arakne Tronc.",ep:1,g:31,l:8,wd:true,fm:true,w:5,an:17,e:[5,4,1,1,50,50,false,false],s:22,p:600};
I.u[6915] = {n:"Mos Kitano",nn:"MOS KITANO",t:9,d:"Cet anneau est si léger qu\'il vous permettra de réaliser n\'importe quelle acrobatie avec vos doigts. Très pratique pour humilier un adversaire dix fois plus petit que vous, en lui écrasant la tête avec le pouce.",ep:1,g:71,l:21,wd:true,fm:true,w:2,s:23,p:1400};
I.u[6916] = {n:"Sac du Petit Moskito",nn:"SAC DU PETIT MOSKITO",t:81,d:"Ce sac en peau et en ailes de Moskitos faisait jadis fureur à la sortie des écoles. Aujourd\'hui, il est totalement démodé, et a été remplacé dans le coeur des enfants, par le Petit sac d\'écolier du Chef Crocodaille. Du grand n\'importe quoi, quand on voit que ce dernier est largement plus cher, et surtout beaucoup plus laid. Une sombre histoire de mode.",ep:1,g:59,l:16,wd:true,fm:true,w:2,s:23,p:900};
I.u[6917] = {n:"Moskitogalurette",nn:"MOSKITOGALURETTE",t:16,d:"Cette coiffe doit son nom surprenant à l\'excentricité du premier Enutrof qui l\'a découverte en dépouillant un vieux Moskito malade. Très utile pour les aventuriers qui débutent, elle permet avant tout, de rendre folles de vous les Araknes affamées.",ep:1,g:80,l:15,wd:true,fm:true,w:2,s:23,p:900};
I.u[6918] = {n:"Amulette du Moskito",nn:"AMULETTE DU MOSKITO",t:1,d:"Moins efficace et moins prisée que la célèbre Araknamu, cette petite amulette a pourtant beaucoup de charme lorsqu\'elle est portée toute seule, sur un corps très poilu.",ep:1,g:75,l:15,wd:true,fm:true,w:2,s:23,p:800};
I.u[6919] = {n:"Anneau du Champ Champ",nn:"ANNEAU DU CHAMP CHAMP",t:9,d:"Le Champ Champ n\'a jamais été connu pour être une créature très sympathique. Il vous empoisonne la vie dès qu\'il peut, il mord assez fort, et a très mauvais caractère. C\'est étonnant comme ces vilaines petites créatures peuvent parfois vous faire penser à un conjoint.",ep:1,g:74,l:3,wd:true,fm:true,w:2,s:24,p:225};
I.u[6920] = {n:"Alliance Du Champ Champ",nn:"ALLIANCE DU CHAMP CHAMP",t:9,d:"Lorsque le Champ Champ se marie, il offre cette alliance à sa dulcinée. Cette dernière étant dépourvue de bras, l\'alliance est déposée soigneusement sur une de ses épines. En vous équipant de cette alliance, vous briserez les liens du mariage qui unissaient ces deux créatures, et dans le meilleur des cas, le Champ Champ vous prendra pour sa dulcinée.",ep:1,g:75,l:3,wd:true,fm:true,w:2,s:24,p:225};
I.u[6921] = {n:"Champcoiffe",nn:"CHAMPCOIFFE",t:16,d:"Autrefois utilisée en infusion, et parfois même fumée, cette coiffe de Champ Champ a tout pour plaire.",ep:1,g:84,l:9,wd:true,fm:true,w:3,s:24,p:425};
I.u[6922] = {n:"Cape du Champ Champ",nn:"CAPE DU CHAMP CHAMP",t:17,d:"La seule chose qui soit plus impressionnante qu\'un Champ Champ enragé, c\'est un Champ Champ enragé avec une cape. En effet, cette cape incarne une certaine forme de sur-puissance vestimentaire, bien qu\'elle soit avouons-le, assez laide.",ep:1,g:76,l:9,wd:true,fm:true,w:2,s:24,p:450};
I.u[6923] = {n:"Amulette du Bandit",nn:"AMULETTE DU BANDIT",t:1,d:"Cette amulette a jadis appartenu au grand bandit " + "\"" + "Thisme" + "\"" + ", réputé pour sa cruauté sans limite et sa mort idiote. Sa vie fût digne d\'un conte d\'Eniripsa, il mourût noyé et écrasé dans un pressoir à huile alors qu\'il venait tout juste de purger une peine de prison de plus d\'une centaine d\'années, pour avoir volé une noix de cajou.",ep:1,g:76,l:16,wd:true,fm:true,w:2,s:25,p:900};
I.u[6924] = {n:"Dagues du Bandit",nn:"DAGUES DU BANDIT",t:5,d:"Ces dagues aiguisées sont populaires auprès des bandits et impopulaires auprès des jeunes aventuriers pour les mêmes raisons, elles disposent d\'un enchantement leur permettant de voler les kamas de ses adversaires. Une forme d\'impôt assez originale.",ep:1,g:9,l:12,wd:true,fm:true,w:1,an:13,e:[5,3,1,1,30,50,false,false],s:25,p:600};
I.u[6925] = {n:"Ceinture du Bandit",nn:"CEINTURE DU BANDIT",t:10,d:"Cette ceinture saillante vous ira à ravir si vous rejoignez un jour la cause des bandits de grand chemin. En attendant, elle ne vous rendra que plus ridicule.",ep:1,g:72,l:15,wd:true,fm:true,w:2,s:25,p:850};
I.u[6926] = {n:"Coiffe du Prespic",nn:"COIFFE DU PRESPIC",t:16,d:"Avec cette coiffe de Prespic sur la tête, vous ressemblez presque à Dévie Cloquette, célèbre chasseuse des temps modernes, prête à tout pour cacher sa calvitie naissante. Pas de chance pour vous si vous vouliez faire de même, vous devrez vous contenter d\'une coiffe de couleur rousse, qui de toute évidence, dégagera une horrible odeur dès qu\'il se mettra à pleuvoir.",ep:1,g:81,l:36,wd:true,fm:true,w:2,s:31,p:3600};
I.u[6927] = {n:"Cape du Prespic",nn:"CAPE DU PRESPIC",t:17,d:"Cette petite cape rousse peut également faire office de descente de lit, ou de paillasson, de quoi remettre au goût du jour l\'intérieur de votre habitation, de manière économique.",ep:1,g:75,l:37,wd:true,fm:true,w:3,s:31,p:3800};
I.u[6928] = {n:"Anneau du Prespic",nn:"ANNEAU DU PRESPIC",t:9,d:"A la fois sobre et tout de même original, cet anneau saura séduire les aventuriers avides d\'apparat de luxes mais complètement fauchés, au point d\'aller dépouiller de ridicules petites créatures sauvages pour s\'orner de bijoux. La mesquinerie ne connaît aucune limite.",ep:1,g:72,l:38,wd:true,fm:true,w:1,s:31,p:4000};
I.u[6929] = {n:"Ceinture du Prespic",nn:"CEINTURE DU PRESPIC",t:10,d:"Idéale pour garder ses hanches au chaud, cette ceinture a tout de même l\'inconvénient d\'irriter et de démanger les peaux douces. A réserver aux durs à cuir.",ep:1,g:71,l:33,wd:true,fm:true,w:2,s:31,p:3000};
I.u[6930] = {n:"Casque du Craqueleur",nn:"CASQUE DU CRAQUELEUR",t:16,d:"Ce casque directement taillé dans la pierre vaut son pesant d\'or. Il saura vous protéger des attaques d\'un Craqueboule tout en vous permettant d\'enchaîner des coups de boules dignes du Désiroboule. De quoi faire perdre la boule à tous vos adversaires.",ep:5,g:85,l:44,wd:true,fm:true,w:200,s:26,p:5300};
I.u[6931] = {n:"Craquelocape",nn:"CRAQUELOCAPE",t:17,d:"Cette cape en pierre est un peu trop rigide, mais elle n\'en reste pas moins une excellente protection contre les jets de pierres, ou plus dangereux encore, contre les crachats de Craqueleurs.",ep:5,g:77,l:46,wd:true,fm:true,w:1,s:26,p:5800};
I.u[6933] = {n:"Bottes du Craqueleur",nn:"BOTTES DU CRAQUELEUR",t:11,d:"Incroyablement lourdes, mais néanmoins assez efficaces, ces bottes en pierre vous permettront enfin d\'avoir une excuse, lorsque vous traînez des pieds pour défendre votre percepteur dans un combat perdu d\'avance.",ep:5,g:52,l:47,wd:true,fm:true,w:10,s:26,p:4700};
I.u[6934] = {n:"Craquamulette",nn:"CRAQUAMULETTE",t:1,d:"Cette amulette est aussi lourde qu\'un Blop boulimique, voilà qui devrait décourager les aventuriers un peu trop fragiles de la porter. En revanche, une fois son poids supporté, il faut reconnaître qu\'elle vous donne fière allure.",ep:5,g:77,l:46,wd:true,fm:true,w:10,s:26,p:4600};
I.u[6935] = {n:"Ceinture du Craqueleur",nn:"CEINTURE DU CRAQUELEUR",t:10,d:"Porter cette ceinture autour des hanches fera de vous un aventurier respecté. La porter sur la tête en guise de bandeau fera de vous la risée du village d\'Astrub.",ep:5,g:74,l:45,wd:true,fm:true,w:10,s:26,p:5400};
I.u[6936] = {n:"Scaracoiffe Verte",nn:"SCARACOIFFE VERTE",t:16,d:"Affublé de cette coiffe originale, vous pourrez enfin faire croire à vos amis que vous êtes un chasseur de Scarafeuilles.",ep:3,g:78,l:44,wd:true,fm:true,w:2,s:27,p:5200};
