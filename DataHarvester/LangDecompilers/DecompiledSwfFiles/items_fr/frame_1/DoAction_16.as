I.u[6884] = {n:"Clef du Donjon du Bworker",nn:"CLEF DU DONJON DU BWORKER",t:84,d:"Cette clef permet de rentrer dans le donjon du Bworker. Attention, elle sera détruite après utilisation.",ep:4,g:713,l:30,wd:true,fm:true,w:1,p:100};
I.u[6885] = {n:"Jeton du Bworker",nn:"JETON DU BWORKER",t:15,d:"Idéal pour jouer à pile ou face ou pour tuer sur place votre ennemi lors d\'un lancer de pièce, ce jeton en or massif fera de vous un joueur heureux, mais peut-être malchanceux.",ep:13,g:90,l:100,wd:true,fm:true,w:1,p:100};
