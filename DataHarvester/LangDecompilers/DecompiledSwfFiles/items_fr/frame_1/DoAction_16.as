I.u[6948] = {n:"Scarature Verte",nn:"SCARATURE VERTE",t:10,d:"La Scarature servait autrefois à donner une indication sur votre niveau en combat à mains nues. Posséder une Scarature verte signifiait que vous étiez un débutant, une rouge, signifiait que vous léchiez le sang de vos ennemis une fois ceux-ci exterminés, une blanche, signifiait que vous étiez une véritable machine à exterminer les ennemis, et une bleue signifiait que vous n\'aviez pas assez de kamas pour vous acheter une des trois autres Scaratures.",ep:3,g:67,l:46,wd:true,fm:true,w:2,s:27,p:5600};
I.u[6949] = {n:"Scarature Blanche",nn:"SCARATURE BLANCHE",t:10,d:"La Scarature servait autrefois à donner une indication sur votre niveau en combat à mains nues. Posséder une Scarature verte signifiait que vous étiez un débutant, une rouge, signifiait que vous léchiez le sang de vos ennemis une fois ceux-ci exterminés, une blanche, signifiait que vous étiez une véritable machine à exterminer les ennemis, et une bleue signifiait que vous n\'aviez pas assez de kamas pour vous acheter une des trois autres Scaratures.",ep:3,g:69,l:46,wd:true,fm:true,w:2,s:29,p:5600};
I.u[6950] = {n:"Scarature Rouge",nn:"SCARATURE ROUGE",t:10,d:"La Scarature servait autrefois à donner une indication sur votre niveau en combat à mains nues. Posséder une Scarature verte signifiait que vous étiez un débutant, une rouge, signifiait que vous léchiez le sang de vos ennemis une fois ceux-ci exterminés, une blanche, signifiait que vous étiez une véritable machine à exterminer les ennemis, et une bleue signifiait que vous n\'aviez pas assez de kamas pour vous acheter une des trois autres Scaratures.",ep:3,g:68,l:46,wd:true,fm:true,w:2,s:28,p:5600};
I.u[6951] = {n:"Scarature Bleue",nn:"SCARATURE BLEUE",t:10,d:"La Scarature servait autrefois à donner une indication sur votre niveau en combat à mains nues. Posséder une Scarature verte signifiait que vous étiez un débutant, une rouge, signifiait que vous léchiez le sang de vos ennemis une fois ceux-ci exterminés, une blanche, signifiait que vous étiez une véritable machine à exterminer les ennemis, et une bleue signifiait que vous n\'aviez pas assez de kamas pour vous acheter une des trois autres Scaratures.",ep:3,g:70,l:46,wd:true,fm:true,w:2,s:30,p:5600};
I.u[6952] = {n:"Coiffe du Mulou",nn:"COIFFE DU MULOU",t:16,d:"Avec cette tête de Mulou sur le crâne, vous ressemblez à Bernaize Mainarht, illustre ménestrel en avance sur son temps, dont la tête fut retrouvée coincée dans la gueule d\'un Mulou agonisant, n\'ayant pas supporté les chansons que le ménestrel avait tenté de lui infliger.",ep:1,g:131,l:61,wd:true,fm:true,w:4,s:32,p:10000};
I.u[6953] = {n:"Bottines du Mulou",nn:"BOTTINES DU MULOU",t:11,d:"Ces bottines recouvertes de poils de Mulou, sont très pratiques pour nettoyer les parquets poussiéreux, ou tout simplement pour nettoyer la tête de vos adversaires lorsqu\'ils sont au sol en train d\'agoniser.",ep:1,g:76,l:61,wd:true,fm:true,w:2,s:32,p:9700};
I.u[6954] = {n:"Cape du Mulou",nn:"CAPE DU MULOU",t:17,d:"Cette peau de Mulou constitue une excellente protection contre le froid, mais également contre les aventuriers aux sens olfactifs un peu trop développés.",ep:1,g:99,l:64,wd:true,fm:true,w:5,s:32,p:11000};
I.u[6955] = {n:"String du Mulou",nn:"STRING DU MULOU",t:10,d:"Utilisé comme fouet, ce string peut se révéler plus efficace qu\'il n\'y paraît, pour remotiver vos compagnons de route, lorsque l\'amour du risque commence à leur faire défaut.",ep:1,g:92,l:49,wd:true,fm:true,w:2,s:32,p:6500};
I.u[6956] = {n:"Anneau du Mulou",nn:"ANNEAU DU MULOU",t:9,d:"Attention, cet anneau rendra votre doigt particulièrement malodorant. Une odeur qui risque d\'en couvrir une autre. C\'est peut-être pour votre doigt et votre entourage une très bonne chose...",ep:1,g:103,l:51,wd:true,fm:true,w:2,s:32,p:7000};
I.u[6957] = {n:"Lame du Craqueleur",nn:"LAME DU CRAQUELEUR",t:6,d:"Cette épée servait autrefois à découper des blocs de granit pour confectionner les murailles des cités prestigieuses telles que Bonta ou Brâkmar. Aujourd\'hui, cette arme est avant tout recherché pour sa capacité à écraser toutes sortes de créatures vivantes, consentantes ou pas.",ep:5,g:54,l:37,wd:true,fm:true,w:10,an:14,tw:true,e:[5,6,1,1,50,50,false,true],s:26,p:3800};
I.u[6958] = {n:"Géniallissime Coiffe de Torte Hou",nn:"GENIALLISSIME COIFFE DE TORTE HOU",t:16,d:"A peine démodée, cette coiffe vous donnera un style unique et vous permettra de regarder sous les jupes des Crâettes en toute discrétion. Idéale pour les soirées d\'été plutôt arrosées.",ep:5,g:82,l:22,wd:true,fm:true,w:12,p:1525};
I.u[6961] = {n:"Anneau du Bandit",nn:"ANNEAU DU BANDIT",t:9,d:"Cet anneau est aussi populaire auprès des dentistes que des bandits, qui s\'en servent pour casser une à une les dents des imprudents. Une forme de prévention comme une autre contre l\'apparition des caries.",ep:1,g:77,l:17,wd:true,fm:true,w:2,s:25,p:1000};
I.u[6962] = {n:"Cadeau Dofusien",nn:"CADEAU DOFUSIEN",t:89,d:"Pas moyen de vous révéler le contenu de ce cadeau, mais sachez qu\'il vous permettra à la fois de vous éclater et de faire la foire. Lorsque c\'est jour de fête, il faut bien ça.",ep:1,g:42,l:1,wd:false,fm:true,w:1,u:true,p:1};
I.u[6963] = {n:"Boules du Minotoboule",nn:"BOULES DU MINOTOBOULE",t:12,d:"Ces énormes boules sont vraiment délicieuses. Vous n\'en verrez pas le bout tout de suite, mais qu\'importe.",ep:1,g:143,l:1,wd:false,fm:true,w:1,u:true,p:1};
I.u[6964] = {n:"Potion de cité :  Brâkmar",nn:"POTION DE CITE :  BRAKMAR",t:12,d:"Cette potion à base de riz permet à toutes les personnes d\'alignement Brâkmarien, de rejoindre la cité de Brâkmar de manière instantanée.",ep:1,g:145,l:1,wd:true,fm:true,w:1,c:"Ps=2|Ps=0|Ps=3",u:true,p:1};
I.u[6965] = {n:"Potion de cité : Bonta",nn:"POTION DE CITE : BONTA",t:12,d:"Cette potion à base de riz permet à toutes les personnes d\'alignement Bontarien, de rejoindre la cité de Bonta de manière instantanée.",ep:1,g:144,l:1,wd:true,fm:true,w:1,c:"Ps=1|Ps=0|Ps=3",u:true,p:1};
I.u[6966] = {n:"Parchemin de Sort Spécial Poisse",nn:"PARCHEMIN DE SORT SPECIAL POISSE",t:75,d:"(Double clic pour apprendre le sort) \nCe parchemin vous permet d\'apprendre le sort Poisse... Réservé aux Srams uniquement.",ep:1,g:14,l:1,wd:true,fm:true,w:1,c:"PG=4",u:true,p:1};
I.u[6967] = {n:"Parchemin d\'intronisation du Mercenaire Apprenti",nn:"PARCHEMIN D\'INTRONISATION DU MERCENAIRE APPRENTI",t:14,d:"Ce parchemin est la clé qui permet à un Mercenaire d\'être promu apprenti.",ep:1,g:5,l:1,wd:true,fm:true,w:0,c:"Ps=3&Pa>59",ut:true,p:1};
I.u[6968] = {n:"Parchemin d\'intronisation chez les Mercenaires",nn:"PARCHEMIN D\'INTRONISATION CHEZ LES MERCENAIRES",t:14,d:"Ce parchemin est la clé qui permet à un Mercenaire d\'être promu Mercenaire à part entière.",ep:1,g:5,l:1,wd:true,fm:true,w:0,c:"Ps=3&Pa>79",ut:true,p:1};
I.u[6969] = {n:"Parchemin d\'intronisation du Grand Mercenaire",nn:"PARCHEMIN D\'INTRONISATION DU GRAND MERCENAIRE",t:14,d:"Ce parchemin est la clé qui permet à un Mercenaire d\'être promu Grand Mercenaire.",ep:1,g:5,l:1,wd:true,fm:true,w:0,c:"Ps=3&Pa>79",ut:true,p:1};
I.u[6970] = {n:"Parchemin d\'intronisation du Maître Mercenaire",nn:"PARCHEMIN D\'INTRONISATION DU MAITRE MERCENAIRE",t:14,d:"Ce parchemin est la clé qui permet à un Mercenaire d\'être promu Maître du Clan des Mercenaires.",ep:1,g:5,l:1,wd:true,fm:true,w:0,c:"Ps=3&Pa>79",ut:true,p:1};
I.u[6971] = {n:"Clint du disciple mercenaire",nn:"CLINT DU DISCIPLE MERCENAIRE",t:16,d:"Ce chapeau est tout de même une assez bonne réplique du célèbre Clint, il conviendra parfaitement aux disciples mercenaires qui voudront se faire passer pour le maître.",ep:1,g:38,l:1,wd:false,fm:true,w:20,c:"Ps=3",p:1};
I.u[6975] = {n:"Clint du mercenaire néophyte",nn:"CLINT DU MERCENAIRE NEOPHYTE",t:16,d:"Ce chapeau surpuissant est l\'insigne des mercenaires. Il a le pouvoir de vous protéger de la pluie et du soleil, et c\'est déjà pas mal...",ep:1,g:38,l:1,wd:false,fm:true,w:20,p:1};
I.u[6978] = {n:"Péki",nn:"PEKI",t:18,d:"Cette créature ressemble de loin à un Chienchien. Sa tête montre clairement qu\'elle a subi d\'énormes traumatismes qui l\'ont poussée à devenir végétarienne. Elle ne se nourrit donc que de végétaux et plantes qui poussent sur l\'île de Pandala.",ep:3,g:12,l:60,wd:true,fm:true,w:10,c:"PZ=1",p:1};
I.u[6980] = {n:"Dofus Vulbis",nn:"DOFUS VULBIS",t:23,d:"Ce Dofus a été pondu par un dragon qui a décidé de rester anonyme. Les aventuriers les plus avisés se demandent toujours pourquoi.",ep:11,g:4,l:6,wd:false,fm:true,w:1,p:1};
I.u[6981] = {n:"Dagues Lassay",nn:"DAGUES LASSAY",t:5,d:"La lame de ces dagues est en glace, ce qui est très pratique pour rafraîchir l\'haleine de vos ennemis, ouvrez-leur la bouche, plantez-y votre grosse dague glacée, et le tour est joué.",ep:19,g:44,l:150,wd:true,fm:true,w:10,an:13,tw:true,e:[10,3,1,1,30,50,false,false],c:"CA>250&CS>150",p:15};
I.u[6982] = {n:"Dagues Honies",nn:"DAGUES HONIES",t:5,d:"Ces dagues ont baigné dans le sang de nombreuses créatures démoniaques agonisantes, comme des enfants par exemple, ce qui leur donne un cachet franchement original.",ep:18,g:50,l:46,wd:true,fm:true,w:15,an:13,e:[10,3,1,1,30,50,false,false],c:"CA>60",p:5775};
I.u[6983] = {n:"Dagues Réceuses",nn:"DAGUES RECEUSES",t:5,d:"Affûtées une fois par décennie seulement, ces dagues n\'en restent pas moins mortelles. Animées d\'une magie des plus obscures, elles se déplacent parfois toutes seules la nuit, pour aller se planter dans le meilleur des cas, dans le derrière des imprudents... Encore une histoire que l\'on raconte aux jeunes Iops pour leur faire peur.",ep:19,g:47,l:143,wd:true,fm:true,w:15,an:13,tw:true,e:[10,3,1,1,30,50,false,false],c:"CS>250&CA>100",p:15};
I.u[6984] = {n:"Dagues de l\'Apprenti Tueur de Bouftons",nn:"DAGUES DE L\'APPRENTI TUEUR DE BOUFTONS",t:5,d:"Ces dagues ont appartenu à un pauvre Sadida, dont la richesse fulgurante fera date dans l\'histoire d\'Amakna. En effet, ce Sadida avait dédié son existence à la chasse aux Bouftons, ce qui ne manquait pas de faire rire tous les habitants du village. Certains badauds venaient même de loin, pour le regarder chasser ces ridicules créatures et pour se moquer de lui. Sa vengeance fut terrible, car entre temps, il était devenu assez riche pour mettre à prix toutes les sales têtes qui s\'étaient moquées de lui.",ep:18,g:46,l:113,wd:true,fm:true,w:15,an:13,tw:true,e:[5,3,1,1,25,50,false,false],c:"CS>300&CA>100",p:32750};
I.u[6986] = {n:"Bouclier du Mont Kipou brisé",nn:"BOUCLIER DU MONT KIPOU BRISE",t:82,d:"Ce brave bouclier a vu trop de batailles, il n\'est plus bon à rien dans son état. Peut-être que Bouwada, au village Pandala, pourra vous le remplacer.",ep:18,g:1,l:1,wd:false,fm:true,w:20,c:"BI=non équipable",p:5400};
I.u[6987] = {n:"Petite Pierre d\'âme hasardeuse",nn:"PETITE PIERRE D\'AME HASARDEUSE",t:83,d:"Cette rune offre une petite chance de capturer des âmes de petite taille.",ep:5,g:1,l:1,wd:false,fm:true,w:1,p:1250};
I.u[6988] = {n:"Sulik",nn:"SULIK",t:16,d:"Autrefois porté par un Xélor sauvage dont la soeur avait été enlevée par des esclavagistes sans scrupules, ce somptueux chapeau fera de vous un sauvage vraiment original.",ep:19,g:92,l:172,wd:true,fm:true,w:10,p:56100};
I.u[6989] = {n:"Krokop",nn:"KROKOP",t:16,d:"Ce chapeau a été confectionné il y a bien longtemps par des apprentis tailleurs qui voulaient se protéger des crises de colères de Mirko le tailleur. Ce dernier avait la fâcheuse habitude de décimer ses apprentis avec des coups de pieds en pleine tête, lorsqu\'ils ne travaillaient pas assez vite. Une façon comme une autre de les remotiver pour qu\'ils se mettent au travail.",ep:19,g:86,l:120,wd:true,fm:true,w:20,p:37025};
I.u[6990] = {n:"Phalène",nn:"PHALENE",t:16,d:"Cette coiffe est un hommage aux cousins lointains des Moskitos, les Phalènes. Bien plus gros, mais plus gentils que les Moskitos, les Phalènes avaient une fâcheuse tendance à chercher à se reproduire avec tout et n\'importe quoi, mais surtout avec n\'importe quoi, ce qui a conduit à une extinction progressive de leur espèce. Cette terrible histoire n\'avait laissé personne indifférent, et de nos jours, il est courant d\'insulter un aventurier de Phalène, lorsque l\'on veut insinuer qu\'il choisit très mal ses conjoints...",ep:19,g:87,l:113,wd:true,fm:true,w:10,p:32925};
I.u[6991] = {n:"Goyave",nn:"GOYAVE",t:16,d:"Cette étrange coiffe ressemble à un gros fruit déshydraté en décomposition, un peu comme vous après une soirée passée à la taverne.",ep:18,g:88,l:46,wd:true,fm:true,w:10,p:5800};
I.u[6992] = {n:"Kannipiou",nn:"KANNIPIOU",t:16,d:"Version embellie du célèbre masque Kanniboule, le Kannipiou vous permettra de passer pour un véritable chasseur de Pious, auprès de vos amis.",ep:12,g:89,l:55,wd:true,fm:true,w:5,p:8000};
I.u[6993] = {n:"Cape Ricéfini",nn:"CAPE RICEFINI",t:17,d:"Hirvé Vélard, célèbre ménestrel de son état, aimait chanter à en perdre la voix. Il fut brûlé vif près de la banque de Bonta, après avoir fait scandale avec sa chanson " + "\"" + "Fais-la frire, Mourir ou vivre" + "\"" + ".",ep:18,g:79,l:46,wd:true,fm:true,w:10,p:5600};
I.u[6994] = {n:"Cape Hôte",nn:"CAPE HOTE",t:17,d:"Cette création hybride entre une hôte et une cape, est franchement étonnante. Si elle avait été confectionnée en cuir, le Captain Chafer l\'aurait adorée.",ep:1,g:80,l:58,wd:true,fm:true,w:10,p:8875};
I.u[6995] = {n:"Cape Houte",nn:"CAPE HOUTE",t:17,d:"Cette cape très puissante a renversé plus d\'un imprudent ayant tenté de la porter, une caractéristique partagée avec la Momie Nova.",ep:18,g:81,l:105,wd:true,fm:true,w:10,p:28300};
I.u[6996] = {n:"Anneau Stradamus",nn:"ANNEAU STRADAMUS",t:9,d:"Stradamus, était un viel Enutrof, complètement ivre, qui lisait l\'avenir dans les choppes de bière. La légende raconte qu\'à sa demande, il fût enseveli à la verticale dans un des murs de la taverne de Brâkmar, pour " + "\"" + "qu\'aucun poltron ne puisse renverser une bière sur sa tombe" + "\"" + ". Le genre de légende colportée au fil des beuveries organisées par les Maîtres Sérianes.",ep:18,g:78,l:48,wd:true,fm:true,w:5,p:6075};
I.u[6997] = {n:"Anneau Fioutioure",nn:"ANNEAU FIOUTIOURE",t:9,d:"Celle qui a créé cet anneau pensait que la fin du monde était proche, elle passait ses journées à graver sur les tables en bois à l\'aide de son sertisseur, qu\'il n\'y avait plus d\'avenir pour les habitants d\'Amakna. La suite des événements, lui montra qu\'elle avait tort.",ep:18,g:79,l:70,wd:true,fm:true,w:5,p:12730};
I.u[6998] = {n:"Alliance d\'Elya Wood",nn:"ALLIANCE D\'ELYA WOOD",t:9,d:"Cette petite alliance est parfaitement adaptée aux petits doigts des Eniripsas. Elle est en revanche presque inutilisable par les Iops à cause de leurs gros doigts boudinés, et leur incapacité à comprendre où se met une alliance.",ep:18,g:84,l:120,wd:true,fm:true,w:15,s:39,p:36000};
I.u[6999] = {n:"Médaille Lahilama",nn:"MEDAILLE LAHILAMA",t:1,d:"Lahilama était un grand chef spirituel qui officiait au fin fond des montagnes. Il s\'ennuyait tellement, qu\'il passait ses journées à coudre des bonnets jaunes pour ses disciples. La folie n\'a pas de limites.",ep:18,g:83,l:31,wd:true,fm:true,w:3,p:2800};
I.u[7000] = {n:"Bambamulette",nn:"BAMBAMULETTE",t:1,d:"Ce ridicule petit morceau de bambou est doté de pouvoirs magiques bien étranges. Il était autrefois porté par les jeunes Pandawas, une fois la puberté passée. Vous devriez peut-être attendre encore quelques années avant de le porter autour du coup...",ep:18,g:78,l:40,wd:true,fm:true,w:5,p:4400};
I.u[7001] = {n:"Talisman du Faucheur d\'Avoine",nn:"TALISMAN DU FAUCHEUR D\'AVOINE",t:1,d:"" + "\"" + "Je ne suis pas un Tikoko, les Trukikols me collent à la peau, je ne suis pas un Tikoko, un Tikokooooooo..." + "\"" + ". Voilà ce que chantait à tue-tête en revenant de ses vacances sur l\'île de Moon, Danioule le célèbre ménestrel paysan, connu pour battre l\'Avoine en chantant.",ep:18,g:84,l:55,wd:true,fm:true,w:10,p:8000};
I.u[7002] = {n:"La Résinette",nn:"LA RESINETTE",t:1,d:"Cette amulette en résine de Bambou diffusera autour de vous une odeur agréable. Très efficace pour dissimuler vos essais de Vents de Panique.",ep:18,g:85,l:94,wd:true,fm:true,w:5,p:22800};
I.u[7003] = {n:"Talisman d\'Elya Wood",nn:"TALISMAN D\'ELYA WOOD",t:1,d:"Ce Talisman a été confectionné avec de la poussière d\'étoiles et de pierres précieuses, prises dans de l\'ambre d\'Abraknyde Ancestral. Le mélange étant parfois explosif, il est déconseillé d\'en fabriquer une vous-même, laissez cela à un Bijoutier que vous n\'aimez pas.",ep:18,g:86,l:135,wd:true,fm:true,w:10,s:39,p:45600};
I.u[7004] = {n:"Moyenne Pierre d\'âme hasardeuse",nn:"MOYENNE PIERRE D\'AME HASARDEUSE",t:83,d:"Cette rune offre une petite chance de capturer des âmes relativement importantes.",ep:5,g:8,l:1,wd:false,fm:true,w:1,p:2500};
I.u[7005] = {n:"Grande Pierre d\'âme hasardeuse",nn:"GRANDE PIERRE D\'AME HASARDEUSE",t:83,d:"Cette rune offre une petite chance de capturer des âmes particulièrement farouches...",ep:5,g:9,l:1,wd:false,fm:true,w:1,p:3750};
I.u[7006] = {n:"Enorme Pierre d\'âme hasardeuse",nn:"ENORME PIERRE D\'AME HASARDEUSE",t:83,d:"Cette rune offre une petite chance de capturer des âmes extrêmement hargneuses...",ep:16,g:10,l:1,wd:false,fm:true,w:1,p:5000};
I.u[7007] = {n:"Petite Pierre d\'âme",nn:"PETITE PIERRE D\'AME",t:83,d:"Cette rune offre une chance correcte de capturer des âmes de petite taille.",ep:5,g:2,l:1,wd:false,fm:true,w:1,p:2500};
I.u[7008] = {n:"Moyenne Pierre d\'âme",nn:"MOYENNE PIERRE D\'AME",t:83,d:"Cette rune offre une chance correcte de capturer des âmes relativement élevées...",ep:5,g:12,l:1,wd:false,fm:true,w:1,p:5000};
I.u[7009] = {n:"Grande Pierre d\'âme",nn:"GRANDE PIERRE D\'AME",t:83,d:"Cette rune offre une chance correcte de capturer des âmes particulièrement farouches...",ep:16,g:13,l:1,wd:false,fm:true,w:1,p:7500};
I.u[7010] = {n:"Pierre d\'âme pleine",nn:"PIERRE D\'AME PLEINE",t:85,d:"Cette pierre d\'âme contient les âmes d\'un groupe de monstres qui ne demandent qu\'à revenir à la vie pour se venger de vous. Elle n\'est cependant utilisable que dans les arènes prévues à cet effet, situées dans les grandes cités.",ep:1,g:1,l:1,wd:false,fm:true,w:1,u:true,p:10};
I.u[7011] = {n:"Cape du Désir O\'Boul",nn:"CAPE DU DESIR O\'BOUL",t:17,d:"Cette cape presque vivante en poils de barbe roux est unique en son genre. Animée d\'une magie extraordinaire, elle a tendance à jouer de nombreux tours à son porteur, en se cachant toujours au moment de partir à l\'aventure, lorsqu\'on a besoin d\'elle. Elle compense heureusement sa fourberie, par une puissance en combat à toute épreuve, qui fait d\'elle une " + "\"" + "véwitable meuchine de guewe" + "\"" + " d\'après certains Wabbits.",ep:13,g:83,l:67,wd:false,fm:true,w:10,s:38,p:11875};
I.u[7012] = {n:"La Doubitch O\'Boul",nn:"LA DOUBITCH O\'BOUL",t:1,d:"Cette amulette est en fait une confiserie. Elle pourrait être mangée si elle n\'avait pas auparavant été roulée sous les aisselles pendant 36 générations, par une horde de femelles Bwork en chaleur.",ep:7,g:87,l:77,wd:true,fm:true,w:10,s:38,p:15425};
I.u[7013] = {n:"Bois de Bambou",nn:"BOIS DE BAMBOU",t:38,d:"Contrairement aux roseaux, le bambou plie et casse. Heureusement, ce sont souvent les dents des aventuriers imprudents qui cassent sous les coups de bambous. Parfois, même la nature se met au service des dentistes sans scrupules.",ep:18,g:673,l:50,wd:true,fm:true,w:5,p:0};
I.u[7014] = {n:"Bois de Bambou Sacré",nn:"BOIS DE BAMBOU SACRE",t:38,d:"Contrairement aux apparences, ce bois est sacré. Seuls les meilleurs Pandawas, ont normalement accès à la coupe de ce bois, mais il arrive parfois que des touristes bûcherons, ne se gênent pas pour repartir avec un souvenir local, de leur voyage sur l\'île de Pandala. On parle alors de " + "\"" + "sacrés touristes" + "\"" + ".",ep:18,g:671,l:100,wd:true,fm:true,w:5,p:0};
I.u[7015] = {n:"Pousses de Bambou",nn:"POUSSES DE BAMBOU",t:38,d:"Ces pousses de bambou sont impropres à la consommation, car elles n\'ont vraiment aucun goût. Il est donc conseillé de les faucher avec une faux rouillée, ou de les laisser macérer quelques années dans du jus de Gloutovore avant de les servir à vos amis. Encore une vieille pratique empruntée aux restaurateurs peu scrupuleux, prêts à tous les subterfuges pour vous faire avaler n\'importe quoi.",ep:1,g:676,l:50,wd:true,fm:true,w:2,p:5};
I.u[7016] = {n:"Bois de Bambou Sombre",nn:"BOIS DE BAMBOU SOMBRE",t:38,d:"Galthouze le jeune Pandawa avait un jour demandé à son père " + "\"" + "Quelle est la différence entre le Bambou, et le Bambou Sombre papa ?" + "\"" + ". Il se prit une bonne claque bien méritée dans la tête pour avoir posé une question aussi stupide, tout le monde sait que ce qui différencie le Bambou du Bambou Sombre, c\'est la taille de leurs graines.",ep:18,g:672,l:60,wd:true,fm:true,w:5,p:0};
I.u[7017] = {n:"Bois de Bambou Doré",nn:"BOIS DE BAMBOU DORE",t:38,d:"Galthouze le jeune Pandawa avait un jour demandé à son père " + "\"" + "Quelle est la différence entre le Bambou, et le Bambou Doré papa ?" + "\"" + ". Il se prit une bonne fessée bien méritée pour avoir posé une question aussi stupide, tout le monde sait que ce qui différencie le Bambou du Bambou Doré, c\'est la taille de leurs racines.",ep:18,g:669,l:45,wd:true,fm:true,w:5,p:20};
I.u[7018] = {n:"Riz",nn:"RIZ",t:34,d:"Collant, ou même gluant, le riz a toujours du succès auprès des jeunes Pandawas affamés.",ep:18,g:10,l:20,wd:true,fm:true,w:2,p:0};
I.u[7020] = {n:"Fleur de Lotus",nn:"FLEUR DE LOTUS",t:38,d:"On raconte que c\'est la plus vieille fleur connue à ce jour, elle symbolise la fécondité, mais aussi le détachement, parce qu\'elle émerge parfois de l\'eau sans se mouiller. Certains chefs Pandawas se servent de ce symbole pour faire comprendre à leurs fidèles qu\'ils doivent vivre dans le monde sans en être affecté.",ep:1,g:141,l:30,wd:true,fm:true,w:1,p:2};
I.u[7022] = {n:"Bâton de Sangroku",nn:"BATON DE SANGROKU",t:4,d:"Ce bâton appartenait au grand-père de Sangroku, Sangrokrane, réputé pour avoir la grosse tête. La légende raconte que ce bâton magique est capable de s\'allonger pour atteindre n\'importe quel ennemi, ou tout simplement pour compenser certains complexes d\'infériorité.",ep:18,g:47,l:120,wd:true,fm:true,w:10,an:12,e:[10,4,1,1,50,60,false,false],c:"CA>250&CV>300",p:36620};
I.u[7023] = {n:"Pierre d\'Aigue-Marine",nn:"PIERRE D\'AIGUE-MARINE",t:51,d:"On trouve généralement cette pierre dans les environs de l\'île de Pandala. Les Pandawas racontent que cette pierre est dotée de pouvoirs particuliers. Tout le monde en parle, mais personne ne les a encore découverts. Un peu comme les Iops intelligents.",ep:1,g:75,l:60,wd:true,fm:true,w:5,p:600};
I.u[7024] = {n:"Pierre de Topaze",nn:"PIERRE DE TOPAZE",t:51,d:"Cette pierre étrange servait il y a très longtemps, de nourriture aux jeunes Craqueboules de certains éleveurs de Pandala. Ces pierres sont devenues si rares, qu\'il est devenu interdit de s\'en servir pour nourrir les Craqueboules.",ep:5,g:76,l:70,wd:true,fm:true,w:5,p:700};
I.u[7025] = {n:"Pierre d\'Agathe",nn:"PIERRE D\'AGATHE",t:51,d:"Cette pierre tire son nom d\'une rivière de Pandala dans laquelle on la trouvait jadis en abondance. Mais les temps ont bien changé, et l\'exploitation intensive de ses gisements, a fini par la rendre presque introuvable.",ep:5,g:128,l:80,wd:true,fm:true,w:5,p:1};
I.u[7026] = {n:"Aigue-Marine",nn:"AIGUE-MARINE",t:50,d:"Cette pierre précieuse transparente et parfois légérement bleutée, est si éclatante, que portée autour du cou, elle permettra de cacher les rides les plus marquées. Les Pandawas n\'hésitent d\'ailleurs pas à lancer aux touristes : " + "\"" + "Avec cette pierre, rendez votre dulcinée heureuse, et acceptez plus facilement les affres du temps qui s\'abattent sur ceux que vous aimez" + "\"" + ". Une bonne façon de faire d\'une pierre, deux coups.",ep:1,g:101,l:60,wd:true,fm:true,w:5,p:600};
I.u[7027] = {n:"Topaze",nn:"TOPAZE",t:50,d:"Très appréciée des bijoutiers pour des raisons un peu douteuses, cette pierre précieuse n\'en reste pas moins une valeur sûre pour attirer l\'attention des Corbacs au milieu des Plaines de Cania.",ep:5,g:102,l:70,wd:true,fm:true,w:5,p:700};
I.u[7028] = {n:"Agathe",nn:"AGATHE",t:50,d:"Cette pierre tient son nom d\'une très puissante femelle Pandawa, connue sous le nom d\'Agathe Pahtes, célèbre dans les lieux de débauches peu recommandables, comme le bar des Pirates de Sufokia par exemple, où elle passa ses dernières années à danser enchaînée à un gros mât de bateau planté au milieu du bar.",ep:5,g:103,l:80,wd:true,fm:true,w:5,p:800};
I.u[7029] = {n:"Ceinture de Sangroku",nn:"CEINTURE DE SANGROKU",t:10,d:"Lorsque Sangroku avait trop abusé des haricots magiques, il serrait bien fort cette ceinture autour de sa taille, pour contenir au maximum, les vents de panique qui l\'ont rendu célèbre à travers toute la province d\'Amakna.",ep:13,g:80,l:86,wd:false,fm:true,w:10,c:"CS>100",p:18400};
I.u[7030] = {n:"Peau de Kanigrou",nn:"PEAU DE KANIGROU",t:59,d:"Une fois épilée, la peau de Kanigrou est très douce, un peu comme pour les femelles Sadidas.",ep:13,g:47,l:70,wd:true,fm:true,w:5,p:1};
I.u[7031] = {n:"Pendule du Gudule",nn:"PENDULE DU GUDULE",t:1,d:"Le pauvre Gudule a perdu la tête suite à un terrible combat avec Many. Heureusement, son amulette elle n\'a pas été abimée pendant l\'affrontement. Elle a gardé ses pouvoirs hypnotisants, mais aussi laxatifs. A utiliser avec modération.",ep:18,g:90,l:46,wd:false,fm:true,w:2,p:5600};
I.u[7032] = {n:"Silicate",nn:"SILICATE",t:39,d:"Ce minerai provient sûrement des profondes mines sombres de l\'île de Pandala dans lesquelles Gardan Framine est tombé éperdument amoureux d\'un spectre de femelle Pandawa. Comme quoi, l\'amour ne connaît aucune limite.",ep:16,g:111,l:70,wd:true,fm:true,w:5,p:0};
I.u[7033] = {n:"Dolomite",nn:"DOLOMITE",t:39,d:"" + "\"" + "Toujours dur, et incassable" + "\"" + " telles étaient les paroles d\'Irko le Mineur Sombre à propos du Dolomite.",ep:16,g:110,l:80,wd:true,fm:true,w:5,p:0};
I.u[7034] = {n:"Casque de Maître Nabur",nn:"CASQUE DE MAITRE NABUR",t:16,d:"Transmis de générations en générations, ce casque difforme a même été porté par Crail en personne. Beaucoup d\'aventuriers lui prêtent des pouvoirs qu\'il n\'a pas, caractéristique partagée avec beaucoup de divinités et de héros des temps modernes.",ep:16,g:93,l:97,wd:false,fm:true,w:10,p:23000};
I.u[7035] = {n:"Pyrute",nn:"PYRUTE",t:40,d:"Cet alliage était utilisé pour forger des lames qui ne rouillaient pas au contact du sang et de la bave des ennemis.",ep:16,g:708,l:100,wd:true,fm:true,w:100,p:0};
I.u[7036] = {n:"Rutile",nn:"RUTILE",t:40,d:"Cet alliage brillant, est très apprécié des bijoutiers ruinés, qui ne peuvent s\'offrir des pierres précieuses et qui en profitent généralement pour confectionner des alliances à un prix défiant toute concurrence. Très pratique lorsque vous tenez à vous marier par intérêt.",ep:16,g:707,l:100,wd:true,fm:true,w:100,p:0};
I.u[7037] = {n:"Canne de Mamie Bonbon",nn:"CANNE DE MAMIE BONBON",t:4,d:"Cette canne appartenait à Mamie Bonbon en personne, une des trois terribles prêtresses de l\'amour. Affûtée à une extrémité et lustrée à une autre, cette canne était surtout célèbre pour sa polyvalence et sa capacité à pouvoir se fourrer n\'importe où. Personne ne pouvait y rester insensible.",ep:18,g:48,l:98,wd:true,fm:true,w:15,an:12,e:[5,5,1,1,50,50,false,false],c:"CS>100&CV>300",p:24500};
I.u[7038] = {n:"Couronne du Roi Gelax",nn:"COURONNE DU ROI GELAX",t:16,d:"Cette couronne a appartenu à l\'illustre Roi Gelax, réputé pour son romantisme débordant et son pouvoir de séduction qui a marqué à vie plus d\'une Gelée femelle... On raconte même, qu\'il est plus ou moins le père de toutes les Gelées de la région d\'Amakna, preuve s\'il en est, de son endurance et de sa fertilité légendaire.",ep:7,g:94,l:79,wd:true,fm:true,w:15,p:15000};
I.u[7039] = {n:"Ceinture Noire de Frank Lee Bruce",nn:"CEINTURE NOIRE DE FRANK LEE BRUCE",t:10,d:"Célèbre combattant du dimanche, Frank s\'est reconverti dans les arts martiaux après avoir lamentablement raté son diplôme de tailleur de pierres.",ep:13,g:81,l:47,wd:true,fm:true,w:5,p:5900};
I.u[7040] = {n:"Arc de Kuri",nn:"ARC DE KURI",t:2,d:"Cet arc a appartenu à Kuri en personne, célèbre prêtresse de l\'amour, et bras gauche de Mamie Bonbon. Ne tournez jamais le dos à une prêtresse de l\'amour, une seule flèche de cet arc peut suffire à vous déchirer le coeur, voire même les fesses, si vous ne faites pas face à votre adversaire.",ep:18,g:43,l:56,wd:true,fm:true,w:10,an:10,tw:true,e:[15,5,2,6,20,40,false,true],p:8300};
I.u[7041] = {n:"Pandrista",nn:"PANDRISTA",t:30,d:"Aussi appelée Galop du Pandawa, la Pandrista est une spécialité de Pandala bien connue des voyageurs. Les symptômes : envie pressante de monter sur le trône suivie d\'une incapacité à mouler un étron digne de ce nom.",ep:18,g:8,l:1,wd:false,fm:true,w:0,m:true,p:1};
I.u[7042] = {n:"Couvre-chef Pandawa",nn:"COUVRE-CHEF PANDAWA",t:14,d:"Contrairement au Pandawa d\'Amakna, le Pandawa de Pandala ne vous donnera pas la grosse tête, juste une grosse envie d\'aller déposer un engrais naturel sur le végétal le plus proche... Ou à défaut, sur un chacha : c\'est tout aussi efficace (pour vous) et surtout plus rigolo.",ep:18,g:31,l:1,wd:false,fm:true,w:1,c:"PG!12",u:true,p:1000};
I.u[7043] = {n:"Dofus des Glaces",nn:"DOFUS DES GLACES",t:23,d:"La légende dit que ce splendide Dofus est sorti du trou givré d\'Aguabrial le dragon de l\'Eau. La légende ne dit pas qu\'il était enrhumé ce jour-là, mais c\'est pourtant facile à deviner.",ep:1,g:5,l:1,wd:false,fm:true,w:1,p:1};
I.u[7044] = {n:"Faux Dofus",nn:"FAUX DOFUS",t:24,d:"Ce Dofus ne ressemble pas aux autres Dofus et pourtant...",ep:18,g:256,l:1,wd:false,fm:true,w:0,p:1};
I.u[7045] = {n:"Une fiole de Pandaburg",nn:"UNE FIOLE DE PANDABURG",t:79,d:"La Pandaburg est une boisson goûtue, qui a du retour, et qui paraîtrait-il rendrait aveugle, muet et sourd, mais lentement. Ça tombe bien, les Pandawas ne sont pas pressés.",ep:18,g:2,l:1,wd:true,fm:true,w:1,p:1};
I.u[7046] = {n:"Une fiole de Pandapiler",nn:"UNE FIOLE DE PANDAPILER",t:79,d:"Selon l\'érudit Hel Munster, cet authentique nectar ravirait la déesse Pandawa elle-même, la rendant toute euphorique. Tout Pandawa qui se respecte devrait offrir ses Pandapilers à sa déesse vénérée.",ep:1,g:3,l:1,wd:true,fm:true,w:1,p:1};
I.u[7047] = {n:"Une fiole de Pandapils",nn:"UNE FIOLE DE PANDAPILS",t:79,d:"Etrange cette vieille coutume pandawaïenne qui veut que l\'on trinque, une plume de Tofu sur la tête, en chantant : " + "\"" + "Pandapils, Pandapils, lalalaLALEREUH !" + "\"" + ". Et encore, ce n\'est pas la pire...",ep:18,g:4,l:1,wd:true,fm:true,w:1,p:1};
I.u[7051] = {n:"Une fiole de Pandarium Tremens",nn:"UNE FIOLE DE PANDARIUM TREMENS",t:79,d:"La Pandarium Tremens est la preuve que Pandawa nous aime et veut que nous soyons heureux. Cette prière de remerciement adressée à la déesse, est aussi un gage de paix entre tous les Pandawas.",ep:0,g:9,l:1,wd:true,fm:true,w:1,p:5000};
I.u[7055] = {n:"Une fiole de Pandneken",nn:"UNE FIOLE DE PANDNEKEN",t:79,d:"" + "\"" + "Les pattes du Bwak sont courtes, il est vrai, mais les allonger ne lui apporterait rien" + "\"" + ". C\'est à ce genre de réflexions que mène la consommation de Pandneken. C\'est pourquoi les Pandawas préfèrent la réserver aux libations en l\'honneur de Pandawa.",ep:18,g:12,l:1,wd:true,fm:true,w:1,p:1};
I.u[7056] = {n:"Krépite",nn:"KREPITE",t:16,d:"Ce chapeau unique a appartenu à un Xélor, connu pour ses idées explosives et sa maîtrise du temps. Il est mort d\'une façon assez peu commune en lançant un Vent de Panique décoiffant, et en oubliant que son chapeau lançait de petites étincelles autour de lui. Sa mort fut paraît-il assez douloureuse, mais si lumineuse, qu\'on raconte qu\'il aurait éclairé la cité de Brâkmar toute entière pendant quelques secondes.",ep:19,g:95,l:149,wd:true,fm:true,w:10,p:45850};
I.u[7057] = {n:"Assistant du shaman",nn:"ASSISTANT DU SHAMAN",t:32,d:"Amenez l\'assistant auprès de la déesse, il vous en sera reconnaissant.",ep:18,g:0,l:1,wd:false,fm:true,w:0,m:true,p:1};
I.u[7058] = {n:"Octovius",nn:"OCTOVIUS",t:16,d:"Cette coiffe a été confectionnée à partir de restes de Kralamoures morts, séchés. Attention, elle risque de reprendre vie si vous vous en servez comme bonnet de bain.",ep:18,g:96,l:77,wd:true,fm:true,w:10,p:15475};
I.u[7059] = {n:"Graine de Pandouille",nn:"GRAINE DE PANDOUILLE",t:58,d:"Cette énorme graine est gorgée d\'une huile indigeste, mais magique. Sa coquille est réputée incassable, de nombreux artisans s\'en servent pour enchanter des armes, et d\'autres l\'utilisent comme cochonnet magique pour jouer aux boules.",ep:18,g:67,l:100,wd:true,fm:true,w:10,p:10};
I.u[7060] = {n:"Pandapiler spéciale",nn:"PANDAPILER SPECIALE",t:24,d:"Le sage Lenald vous a donné cette Pandapiler pour l\'apporter au Maître Pandawushu de la voie de l\'eau.",ep:18,g:501,l:1,wd:false,fm:true,w:0,p:1};
I.u[7061] = {n:"Transformation Chacha",nn:"TRANSFORMATION CHACHA",t:31,d:"L\'eau de la source jus-sang-ki-eau du Chacha vous a transformé temporairement en Chacha, le moindre choc vous fera redevenir normal.",ep:18,g:21,l:1,wd:false,fm:true,w:0,m:true,p:1};
I.u[7062] = {n:"Poochan",nn:"POOCHAN",t:32,d:"Vous devez amener Poochan où se trouve son futur mari.",ep:18,g:21,l:1,wd:false,fm:true,w:0,m:true,p:1};
I.u[7068] = {n:"Farine de Riz",nn:"FARINE DE RIZ",t:52,d:"Cette farine est parfaite pour faire du ciment ou de la litière d\'Ouginak.",ep:18,g:7,l:50,wd:true,fm:true,w:2,p:0};
I.u[7069] = {n:"Bouclier de Sidimote enfoncé",nn:"BOUCLIER DE SIDIMOTE ENFONCE",t:82,d:"Ce brave bouclier a vu trop de batailles, il n\'est plus bon à rien dans son état. Peut-être que Bouwada, au village Pandala, pourra vous le remplacer.",ep:18,g:8,l:1,wd:false,fm:true,w:10,c:"BI=non équipable",p:15650};
