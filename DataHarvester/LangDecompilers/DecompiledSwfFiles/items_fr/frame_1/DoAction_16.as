I.u[6601] = {n:"Poudre d\'Artifice",nn:"POUDRE D\'ARTIFICE",t:48,d:"Lorsque vos invités commencent à s\'ennuyer, la meilleure solution reste l\'utilisation de la célèbre Fée d\'Artifice, à construire vous-même à partir de cette poudre et de divers ingrédients. Cette Fée saura rendre votre soirée franchement explosive.",ep:16,g:670,l:1,wd:true,fm:true,w:1,p:100};
I.u[6602] = {n:"Sang de rat d\'Hyoactif",nn:"SANG DE RAT D\'HYOACTIF",t:24,d:"Les normes de sécurité Bontarienne prévoient de se tenir à une distance de 100 pouces de toute substance Hyoactive sous peine de multiplier son temps de jeu par trois.",ep:2,g:32,l:1,wd:true,fm:true,w:0,p:1};
I.u[6603] = {n:"Peluche du Bouftou",nn:"PELUCHE DU BOUFTOU",t:61,d:"Cette peluche est la réplique exacte d\'un Bouftou, ses créateurs ont même poussé le vice jusqu\'à mettre la propre odeur du Bouftou sur la laine de cette peluche. Ne vous étonnez donc pas, si vous sentez mauvais en vous réveillant le matin.",ep:13,g:671,l:1,wd:true,fm:true,w:10,p:1};
I.u[6604] = {n:"Kouin-Kouin",nn:"KOUIN-KOUIN",t:18,d:"Au delà du Tofu, du Bouftou où encore du Iop, le Kouin-kouin excelle dans la crétinerie.",ep:1,g:8,l:1,wd:true,fm:false,w:10,c:"PZ=1",p:1};
I.u[6605] = {n:"Peluche du Bouftou Royal",nn:"PELUCHE DU BOUFTOU ROYAL",t:61,d:"Cette peluche est en tout point royale, des coutures, jusqu\'au rembourrage, rien n\'a été laissé au hasard. Un excellent cadeau pour les enfants gâtés ou les gosses de riches.",ep:13,g:672,l:1,wd:true,fm:true,w:10,p:1};
I.u[6606] = {n:"Peluche du Dopeul Sadida",nn:"PELUCHE DU DOPEUL SADIDA",t:61,d:"Cette peluche contient dans son rembourrage un gros réveil, très utile pour les Sadidas qui ont le réveil difficile. En revanche, il leur est déconseillé de la prendre avec eux lorsqu\'il vont sous un charme, le temps ne compte plus dans ces moments-là !",ep:13,g:673,l:1,wd:true,fm:true,w:10,p:1};
I.u[6607] = {n:"Peluche du Dopeul Ecaflip",nn:"PELUCHE DU DOPEUL ECAFLIP",t:61,d:"Cette petite peluche amuse les Ecaflips de tout âge.",ep:13,g:674,l:1,wd:true,fm:true,w:10,p:1};
I.u[6608] = {n:"Peluche du Dopeul Iop",nn:"PELUCHE DU DOPEUL IOP",t:61,d:"Cette peluche ressemble dans un certain sens à un vrai Iop, stupide et bête comme ses pieds, son crâne, ne contient même pas de rembourrage.",ep:13,g:675,l:1,wd:true,fm:true,w:10,p:1};
I.u[6610] = {n:"Peluche du Dopeul Sram",nn:"PELUCHE DU DOPEUL SRAM",t:61,d:"Cette peluche a été volée à un jeune Sram, qui lui-même l\'avait dérobée à un de ses camarades.",ep:13,g:677,l:1,wd:true,fm:true,w:10,p:1};
I.u[6611] = {n:"Peluche du Dopeul Eniripsa",nn:"PELUCHE DU DOPEUL ENIRIPSA",t:61,d:"Cette peluche pourrait presque voler toute seule si elle si ces ailes n\'étaient pas usées. En effet, cette peluche est loin d\'être de première main. Mais qu\'importe, ça reste un joli un cadeau.",ep:13,g:678,l:1,wd:true,fm:true,w:10,p:1};
I.u[6612] = {n:"Peluche du Dopeul Enutrof",nn:"PELUCHE DU DOPEUL ENUTROF",t:61,d:"Attention, cette peluche est vraiment très lourde. En effet, elle est rembourrée avec de la Pierre de Bauxite, et des Pépites d\'Or.",ep:13,g:679,l:1,wd:true,fm:true,w:10,p:1};
I.u[6613] = {n:"Peluche du Dopeul Féca",nn:"PELUCHE DU DOPEUL FECA",t:61,d:"Cette peluche est presque incassable, elle conviendra parfaitement aux jeunes Fécas turbulents et peu soigneux.",ep:13,g:680,l:1,wd:true,fm:true,w:10,p:1};
I.u[6614] = {n:"Peluche du Dopeul Osamodas",nn:"PELUCHE DU DOPEUL OSAMODAS",t:61,d:"On raconte que cette peluche peut invoquer d\'autres petites peluches si elle est correctement entretenue. Une vieille légende pour forcer les jeunes Osamodas à ne pas la manger.",ep:13,g:681,l:1,wd:true,fm:true,w:10,p:1};
I.u[6615] = {n:"Peluche du Dopeul Crâ",nn:"PELUCHE DU DOPEUL CRA",t:61,d:"Cette peluche à l\'allure toute fière, est faîte pour être envoyée de très loin sur vos camarades de jeu, une sorte de boomerang, mais qui ne revient pas à l\'envoyeur.",ep:13,g:682,l:1,wd:true,fm:true,w:10,p:1};
I.u[6616] = {n:"Peluche du Dopeul Xélor",nn:"PELUCHE DU DOPEUL XELOR",t:61,d:"Cette peluche fait un bruit étrange. Elle semble contenir dans le rembourrage de sa tête, une horloge. De quoi bercer le sommeil des jeunes Xélors.",ep:13,g:683,l:1,wd:true,fm:true,w:10,p:1};
I.u[6617] = {n:"Peluche du Sanglier",nn:"PELUCHE DU SANGLIER",t:61,d:"Cette peluche ressemble à s\'y méprendre à une peluche de Sanglier. Impressionnant. Vous noterez que ses poils sont de véritables poils de Sangliers. Un gage de qualité, assurément.",ep:13,g:684,l:1,wd:true,fm:true,w:10,p:1};
I.u[6618] = {n:"Peluche du Tofu",nn:"PELUCHE DU TOFU",t:61,d:"Qui n\'a jamais rêvé de serrer dans ses bras, une réplique de Tofu ? Peut-être vous. Dans ce cas, vous achetez n\'importe quoi ou vos amis ne savent pas vous offrir des cadeaux.",ep:13,g:685,l:1,wd:true,fm:true,w:10,p:1};
I.u[6619] = {n:"Peluche du Champ",nn:"PELUCHE DU CHAMP",t:61,d:"Ce champ-là n\'a pas d\'épines douloureuses ou de poison paralysant. Il est tout de même redoutable pour faire peur aux Iops les plus naïfs.",ep:13,g:686,l:1,wd:false,fm:true,w:10,p:1};
I.u[6620] = {n:"Peluche du Prespic",nn:"PELUCHE DU PRESPIC",t:61,d:"Ce Prespic en peluche n\'enverra pas d\'aiguilles et ne se rendra pas invisible. Mais il remplira son rôle de peluche à merveille et rendra vos amis terriblement jaloux.",ep:13,g:687,l:1,wd:true,fm:true,w:10,p:1};
I.u[6621] = {n:"Peluche du Wabbit",nn:"PELUCHE DU WABBIT",t:61,d:"Le Wabbit en peluche reste un incontournable, peut-être parce qu\'il est indémodable. Vous n\'avez pas cette chance.",ep:13,g:688,l:1,wd:true,fm:true,w:10,p:1};
I.u[6622] = {n:"Hochet du Dark Vlad",nn:"HOCHET DU DARK VLAD",t:15,d:"Ce hochet a appartenu au Dark Vlad en personne, ce qui suffit à le rendre inestimable, même s\'il s\'agit d\'un hochet  classique.",ep:13,g:696,l:1,wd:true,fm:true,w:1,p:2000};
I.u[6624] = {n:"Peluche du Dragon Cochon",nn:"PELUCHE DU DRAGON COCHON",t:61,d:"Cette peluche est vraiment unique en son genre. Mais attention, elle est destinée à un public averti.",ep:13,g:689,l:1,wd:true,fm:true,w:10,p:1};
I.u[6625] = {n:"Bilboquet Du Maître Corbac",nn:"BILBOQUET DU MAITRE CORBAC",t:15,d:"Ce magnifique jouet, a appartenu au Maître Corbac en personne. Trop stupide pour comprendre comment il fonctionnait, il s\'en servait pour taper sur la tête de ses chers petits Corbacs. Chacun ses distractions.",ep:13,g:710,l:1,wd:true,fm:true,w:1,p:2000};
I.u[6626] = {n:"Corde à Sauter du Captain Chafer",nn:"CORDE A SAUTER DU CAPTAIN CHAFER",t:15,d:"Pour entretenir ses articulations, le Captain Chafer utilisait cette fameuse corde à sauter. Depuis qu\'il a disparu d\'Amakna, elle passe de main en main.",ep:13,g:695,l:1,wd:true,fm:true,w:1,p:50};
I.u[6627] = {n:"Petite Fée d\'Artifice Multicolore",nn:"PETITE FEE D\'ARTIFICE MULTICOLORE",t:74,d:"Cette Fée d\'artifice éblouira et étonnera vos amis les plus blasés. Très utile pour fêter le départ de votre guilde.",ep:1,g:30,l:1,wd:false,fm:true,w:1,ut:true,p:350};
