I.u[6652] = {n:"Boule à neige foire du trooll",nn:"BOULE A NEIGE FOIRE DU TROOLL",t:15,d:"cette jolie boule à neige donnera toute sa splendeur sur un rebord de cheminée",ep:13,g:694,l:1,wd:true,fm:true,w:1,p:15};
I.u[6653] = {n:"Ticket de la foire du Trool",nn:"TICKET DE LA FOIRE DU TROOL",t:24,d:"Ce ticket vous permet de rentrer dans la foire.",ep:13,g:94,l:1,wd:true,fm:true,w:0,p:1};
I.u[6654] = {n:"Bottes du Gardien des Egoûts",nn:"BOTTES DU GARDIEN DES EGOUTS",t:11,d:"Comble de l\'ironie, le gardien des égoûts s\'est fait dévorer par la Gardienne des Egoûts. Il a été digéré pendant plus de cent ans avant de finir par mourir. Heureusement, ses bottes étaient plus résistantes que lui et sont encore en excellent état.",ep:16,g:28,l:83,wd:true,fm:true,w:10,p:17800};
I.u[6655] = {n:"Sac Surprise du Chanceux",nn:"SAC SURPRISE DU CHANCEUX",t:89,d:"Les personnes qui ont déposé ça dans votre banque sont vraiment généreuses, et ont voulu vous témoigner leur amitié. (DOUBLE CLIC POUR OUVRIR LE SAC)",ep:1,g:32,l:1,wd:false,fm:true,w:200,u:true,p:500};
I.u[6656] = {n:"Sac Surprise du Chanceux",nn:"SAC SURPRISE DU CHANCEUX",t:89,d:"Les personnes qui ont déposé ça dans votre banque sont vraiment généreuses, et ont voulu vous témoigner leur amitié. (DOUBLE CLIC POUR OUVRIR LE SAC)",ep:1,g:32,l:1,wd:false,fm:true,w:200,u:true,p:500};
I.u[6657] = {n:"Sac à remplacer",nn:"SAC A REMPLACER",t:89,d:"ne pas traduire ce texte.",ep:1,g:0,l:1,wd:false,fm:true,w:1,p:1};
I.u[6658] = {n:"Peluche du Mulou",nn:"PELUCHE DU MULOU",t:61,d:"Cette magnifique peluche en véritables poils de Mulou, vous tiendra chaud la nuit, tout en diffusant dans votre chambre, une agréable odeur de transpiration de Mulou mort.",ep:13,g:699,l:1,wd:true,fm:true,w:10,p:1};
I.u[6659] = {n:"Faux Koinkoin",nn:"FAUX KOINKOIN",t:15,d:"Ne vous y fiez pas, ce koinkoin est un faux, donnez le à Krip Soue à côté du stand de canards pour recevoir une récompense.",ep:1,g:700,l:1,wd:true,fm:true,w:1,p:1};
I.u[6660] = {n:"Chapeau du Marié",nn:"CHAPEAU DU MARIE",t:16,d:"Avec un tel chapeau qui saurait vous dire non ? \nA vous la gloire et les conquêtes !",ep:2,g:53,l:1,wd:true,fm:true,w:5,c:"PS=0",p:1000};
I.u[6661] = {n:"Canne à Koinkoin",nn:"CANNE A KOINKOIN",t:20,d:"Cette canne vous permet de pêcher un koinkoin dans le stand de la foire du Trool.",ep:13,g:8,l:1,wd:true,fm:true,w:10,an:18,e:[0,10,1,1,50,2,false,false],p:1};
I.u[6662] = {n:"Chapeau de la Mariée",nn:"CHAPEAU DE LA MARIEE",t:16,d:"Faites-le fondre comme une gelée menthe au soleil avec cette merveilleuse coiffe en soie de larve du sud, lavable à l\'eau douce.",ep:2,g:54,l:1,wd:true,fm:true,w:5,c:"PS=1",p:1000};
I.u[6663] = {n:"Bouquet de Roses démoniaques",nn:"BOUQUET DE ROSES DEMONIAQUES",t:6,d:"Avec ce superbe bouquet de pétales de roses démoniaques, vous montrerez à votre bien aimé(e) que vous êtes prêt(e) à braver 1000 et 1 dangers pour sa jolie frimousse. Et cela sans le moindre Bobo, pour une somme modique adaptée aux petits budgets.",ep:2,g:47,l:1,wd:false,fm:true,w:3,an:14,e:[1,4,1,1,50,50,false,true],p:70};
I.u[6664] = {n:"Parchemin de Sort Spécial Félintion",nn:"PARCHEMIN DE SORT SPECIAL FELINTION",t:75,d:"(Double clic pour apprendre le sort) \nCe parchemin vous permet d\'apprendre le sort Félintion... Réservé aux Ecaflips uniquement.",ep:2,g:5,l:1,wd:true,fm:true,w:1,c:"PG=6",u:true,p:1};
I.u[6666] = {n:"Sucette Tofu",nn:"SUCETTE TOFU",t:42,d:"Cette délicieuse sucette est aromatisée aux extraits de Tofus. Un régal pour vos papilles, un cataclysme pour votre haleine.",ep:13,g:147,l:1,wd:false,fm:true,w:1,u:true,ut:true,p:30};
I.u[6667] = {n:"Sucette Trool",nn:"SUCETTE TROOL",t:42,d:"Une sucette qui a vraiment du goût et qui est surtout interminable. A consommer entre amis de préférence.",ep:13,g:148,l:1,wd:false,fm:true,w:1,u:true,ut:true,p:300};
I.u[6668] = {n:"Hot Dog de Tofu",nn:"HOT DOG DE TOFU",t:42,d:"Cette saucisse bien grasse vous permettra, de reprendre des forces, pour pouvoir digérer les autres saucisses que vous venez de consommer.",ep:13,g:150,l:1,wd:false,fm:true,w:1,u:true,ut:true,p:300};
I.u[6669] = {n:"Burger de Trool",nn:"BURGER DE TROOL",t:42,d:"Cet impressionnant burger, pourrait remettre sur pieds une armée de Chafers affamés. A consommer avec modération, l\'abus de Trool est dangereux pour la santé.",ep:13,g:149,l:1,wd:false,fm:true,w:1,u:true,ut:true,p:600};
I.u[6670] = {n:"Frites de Tournesol Sauvage",nn:"FRITES DE TOURNESOL SAUVAGE",t:42,d:"Ces frites sont découpées dans des tiges de Tournesol Sauvage, puis elles sont cuîtes dans l\'huile de Tournesol Sauvage bouillante, histoire qu\'elles se sentent un peu comme chez elles, lorsqu\'elles sont en train de frire.",ep:13,g:151,l:1,wd:false,fm:true,w:1,u:true,p:300};
I.u[6671] = {n:"Haricot",nn:"HARICOT",t:58,d:"...",ep:1,g:471,l:1,wd:true,fm:true,w:1,p:1};
I.u[6672] = {n:"Farine de Xavier le Boulanger",nn:"FARINE DE XAVIER LE BOULANGER",t:52,d:"Cette farine est d\'une légèreté et d\'une finesse extraordinaire, elle est tellement douce, qu\'elle est utilisée pour garder les fesses des jeunes Iops bien au sec. Elle est ensuite réutilisée pour donner encore plus de goût aux pains.",ep:1,g:250,l:100,wd:true,fm:true,w:1,p:0};
I.u[6673] = {n:"Plume de Tofu Noir",nn:"PLUME DE TOFU NOIR",t:24,d:"Une plume noire de Tofu Noir...",ep:1,g:96,l:1,wd:false,fm:true,w:0,p:1};
I.u[6676] = {n:"Epée d\'Urandal",nn:"EPEE D\'URANDAL",t:6,d:"Cette épée est un cadeau honorifique que l\'on donne uniquement aux mercenaires accomplis.Mais elle coupe aussi très bien.",ep:1,g:25,l:25,wd:false,fm:true,w:20,an:14,tw:true,e:[10,4,1,1,40,40,false,false],c:"Ps=3",p:1};
I.u[6677] = {n:"Puissante Epée de Sériane-Kerm",nn:"PUISSANTE EPEE DE SERIANE-KERM",t:6,d:"Cette épée est un cadeau honorifique que l\'on donne uniquement aux mercenaires accomplis. Mais elle coupe très bien aussi.",ep:1,g:25,l:70,wd:false,fm:true,w:20,an:14,tw:true,e:[10,4,1,1,40,40,false,false],c:"Ps=3",p:10};
