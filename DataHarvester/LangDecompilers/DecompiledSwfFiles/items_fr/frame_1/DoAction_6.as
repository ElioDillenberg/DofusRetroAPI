I.u[2235] = {n:"Brâkmar Attitude",nn:"BRAKMAR ATTITUDE",t:31,d:"Vous avez La Brâkmar attitude, jusqu\'à ce que vous rentriez en combat.",ep:1,g:16,l:1,wd:true,fm:true,w:0,m:true,u:true,p:1};
I.u[2236] = {n:"Costume Bontesque",nn:"COSTUME BONTESQUE",t:14,d:"Avec cet accoutrement, un bon nombre de voyageurs vous salueront et vous demanderont leurs chemins.",ep:1,g:25,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:1};
I.u[2237] = {n:"Costume Brâkmaresque",nn:"COSTUME BRAKMARESQUE",t:14,d:"Muni de ces affaires, vous serez redouté de tous.",ep:1,g:26,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:1};
I.u[2239] = {n:"Poudre d\'Eniripsa",nn:"POUDRE D\'ENIRIPSA",t:48,d:"Cette poudre est produite en petite quantité à partir d\'ailes d\'Eniripsas. Elle semble avoir des vertus curatives sur les familiers.",ep:3,g:260,l:1,wd:true,fm:true,w:1,p:1000};
I.u[2240] = {n:"Petite Fée d\'Artifice Rouge",nn:"PETITE FEE D\'ARTIFICE ROUGE",t:74,d:"Idéal pour les petites fêtes de mariage, parfait pour la fête du Slip du Captain Chafer.",ep:16,g:28,l:1,wd:true,fm:true,w:1,ut:true,p:350};
I.u[2241] = {n:"Gelée Menthe Royale",nn:"GELEE MENTHE ROYALE",t:110,d:"La Gelée Royale a détroussé plus d\'un aventurier un peu trop entreprenant, la voila enfin anéantie. Attention, il est déconseillé de la  consommer comme une vulgaire Gelée Menthe.",ep:7,g:408,l:100,wd:true,fm:true,w:1,p:100};
I.u[2242] = {n:"Gelée Fraise Royale",nn:"GELEE FRAISE ROYALE",t:110,d:"Du concentré de Gelée Fraise Royale peut transformer n\'importe quel tabi pacifiste en machine de guerre assoiffée de sang. Attention donc à ne pas en servir en apéritif à vos amis.",ep:7,g:409,l:150,wd:true,fm:true,w:1,p:100};
I.u[2245] = {n:"Pince de Scorbute",nn:"PINCE DE SCORBUTE",t:47,d:"Autrefois utilisée pour se gratter le bas du dos, elle est aussi très pratique une fois accrochée à un mur, pour y faire tenir des capes. Ça marche également si vous vous l\'accrochez dans le bas du dos quoique un poil plus douloureux.",ep:14,g:411,l:36,wd:true,fm:true,w:1,p:1};
I.u[2246] = {n:"Couronne Brisée du Tofu Royal",nn:"COURONNE BRISEE DU TOFU ROYAL",t:15,d:"Cette couronne a été brisée par de puissantes attaques magiques, ou par le dernier coup de boule que le Tofu Royal a tenté de vous assener.",ep:3,g:412,l:100,wd:true,fm:true,w:10,p:250};
I.u[2247] = {n:"Plume de Tofu Royal",nn:"PLUME DE TOFU ROYAL",t:53,d:"Il n\'y a qu\'un Iop pour ne pas se rendre compte que cette plume provient de la croupe d\'un Tofu Royal. Pourtant ça se voit, la plume est plus grosse.",ep:3,g:413,l:100,wd:true,fm:true,w:11,p:250};
I.u[2248] = {n:"Poil d\'Ouginak",nn:"POIL D\'OUGINAK",t:54,d:"Ce Poil tient chaud, la preuve, votre main est moite depuis que vous l\'observez. A moins que ce ne soit la peur des représailles du prochain Ouginak qui vous verra avec ce poil dans la poche. De quoi aboyer de rage. \nWa-hoooo.",ep:14,g:414,l:51,wd:true,fm:true,w:1,p:1};
I.u[2249] = {n:"Feuille de Tronknyde",nn:"FEUILLE DE TRONKNYDE",t:36,d:"Cette Feuille doit sûrement fournir au Tronknyde la force nécessaire pour se battre. Encore faut il savoir comment bien l\'utiliser. Ce qui vient tout de suite à l\'idée, c\'est de la hacher et de l\'incorporer à des haricots. Un régal.",ep:2,g:415,l:35,wd:true,fm:true,w:1,p:1};
I.u[2250] = {n:"Bois de Tronknyde",nn:"BOIS DE TRONKNYDE",t:38,d:"Ce morceau de Bois sonne creux. La sève verte qui s\'en échappe vous signale que cette partie n\'était rien d\'autre que le nez du Tronknyde.",ep:9,g:416,l:35,wd:true,fm:true,w:1,p:100};
I.u[2251] = {n:"Méga Pierre du Craqueleur",nn:"MEGA PIERRE DU CRAQUELEUR",t:51,d:"Je ne vous jette pas la Mégapierre Pierre !",ep:5,g:417,l:45,wd:true,fm:true,w:15,p:150};
I.u[2252] = {n:"Dent en Or du Craqueleur",nn:"DENT EN OR DU CRAQUELEUR",t:47,d:"Vous venez d\'édenter sans scrupule le Craqueleur qui croisait votre chemin. Dommage qu\'il n\'en possédait qu\'une seule, un dentier aurait facilement couvert les frais de Zaap durant un bon mois.",ep:5,g:418,l:31,wd:true,fm:true,w:5,p:1};
I.u[2253] = {n:"Fleur de Gloutovore",nn:"FLEUR DE GLOUTOVORE",t:35,d:"Cette Fleur facilite le transit intestinal. Elle est vivement recommandé après un séjour chez votre cuisinier favori.",ep:12,g:420,l:42,wd:true,fm:true,w:1,p:100};
I.u[2254] = {n:"Feuille de Fourbasse",nn:"FEUILLE DE FOURBASSE",t:36,d:"La feuille de Fourbasse a un goût très particulier, entre la noisette et la salace peu fraiche. Vos plats auront en tout cas un petit parfum de fourbasserie si vous souhaitez toutefois la goûter.",ep:2,g:421,l:22,wd:true,fm:true,w:1,p:1};
I.u[2255] = {n:"Rapport de l\'espion",nn:"RAPPORT DE L\'ESPION",t:24,d:"Aucune activité dangereuse en ce moment. L\'organisation des miliciens est perturbée par l\'assassinat de l\'un des leurs. Il semblerait que ce soit un bontarien qui ait fait le coup",ep:2,g:55,l:1,wd:true,fm:true,w:0,p:1};
I.u[2256] = {n:"Carcasse de rat d\'égouts",nn:"CARCASSE DE RAT D\'EGOUTS",t:24,d:"Ce rat n\'est plus que de la viande froide. Un de moins qui traîne dans l\'entrée des égouts.",ep:2,g:121,l:1,wd:true,fm:true,w:0,p:1};
I.u[2257] = {n:"Morceau de mur d\'égout",nn:"MORCEAU DE MUR D\'EGOUT",t:24,d:"Ce morceau de mur d\'égout a été retrouvé dans l\'estomac d\'un bidule",ep:2,g:55,l:1,wd:true,fm:true,w:0,p:1};
I.u[2259] = {n:"Dent de Karne",nn:"DENT DE KARNE",t:47,d:"Vous avez dans la main le jeu le plus ancien de la contrée d\'Amakna. les règles sont simples: lancez une Dent et tentez de la ratrapper avec la même main, tout en récupérant une deuxième Dent de Karne au sol.",ep:4,g:13,l:16,wd:true,fm:true,w:1,p:20};
I.u[2260] = {n:"Carte d\'un brâkmarien",nn:"CARTE D\'UN BRAKMARIEN",t:24,d:"Quelqu\'un a glissé cette carte de visite dans votre équipement. Il semble que ce soit un brâkmarien. Cette carte annonce que les Guerriers sont intéressés par vos talents d\'assassinat et vous propose de rejoindre la folle aventure brâkmarienne.",ep:2,g:72,l:1,wd:true,fm:true,w:0,c:"PO=2177",p:1};
I.u[2261] = {n:"Esprit Renifleur",nn:"ESPRIT RENIFLEUR",t:30,d:"Un esprit renifleur vous hante. Il se nourrit de votre odeur et de votre vitalité.",ep:1,g:7,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2262] = {n:"Description de l\'assassin",nn:"DESCRIPTION DE L\'ASSASSIN",t:24,d:"Vous avez trouvé un Bontarien qui empestait l\'odeur du sang. Toutes les informations que vous avez pu rassembler sur ce personnage sont inscrites sur cette feuille de papier.",ep:2,g:55,l:1,wd:true,fm:true,w:0,c:"PO=2177",p:1};
I.u[2263] = {n:"Bâton renifleur de Sang",nn:"BATON RENIFLEUR DE SANG",t:80,d:"Ce bâton est possédé par un esprit renifleur. On peut libérer cet esprit qui foncera alors tout droit jusqu\'à croiser un objet possédant l\'odeur qu\'il aime. Il hantera alors cette personne quelques temps avant de repartir aux pays des esprits.",ep:16,g:25,l:1,wd:true,fm:true,w:10,ut:true,p:1};
I.u[2264] = {n:"Peau de Gobelin",nn:"PEAU DE GOBELIN",t:59,d:"Un Gobelin est souvent moqueur et sournois, mais si vous le dépecez, il fera moins le malin ce satané Gobelin.",ep:4,g:422,l:16,wd:true,fm:true,w:1,p:1};
I.u[2265] = {n:"Description d\'un Bontarien",nn:"DESCRIPTION D\'UN BONTARIEN",t:24,d:"Vous avez libéré un esprit renifleur mais il ne s\'est pas fixé sur la personne visée. Vous avez noté quelques informations de la personne sur ce bout de papier afin de vous en souvenir",ep:2,g:55,l:1,wd:true,fm:true,w:0,c:"PO!2177",p:1};
I.u[2266] = {n:"Lait de Cochon de Lait",nn:"LAIT DE COCHON DE LAIT",t:15,d:"Ce Lait de Cochon provient d\'un Cochon de Lait. On sait jamais, au cas ou vous pensiez l\'avoir trouvé sur un Tofu, il n\'en est rien.",ep:1,g:423,l:18,wd:true,fm:true,w:1,p:1};
I.u[2267] = {n:"Dragolait",nn:"DRAGOLAIT",t:15,d:"Ce Dragolait est une denrée rare, car personne dans la contrée n\'a encore vu un Dragon Cochon se laisser traire. On imagine aisément qu\'il faut au minimum sept personnes pour tenir la bestiole tandis que la huitième doit traire très très rapidement.",ep:2,g:424,l:70,wd:true,fm:true,w:2,p:1};
I.u[2268] = {n:"Scalp de Bwork Archer",nn:"SCALP DE BWORK ARCHER",t:15,d:"Voici un Scalp qui pourrait lancer une nouvelle mode chez les Enutrofs : le poustiche. Un peu de produit anti-poux et vous serez prêt à concourir pour l\'Enutrof le plus classe du monde.",ep:4,g:425,l:24,wd:true,fm:true,w:1,p:1};
I.u[2269] = {n:"Oeil de Larve bleue",nn:"OEIL DE LARVE BLEUE",t:24,d:"Vous venez de récupèrer cet Oeil intact. Bien préparé, des yeux brouillés sont un régal pour un petit déjeuner. Cet oeil de larve prouvera à Oto que vous avez bien tué une larve bleue.",ep:1,g:70,l:1,wd:true,fm:true,w:0,p:1};
I.u[2270] = {n:"Grappe d\'Oeufs de larve",nn:"GRAPPE D\'OEUFS DE LARVE",t:24,d:"Cette grappe d\'œufs ressemble à une grosse grappe de raisin. Mais il vaut mieux éviter de la manger.",ep:1,g:83,l:1,wd:true,fm:true,w:0,p:1};
I.u[2271] = {n:"Cuir Violet de Bwork",nn:"CUIR VIOLET DE BWORK",t:56,d:"Beaucoup de mystères planent encore autour de cet artisanat Bwork. Certains aventuriers pensent que la teinte violacée proviendrait des Gobelins, mais le cuir a une origine inconnue. Bizarre.",ep:4,g:426,l:31,wd:true,fm:true,w:1,p:1};
I.u[2272] = {n:"Larve Dorée",nn:"LARVE DOREE",t:32,d:"Cette larve vient de naître d\'un oeuf. Son premier regard s\'étant posé sur vous, elle vous considère comme sa mère.",ep:1,g:5,l:1,wd:true,fm:true,w:10,m:true,p:1};
I.u[2273] = {n:"Tissu Pourpre",nn:"TISSU POURPRE",t:55,d:"Léger et doux au toucher, voila ce que l\'on peut dire de cet étrange Tissu Pourpre. Il est écrit sur une étiquette à l\'intérieur " + "\"" + "made in taille one" + "\"" + ".",ep:2,g:427,l:23,wd:true,fm:true,w:1,p:1};
I.u[2274] = {n:"Bougie du Mineur Sombre",nn:"BOUGIE DU MINEUR SOMBRE",t:15,d:"Prenez un Enutrof. Quand il fait noir dans les mines, on ne le voit plus. Le Mineur Sombre, lorsqu\'il est dans le noir, on ne le voit pas non plus, puisque maintenant vous avez sa bougie.",ep:1,g:428,l:24,wd:true,fm:true,w:10,p:10000};
I.u[2275] = {n:"Cuir de Porkass",nn:"CUIR DE PORKASS",t:56,d:"Le Porkass a failli disparaître de la chaîne alimentaire. Il a fallut accoupler des Sangliers et des Cochons de Lait pour à nouveau retrouver les Plaines de Cania remplies de Porkass. Depuis, celui-ci en veut à la terre entière. Quel caractère de porc.",ep:2,g:429,l:40,wd:true,fm:true,w:10,p:1};
I.u[2277] = {n:"Linceul",nn:"LINCEUL",t:55,d:"Pour gagner en crédibilité, les fantômes s\'enrobent d\'un Linceul leur permettant de terroriser les aventuriers perdus.",ep:14,g:430,l:65,wd:true,fm:true,w:1,p:1000};
I.u[2278] = {n:"Tissu Invisible",nn:"TISSU INVISIBLE",t:55,d:"Ce tissu servit à Harry Taupeur, chasseur de taupe. Il s\'enroulait dedans afin de piéger les Taupes qui infestaient son jardin. C\'est le charismatique Captain Chafer qui se l\'est approprié avant de disparaître dans des circonstances bien étranges...",ep:1,g:431,l:23,wd:true,fm:true,w:1,p:1};
I.u[2279] = {n:"Dent Pointue",nn:"DENT POINTUE",t:47,d:"Cette Dent appartenait à un Vampire, cela ne fait aucun doute. On peut le constater en observant de près le bout de la Dent qui s\'avère être on ne peut plus pointue.",ep:3,g:432,l:15,wd:true,fm:true,w:1,p:1};
I.u[2280] = {n:"Paille Vampiresque",nn:"PAILLE VAMPIRESQUE",t:15,d:"Ne sous-estimez jamais un Vampire surtout avec une paille dans la main, il pourrait vous sucer le sang jusqu\' à plus soif.",ep:3,g:433,l:22,wd:true,fm:true,w:1,p:1};
I.u[2281] = {n:"Tissu Sanguin",nn:"TISSU SANGUIN",t:55,d:"Ce tissu a l\'air vivant, les Vampires s\'en servent souvent pour défiler dans de jolies accoutrements lors des bals costumés.",ep:3,g:434,l:15,wd:true,fm:true,w:1,p:1};
I.u[2282] = {n:"Touffe de Cheveux Blancs",nn:"TOUFFE DE CHEVEUX BLANCS",t:15,d:"Voici la preuve que certains monstres se font beaucoup de soucis.",ep:1,g:435,l:24,wd:true,fm:true,w:1,p:1};
I.u[2283] = {n:"Oeil de Kwoan",nn:"OEIL DE KWOAN",t:109,d:"Cet oeil de Kwoan a des effets hypnotiques surprenants, autant dire qu\'il faut toujours garder un oeil sur vous quand vous le manipulez.",ep:3,g:436,l:8,wd:true,fm:true,w:1,p:1};
I.u[2284] = {n:"Note de Harry Stote",nn:"NOTE DE HARRY STOTE",t:24,d:"L\'entrée de la grotte légendaire aux larves dorées se trouve un peu à l\'ouest d\'Amakna. Les pouvoirs latents de la larve dorée te permettront d\'y rentrer. Attention si ta petite larve se prend des dégâts elle se précipitera dans les bras de son père.",ep:2,g:55,l:1,wd:true,fm:true,w:0,p:1};
I.u[2285] = {n:"Tissu Sombre",nn:"TISSU SOMBRE",t:55,d:"La particularité de ce Tissu n\'est plus sujet à controverse. Cela dit, il est très irritant au toucher, sûrement parce qu\'un Ouginak s\'est dans le meilleur des cas, mouché dessus pendant des années.",ep:3,g:437,l:50,wd:true,fm:true,w:1,p:1};
I.u[2286] = {n:"Bandelettes",nn:"BANDELETTES",t:15,d:"Ces bandelettes sont révolutionnaires : triple épaisseur, parfumées à la violette, ultra résistantes. C\'est vraiment un produit de qualité.",ep:2,g:438,l:51,wd:true,fm:true,w:1,p:1};
I.u[2287] = {n:"Cuir Noir",nn:"CUIR NOIR",t:56,d:"Ce cuir est déjà traité, sa provenance n\'est pas certaine mais la couleur provient probablement d\'une teinture spéciale.",ep:1,g:439,l:74,wd:true,fm:true,w:1,p:1};
I.u[2288] = {n:"Patte de Wabbit",nn:"PATTE DE WABBIT",t:103,d:"Bon nombres de touristes vont sur l\'île de Wabbits essentiellement pour se munir d\'une Patte de Wabbit car elle porte bonheur. Malheureusement, ça n\'est pas le cas, mais il ne faut pas le dire, le tourisme en serait fort pénalisé.",ep:8,g:440,l:18,wd:true,fm:true,w:1,p:1};
I.u[2290] = {n:"Carapace de Scarafeuille Blanc",nn:"CARAPACE DE SCARAFEUILLE BLANC",t:107,d:"Il ne faut pas grand chose pour avoir un bel abat-jour chez soi. Soit dit en passant, cette carapace doit avoir tout de même une autre utilité. Enfin espérons-le.",ep:3,g:441,l:32,wd:true,fm:true,w:1,p:1};
I.u[2291] = {n:"Carapace de Scarafeuille Bleu",nn:"CARAPACE DE SCARAFEUILLE BLEU",t:107,d:"Il ne faut pas grand chose pour avoir un bel abat-jour chez soi. Soit dit en passant, cette carapace doit avoir tout de même une autre utilité. Enfin espérons-le.",ep:3,g:442,l:32,wd:true,fm:true,w:1,p:1};
I.u[2292] = {n:"Carapace de Scarafeuille Rouge",nn:"CARAPACE DE SCARAFEUILLE ROUGE",t:107,d:"Il ne faut pas grand chose pour avoir un bel abat-jour chez soi. Soit dit en passant, cette carapace doit avoir tout de même une autre utilité. Enfin espérons-le.",ep:3,g:443,l:32,wd:true,fm:true,w:1,p:1};
I.u[2293] = {n:"Carapace de Scarafeuille Vert",nn:"CARAPACE DE SCARAFEUILLE VERT",t:107,d:"Il ne faut pas grand chose pour avoir un bel abat-jour chez soi. Soit dit en passant, cette carapace doit avoir tout de même une autre utilité. Enfin espérons-le.",ep:3,g:444,l:32,wd:true,fm:true,w:1,p:1};
I.u[2294] = {n:"Viscères de Scarafeuille",nn:"VISCERES DE SCARAFEUILLE",t:15,d:"Au premier abord, ces viscères ne semblent pas très appétissants. Et pourtant, hâchés avec de fines herbes séchées au soleil, un peu de menthe sauvage, et cuits à l\'étouffée dans un vieux chaudron rouillé, ces viscères combleront de joie le plus gourmet des Chafers.",ep:3,g:445,l:32,wd:true,fm:true,w:1,p:1};
I.u[2295] = {n:"Fragment d\'Epée Reptilienne",nn:"FRAGMENT D\'EPEE REPTILIENNE",t:15,d:"Avec ce Fragment d\'Epée accroché à votre cheminée, vous pourrez vous vanter de dompter le Crocodaille tel l\'excentrique Mikoule, plus connu sous le nom de Crocodaille Dandi.",ep:4,g:446,l:69,wd:true,fm:true,w:10,p:1};
I.u[2296] = {n:"Bouclier Reptilien",nn:"BOUCLIER REPTILIEN",t:15,d:"A quoi pourrait bien vous servir ce magnifique bouclier orné de pierres précieuses ? En attendant que vous ne trouviez la réponse, il peut déjà vous servir de plateau pour apporter le thé à vos amis.",ep:4,g:447,l:69,wd:true,fm:true,w:15,p:1};
I.u[2297] = {n:"Carreau d\'Arbalète",nn:"CARREAU D\'ARBALETE",t:15,d:"Ce Carreau peut vous servir à confectionner une arme courte mais tranchante. Elle peut aussi faire office de cure-dents, à vous de voir.",ep:1,g:125,l:34,wd:true,fm:true,w:1,p:1};
I.u[2299] = {n:"Epaulette du Dark Vlad",nn:"EPAULETTE DU DARK VLAD",t:15,d:"Cette épaulette a dû essuyer plus d\'une bataille. Même si d\'apparence elle ne semble pas très résistante, mieux vaux ne pas frapper le Dark Vlad à l\'épaule si vous tenez à votre arme.",ep:2,g:449,l:120,wd:true,fm:true,w:5,p:80};
I.u[2300] = {n:"Fragment d\'Os",nn:"FRAGMENT D\'OS",t:47,d:"Ce Fragment d\'Os provient soit d\'un reste de Chafer datant du néotilique, soit d\'un repas de Meulou. Mais il y a de grandes chances pour que ce ne soit aucun des deux.",ep:9,g:450,l:43,wd:true,fm:true,w:1,p:1};
I.u[2301] = {n:"Moustache de Kolérat",nn:"MOUSTACHE DE KOLERAT",t:15,d:"Peu résistant, la Moustache pourra vous servir à faire le mariol devant vos amis. Ressembler par exemple à Salve et Torh Bali, Grands peintres Brâkmarien.",ep:2,g:451,l:9,wd:true,fm:true,w:1,p:1};
I.u[2302] = {n:"Langue de Crabe",nn:"LANGUE DE CRABE",t:15,d:"Les têtes pensantes d\'Amakna se demandent encore quelle est l\'utilité de cette langue pour le crabe. Ils seraient probablement offusqués s\'ils connaissaient la réponse.",ep:1,g:452,l:4,wd:true,fm:true,w:1,p:1};
I.u[2303] = {n:"Carapace vide de Crabe",nn:"CARAPACE VIDE DE CRABE",t:107,d:"Correctement vidée, cette Carapace de Crabe peut contenir divers objets. Toutefois, les objets en question garderont éternellement une odeur de Crabe plus ou moins troublante.",ep:1,g:453,l:4,wd:true,fm:true,w:2,p:1};
I.u[2304] = {n:"Fragment de Pierre Polie",nn:"FRAGMENT DE PIERRE POLIE",t:51,d:"Ce Fragment provient de la fesse gauche d\'un Craqueleur des Plaines. Pour éclater une Arakne, il n\'y a pas mieux, à part le Craqueleur en personne peut-être.",ep:5,g:454,l:45,wd:true,fm:true,w:5,p:1};
I.u[2305] = {n:"Fragment de Pierre Pointue",nn:"FRAGMENT DE PIERRE POINTUE",t:51,d:"Ce Fragment provient de l\'omoplate droit d\'un Craqueleur des Plaines. A moins que ce ne soit une partie du bas ventre. Dans tous les cas, cette pierre n\'est pas à placer n\'importe où.",ep:5,g:455,l:45,wd:true,fm:true,w:5,p:1};
I.u[2306] = {n:"Coeur de Craqueleur",nn:"COEUR DE CRAQUELEUR",t:51,d:"Coeur de pierre, ne vois tu pas en moi l\'amour que je te porte. Si tu continues, je te laisse par terre, je ne vais pas me laisser saouler de la sorte.",ep:5,g:456,l:45,wd:true,fm:true,w:5,p:1};
I.u[2307] = {n:"Bouclier de Chafer Lancier",nn:"BOUCLIER DE CHAFER LANCIER",t:24,d:"...",ep:2,g:75,l:1,wd:true,fm:true,w:0,p:1};
I.u[2308] = {n:"Badge Brâkmarien",nn:"BADGE BRAKMARIEN",t:24,d:"Ce badge permet d\'identifier un guerrier lorsqu\'il périt sur un champ de bataille. Il paraît que certains en font collection, à Bonta.",ep:2,g:76,l:1,wd:true,fm:true,w:0,p:1};
I.u[2309] = {n:"Bougie Rouge",nn:"BOUGIE ROUGE",t:24,d:"Une bougie rouge pas comme les autres...",ep:1,g:77,l:1,wd:true,fm:true,w:0,p:1};
I.u[2310] = {n:"Bougie Verte",nn:"BOUGIE VERTE",t:24,d:"...",ep:2,g:78,l:1,wd:true,fm:true,w:0,p:1};
I.u[2311] = {n:"Bougie Bleue",nn:"BOUGIE BLEUE",t:24,d:"...",ep:2,g:79,l:1,wd:true,fm:true,w:0,p:1};
I.u[2312] = {n:"Poudre de Sang",nn:"POUDRE DE SANG",t:24,d:"...",ep:2,g:80,l:1,wd:true,fm:true,w:0,p:1};
I.u[2313] = {n:"Respirateur Aquatique",nn:"RESPIRATEUR AQUATIQUE",t:24,d:"...",ep:2,g:81,l:1,wd:true,fm:true,w:0,p:1};
I.u[2314] = {n:"Fioles de Jiva",nn:"FIOLES DE JIVA",t:24,d:"...",ep:2,g:82,l:1,wd:true,fm:true,w:0,p:1};
I.u[2315] = {n:"Poils de Crocs Glands",nn:"POILS DE CROCS GLANDS",t:54,d:"Broujitte Fardot lança une interdiction jadis, sur l\'abattage des jeunes Crocs Glands. Depuis son exil, la chasse a repris et les tailleurs se remplissent à nouveau les poches.",ep:1,g:196,l:19,wd:true,fm:true,w:1,p:1};
I.u[2316] = {n:"Croc Jaune",nn:"CROC JAUNE",t:47,d:"Le Croc Jaune de Croc Gland apporte une peur bleue à tous ceux qui sous estimeraient son pouvoir.",ep:1,g:458,l:19,wd:true,fm:true,w:1,p:1};
I.u[2317] = {n:"Oreilles de Loup",nn:"OREILLES DE LOUP",t:106,d:"C\'est en observant de près que l\'on se demande comment un Croc Gland entend quelquechose avec toutes les bêtes et autres crasses qu\'il possède dans ses Oreilles.",ep:1,g:459,l:19,wd:true,fm:true,w:1,p:1};
I.u[2318] = {n:"Patte d\'ours",nn:"PATTE D\'OURS",t:103,d:"Certains gobelins portent une patte d\'ours autour du cou. Une superstition dont tout le monde a oublié l\'origine et la signification.",ep:1,g:460,l:12,wd:true,fm:true,w:1,p:1};
I.u[2320] = {n:"Collier Cassé d\'Ouginak",nn:"COLLIER CASSE D\'OUGINAK",t:15,d:"Comme tout bon Ouginak, son Collier donne des informations. Il est écrit: " + "\"" + "je suis perdu, mon quartier se trouve sur le sentier des Landes de Sidimote, merci de me ramener à mes maîtres Brâkmariens, ils vous offriront des baffes en remerciements." + "\"" + "",ep:14,g:462,l:51,wd:true,fm:true,w:5,p:1};
I.u[2321] = {n:"Babines d\'Ouginak",nn:"BABINES D\'OUGINAK",t:15,d:"Vous avez l\'impression que ces Babines d\'Ouginak pourraient suinter de la bave indéfiniment. Peut-être parce que cet animal est réputé pour sa gourmandise éternelle.",ep:14,g:463,l:51,wd:true,fm:true,w:1,p:1};
I.u[2322] = {n:"Crocs de Rats",nn:"CROCS DE RATS",t:47,d:"Ne vous-y méprenez pas, ces petites quenottes sont en fait de vrais crocs aiguisés, malodorants et usés. Ils servent parfois à remplacer les dents en or des Enutrofs un peu trop avares.",ep:1,g:464,l:50,wd:true,fm:true,w:1,p:1};
I.u[2323] = {n:"Colonne Vertebrale",nn:"COLONNE VERTEBRALE",t:47,d:"Voici une Colonne Vertébrale de bonne facture. Ce n\'est pas une raison pour vous éclater le dos non plus.",ep:1,g:465,l:34,wd:true,fm:true,w:2,p:1};
I.u[2324] = {n:"Pastèque",nn:"PASTEQUE",t:46,d:"...",ep:1,g:466,l:1,wd:true,fm:true,w:2,p:1};
I.u[2325] = {n:"Banane",nn:"BANANE",t:46,d:"Le célèbre ménestrel Geror Lendorman composa jadis une ode en l\'honneur de ce fruit : " + "\"" + "La banane des gens heureux" + "\"" + ".",ep:1,g:467,l:1,wd:true,fm:true,w:1,p:1};
I.u[2326] = {n:"Orange",nn:"ORANGE",t:46,d:"...",ep:1,g:468,l:1,wd:true,fm:true,w:1,p:1};
I.u[2327] = {n:"Pomme",nn:"POMME",t:46,d:"...",ep:1,g:469,l:1,wd:true,fm:true,w:1,p:1};
I.u[2328] = {n:"Petite Perle Dorée",nn:"PETITE PERLE DOREE",t:15,d:"Cette perle dorée est un cadeau de la petite larve que vous avez accompagnée. Elle est donc pour vous d\'une grande valeur sentimentale. Sa douceur et sa couleur en font un cadeau parfait pour une personne un tant soit peu féminine.",ep:5,g:483,l:1,wd:true,fm:true,w:1,p:1};
I.u[2329] = {n:"Poire",nn:"POIRE",t:46,d:"...",ep:1,g:470,l:1,wd:true,fm:true,w:1,p:1};
I.u[2330] = {n:"Capsule Explosive",nn:"CAPSULE EXPLOSIVE",t:15,d:"Cette capsule contient des extraits d\'agrumes magiques. Il est conseillé de l\'agiter avant de l\'utiliser pour que la pulpe ne reste pas dans le fond.",ep:4,g:711,l:23,wd:true,fm:true,w:1,p:1};
I.u[2331] = {n:"Aubergine",nn:"AUBERGINE",t:46,d:"Pas réellement nutritive, certains accompagnent l\'aubergine avec toutes sortes de condiments. Ce légume fruit a mauvaise réputation mais il est pourtant bénéfique pour la santé. Sauf pour celle des Bworks mais personne ne sait pourquoi.",ep:1,g:472,l:1,wd:true,fm:true,w:1,p:10};
I.u[2332] = {n:"Bonbon de Consolation",nn:"BONBON DE CONSOLATION",t:42,d:"Ce bonbon vous a permis de ne pas pleurer.",ep:13,g:135,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:1};
I.u[2333] = {n:"Bonbon Rouge Surpuissant",nn:"BONBON ROUGE SURPUISSANT",t:42,d:"Ce bonbon, augmentera votre attention lors des combats, pas de chance, il a vraiment un goût immonde. (DOUBLE CLIC POUR UTILISER, bonus de 20 en sagesse et de 10 en Prospection pour les 30 prochains combats).",ep:1,g:136,l:1,wd:true,fm:false,w:1,c:"PO!1723",u:true,p:1};
I.u[2336] = {n:"Crâne de chafer",nn:"CRANE DE CHAFER",t:47,d:"Ce crâne-là ne cliquettera plus jamais... quoique.",ep:1,g:480,l:16,wd:true,fm:true,w:1,p:1};
I.u[2337] = {n:"Livre Magique",nn:"LIVRE MAGIQUE",t:25,d:"Ce livre semble magique. Très étrange.",ep:1,g:7,l:1,wd:true,fm:true,w:1,p:50};
I.u[2338] = {n:"Vieux Bouquin",nn:"VIEUX BOUQUIN",t:25,d:"...",ep:1,g:8,l:1,wd:true,fm:true,w:1,p:100};
I.u[2339] = {n:"Liste d\'equipements standards",nn:"LISTE D\'EQUIPEMENTS STANDARDS",t:24,d:"Un guerrier de valeur moyenne se doit de porter autre chose que des haillons ou, au contraire, des choses clinquantes, il préfèrera donc: \n-Un camaieu au majeur \n-Un bandeau et une ceinture de vitalité \n-Des bottes de Kluh \n-Une cape brâkmarienne",ep:2,g:41,l:1,wd:true,fm:true,w:0,p:1};
I.u[2340] = {n:"Caleçon de milicien",nn:"CALECON DE MILICIEN",t:24,d:"Ceci est bien un caleçon de milicien. Vous devez être un sacré fétichiste pour posséder ceci. Ou bien quelqu\'un veut rire de vous.",ep:2,g:84,l:1,wd:true,fm:true,w:0,p:1};
I.u[2341] = {n:"Arc de Mech Riorraw",nn:"ARC DE MECH RIORRAW",t:2,d:"Cet arc a été conçu par Mech Riorraw, il est calibré pour pouvoir traverser la rivière de lave et atteindre le haut des murailles de Brâkmar, ni plus, ni moins.",ep:16,g:8,l:10,wd:true,fm:true,w:100,an:10,tw:true,e:[0,6,20,21,0,30,false,true],c:"Ps=1",p:0};
I.u[2342] = {n:"Bague d\'allégeance",nn:"BAGUE D\'ALLEGEANCE",t:24,d:"Cette bague vous a été donnée par Domen. Lui rendre signifie que vous êtes pour la destruction de toute chose. Ce qui vous place du coté des adorateurs de Djaul.",ep:2,g:86,l:1,wd:true,fm:true,w:0,p:1};
I.u[2343] = {n:"Bracelet de Djaul",nn:"BRACELET DE DJAUL",t:24,d:"Ce bracelet vous a été donné par Djaul. Le porter signifie que vous agréez ses propos.",ep:2,g:85,l:1,wd:true,fm:true,w:0,p:1};
I.u[2344] = {n:"Lettre pour Oto",nn:"LETTRE POUR OTO",t:24,d:"Cette lettre vous a été remise par Domen pour Oto.",ep:2,g:65,l:1,wd:true,fm:true,w:0,p:1};
I.u[2345] = {n:"Oeil de tortue",nn:"OEIL DE TORTUE",t:24,d:"Cette tortue ne fera plus la maligne maintenant.",ep:2,g:70,l:1,wd:true,fm:true,w:0,p:1};
I.u[2346] = {n:"Costume Roublardesque",nn:"COSTUME ROUBLARDESQUE",t:14,d:"Non, ce n\'est pas le fameux costume du dragovamp, la célèbre chauve-souris volante. Ceci est l\'accoutrement d\'un habitant du cimetière, un Roublard en quelques sortes.",ep:1,g:27,l:1,wd:true,fm:true,w:10,p:1};
I.u[2347] = {n:"Compte-rendu de mission",nn:"COMPTE-RENDU DE MISSION",t:24,d:"Ce compte-rendu explique comment s\'est passé l\'assaut de Brâkmar par la muraille Est.",ep:2,g:55,l:1,wd:true,fm:true,w:0,p:1};
I.u[2348] = {n:"Roublard Attitude",nn:"ROUBLARD ATTITUDE",t:31,d:"Vous avez La Roublard attitude, jusqu\'à ce que vous rentriez en combat.",ep:1,g:17,l:1,wd:true,fm:true,w:0,m:true,u:true,p:1};
I.u[2349] = {n:"Lettre pour Oto",nn:"LETTRE POUR OTO",t:24,d:"Cette lettre vous a été remise par Domen pour Oto. Mais vous avez oublié de la lui donner.",ep:2,g:65,l:1,wd:true,fm:true,w:0,p:1};
I.u[2350] = {n:"Touffe de cheveux",nn:"TOUFFE DE CHEVEUX",t:24,d:"Il semble que cette touffe de cheveux ait appartenu à l\'un de vos ennemis.",ep:2,g:30,l:1,wd:true,fm:true,w:0,p:1};
I.u[2351] = {n:"Pain du Pinchaut",nn:"PAIN DU PINCHAUT",t:33,d:"Dans ce pain, il y a à boire et à manger, en l\'essorant bien, il est possible de remplir une chopine de bière.",ep:16,g:13,l:1,wd:true,fm:true,w:2,u:true,ut:true,p:1};
I.u[2352] = {n:"Lettre de Danathor",nn:"LETTRE DE DANATHOR",t:24,d:"" + "\"" + "J\'ai à te parler, viens me voir dès que tu as un moment de libre, à bientôt mon jeune ami." + "\"" + "",ep:2,g:65,l:1,wd:true,fm:true,w:0,p:1};
I.u[2353] = {n:"Lettre de Larry Poller",nn:"LETTRE DE LARRY POLLER",t:24,d:"" + "\"" + "Bonjour, je suis Larry Poller, nous nous sommes rencontrés lors de la libération du Colonel Lyeno. J\'ai un service à vous demander. Venez me voir quand vous aurez un moment. A bientôt." + "\"" + "",ep:2,g:65,l:1,wd:true,fm:true,w:0,p:1};
I.u[2354] = {n:"Cervelle de tortue",nn:"CERVELLE DE TORTUE",t:24,d:"Cette cervelle de tortue, si elle est bien préparé, devient un plat succulent.",ep:2,g:89,l:1,wd:true,fm:true,w:0,p:1};
I.u[2355] = {n:"Poussière d\'Oni",nn:"POUSSIERE D\'ONI",t:24,d:"Cette poussière bleutée est un catalyseur magique très puissant.",ep:2,g:88,l:1,wd:true,fm:true,w:0,p:1};
I.u[2356] = {n:"Miracle",nn:"MIRACLE",t:31,d:"Vous avez un pouvoir miraculeux, jusqu\'à ce que vous rentriez en combat.",ep:13,g:19,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2357] = {n:"Bois d\'Oliviolet",nn:"BOIS D\'OLIVIOLET",t:38,d:"Ce Bois a l\'air de résister à toutes les températures. En effet, n\'essayez pas de faire un feu pour vous réchauffer, vous risqueriez de mourir de froid avant que le Bois ne prenne.",ep:14,g:481,l:50,wd:true,fm:true,w:5,p:0};
I.u[2358] = {n:"Bois de Bombu",nn:"BOIS DE BOMBU",t:38,d:"Ce Bois est le plus tendre de tous. Il est maléable à souhait et procure de réels plaisirs à travailler.",ep:13,g:482,l:50,wd:true,fm:true,w:5,p:0};
I.u[2359] = {n:"Broche de Heticili Ufot",nn:"BROCHE DE HETICILI UFOT",t:24,d:"Cette petite broche vous a été donné par Heticili Ufot.",ep:2,g:69,l:1,wd:true,fm:true,w:0,p:1};
I.u[2360] = {n:"Acte de propriété",nn:"ACTE DE PROPRIETE",t:24,d:"Cet acte de propriété est celui de la maison de Heticili Ufot.",ep:2,g:26,l:1,wd:false,fm:true,w:0,p:1};
I.u[2361] = {n:"Arc de Mulic Bere",nn:"ARC DE MULIC BERE",t:2,d:"Cet arc est une invention du génial Mulic Bere. Sa portée maximum est tout bonnement incroyable. Le problème c\'est que la portée minimum est presque aussi grande.",ep:16,g:10,l:10,wd:true,fm:true,w:100,an:10,tw:true,e:[0,6,17,23,0,30,false,true],p:0};
I.u[2362] = {n:"Prédiction de Paro Cabanne",nn:"PREDICTION DE PARO CABANNE",t:24,d:"La prédiction que Paro Cabanne a donné sur les prochaines éléctions.",ep:2,g:55,l:1,wd:true,fm:true,w:0,p:1};
I.u[2364] = {n:"Pierre du Savoir",nn:"PIERRE DU SAVOIR",t:50,d:"Cette pierre précieuse sait tout et partage de temps en temps.",ep:0,g:485,l:1,wd:true,fm:true,w:1,p:1};
I.u[2365] = {n:"Feuille de ronce d\'Hestia",nn:"FEUILLE DE RONCE D\'HESTIA",t:36,d:"Cette feuille de ronce, d\'apparence anodine, est une plante indispensable pour certaines recettes étranges.",ep:16,g:415,l:1,wd:true,fm:true,w:1,p:1};
I.u[2366] = {n:"Canne à Kralamour",nn:"CANNE A KRALAMOUR",t:20,d:"Cette canne, de métal et de diamant, est la seule canne capable de soutenir le poids du kralamour. Elle est aussi imprégné de l\'odeur de kralamourette afin d\'avoir une petite chance de pêcher ce monstre.",ep:1,g:8,l:1,wd:false,fm:true,w:10,an:18,e:[2,6,1,9,30,30,false,false],p:1};
I.u[2367] = {n:"L\'Ami Léhunui",nn:"L\'AMI LEHUNUI",t:10,d:"Léhunui savait se faire des amis, il conviait souvent ses camarades à de grandes soirées costumées. Il avait aussi pour réputation de mettre toujours les petits plats dans les grands et d\'offrir de somptueux bijoux pour vous remercier de venir chez lui.",ep:8,g:21,l:37,wd:true,fm:true,w:20,c:"CW>49&Ps=1",p:3700};
I.u[2368] = {n:"Ramoume l\'Eternelle",nn:"RAMOUME L\'ETERNELLE",t:10,d:"Ramoume était un ancien ami d\'enfance de Nova, ils se sont perdus de vue quand Ramoume décida de faire une sieste éternelle.",ep:2,g:22,l:37,wd:true,fm:true,w:50,c:"CW>49&Ps=2",p:1000};
I.u[2369] = {n:"Puissante Ceinture Fulgurante",nn:"PUISSANTE CEINTURE FULGURANTE",t:10,d:"Cette magnifique ceinture a été confectionnée suite à un pari fou. Le défi était d\'améliorer une des plus belles ceintures conçue sur Amakna : la ceinture Fulgurante. Le pari fut réussi par Pakape Prométhé.",ep:2,g:26,l:80,wd:true,fm:true,w:150,p:100000};
I.u[2370] = {n:"La Végète Hâtif",nn:"LA VEGETE HATIF",t:10,d:"Equipement d\'une légèreté incompararable, la Végéte Hâtif vous apporte quelques grammes de finesses dans ce monde de Bwork en rût.",ep:2,g:23,l:41,wd:true,fm:true,w:5,c:"CA>50",p:4575};
I.u[2371] = {n:"La Mouthmou",nn:"LA MOUTHMOU",t:10,d:"Cette ceinture vous est conseillée par Jiva en personne, pour les nuits froides et rigoureuses. En cas de tâches, elle se lave avec du Rimennelle.",ep:13,g:24,l:27,wd:true,fm:true,w:30,p:2775};
I.u[2372] = {n:"Bottes de Lymphe a Tik",nn:"BOTTES DE LYMPHE A TIK",t:11,d:"Ces bottes donnent l\'impression de marcher constamment dans l\'herbe fraîche. Elles sont donc idéales pour ceux dont l\'odeur des pieds se rapproche plus du Clakoss, que de la Gelée Menthe.",ep:5,g:18,l:39,wd:true,fm:true,w:5,c:"CC>50",p:4100};
I.u[2373] = {n:"Moon Boots",nn:"MOON BOOTS",t:11,d:"Le poil de Moon, nécessaire pour cette paire de bottes, est sans aucun doute le poil le plus chaud pour les pieds, même si porter ses bottes en souvenir de Moon vous fera chaud au coeur.",ep:14,g:19,l:62,wd:true,fm:true,w:30,p:10000};
I.u[2374] = {n:"Bottes Tarsy",nn:"BOTTES TARSY",t:11,d:"Tarsy était un ermite qui n\'appréciait pas trop les visites. Pour preuve, il recevait ses invités en leur montrant de très près la pointure de ses Bottes.",ep:4,g:20,l:38,wd:true,fm:true,w:40,p:4000};
I.u[2375] = {n:"Bottes Orino",nn:"BOTTES ORINO",t:11,d:"Grâce à ces bottes, vous serez à même d\'entendre le vent dans les voiles d\'un bateau situé en pleine Mer d\'Asse, ou d\'entendre les gémissements d\'un Chafer Lancier, perdu dans les tréfonds du labyrinthe.",ep:13,g:21,l:77,wd:true,fm:true,w:40,p:15500};
I.u[2376] = {n:"Hache Hémal",nn:"HACHE HEMAL",t:19,d:"Cette hache est la seule capable de résister aux pouvoirs du Kaerlier et, donc, la seule à pouvoir le couper.",ep:1,g:18,l:1,wd:true,fm:true,w:10,e:[5,6,1,1,45,45,false,false],c:"PJ>2,79",p:1};
I.u[2378] = {n:"Sang de Roche",nn:"SANG DE ROCHE",t:51,d:"Cette pierre, à la couleur cramoisie, permet de fabriquer de très puissantes potions.",ep:16,g:459,l:1,wd:true,fm:true,w:0,p:1};
I.u[2379] = {n:"Bois de Kaerlier",nn:"BOIS DE KAERLIER",t:38,d:"Ce bois provient du très rare Kaerlier. Ses propriétés sont plutôt...étonnantes.",ep:14,g:140,l:40,wd:true,fm:true,w:5,p:20};
I.u[2380] = {n:"Cape de Bou",nn:"CAPE DE BOU",t:17,d:"Cette Cape est bien étrange, elle est faite de bouts de boue du Boo, mais elle tient tout de même debout. Une fois sur vos épaules, elle pourra même faire de vous un boute-en-train.",ep:13,g:35,l:41,wd:true,fm:true,w:10,p:4500};
I.u[2381] = {n:"Cape Flotteuse",nn:"CAPE FLOTTEUSE",t:17,d:"Cette Cape fut découpée dans un fanion, anciennement situé sur la plus haute tour du chateau d\'Amakna. C\'est sûrement pour cela qu\'elle sent le linge séché au grand air.",ep:5,g:36,l:61,wd:true,fm:true,w:10,c:"CI>90",p:9500};
I.u[2382] = {n:"Cape Maimpa",nn:"CAPE MAIMPA",t:17,d:"Maimpa était un Ecaflip qui se moquait du vide, prétextant qu\'il retomberait toujours sur ses pattes. Mais un jour, il atterrit sur un Trool affamé. Il fût enterré le jour même à l\'endroit où l\'on a trouvé ses restes. Un peu partout en quelques sortes.",ep:14,g:37,l:36,wd:true,fm:true,w:10,p:6500};
I.u[2383] = {n:"Cape Sanguine",nn:"CAPE SANGUINE",t:17,d:"A base de tissu sanguin, la Cape Sanguine a un aspect cartonneux au toucher, comme si la teinture rougeâtre avait coagulé.",ep:7,g:38,l:50,wd:true,fm:true,w:10,c:"CS>90",p:6000};
I.u[2384] = {n:"Bottes Animales",nn:"BOTTES ANIMALES",t:11,d:"On peut le dire, ces Bottes ont un côté comment dire... Hmmm... Bestial. Ne vous étonnez donc pas si un Mulou tombe amoureux de vos Bottes. C\'est peut-être la pire chose qui puisse vous arriver.",ep:14,g:22,l:94,wd:true,fm:true,w:40,p:23000};
I.u[2385] = {n:"La Brouteuse",nn:"LA BROUTEUSE",t:17,d:"Idéale pour le jardin, la Brouteuse peut aussi servir de paillasson pour votre entrée de maison. Conçue à la base pour les Sadidas, elle n\'abîme pas les poils, même les plus fragiles. Certains vont même jusqu\'à dire qu\'elle ralentit leur chute.",ep:12,g:39,l:88,wd:true,fm:true,w:10,p:20000};
I.u[2386] = {n:"La Guenille",nn:"LA GUENILLE",t:17,d:"Cette étrange Cape puise toute sa force dans son aspect vétuste. Plus elle sera sale, et plus elle vous servira.",ep:13,g:40,l:53,wd:true,fm:true,w:10,p:7000};
I.u[2387] = {n:"La Poilue",nn:"LA POILUE",t:17,d:"Il faut avouer que cette Cape possède une forte odeur très désagréable. En essayant de mettre des mots sur cette infection, on peut dire que c\'est tout simplement un mélange de poils de monstres divers qui créent ce magnifique pot pourri.",ep:14,g:41,l:63,wd:true,fm:true,w:10,c:"CW>30&CV>50",p:10500};
I.u[2388] = {n:"L\'Amulette du Puzzle de la Griffe",nn:"L\'AMULETTE DU PUZZLE DE LA GRIFFE",t:1,d:"Autrefois, un Ecaflip nommé Dodge, éclata en mille morceaux une carte au trésor gravée sur une pierre. Croyant réussir à reformer la carte, il garda la plus grosse partie pour en faire une Amulette mystérieuse. Nul ne sait ce que le trésor est devenu.",ep:14,g:36,l:44,wd:true,fm:true,w:5,p:4000};
I.u[2389] = {n:"Amulette du Badoul",nn:"AMULETTE DU BADOUL",t:1,d:"Dodge adorait exploser tout ce qui avait de la valeur à ses yeux. Il disait que c\'était la preuve qu\'il existait. Chacun son truc.",ep:14,g:37,l:32,wd:true,fm:true,w:5,p:3200};
I.u[2390] = {n:"Amulette de la Boule",nn:"AMULETTE DE LA BOULE",t:1,d:"Cette Amulette est presque parfaite, elle accroît considérablement la sagesse de celui qui la porte autour du cou. Très utile pour paraître plus sage auprès de vos amis, ou vous faire passer pour un écolier modèle.",ep:5,g:38,l:49,wd:true,fm:true,w:1,p:6400};
I.u[2391] = {n:"Amulette de la Bouboule",nn:"AMULETTE DE LA BOUBOULE",t:1,d:"Un vieux dicton de Sufokia dit: de la Boule un jour, de la Bouboule peut-être un autre jour. Vous pourrez constatez que, plus le dicton est vieux, plus il s\'avère ridicule.",ep:5,g:39,l:98,wd:true,fm:true,w:1,p:24400};
I.u[2392] = {n:"Dragolyre Remaniée",nn:"DRAGOLYRE REMANIEE",t:1,d:"Grâce aux multiples talents des artisans d\'Amakna, la Dragolyre Remaniée excelle dans la perfection.",ep:11,g:25,l:42,wd:true,fm:true,w:1,p:5000};
I.u[2393] = {n:"Omelette Amulette",nn:"OMELETTE AMULETTE",t:1,d:"L\'osamodas Onehaieuf Bek cherchait absolument à gagner du mérite devant les membres de sa guilde, et décida alors de faire une Amulette révolutionnaire. Il y parvint en préparant un petit déjeuner à ses amis. Étrange.",ep:1,g:40,l:17,wd:true,fm:true,w:10,p:1000};
I.u[2394] = {n:"La Bourgeonette",nn:"LA BOURGEONETTE",t:1,d:"Gorgée de soleil, la Bourgeonette réchauffe les coeurs en plus de procurer une sensation d\'invulnérabilité face à la terre. Elle est aussi tellement légère qu\'on oublie souvent de l\'enlever à l\'heure du coucher.",ep:9,g:41,l:32,wd:true,fm:true,w:1,p:3000};
I.u[2395] = {n:"Le foulard du Lard Fou",nn:"LE FOULARD DU LARD FOU",t:1,d:"Le Lard Fou était un petit démon bien vil qui ne pouvait s\'empêcher de rester planté devant les gens sans bouger. Il était aussi résistant à la magie, ce qui imposait le respect à l\'époque. Depuis, le Lard Fou se terre quelque part et attend.",ep:8,g:42,l:42,wd:true,fm:true,w:15,p:5000};
I.u[2396] = {n:"La Lou Ptiote",nn:"LA LOU PTIOTE",t:1,d:"Sylveline n\'était pas très futée. Sa mère, Lou, lui offrit cette lanterne en disant " + "\"" + "Tiens! Au moins tu seras une lumière !" + "\"" + ". Elle non plus n\'était pas une lanterne. Ce qui ne l\'a pas empêchée d\'avoir un éclair de génie qui illumina la vie de sa fille.",ep:7,g:43,l:15,wd:true,fm:true,w:5,p:1500};
I.u[2397] = {n:"La Bibelaw",nn:"LA BIBELAW",t:1,d:"Law-bibe était un vieillard qui adorait assembler n\'importe quoi pour en faire un objet. Un jour, il créa ce médaillon aux formes intrigantes. Une fois autour du cou, il ressentit une étrange sensation. Bien étrange sensation même...",ep:13,g:44,l:30,wd:true,fm:true,w:10,p:3000};
I.u[2398] = {n:"L\'Oeil de Kanigrou",nn:"L\'OEIL DE KANIGROU",t:1,d:"Offert à Melyonen par le célèbre Kanigrou  Rook Y Balboua, pour la remercier de lui avoir présenté celle qui est maintenant sa femme, Ar d\'rieeeeeeeeeneuh comme il aime à l\'appeler.",ep:13,g:45,l:52,wd:true,fm:true,w:1,p:7000};
I.u[2399] = {n:"La Veaux Doux",nn:"LA VEAUX DOUX",t:1,d:"Jozébo Vez était un berger de Veaux, lointains cousins des Bouftous. Dans son troupeau, quelques bêtes possédaient une fourrure plus douce que les autres. Elle se revendait bien plus cher et pour différencier ses bêtes, il attachait ce collier à leur cou.",ep:13,g:46,l:15,wd:true,fm:true,w:1,p:1};
I.u[2400] = {n:"Bottes Animales Sombres",nn:"BOTTES ANIMALES SOMBRES",t:11,d:"On peut le dire, ces Bottes ont un côté comment dire, .....hmmm.... Sombre. Ne vous étonnez donc pas si un Vampire tombe en amour devant vous et vos Bottes. C\'est peut-être la pire chose qui puisse vous arriver.",ep:14,g:23,l:122,wd:true,fm:true,w:35,p:38000};
I.u[2401] = {n:"Carte de Kwelsh",nn:"CARTE DE KWELSH",t:15,d:"Cette carte est liée au maître Kwelsh. C\'est tout ce que vous savez et que vous devez savoir.",ep:16,g:224,l:1,wd:true,fm:true,w:10,p:1};
I.u[2402] = {n:"Epée de Sériane-Kerm",nn:"EPEE DE SERIANE-KERM",t:6,d:"Cette épée est un cadeau honorifique que l\'on donne uniquement aux mercenaires accomplis. Mais elle coupe très bien aussi.",ep:1,g:25,l:30,wd:false,fm:true,w:20,an:14,tw:true,e:[10,4,1,1,40,40,false,false],c:"Ps=3",p:1};
I.u[2403] = {n:"Chtévu",nn:"CHTEVU",t:10,d:"Autrefois utilisée pour les libertés conditionnelles, la Chtévu possède paraît-il un pouvoir supplémentaire par rapport aux autres ceintures. Mais personne n\'a encore trouvé. Mieux vaut donc garder un oeil sur cette ceinture.",ep:13,g:25,l:49,wd:true,fm:true,w:10,p:6100};
I.u[2404] = {n:"Ceinture du Rat",nn:"CEINTURE DU RAT",t:10,d:"Cette ceinture a appartenu à un Rat d\'Egoût, elle lui allait à merveille, bien moulante au niveau des hanches, elle lui permettait de cacher ses poignées d\'amour.",ep:2,g:27,l:55,wd:true,fm:true,w:10,p:8100};
I.u[2409] = {n:"Kwakoiffe de Flammes",nn:"KWAKOIFFE DE FLAMMES",t:16,d:"La Kwakoiffe de Flammes vous donne réellement une allure de tombeur, ce qui est très pratique pour mettre à terre vos adversaires, lorsque ceux-ci n\'arrivent plus à tenir debout tellement ils rigolent, en vous voyant affublé de cette coiffe.",ep:13,g:39,l:41,wd:true,fm:true,w:5,s:2,p:4600};
I.u[2410] = {n:"Abracaska",nn:"ABRACASKA",t:16,d:"L\'Abracaska permet d\'éviter les gueules de bois.",ep:1,g:40,l:45,wd:true,fm:true,w:50,s:3,p:5500};
I.u[2411] = {n:"Coiffe du Bouftou",nn:"COIFFE DU BOUFTOU",t:16,d:"Ce superbe chapeau permet à son porteur de gagner force et intelligence si ce dernier parvient à s\'accommoder de l\'odeur.",ep:1,g:41,l:10,wd:true,fm:true,w:15,s:1,p:2050};
I.u[2412] = {n:"Kwape de Flammes",nn:"KWAPE DE FLAMMES",t:17,d:"Très peu irritante, mais un peu quand même il faut l\'avouer, cette cape ne pourra pas hélas vous faire voler, ni même planer. Même en y cousant un grand " + "\"" + "M" + "\"" + " dessus, ça ne marchera pas, certains y croyaient pourtant dur comme fer. Dommage.",ep:8,g:42,l:41,wd:true,fm:true,w:5,s:2,p:4600};
I.u[2413] = {n:"Abracapa",nn:"ABRACAPA",t:17,d:"Vêtu de Feuilles d\'Abraknydes, vous pourrez vous pavaner au beau milieu de la forêt sans que l\'on vous reconnaisse vraiment. Attention toutefois aux Abraknydes en quête de bourgeonnements printaniers.",ep:1,g:43,l:45,wd:true,fm:true,w:17,s:3,p:5500};
I.u[2414] = {n:"Cape Bouffante",nn:"CAPE BOUFFANTE",t:17,d:"Cette cape a tout pour plaire : sa résistance, son confort, sa couleur, sa rareté... En revanche son odeur est redoutable pour les parois nasales. Mais bon, on ne peut pas tout avoir non plus.",ep:1,g:44,l:10,wd:true,fm:true,w:30,s:1,p:2780};
I.u[2415] = {n:"Kwaklame de Flammes",nn:"KWAKLAME DE FLAMMES",t:6,d:"La Kwaklame de Flammes est une arme vraiment stylée. A défaut de faire peur à votre adversaire, elle vous servira à l\'impressionner, pour ensuite mieux le surprendre par une vieille ruse de fourbe.",ep:5,g:52,l:41,wd:true,fm:true,w:5,an:14,e:[5,5,1,1,50,50,false,false],c:"CI>50&CV>50",s:2,p:4600};
I.u[2416] = {n:"Marteau du Bouftou",nn:"MARTEAU DU BOUFTOU",t:7,d:"Le manuel de l\'utilisateur explique que ce surprenant marteau est capable de libérer une énergie colossale lorsqu\'il est correctement manié. Encore un manuel Osamodas traduit en Sadida...",ep:1,g:23,l:10,wd:true,fm:true,w:25,an:15,e:[5,5,1,1,40,40,false,true],s:1,p:845};
I.u[2417] = {n:"Abraton",nn:"ABRATON",t:4,d:"Ce bâton a gardé toute l\'agressivité de l\'Abraknyde, mais n\'a pas conservé l\'odeur de bois moisi qui caractérise souvent les vieux Abraknydes.",ep:1,g:23,l:45,wd:true,fm:true,w:10,an:12,tw:true,e:[3,4,1,1,30,50,false,false],s:3,p:5500};
I.u[2418] = {n:"Kwakanneau de Flammes",nn:"KWAKANNEAU DE FLAMMES",t:9,d:"Grâce à cet anneau, si vous vous perdez en allant délivrer un message, votre maître pourra peut-être vous retrouver.",ep:5,g:43,l:42,wd:true,fm:true,w:2,s:2,p:4900};
I.u[2419] = {n:"Anneau de Bouze le Clerc",nn:"ANNEAU DE BOUZE LE CLERC",t:9,d:"Cet anneau provient du fameux Bouze le Clerc, sauveur des temps modernes. Sa phrase culte parlait d\'un fin Eni et d\'au-delà.",ep:1,g:44,l:10,wd:true,fm:true,w:10,s:1,p:775};
I.u[2420] = {n:"Anobra",nn:"ANOBRA",t:9,d:"Travailler la Branche d\'Abraknyde n\'est pas chose aisée. Mais avec de la persévérance, on peut finir par obtenir un anneau à la fois sobre, élégant et raffiné. Tout le contraire d\'un Sadida dans une taverne.",ep:1,g:45,l:40,wd:true,fm:true,w:10,s:3,p:4500};
I.u[2421] = {n:"Kwakobottes de Flammes",nn:"KWAKOBOTTES DE FLAMMES",t:11,d:"Muni de ces ergots de Kwak de Feu, il ne vous reste plus qu\'à marcher sur des oeufs pour ne pas vous les faire voler par un Sram mal intentionné.",ep:5,g:25,l:40,wd:true,fm:true,w:5,s:2,p:4400};
I.u[2422] = {n:"Boufbottes",nn:"BOUFBOTTES",t:11,d:"Ces Bottes sont d\'un confort inégalé, l\'intérieur a l\'air d\'être doublé en laine de Bouftou, mais on peut ressentir également comme un liquide circulant entre les orteils. Reste à savoir si c\'est de la Bave ou du Sang de Bouftou. Peu importe.",ep:1,g:26,l:10,wd:true,fm:true,w:15,s:1,p:1700};
I.u[2423] = {n:"Abrabottes",nn:"ABRABOTTES",t:11,d:"Les effets de cette paire de Bottes peuvent aussi parfaitement être retrouvés en se rendant dans une taverne. Il faut ensuite consommer un peu, voire beaucoup pour un effet optimum. Le lendemain vous aurez des jambes aussi lourdes que ces bottes.",ep:1,g:27,l:45,wd:true,fm:true,w:20,s:3,p:4500};
I.u[2424] = {n:"Amukwak de Flammes",nn:"AMUKWAK DE FLAMMES",t:1,d:"Avec toutes ces plumes autour du cou, vous ressemblerez à Aldo Rado, grand Sacrieur aventurier en quête d\'une cité d\'or.",ep:5,g:47,l:42,wd:true,fm:true,w:3,s:2,p:4900};
I.u[2425] = {n:"Amulette du Bouftou",nn:"AMULETTE DU BOUFTOU",t:1,d:"Une bonne Corne de Bouftou, c\'est une Corne qui procure calme et sagesse. Un Bon pendentif de Bouftou vous procurera donc sagesse et calme. Ou l\'inverse. Ou autre chose. Ou alors rien.",ep:1,g:48,l:3,wd:true,fm:true,w:10,s:1,p:550};
I.u[2426] = {n:"L\'Araknamu",nn:"L\'ARAKNAMU",t:1,d:"Cette petite Arakne est la preuve vivante que l\'on est pas obligé d\'empailler un animal pour en faire une Amulette. Elle vous chatouillera le cou, et tissera peut-être une toile devant votre bouche, si vous baillez trop souvent, ou si vous avez trop mauvaise haleine.",ep:1,g:49,l:20,wd:true,fm:true,w:10,s:3,p:1300};
I.u[2427] = {n:"Kwakture de Flammes",nn:"KWAKTURE DE FLAMMES",t:10,d:"Pour parader comme un Kwak de Flammes pendant la saison des amours, cette ceinture sera parfaite, à condition de ne porter que ça.",ep:12,g:28,l:41,wd:true,fm:true,w:5,s:2,p:4600};
I.u[2428] = {n:"Ceinture du Bouftou",nn:"CEINTURE DU BOUFTOU",t:10,d:"Rapide à équiper, cette ceinture s\'avère tout de même assez légère pour sa taille. La laine donne l\'impression d\'être neuve. Normal, il a fallu selon la légende, dépecer une centaine de bouftons pour avoir cet aspect lisse et soyeux.",ep:1,g:29,l:20,wd:true,fm:true,w:10,s:1,p:1253};
I.u[2429] = {n:"Abrature",nn:"ABRATURE",t:10,d:"Cette ceinture vous apportera assurance et abdominaux en bois. Un traitement anti-Arakne est néanmoins recommandé tous les ans. Un peu comme la coiffure d\'un Sadida, avec heureusement trois fois moins d\'anti-parasites.",ep:1,g:30,l:45,wd:true,fm:true,w:25,s:3,p:4500};
I.u[2430] = {n:"Flaque de Gelée Bleue",nn:"FLAQUE DE GELEE BLEUE",t:24,d:"Tu as été une vraie brute envers cette Gelée Bleue. Un jour, qui sait, tu paieras pour ce crime.",ep:7,g:91,l:1,wd:true,fm:true,w:0,p:1};
I.u[2431] = {n:"Flaque de Gelée Citron",nn:"FLAQUE DE GELEE CITRON",t:24,d:"Tu as été une vraie brute envers cette Gelée Citron. Un jour, qui sait, tu te feras aplatir comme elle.",ep:7,g:90,l:1,wd:true,fm:true,w:0,p:1};
I.u[2432] = {n:"Flaque de Gelée Fraise",nn:"FLAQUE DE GELEE FRAISE",t:24,d:"Tu as été une vraie brute envers cette Gelée Fraise. Un jour, qui sait, tu ressembleras à une grosse tâche difforme comme elle.",ep:7,g:92,l:1,wd:true,fm:true,w:0,p:1};
I.u[2433] = {n:"Flaque de Gelée Menthe",nn:"FLAQUE DE GELEE MENTHE",t:24,d:"Tu as été une vraie brute envers cette Gelée Menthe. Un jour, qui sait, il ne restera de toi que des restes, comme elle.",ep:7,g:93,l:1,wd:true,fm:true,w:0,p:1};
I.u[2435] = {n:"L\'Eni Kère",nn:"L\'ENI KERE",t:11,d:"Kère fut le seul Eniripsa à remporter devant les Ecaflips, le grand Marathon d\'Amakna. C\'est grâce à ses bottes artisanales qu\'il traversa la Mèr d\'Asse et distança tous les écaflips médusés de voir un Eni courir sur l\'eau. La classe.",ep:5,g:29,l:45,wd:true,fm:true,w:5,p:4800};
I.u[2436] = {n:"Gelée Citron",nn:"GELEE CITRON",t:110,d:"Ce pot de Gelée Citron a une apparence étrange. Selon Elrik Ola, cette gelée proviendrait de chez lui. Plus précisément, de l\'autre coin de la galaxie. Autant dire que ce n\'est pas la porte à côté.",ep:7,g:490,l:45,wd:true,fm:true,w:1,p:1};
I.u[2437] = {n:"Gelée Citron Royale",nn:"GELEE CITRON ROYALE",t:110,d:"Selon certaines rumeurs, la Gelée Citron Royale est bien meilleure que les autres gelées. Peut-être parce que le citron cache le goût de la gelée.",ep:7,g:491,l:200,wd:true,fm:true,w:1,p:100};
I.u[2438] = {n:"Boufcoiffe Royale",nn:"BOUFCOIFFE ROYALE",t:16,d:"La Boufcoiffe Royale est un objet indémodable. Elégante, elle procure à son porteur force et intelligence.",ep:1,g:42,l:50,wd:true,fm:true,w:45,s:4,p:100};
I.u[2440] = {n:"Epée Royale du Bouftou",nn:"EPEE ROYALE DU BOUFTOU",t:6,d:"Cette épée ne sait pas trop sur quel sabot danser... Elle inflige des dégâts de chaque élément.",ep:1,g:30,l:70,wd:true,fm:true,w:65,an:14,e:[4,4,1,1,30,50,false,false],s:4,p:30000};
I.u[2441] = {n:"Anneau Royal du Bouftou",nn:"ANNEAU ROYAL DU BOUFTOU",t:9,d:"Cet anneau augmente la vie de son porteur, tout comme l\'odeur de la femelle du Bouftou Royal augmente son charme.",ep:1,g:46,l:30,wd:true,fm:true,w:5,s:4,p:100};
I.u[2442] = {n:"Boufbottes Royales",nn:"BOUFBOTTES ROYALES",t:11,d:"Elles brillent, elles respirent la puissance, ces bottes sont en tous points royales.",ep:1,g:30,l:50,wd:true,fm:true,w:50,s:4,p:6600};
I.u[2443] = {n:"Amulette Royale du Bouftou",nn:"AMULETTE ROYALE DU BOUFTOU",t:1,d:"Cette amulette augmente la force et l\'intelligence de son porteur.",ep:1,g:50,l:50,wd:true,fm:true,w:10,s:4,p:5000};
I.u[2444] = {n:"Ceinture Royale du Bouftou",nn:"CEINTURE ROYALE DU BOUFTOU",t:10,d:"Cette magnifique ceinture augmente la vitalité de son porteur et lui permet de porter plus facilement ses objets. Elle si douce qu\'elle permet même de ligoter sa dulcinée lorsque celle-ci refuse de rester à la maison.",ep:1,g:31,l:52,wd:true,fm:true,w:10,s:4,p:7000};
I.u[2445] = {n:"Cape Bouffante Royale",nn:"CAPE BOUFFANTE ROYALE",t:17,d:"Cette cape portée par le grand Bouftou Royal confère, en plus d\'un style fabuleux, une force hors du commun.",ep:1,g:46,l:50,wd:true,fm:true,w:60,s:4,p:50000};
I.u[2446] = {n:"Gelocape",nn:"GELOCAPE",t:17,d:"Dépourvue de fils et de poils, cette Cape reste un mystère pour les têtes pensantes d\'Amakna, ce qui ne doit pas représenter plus d\'une dizaine de personnes.",ep:7,g:47,l:60,wd:true,fm:true,w:10,s:6,p:1000};
I.u[2447] = {n:"Gelocoiffe",nn:"GELOCOIFFE",t:16,d:"Avec cette coiffe sur la tête, vous pouvez être certain que vos amis rêveront enfin de vous lécher. Voilà un chapeau qui aurait comblé le Captain Chafer à la grande époque.",ep:5,g:43,l:60,wd:true,fm:true,w:23,s:6,p:9500};
I.u[2448] = {n:"Cervelle de Bouftou",nn:"CERVELLE DE BOUFTOU",t:15,d:"Aucun doute, cette Cervelle provient bien du Bouftou. Bien cuisiné, ce plat est le préféré de tous les petits Iops qui veulent devenir grands et forts. Ça rend aussi intelligent, mais si on le disait aux enfants Iops, ils en mangeraient beaucoup moins.",ep:1,g:492,l:9,wd:true,fm:true,w:1,p:1};
I.u[2449] = {n:"Armée de Tiques",nn:"ARMEE DE TIQUES",t:15,d:"Selon les chasseurs, une Armée de Tiques affamées pourrait dévorer toute la Laine d\'un Bouftou en une nuit. Quelques piliers de bar certifient même qu\'elle peut venir à bout de la magnifique crinière d\'un Sadida.",ep:1,g:493,l:9,wd:true,fm:true,w:1,p:1};
I.u[2451] = {n:"Sabot de Bouftou",nn:"SABOT DE BOUFTOU",t:47,d:"Autrefois utilisé comme sous-verre, certains alchimistes Eniripsas, assez coquets admettons le, recyclent le Sabot de Bouftou pour en faire des Bols. De quoi donner du goût à leurs céréales préférées.",ep:1,g:495,l:9,wd:true,fm:true,w:1,p:1};
I.u[2453] = {n:"Oeil de Bouftou",nn:"OEIL DE BOUFTOU",t:109,d:"Autrefois, on faisait de cet Oeil une soupe extra. Succulente à un point que l\'on a dû bannir la recette des livres, car bon nombre de chasseurs n\'allaient récupérer que les yeux et laissaient fumante la carcasse du pauvre Bouftou.",ep:1,g:497,l:9,wd:true,fm:true,w:1,p:1};
I.u[2454] = {n:"Estomac de Bouftou",nn:"ESTOMAC DE BOUFTOU",t:15,d:"Un Estomac de ce genre possède des qualités élastiques efficaces. Par contre, ne vous étonnez pas s\'il reste à l\'intérieur une bouillie de Trèfles, au contraire, réjouissez-vous Mesdemoiselles, cette bouillie est très bonne pour la peau.",ep:1,g:498,l:9,wd:true,fm:true,w:1,p:1};
I.u[2455] = {n:"Sang de Bouftou",nn:"SANG DE BOUFTOU",t:15,d:"Impossible d\'aller chasser sans se passer de ce Sang de Bouftou tellement son odeur attire les Bouftous les plus gros. Il attire aussi très efficacement les Moskitos sans que l\'on sache encore pourquoi.",ep:1,g:499,l:9,wd:true,fm:true,w:1,p:1};
I.u[2460] = {n:"Oeil de Boufton",nn:"OEIL DE BOUFTON",t:109,d:"Ce n\'est pas que ce Boufton garde un Oeil sur vous, mais vous avez quand même l\'impression qu\'il vous en veut.",ep:1,g:500,l:2,wd:true,fm:true,w:1,p:1};
I.u[2462] = {n:"Cervelle de Chef de Guerre Bouftou",nn:"CERVELLE DE CHEF DE GUERRE BOUFTOU",t:15,d:"Forcément, le Chef de Guerre Bouftou dont vous venez d\'extraire la Cervelle, va marcher beaucoup moins bien.",ep:1,g:501,l:19,wd:true,fm:true,w:1,p:1};
I.u[2463] = {n:"Dent de Chef de Guerre Bouftou",nn:"DENT DE CHEF DE GUERRE BOUFTOU",t:47,d:"Extraire une Dent de Chef de Guerre n\'est pas donnée à tout le monde, il faut l\'avouer. Notez que cette Dent a beaucoup plus de valeur si elle est abîmée. N\'allez pas me demander pourquoi, il paraît que c\'est une histoire d\'équilibre économique.",ep:1,g:502,l:19,wd:true,fm:true,w:1,p:1};
I.u[2464] = {n:"Queue de Chef de Guerre Bouftou",nn:"QUEUE DE CHEF DE GUERRE BOUFTOU",t:65,d:"Restez terré dans les Bois, observez patiemment, sans faire de bruit, et là, peut-être, vous verrez un Chef de Guerre Bouftou frétiller de la queue. Mais ce petit geste anodin n\'a lieu que pendant la période d\'accouplement.",ep:1,g:503,l:19,wd:true,fm:true,w:1,p:1};
I.u[2465] = {n:"Corne de Chef de Guerre Bouftou",nn:"CORNE DE CHEF DE GUERRE BOUFTOU",t:47,d:"Moins doux que son congénère, le Chef de Guerre Bouftou a plus de facilité pour embrocher les Srams et autres races téméraires. On peut souvent trouver des bouts de tissus attachés à leurs cornes, un peu comme une sorte de trophée.",ep:1,g:504,l:19,wd:true,fm:true,w:1,p:1};
I.u[2466] = {n:"Langue de Chef de Guerre Bouftou",nn:"LANGUE DE CHEF DE GUERRE BOUFTOU",t:15,d:"L\'expression " + "\"" + "avoir une haleine chargée" + "\"" + " tombe à pic pour le Chef de Guerre Bouftou. En effet, on a toujours le sentiment qu\'il a mangé un Trool pas frais.",ep:1,g:505,l:19,wd:true,fm:true,w:1,p:1};
I.u[2467] = {n:"Sabot de Chef de Guerre Bouftou",nn:"SABOT DE CHEF DE GUERRE BOUFTOU",t:47,d:"Selon les cordonniers, ce Sabot est l\'un des plus résistants et le plus malléable de la Province. Reste à savoir ce qu\'ils entendent par " + "\"" + "malléable " + "\"" + ". À trop les écouter, on pourrait comprendre qu\'ils peuvent confectionner des bottes à l\'aide de ce Sabot.",ep:1,g:538,l:18,wd:true,fm:true,w:1,p:1};
I.u[2468] = {n:"Estomac de Chef de Guerre Bouftou",nn:"ESTOMAC DE CHEF DE GUERRE BOUFTOU",t:15,d:"À l\'intérieur de cet Estomac se cache une surprise. Petit doigt d\'Enutrof, bout de nez d\'Osamodas... On pourrait quasiment se reconstruire un Héros avec tous ces morceaux.",ep:1,g:506,l:19,wd:true,fm:true,w:1,p:1};
I.u[2469] = {n:"Gelano",nn:"GELANO",t:9,d:"Inutile de vous ronger les ongles avec cet anneau, léchez-vous les doigts !",ep:7,g:47,l:60,wd:true,fm:true,w:5,s:6,p:10000};
I.u[2470] = {n:"Gelobottes",nn:"GELOBOTTES",t:11,d:"Ces bottes sont très confortables en été, car elles libèrent une odeur de fraise des bois, couvrant facilement l\'odeur de transpiration qui pourrait se dégager des orteils d\'un vieux Sadida aux pieds poilus.",ep:5,g:31,l:60,wd:true,fm:true,w:50,s:6,p:1000};
I.u[2471] = {n:"Geloture",nn:"GELOTURE",t:10,d:"Le plus dur avec cette Geloture, c\'est qu\'en cas de fringale, on est vite tenté de croquer dedans.",ep:7,g:32,l:60,wd:true,fm:true,w:15,s:6,p:1000};
I.u[2472] = {n:"Gelamu",nn:"GELAMU",t:1,d:"Voilà une amulette, qui trahira votre goût prononcé pour les orgies de Gelée. On peut trouver mieux pour se mettre en valeur ou briller en société, mais il faut assumer jusqu\'au bout votre faible pour la gelée.",ep:5,g:51,l:60,wd:true,fm:true,w:4,s:6,p:1000};
I.u[2473] = {n:"Cape de l\'Aventurier",nn:"CAPE DE L\'AVENTURIER",t:17,d:"Classe mais peu performante, cette cape comblera tout de même les jeunes aventuriers.",ep:1,g:48,l:1,wd:true,fm:true,w:7,s:5,p:100};
I.u[2474] = {n:"Chapeau de l\'Aventurier",nn:"CHAPEAU DE L\'AVENTURIER",t:16,d:"A part faire fuir les nanas, ce chapeau n\'a pas vraiment grande utilité.",ep:1,g:44,l:1,wd:true,fm:true,w:12,s:5,p:100};
I.u[2475] = {n:"Anneau de l\'Aventurier",nn:"ANNEAU DE L\'AVENTURIER",t:9,d:"Un petit anneau sympathique pour les débutants, et qui amusera les jeunes enfants, ou les Ecaflips.",ep:1,g:48,l:1,wd:true,fm:true,w:5,s:5,p:10};
I.u[2476] = {n:"Bottes de l\'Aventurier",nn:"BOTTES DE L\'AVENTURIER",t:11,d:"Bottes de simple facture mais idéales pour les jeunes se lançant sur le dur chemin de l\'aventure.",ep:1,g:32,l:1,wd:true,fm:true,w:15,s:5,p:10};
I.u[2477] = {n:"Ceinture de l\'aventurier",nn:"CEINTURE DE L\'AVENTURIER",t:10,d:"Une petite ceinture pour les aventuriers en herbe.",ep:1,g:33,l:1,wd:true,fm:true,w:6,s:5,p:10};
I.u[2478] = {n:"Amulette de l\'Aventurier",nn:"AMULETTE DE L\'AVENTURIER",t:1,d:"Pas très classe, pas très efficace, mais sympathique lorsque l\'on possède la panoplie.",ep:1,g:52,l:1,wd:true,fm:true,w:2,s:5,p:10};
I.u[2479] = {n:"Crête de Porkass",nn:"CRETE DE PORKASS",t:15,d:"Cette coiffure est signe de maturité chez le Porkass. C\'est pour cela que l\'on voit de temps en temps de jeunes Eniripsas en quête de puberté, se promener avec une crête en ville. Par contre, ils n\'osent pas vraiment aller dans les Plaines Rocheuses avec.",ep:13,g:507,l:18,wd:true,fm:true,w:1,p:1};
I.u[2480] = {n:"Slip en cuir de Porkass",nn:"SLIP EN CUIR DE PORKASS",t:56,d:"" + "\"" + "Un Porkass sans Slip est un Porkass Mort" + "\"" + ", telle était l\'expression favorite de Dame Harre, Féca adepte du voyeurisme et du bizutage. Son passe-temps préféré était d\'obliger ses " + "\"" + "amis" + "\"" + " à enfiler sur la tête le Slip pas très propre du Porkass.",ep:13,g:508,l:18,wd:true,fm:true,w:1,p:1};
I.u[2481] = {n:"Groin de Porkass",nn:"GROIN DE PORKASS",t:15,d:"Ce Groin, rose comme des fesses des nourrissons Osamodas, doit sûrement être utile pour une recette culinaire. Ou alors un joli collier de Groin, mais il faut quand même être courageux pour le porter et assumer les regards amusés des voisins.",ep:13,g:509,l:18,wd:true,fm:true,w:1,p:1};
I.u[2482] = {n:"Soulier de Porkass",nn:"SOULIER DE PORKASS",t:15,d:"Autrefois, un Soulier similaire avait été trouvé sur les marches d\'un grand palais royal. Des rumeurs raconteraient que ce Soulier fut perdu suite au départ pour le moins hâtif d\'une porkassette, qui eut peur de rentrer après les douze coups de minuit.",ep:13,g:510,l:18,wd:true,fm:true,w:2,p:1};
I.u[2483] = {n:"Dentier de Porkass",nn:"DENTIER DE PORKASS",t:47,d:"Certains Porkass naissent sans dents. D\'autres, après s\'être tapés une bonne tranche, se réveillent édentés, sans se souvenir de quoi que ce soit.",ep:13,g:511,l:30,wd:true,fm:true,w:1,p:1};
I.u[2484] = {n:"Trident Cassé",nn:"TRIDENT CASSE",t:15,d:"Voici un petit souvenir de votre ami Porkass. En espérant qu\'il ne vous en tienne pas rigueur au point de vous traquer à travers la Province. Caractériel comme un cochon, il en est bien capable, méfiez-vous.",ep:13,g:512,l:18,wd:true,fm:true,w:2,p:1};
I.u[2485] = {n:"Végacoiffe",nn:"VEGACOIFFE",t:16,d:"Le top du top de la mode, mais uniquement au nord-est d\'Amakna.",ep:1,g:45,l:40,wd:true,fm:true,w:25,s:10,p:4500};
I.u[2486] = {n:"Oreille de Porkass",nn:"OREILLE DE PORKASS",t:106,d:"Cette Oreille est un bon moyen de se faire respecter, elle fait office de trophée et prouve que vous domptez les Porkass à la baguette. Attention toutefois à ne pas vous la raconter auprès d\'autres Porkass, ils ne vous acclameront sûrement pas, eux.",ep:13,g:513,l:18,wd:true,fm:true,w:1,p:1};
I.u[2487] = {n:"Carapace de Scorbute",nn:"CARAPACE DE SCORBUTE",t:107,d:"Idéal pour confectionner de lourdes armures, le Scorbute était chassé jadis par la Milice de Brâkmar, car leur taux de reproduction faisait exploser tous les records. Même les Gelées n\'arrivaient pas à se reproduire aussi vite.",ep:14,g:514,l:36,wd:true,fm:true,w:1,p:1};
I.u[2488] = {n:"Viscères de Scorbute",nn:"VISCERES DE SCORBUTE",t:15,d:"Au premier abord, il est certain que ces viscères ne sont pas très appétissants. Et vous avez bien raison, les viscères de Scorbute sont infâmes, même cuisinés avec amour.",ep:14,g:445,l:36,wd:true,fm:true,w:1,p:1};
I.u[2489] = {n:"Vegacape",nn:"VEGACAPE",t:17,d:"Une cape élégante qui vous rendra irrésistible auprès des amateurs de belles plantes.",ep:1,g:49,l:58,wd:true,fm:true,w:15,s:10,p:1000};
I.u[2490] = {n:"Patte de Scorbute",nn:"PATTE DE SCORBUTE",t:103,d:"Il est peu commun de pouvoir récupérer une Patte de Scorbute dans un état aussi correct. En espérant que vous vous en servirez à bon escient, et non, pour crocheter des portes. Quoique...",ep:14,g:515,l:36,wd:true,fm:true,w:1,p:1};
I.u[2491] = {n:"Oeil d\'Ouginak",nn:"OEIL D\'OUGINAK",t:109,d:"Inquiétant, mais palpitant, cet Oeil d\'Ouginak ne reflète pas vraiment ce que l\'on attend. Par exemple, au lieu de vous voir dedans, vous voyez votre homologue Dopeul. Allez comprendre.",ep:14,g:516,l:51,wd:true,fm:true,w:1,p:1};
I.u[2492] = {n:"Chaîne de l\'Ouginak",nn:"CHAINE DE L\'OUGINAK",t:15,d:"Cette chienne de Chaîne provient évidemment d\'un Ouginak. Avant qu\'elle ne soit brisée, elle servait probablement à attacher l\'Ouginak pour éviter qu\'il s\'échappe.",ep:14,g:517,l:51,wd:true,fm:true,w:2,p:1};
I.u[2493] = {n:"Coeur d\'Ouginak",nn:"COEUR D\'OUGINAK",t:15,d:"Ce coeur vous restera fidèle. Tant que vous ne le vendrez, ni ne l\'échangerez, il restera avec vous dans le fond de votre sac. Quelle délicate attention.",ep:14,g:518,l:51,wd:true,fm:true,w:1,p:1};
I.u[2494] = {n:"Pommeau d\'Epée",nn:"POMMEAU D\'EPEE",t:15,d:"Il a sûrement fallu un coup surpuissant pour venir à bout de cette Épée mythique. En effet, l\'Épée d\'un Ouginak procure à celui qui l\'utilise, une envie de hurler de joie. Wa-hooooo !",ep:14,g:519,l:51,wd:true,fm:true,w:2,p:1};
I.u[2495] = {n:"Oeil de Kolérat",nn:"OEIL DE KOLERAT",t:109,d:"Cet Oeil brille la nuit, même une fois arraché à son propriétaire. De quoi s\'éclairer à moindre frais.",ep:2,g:520,l:9,wd:true,fm:true,w:1,p:1};
I.u[2496] = {n:"Queue de Kolérat",nn:"QUEUE DE KOLERAT",t:65,d:"Tranchée convenablement, la Queue de Kolérat a le goût de saucisson. Ce qui est étrange, c\'est que si vous coupez de trop grosses tranches, le goût de saucisson disparaît totalement. Encore une énigme culinaire de plus dans ce monde.",ep:2,g:521,l:9,wd:true,fm:true,w:1,p:1};
I.u[2497] = {n:"Oreille de Kolérat",nn:"OREILLE DE KOLERAT",t:106,d:"Portez-la à votre oreille, et vous entendrez la mer, parait-il.",ep:2,g:522,l:9,wd:true,fm:true,w:1,p:1};
I.u[2498] = {n:"Vegamu",nn:"VEGAMU",t:1,d:"La Vegamu a parfois tendance à attirer les Moskitos, qui de loin la prennent pour une copine. Ils sont souvent déçus une fois qu\'ils tombent sur le porteur de l\'amulette.",ep:1,g:53,l:40,wd:true,fm:true,w:20,s:10,p:1000};
I.u[2499] = {n:"Museau de Kolérat",nn:"MUSEAU DE KOLERAT",t:15,d:"Le Kolérat possède un flair hors du commun. Il n\'arrive pas à la cheville de l\'Ouginak, mais arrive à sentir celui-ci à trois cents mètres à la ronde. Et ça, c\'est vraiment très pratique pour éviter ses prédateurs.",ep:2,g:523,l:9,wd:true,fm:true,w:1,p:1};
I.u[2500] = {n:"Kolérat Mort",nn:"KOLERAT MORT",t:15,d:"Vous avez bien fait de le ramasser, il est intact. Mort, mais intact.",ep:2,g:524,l:9,wd:true,fm:true,w:10,p:1};
I.u[2501] = {n:"Queue de Croc Gland",nn:"QUEUE DE CROC GLAND",t:65,d:"Le Dieu Osamodas ne supporte pas que l\'on touche aux animaux sauvages : " + "\"" + "Ils sont si mignons" + "\"" + " hurle-t-il constamment. Il est marrant, ça se voit qu\'il n\'a jamais subi une ablation de la part du " + "\"" + "mignon" + "\"" + ".",ep:1,g:525,l:19,wd:true,fm:true,w:1,p:1};
I.u[2502] = {n:"Papatte de Croc Gland",nn:"PAPATTE DE CROC GLAND",t:103,d:"Les Papattes de Croc Gland sont assez prisées par les gastronomes du coin. Elle possède des vertus magiques, comme celle de porter chance à son possesseur. C\'est déjà pas mal.",ep:1,g:526,l:19,wd:true,fm:true,w:1,p:1};
I.u[2503] = {n:"Etoffe de Croc Gland",nn:"ETOFFE DE CROC GLAND",t:55,d:"Avec ceci, vous pourrez être aussi espiègle que le Croc Gland. Si vos rêves étaient d\'arborer une Étoffe sur votre tête et de vous déplacer de manière féline, eh bien, vous faites de biens drôles de rêves, soit dit entre nous.",ep:1,g:200,l:19,wd:true,fm:true,w:1,p:1};
I.u[2504] = {n:"Langue de Croc Gland",nn:"LANGUE DE CROC GLAND",t:15,d:"Cette Langue a dû traîner dans pas mal d\'endroits avant de se retrouver dans votre sac.",ep:1,g:527,l:19,wd:true,fm:true,w:1,p:1};
I.u[2505] = {n:"Croquette",nn:"CROQUETTE",t:15,d:"Pour attirer un Croc Gland dans les mailles de la domestication, lancez-lui une Croquette, ils en raffolent. Il ne vous l\'avouera pas, mais vous vous serez fait un ami pour la vie.",ep:1,g:528,l:19,wd:true,fm:true,w:1,p:1};
I.u[2506] = {n:"Grelot",nn:"GRELOT",t:15,d:"Muni d\'un Grelot, le Serpentin s\'entend à des Kilomètres à la ronde. Heureusement d\'ailleurs, car à deux mètres, on pourrait le manquer à cause de la végétation luxuriante des Plaines de Cania.",ep:13,g:529,l:10,wd:true,fm:true,w:1,p:1};
I.u[2507] = {n:"Langue Fourchue",nn:"LANGUE FOURCHUE",t:15,d:"Armé de cette Langue, vous pourrez fouetter vos amis. Ce sont vos collègues Osamodas et Sacrieurs qui vont être contents.",ep:13,g:530,l:10,wd:true,fm:true,w:1,p:1};
I.u[2508] = {n:"Venin de Serpentin",nn:"VENIN DE SERPENTIN",t:15,d:"Redoutable, ce Venin peut venir à bout d\'une armada de Bouftous enragés. Ils servaient d\'ailleurs autrefois à maîtriser les troupeaux, on en mettait un peu sur un bâton, et le tour était joué.",ep:13,g:531,l:10,wd:true,fm:true,w:1,p:1};
I.u[2509] = {n:"Dent de Serpentin",nn:"DENT DE SERPENTIN",t:47,d:"Cette Dent appartenait à un Serpentin, cela ne fait aucun doute. On peut le constater en observant de près le bout de la Dent qui s\'avère être on ne peut plus pointu.",ep:13,g:432,l:10,wd:true,fm:true,w:1,p:1};
I.u[2510] = {n:"Estomac de Sanglier des Plaines",nn:"ESTOMAC DE SANGLIER DES PLAINES",t:15,d:"Cet Estomac peut contenir des choses affolantes, une Iopette serait heureuse de voir toutes les Épées qu\'elle pourrait ranger dedans.",ep:13,g:532,l:44,wd:true,fm:true,w:1,p:1};
I.u[2511] = {n:"Cuir de Sanglier des Plaines",nn:"CUIR DE SANGLIER DES PLAINES",t:56,d:"Avec les propriétés de ce Cuir, les artisans cordonniers ont beaucoup hésité avant de confectionner des ceintures et des bottes. Le problème crucial était que ce Cuir ne sentait rien. Bon nombre de clients pensaient avoir affaire à du faux Cuir.",ep:2,g:410,l:44,wd:true,fm:true,w:10,p:1};
I.u[2512] = {n:"Queue de Sanglier des Plaines",nn:"QUEUE DE SANGLIER DES PLAINES",t:65,d:"Pas mal de braconniers Brâkmariens collectionnent ce trophé poilu. Encore aujourd\'hui, les raisons de ce pillage restent inconnues.",ep:13,g:536,l:44,wd:true,fm:true,w:1,p:1};
I.u[2513] = {n:"Etoffe de Sanglier des Plaines",nn:"ETOFFE DE SANGLIER DES PLAINES",t:55,d:"Contrairement à son Cuir, l\'Etoffe du Sanglier des Plaines dispose d\'un fumet radical contre le nez bouché. Pour preuve, cette odeur permettrait même, selon la légende, de réveiller un Sadida en hibernation.",ep:13,g:537,l:44,wd:true,fm:true,w:1,p:1};
I.u[2514] = {n:"Sabot de Sanglier des Plaines",nn:"SABOT DE SANGLIER DES PLAINES",t:47,d:"Généralement, les Sabots de Sanglier sont couverts de boue ou de terre, mais par moment, il ne faut pas s\'étonner d\'y dénicher des Biblops de toutes les couleurs. Oui, comme vous, le Sanglier des Plaines n\'aime pas les Biblops.",ep:13,g:539,l:44,wd:true,fm:true,w:1,p:1};
I.u[2515] = {n:"Groin de Sanglier des Plaines",nn:"GROIN DE SANGLIER DES PLAINES",t:15,d:"À première vue, ce Groin n\'est pas de toute fraîcheur. Et bien détrompez-vous, celui-ci est de loin le plus frais et tendre de tous les groins.",ep:13,g:540,l:44,wd:true,fm:true,w:1,p:1};
I.u[2516] = {n:"Défense de Sanglier des Plaines",nn:"DEFENSE DE SANGLIER DES PLAINES",t:47,d:"Cette défense vous rappellera que le Sanglier des Plaines est le plus antipathique des Sangliers. En effet, elle est aussi usée qu\'une Dent de Wabbit abîmée par les Cawottes.",ep:13,g:541,l:44,wd:true,fm:true,w:1,p:1};
I.u[2517] = {n:"Mégacape",nn:"MEGACAPE",t:17,d:"Cette cape appartenait à un célèbre héros, connu pour avoir combattu le terrible docteur Wily qui voulait envahir et dominer le monde des douze avec ses propres invocations. Le docteur Wily mourut avant que toutes ses invocations n\'eurent le temps d\'attaquer.",ep:1,g:50,l:45,wd:true,fm:true,w:2,s:76,p:5500};
I.u[2518] = {n:"Cape Vioutifoule",nn:"CAPE VIOUTIFOULE",t:17,d:"Silvia de Jodaw a cousu elle-même cette surprenante cape à partir de vieux rideaux. Heureusement, ça ne se voit pas trop, puisqu\'elle ressemble à s\'y méprendre à une serpillière bouillie dans du sang de Porkass.",ep:1,g:51,l:45,wd:true,fm:true,w:2,s:75,p:5500};
I.u[2519] = {n:"Mégacoiffe",nn:"MEGACOIFFE",t:16,d:"Pour assurer dans les situations les plus extrêmes, et briller en société, cette coiffe devrait faire l\'affaire.",ep:1,g:46,l:44,wd:true,fm:true,w:2,s:76,p:5200};
I.u[2520] = {n:"Casque Vioutifoule",nn:"CASQUE VIOUTIFOULE",t:16,d:"Ce casque a appartenu au bon vieux Jojo, célèbre héros de l\'ancien temps. Connu pour ses divers exploits, on a tout de même du mal à comprendre la passion qui anime ses fans.",ep:1,g:47,l:44,wd:true,fm:true,w:2,s:75,p:5200};
I.u[2521] = {n:"Mégabottes",nn:"MEGABOTTES",t:11,d:"Roll, la gentille sœur du propriétaire de ces bottes, était spécialisée en taxidermie astrale. Jadis, personne n\'était capable de comprendre la différence entre la taxidermie, et la taxidermie astrale. Mais on peut sans peine, imaginer de nos jours, que la taxidermie astrale permet de confectionner des créatures empaillées astrales. Si ça vous paraît évident maintenant, c\'est peut-être parce que vos ancêtres étaient stupides.",ep:1,g:33,l:43,wd:true,fm:true,w:2,s:76,p:5000};
I.u[2522] = {n:"Bottes Vioutifoule",nn:"BOTTES VIOUTIFOULE",t:11,d:"Ces bottes permettaient au célèbre Jojo d\'augmenter la puissance de son célèbre coup de pied rotatif, également connu sous le nom de " + "\"" + "coup rouge et chaud" + "\"" + ". On se demande toujours pourquoi il était appelé ainsi, puisque ses bottes étaient blanches.",ep:1,g:34,l:43,wd:true,fm:true,w:2,s:75,p:5000};
I.u[2523] = {n:"Mégature",nn:"MEGATURE",t:10,d:"Cette ceinture a été spécialement conçue pour protéger les hanches des héros. Elle ne vous sera donc d\'aucune utilité.",ep:1,g:34,l:45,wd:true,fm:true,w:2,s:76,p:5600};
I.u[2524] = {n:"Ceinture Vioutifoule",nn:"CEINTURE VIOUTIFOULE",t:10,d:"On raconte que cette ceinture aurait été fabriquée par le capitaine Bloue en personne. A peine croyable, et pourtant vous y croyez tout de même.",ep:1,g:35,l:46,wd:true,fm:true,w:2,s:75,p:5600};
I.u[2525] = {n:"Coeur du Maitre Corbac",nn:"COEUR DU MAITRE CORBAC",t:15,d:"Sans son Coeur, son ramage va en prendre un coup. Mais surtout ne lui dites pas, car le Maître Corbac est très susceptible.",ep:13,g:328,l:160,wd:true,fm:true,w:1,p:1};
I.u[2526] = {n:"Faux du Maitre Corbac",nn:"FAUX DU MAITRE CORBAC",t:15,d:"Cette faux était destinée à la base au fauchage du Iop, mais le Maître Corbac s\'est vite aperçu qu\'elle était si aiguisée, qu\'elle pouvait être utilisée pour faucher tout et n\'importe qui.",ep:1,g:557,l:160,wd:true,fm:true,w:10,p:1};
I.u[2527] = {n:"Clakoss",nn:"CLAKOSS",t:15,d:"Le Maître est plein de surprises, dans sa poche se tenait un Clakoss, célèbre fromage royal. Le fait qu\'il soit entamé prouve que le Maître Corbac l\'adore. Au niveau de l\'odeur, on ne peut que vous conseiller de ne pas le garder sur vous.",ep:13,g:558,l:160,wd:true,fm:true,w:1,p:1};
I.u[2528] = {n:"Ongle de Kanigrou",nn:"ONGLE DE KANIGROU",t:47,d:"Il faut être puissant et rapide pour récupérer un Ongle sur un Kanigrou. Puissant pour l\'extraire et rapide pour ne pas se prendre une baffe géante par l\'animal.",ep:13,g:559,l:70,wd:true,fm:true,w:1,p:1};
I.u[2529] = {n:"Potion de métal liquide",nn:"POTION DE METAL LIQUIDE",t:26,d:"Cette potion permet de redonner un peu de réalité à une lame éthérée.",ep:1,g:13,l:1,wd:true,fm:true,w:1,c:"Pj=17|Pj=31|Pj=14|Pj=11|Pj=20",p:10};
I.u[2530] = {n:"Carabottes",nn:"CARABOTTES",t:11,d:"Elles sont un peu lourdes, mais ces charmantes Carabottes feront de vous la reine de la nuit.",ep:12,g:35,l:60,wd:true,fm:true,w:50,s:9,p:1000};
I.u[2531] = {n:"Caracoiffe",nn:"CARACOIFFE",t:16,d:"Comme vous, la tortue a vraiment une toute petite tête. Ca tombe bien, ce chapeau est spécialement prévu pour ça.",ep:12,g:48,l:60,wd:true,fm:true,w:24,s:9,p:10000};
I.u[2532] = {n:"Caracape",nn:"CARACAPE",t:17,d:"Une cape pratique et résistante, qui ne fera pas de vous une célébrité, mais comme vous n\'en étiez pas une jusqu\'à maintenant, où est le problème ?",ep:12,g:52,l:60,wd:true,fm:true,w:15,s:9,p:10000};
I.u[2534] = {n:"Kwape de Glace",nn:"KWAPE DE GLACE",t:17,d:"Idéale pour se rafraîchir sur l\'île de Moon pendant la saison chaude, et parfaite pour refroidir les ardeurs de vos amis les plus entreprenants, cette cape saura répondre aux attentes de l\'aventurier des temps modernes.",ep:8,g:53,l:41,wd:true,fm:true,w:5,s:13,p:4600};
I.u[2535] = {n:"Kwakoiffe de Glace",nn:"KWAKOIFFE DE GLACE",t:16,d:"Cette Kwakoiffe de Glace vous permettra de toujours avoir des idées fraîches, même lorsque vous serez à court d\'idées. Elle n\'est cependant pas assez puissante pour un Iop.",ep:13,g:49,l:41,wd:true,fm:true,w:5,s:13,p:4600};
I.u[2537] = {n:"Kwakture de Glace",nn:"KWAKTURE DE GLACE",t:10,d:"Idéale pour vous raffermir les hanches, cette ceinture vous permettra de garder bien au frais ce que vous avez en dessous de la ceinture.",ep:12,g:36,l:41,wd:true,fm:true,w:5,s:13,p:4600};
I.u[2538] = {n:"Potion de métal lourd liquide",nn:"POTION DE METAL LOURD LIQUIDE",t:26,d:"Cette potion permet de redonner de la réalité à une lame éthérée.",ep:1,g:14,l:1,wd:true,fm:true,w:1,c:"Pj=17|Pj=31|Pj=14|Pj=11|Pj=20",p:10};
I.u[2539] = {n:"Potion de Copeaux de Bois",nn:"POTION DE COPEAUX DE BOIS",t:26,d:"Cette potion permet de redonner un peu de réalité à une oeuvre sculptée étherée.",ep:1,g:16,l:1,wd:true,fm:true,w:1,c:"Pj=13|Pj=19|Pj=18",p:10};
I.u[2540] = {n:"Potion de Copeaux de Bois Solides",nn:"POTION DE COPEAUX DE BOIS SOLIDES",t:26,d:"Cette potion permet de redonner de la réalité à une oeuvre sculptée étherée.",ep:1,g:17,l:1,wd:true,fm:true,w:1,c:"Pj=13|Pj=19|Pj=18",p:10};
I.u[2541] = {n:"Potion de métal précieux liquide",nn:"POTION DE METAL PRECIEUX LIQUIDE",t:26,d:"Cette potion permet de redonner une grande part de réalité à une lame éthérée.",ep:2,g:15,l:1,wd:true,fm:true,w:1,c:"Pj=17|Pj=31|Pj=14|Pj=11|Pj=20",p:10};
I.u[2542] = {n:"Potion de Foyer",nn:"POTION DE FOYER",t:12,d:"Cette Potion permet de retourner à n\'importe quel moment dans votre foyer.",ep:1,g:142,l:1,wd:true,fm:true,w:2,c:"CO=1",u:true,p:100};
I.u[2543] = {n:"Potion de Copeaux de Bois Rares",nn:"POTION DE COPEAUX DE BOIS RARES",t:26,d:"Cette potion permet de redonner une grande part de réalité à une oeuvre sculptée étherée.",ep:9,g:18,l:1,wd:true,fm:true,w:1,c:"Pj=13|Pj=19|Pj=18",p:10};
I.u[2544] = {n:"Corbalame",nn:"CORBALAME",t:6,d:"Terriblement tranchante et incroyablement classe, cette lame risque de faire des jaloux parmi votre entourage de Corbacs.",ep:1,g:32,l:45,wd:true,fm:true,w:25,an:14,e:[5,5,1,1,20,50,false,false],s:8,p:4500};
I.u[2545] = {n:"Corbottes",nn:"CORBOTTES",t:11,d:"Ces bottes donnent à leur porteur une impression d\'incroyable légèreté, mais aussi un air stupide.",ep:1,g:37,l:45,wd:true,fm:true,w:1,s:8,p:1000};
I.u[2546] = {n:"Corbacoiffe",nn:"CORBACOIFFE",t:16,d:"Grâce à cette magnifique coiffe, vous aurez au moins autant de classe que le tristement célèbre Maître Corbac.",ep:1,g:50,l:45,wd:true,fm:true,w:23,s:8,p:1000};
I.u[2547] = {n:"Corbacape Mastralis",nn:"CORBACAPE MASTRALIS",t:17,d:"Les meilleurs tailleurs d\'Amakna se sont associés jadis pour utiliser les restes de la dépouille du Maître Corbac afin d\'en faire une cape somptueuse. Avec une telle cape sur les épaules, le maître c\'est vous.",ep:13,g:56,l:127,wd:true,fm:true,w:30,p:41000};
I.u[2549] = {n:"Boîte de Kanigrou",nn:"BOITE DE KANIGROU",t:15,d:"Cette conserve contient du Kanigrou en morceaux, préalablement découpé puis mis en boîte. Très pratique à transporter, le Kanigrou en morceaux est vraiment le meilleur ami de l\'homme.",ep:13,g:560,l:70,wd:true,fm:true,w:1,p:1};
I.u[2550] = {n:"Etoffe de Kanigrou",nn:"ETOFFE DE KANIGROU",t:55,d:"Avec cette Étoffe, vous pourrez faire un substitut capillaire de toute beauté. On peut même dire que l\'on vous remarquera de loin, très loin.",ep:13,g:561,l:70,wd:true,fm:true,w:1,p:1};
I.u[2551] = {n:"Oreille de Kanigrou",nn:"OREILLE DE KANIGROU",t:106,d:"Voici le plat préféré de l\'Ouginak: Oreille de Kanigrou sauce nature. Mais traditionnellement, on la cuisine avec beaucoup de menthe sauvage et quelques Ongles pour le goût.",ep:13,g:562,l:70,wd:true,fm:true,w:1,p:1};
I.u[2552] = {n:"Oeil de Kanigrou",nn:"OEIL DE KANIGROU",t:109,d:"L\'oeil de Kanigrou est devenu un phénomène de mode, il est très courant de voir se promener des aventuriers avec l\'Oeil autour du cou.",ep:13,g:563,l:70,wd:true,fm:true,w:1,p:1};
I.u[2553] = {n:"Gros Boulet",nn:"GROS BOULET",t:15,d:"Voici un véritable Boulet. Lourd à souhait, il ne cessera jamais de vous perforer le crâne quand bon lui semble. Rien de plus ennuyeux qu\'un gros Boulet en quelque sorte.",ep:1,g:564,l:40,wd:true,fm:true,w:500,p:1};
I.u[2554] = {n:"Côtes de Porkass",nn:"COTES DE PORKASS",t:47,d:"Les Côtes de Porkass ont une saveur particulière. Certaines personnes en raffolent avec un jus épicé, d\'autres les préfèrent crues. Le Porkass quant à lui, préfèrera les vôtres. A chacun sa recette.",ep:13,g:159,l:40,wd:true,fm:true,w:1,p:1};
I.u[2555] = {n:"Selle en Cuir",nn:"SELLE EN CUIR",t:56,d:"À croire que le Porkass est douillet et qu\'il ne l\'assume pas, car sous ses fesses, était cachée une selle munie d\'un rembourrage de qualité. Une fois retravaillée, on pourrait faire de cette selle, d\'excellentes pantoufles.",ep:13,g:565,l:40,wd:true,fm:true,w:5,p:1};
I.u[2556] = {n:"Pollen de Blop",nn:"POLLEN DE BLOP",t:15,d:"Voici la source de toutes les allergies bénignes de ce monde. Le seul souci, c\'est que toutes les allergies mortelles commencent par une allergie bénigne. En gros, ne traînez pas avec ce Pollen dans vos poches si vous n\'en avez pas besoin.",ep:5,g:566,l:34,wd:true,fm:true,w:1,p:1};
I.u[2557] = {n:"Pétale de Blop",nn:"PETALE DE BLOP",t:35,d:"Aussi étrange que cela puisse paraître, les Pétales de Blops ne sont pas comestibles, aussi bien cuits que crus. Qui l\'eut cru ?",ep:5,g:567,l:34,wd:true,fm:true,w:1,p:1};
I.u[2558] = {n:"Blop Mort",nn:"BLOP MORT",t:15,d:"Vous avez bien fait de le ramasser, il est intact. Mort, mais intact.",ep:5,g:568,l:34,wd:true,fm:true,w:10,p:1};
I.u[2559] = {n:"Crinière de Trool",nn:"CRINIERE DE TROOL",t:15,d:"Une Crinière comme celle-ci, ça ne court pas les rues. Seuls les Enutrofs les plus riches peuvent se permettre de s\'en faire un substitut capillaire.",ep:4,g:569,l:115,wd:true,fm:true,w:1,p:1};
I.u[2560] = {n:"Epaulette de Trool",nn:"EPAULETTE DE TROOL",t:15,d:"Il vaut mieux observer cette Épaulette dans ces conditions que de l\'admirer encore accroché sur l\'épaule du Trool. Manquerait plus qu\'il vous fasse un défilé !",ep:4,g:570,l:115,wd:true,fm:true,w:5,p:1};
I.u[2561] = {n:"Bracelet de Force de Trool",nn:"BRACELET DE FORCE DE TROOL",t:15,d:"Comme si le Trool avait besoin d\'un Bracelet de Force... Cette incroyable machine à broyer les os en possède un à chaque poignet.",ep:4,g:571,l:115,wd:true,fm:true,w:5,p:1};
I.u[2562] = {n:"Slip de Trool",nn:"SLIP DE TROOL",t:56,d:"À voir l\'état de son Slip, le Trool s\'assoit un peu n\'importe où. C\'est aussi grâce à ce Slip que l\'on peut déterminer l\'âge de son propriétaire. A vu de nez, bien sûr...",ep:4,g:572,l:115,wd:true,fm:true,w:1,p:1};
I.u[2563] = {n:"Cale en bois du Tronknyde",nn:"CALE EN BOIS DU TRONKNYDE",t:38,d:"Pour le Tronknyde, cette Cale sert de pied, car le pied sert de Cale, malheureusement, le Tronknyde est toujours à la recherche d\'un cale-pied.",ep:9,g:573,l:35,wd:true,fm:true,w:2,p:1};
I.u[2564] = {n:"Sourcils de Tronknyde",nn:"SOURCILS DE TRONKNYDE",t:38,d:"Vous vouliez connaître la technique du Tronknyde pour effrayer les visiteurs imprudents ? Et bien,vous l\'avez dans les mains : une bonne grosse paire de Sourcils bien épais.",ep:9,g:574,l:35,wd:true,fm:true,w:2,p:1};
I.u[2565] = {n:"Tranche de Tronknyde",nn:"TRANCHE DE TRONKNYDE",t:15,d:"Après vous être taillé une bonne Tranche de ce Tronknyde, il doit bien tirer la tronche à présent.",ep:9,g:575,l:35,wd:true,fm:true,w:2,p:1};
I.u[2566] = {n:"Tranche Magique de Tronknyde",nn:"TRANCHE MAGIQUE DE TRONKNYDE",t:15,d:"Cette Tranche à l\'air différente des autres Tranches de Tronknyde. Au moment ou elle fut coupée, une lumière aveuglante a jailli du Tronknyde et a ensuite disparu, ne laissant au sol que la Tranche.",ep:9,g:576,l:35,wd:true,fm:true,w:2,p:1};
I.u[2567] = {n:"Anticipation",nn:"ANTICIPATION",t:29,d:"Cette bénédiction permet d\'être plus vif pour les prochains combats.",ep:1,g:6,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2568] = {n:"Reflexes Soushumains",nn:"REFLEXES SOUSHUMAINS",t:30,d:"Cette malédiction vous plonge dans un état de semi-conscience et vos réflexes en sont amoindris pour le prochain combat",ep:1,g:6,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2569] = {n:"Parchemin de Reflexes Surhumain",nn:"PARCHEMIN DE REFLEXES SURHUMAIN",t:14,d:"Ce Parchemin permet d\'être plus vif pour le prochain combat.",ep:1,g:28,l:1,wd:true,fm:true,w:1,ut:true,p:250};
I.u[2570] = {n:"Parchemin de Reflexes Soushumains",nn:"PARCHEMIN DE REFLEXES SOUSHUMAINS",t:14,d:"Ce Parchemin permet d\'être un brin inerte pour le prochain combat.",ep:1,g:29,l:1,wd:true,fm:true,w:1,ut:true,p:250};
I.u[2571] = {n:"Patte de Prespic",nn:"PATTE DE PRESPIC",t:103,d:"Comment un Prespic, et vous devez vous poser sûrement la même question, fait-il pour être aussi agile avec ces deux malheureux doigts de patte? Le mystère reste entier.",ep:1,g:577,l:18,wd:true,fm:true,w:1,p:1};
I.u[2572] = {n:"Estomac de Prespic",nn:"ESTOMAC DE PRESPIC",t:15,d:"" + "\"" + "À force de manger la même chose, tu vas finir par ressembler à ce que tu manges" + "\"" + ". Tel était la sage parole du cuisinier Bontarien Jon-kierre Poff. Voilà qui explique pourquoi le Prespic a l\'estomac verdi par le gazon ingéré.",ep:1,g:578,l:18,wd:true,fm:true,w:1,p:1};
I.u[2573] = {n:"Queue de Prespic",nn:"QUEUE DE PRESPIC",t:65,d:"Le Prespic fait usage de sa queue très rarement, et fort heureusement d\'ailleurs, car la puissance qui réside dans celle-ci se révèle surprenante. Certains bûcherons ont même vu des Prespic en pleine nuit trimbaler des rondins de Bois avec leurs queues.",ep:1,g:579,l:18,wd:true,fm:true,w:1,p:1};
I.u[2574] = {n:"Crinière de Prespic",nn:"CRINIERE DE PRESPIC",t:15,d:"Pour les soirées dites " + "\"" + "Vie-Ail-Pie" + "\"" + ", cette Crinière sera du plus bel effet. Emmenez un ami Osamodas et on vous prendra pour son invocation.",ep:1,g:580,l:18,wd:true,fm:true,w:1,p:1};
I.u[2575] = {n:"Scalp de Mulou",nn:"SCALP DE MULOU",t:15,d:"À votre arrivée, le Mulou avait certainement la même intention que vous, c\'est-à-dire de vous extraire avec plus ou moins de diplomatie votre Scalp. Le voilà pris de vitesse.",ep:1,g:581,l:70,wd:true,fm:true,w:1,p:1};
I.u[2576] = {n:"Scalp de Milimulou",nn:"SCALP DE MILIMULOU",t:15,d:"À votre arrivée, le Milimulou avait certainement la même intention que vous, c\'est-à-dire de vous extraire avec plus ou moins de délicatesse votre Scalp. Le voilà bien malin sans le haut de son crâne.",ep:1,g:582,l:22,wd:true,fm:true,w:1,p:1};
I.u[2577] = {n:"Scalp de Meulou",nn:"SCALP DE MEULOU",t:15,d:"À votre arrivée, le Meulou avait certainement la même intention que vous, c\'est-à-dire de vous extraire avec plus ou moins de tact votre Scalp. Le voilà prêt à attraper un rhume du cerveau.",ep:2,g:583,l:198,wd:true,fm:true,w:1,p:1};
I.u[2578] = {n:"Oeil de Mulou",nn:"OEIL DE MULOU",t:109,d:"Même arraché, le regard du Mulou reste porté sur vous. Et si vous regardez attentivement, vous aurez tendance à penser qu\'il vous suit des yeux, ou plutôt de l\'oeil.",ep:1,g:584,l:70,wd:true,fm:true,w:1,p:1};
I.u[2579] = {n:"Oeil du Milimulou",nn:"OEIL DU MILIMULOU",t:109,d:"Même arraché, le regard du Milimulou reste porté sur vous. Et si vous regardez attentivement, l\'oeil cligne encore des paupières. Quelle vitalité ce Milimulou.",ep:1,g:585,l:22,wd:true,fm:true,w:1,p:1};
I.u[2580] = {n:"Oeil de Meulou",nn:"OEIL DE MEULOU",t:109,d:"Même arraché, le regard du Meulou reste porté sur vous. Et si vous regardez attentivement, il fronce les sourcils quand vous le pointez du doigt. Décidément, le Meulou n\'est pas la pour la boutade.",ep:2,g:586,l:198,wd:true,fm:true,w:1,p:1};
I.u[2581] = {n:"Dents de Meulou",nn:"DENTS DE MEULOU",t:47,d:"Dans la famille des Mulous, si vous cherchez celui qui a les plus grandes dents, vous venez de faire bonne pioche. C\'est pour mieux vous baver dessus mes enfants.",ep:2,g:587,l:198,wd:true,fm:true,w:1,p:1};
I.u[2582] = {n:"Testicules de Meulou",nn:"TESTICULES DE MEULOU",t:15,d:"Si le prix des Testicules de Mulou est scandaleux, vous comprendrez que ce que vous avez en main vaut au bas mot une sacrée paire... de lingots.",ep:2,g:588,l:198,wd:true,fm:true,w:20,p:1};
I.u[2583] = {n:"Oeil de Crabe",nn:"OEIL DE CRABE",t:109,d:"Les marins d\'eau douce sont assez friands de cet Oeil de Crabe, car on peut en capturer sur la terre ferme.",ep:1,g:589,l:4,wd:true,fm:true,w:1,p:1};
I.u[2584] = {n:"Main de Boo",nn:"MAIN DE BOO",t:15,d:"Cette Main de Boo est recouverte de boue. À moins qu\'il n\'y ait rien sous cette boue, même pas la Main du Boo.",ep:2,g:590,l:19,wd:true,fm:true,w:1,p:1};
I.u[2585] = {n:"Coeur de Boue du Boo",nn:"COEUR DE BOUE DU BOO",t:15,d:"Muni de son Coeur de Boue, le Boo est dur à classer dans la chaîne alimentaire. En réfléchissant un peu, le Boo viendrait sûrement au bout de la chaîne.",ep:2,g:591,l:19,wd:true,fm:true,w:1,p:1};
I.u[2586] = {n:"Aile de Dragodinde Rousse Sauvage",nn:"AILE DE DRAGODINDE ROUSSE SAUVAGE",t:104,d:"Cette Aile sent le soufre. Tout bon éleveur de Dragodindes vous dira que c\'est la partie la plus tendre pour un barbeulcu. Assaisonnez tout de même la viande, car l\'Aile est la partie la plus fade.",ep:2,g:592,l:29,wd:true,fm:true,w:1,p:1};
I.u[2587] = {n:"Hachette de Bûcheron",nn:"HACHETTE DE BUCHERON",t:19,d:"Cette hachette est l\'outil indispensable à tout bûcheron. Non pas qu\'elle soit réellement utile, impossible de couper un arbre avec, mais sa présence à la ceinture les calme et les rassure.",ep:1,g:6,l:3,wd:true,fm:true,w:10,an:17,e:[5,5,1,1,30,30,false,false],c:"CV>2",p:20};
I.u[2588] = {n:"Aile de Dragodinde",nn:"AILE DE DRAGODINDE",t:104,d:"On peut dire que la Dragodinde domestique est bien meilleure que sa cousine sauvage. Déjà, elle n\'a pas un arrière-goût de soufre, mais la chair est goûteuse à souhait, ce qui en fait le plat favori de tous les petits.",ep:2,g:593,l:29,wd:true,fm:true,w:1,p:1};
I.u[2589] = {n:"Hachette Hévlalav",nn:"HACHETTE HEVLALAV",t:19,d:"Cette petite hache ne sait pas couper les arbres, mais peut servir à se défendre contre certains petits monstres. Par contre il vous faudra toujours courir si c\'est un Minotoror.",ep:1,g:7,l:6,wd:true,fm:true,w:10,an:17,e:[10,4,1,1,40,40,false,false],p:60};
I.u[2590] = {n:"Hache Vôr\'Om",nn:"HACHE VOR\'OM",t:19,d:"Cette série de haches fut conçue par un bwork apprenti forgeur. Elle est bien équilibrée et relativement puissante. Ce bwork venait de créer LA hache de guerre pour jeune aventurier.",ep:1,g:13,l:12,wd:true,fm:true,w:15,an:17,e:[10,4,1,1,40,40,false,false],c:"CV>10",p:120};
I.u[2591] = {n:"Queue de Dragodinde Rousse Sauvage",nn:"QUEUE DE DRAGODINDE ROUSSE SAUVAGE",t:65,d:"L\'intérêt de cette queue est simple: posez-la sur un rocher rond et faites tourner la Queue. Quand la Queue s\'arrête, suivez la direction indiquée par la pointe. Telle était la façon de se déplacer des Sadida pendant des millénaires.",ep:2,g:594,l:27,wd:true,fm:true,w:1,p:1};
I.u[2592] = {n:"Coupeuse de Bombu",nn:"COUPEUSE DE BOMBU",t:19,d:"Très légère et maniable, cette hache facilite les gestes amples nécessaires afin de couper le Bombu en toute sécurité.",ep:13,g:15,l:39,wd:true,fm:true,w:1,an:17,e:[10,5,1,1,40,40,false,false],c:"CV>20&CW>20",p:390};
I.u[2593] = {n:"Elagueuse d\'Oliviolet",nn:"ELAGUEUSE D\'OLIVIOLET",t:19,d:"Cette hache, faite d\'Oliviolet, est la hache la plus à même à résister aux ondes violettes de l\'Oliviolet. Combattre le mal par le mal en quelque sorte.",ep:14,g:14,l:42,wd:true,fm:true,w:20,an:17,e:[10,4,1,1,40,40,false,false],c:"CV>60",p:420};
I.u[2594] = {n:"Queue de Dragodinde",nn:"QUEUE DE DRAGODINDE",t:65,d:"Étrangement, la Queue de Dragodinde n\'a aucun pouvoir magique. A l\'instar d\'un Crocodaille, la Queue repousse rapidement. Une légende raconte aussi qu\'une Dragodinde avait poussé au bout de la Queue. Vous voilà prévenu.",ep:2,g:595,l:17,wd:true,fm:true,w:1,p:1};
I.u[2595] = {n:"Hache de Pierre",nn:"HACHE DE PIERRE",t:19,d:"Cette hache, très primitive, n\'en reste pas moins efficace. Un peu comme certains vieillards.",ep:5,g:2,l:51,wd:true,fm:true,w:20,an:17,e:[10,4,1,1,40,40,false,false],c:"CS>50&CV>60",p:510};
I.u[2596] = {n:"Lait de Dragodinde",nn:"LAIT DE DRAGODINDE",t:15,d:"Les éleveurs apprécient le fait de pouvoir traire leurs Dragodindes. Le seul problème, c\'est que ce Lait ne se vend pas, car il est infect. Il sert donc juste à nourrir les Dragodindonettes.",ep:2,g:424,l:29,wd:true,fm:true,w:1,p:1};
I.u[2597] = {n:"Hache de Brèche",nn:"HACHE DE BRECHE",t:19,d:"Cette longue hache se faufile dans les moindres interstices des arbres pour mieux les fendre en deux. Son utilité en combat est la même, l\'adversaire ayant toujours un interstice ou deux à exploiter.",ep:14,g:16,l:54,wd:true,fm:true,w:20,an:17,e:[10,5,1,1,40,40,false,false],c:"CV>70",p:540};
I.u[2598] = {n:"Pic de Dragodinde Rousse Sauvage",nn:"PIC DE DRAGODINDE ROUSSE SAUVAGE",t:47,d:"Ce Pic n\'est autre qu\'une protubérance osseuse. La Dragodinde s\'en sert tellement peu, que l\'on se demandait comment faisait-elle pour les abîmer autant. Depuis que les Biologistes l\'ont vu se frotter aux Trools, on comprend mieux.",ep:2,g:596,l:29,wd:true,fm:true,w:1,p:1};
I.u[2599] = {n:"Pic de Dragodinde",nn:"PIC DE DRAGODINDE",t:47,d:"Situé sur le dos de la Dragodinde, ce Pic pourra peut-être vous servir à vous gratter l\'entrejambe, si vous parvenez à monter une Dragodinde.",ep:2,g:597,l:29,wd:true,fm:true,w:1,p:1};
I.u[2600] = {n:"Coupeuse de Sifflet",nn:"COUPEUSE DE SIFFLET",t:19,d:"Cette grosse hache fait un bruit monstrueux lorsqu\'elle s\'abat sur quelque chose, ce qui a souvent pour effet de provoquer le silence autour de soi. C\'est toujours ça de pris.",ep:14,g:17,l:66,wd:true,fm:true,w:22,an:17,e:[10,4,1,1,40,40,false,false],c:"CS>30&CV>30&CA>30",p:660};
I.u[2601] = {n:"Hache Darsson",nn:"HACHE DARSSON",t:19,d:"Darsson était un vaillant Enutrof adroit comme ses pieds. Faisant souvent tomber sa pelle en combat, il eût l\'ingénieuse idée d\'attacher une hache de rechange à sa ceinture. Cela lui sauva la vie plusieurs fois, jusqu\'à ce qu\'il fasse tomber sa hache...",ep:14,g:18,l:57,wd:true,fm:true,w:15,an:17,e:[12,4,1,1,30,30,false,false],c:"CC>20&CA>20&CV>20",p:570};
I.u[2602] = {n:"Pétale de Trukikol",nn:"PETALE DE TRUKIKOL",t:35,d:"Pas plus gros qu\'un Oeil de Crabe, le Pétale de Trukikol est constitué de plusieurs bulles collantes et spongieuses au toucher. Il sert le plus souvent à colmater les trous dans les toits après les rigoureuses tempêtes de fin d\'année.",ep:12,g:598,l:12,wd:true,fm:true,w:1,p:1};
I.u[2603] = {n:"Trancheuse de Partie",nn:"TRANCHEUSE DE PARTIE",t:19,d:"Cette hache est très utile pour séparer certaines parties récalcitrantes d\'un arbre, notamment les petites boursoufflures du tronc, ceci dans le but d\'avoir un arbre parfaitement lisse et rond.",ep:13,g:2,l:66,wd:true,fm:true,w:22,an:17,e:[10,4,1,1,40,40,false,false],c:"CV>50&CS>30",p:660};
I.u[2604] = {n:"Bardiche du Milicien",nn:"BARDICHE DU MILICIEN",t:19,d:"Cette bardiche est principalement employée par les miliciens d\'Amakna. Sa longueur et son efficacité doit probablement compenser d\'autres choses.",ep:13,g:3,l:78,wd:true,fm:true,w:30,an:17,e:[15,5,1,1,40,40,false,false],c:"CV>30&CS>30&CW>30&CC>5",p:780};
I.u[2605] = {n:"Peau de Trukikol",nn:"PEAU DE TRUKIKOL",t:59,d:"Ne soyez pas étonné si l\'on vous dit que la Peau de Trukikol colle. Grâce à elle, on arrive à tout faire tenir au plafond. Il faut avouer que c\'est aussi grâce au paysan qui a breveté l\'idée : un dénommé Roulio de Scautche.",ep:12,g:599,l:12,wd:true,fm:true,w:1,p:1};
I.u[2606] = {n:"Hachette Savante",nn:"HACHETTE SAVANTE",t:19,d:"Sur cette grosse hachette, de multiples inscriptions gravées rappellent des notions Amakniennes de base. Son inventeur Claus Ansipedi désirait donner un semblant de culture aux brûtasses finies...peine perdue, il est mort le crâne fendu.",ep:4,g:4,l:81,wd:true,fm:true,w:30,an:17,e:[10,4,1,1,40,40,false,false],c:"CV>50&CA>50&CI>50",p:810};
I.u[2607] = {n:"Trukikol Mort",nn:"TRUKIKOL MORT",t:15,d:"Vous avez bien fait de le ramasser, il est intact. Mort, mais intact.",ep:12,g:600,l:14,wd:true,fm:true,w:10,p:1};
I.u[2608] = {n:"Francique Basquaise",nn:"FRANCIQUE BASQUAISE",t:19,d:"Cette hache fait souvent le tour du monde en commençant par le bas. Et pour cause c\'est dans le sud d\'Amakna que ce type de hache est forgée, par une petite guilde de chasseurs de trésors réputée pour leur efficacité.",ep:18,g:1,l:84,wd:true,fm:true,w:18,an:17,e:[10,4,1,1,40,40,false,false],c:"CV>100&CS>50",p:840};
I.u[2609] = {n:"Carapace Verte Vide",nn:"CARAPACE VERTE VIDE",t:107,d:"Mais où se cache le propriétaire ? Il a dû sortir s\'acheter du pain avec de la tomate dessus.",ep:7,g:601,l:24,wd:true,fm:true,w:2,p:1};
I.u[2610] = {n:"Carapace Rouge Vide",nn:"CARAPACE ROUGE VIDE",t:107,d:"Mais ou se cache le propriétaire? il a dû déménager depuis peu.",ep:7,g:602,l:24,wd:true,fm:true,w:2,p:1};
I.u[2611] = {n:"Carapace Jaune Vide",nn:"CARAPACE JAUNE VIDE",t:107,d:"Mais ou se cache le propriétaire ? il fait peut-être une sieste à l\'intérieur.",ep:7,g:603,l:24,wd:true,fm:true,w:2,p:1};
I.u[2612] = {n:"Vouge à Poisson",nn:"VOUGE A POISSON",t:19,d:"Cette longue hache est utilisée par les marins experts pour affronter les poissons géants qui attaquent leurs navires. On imagine assez bien les dégâts qu\'elle peut faire contre des monstres plus petits.",ep:14,g:4,l:87,wd:true,fm:true,w:32,an:17,e:[15,5,1,1,40,40,false,false],c:"CV>50&CA>50",p:870};
I.u[2613] = {n:"Carapace Bleue Vide",nn:"CARAPACE BLEUE VIDE",t:107,d:"Mais où se cache le propriétaire ? il doit être sous la douche.",ep:7,g:604,l:24,wd:true,fm:true,w:2,p:1};
I.u[2614] = {n:"Hache de Dame Oise",nn:"HACHE DE DAME OISE",t:19,d:"La Dame Oise était une ravissante princesse qui respirait la candeur et la fraîcheur. Tout le monde fut bien surpris lorsque, lors d\'un raid Bwork sur sa cité, elle sortit cette hache et fracassa à elle seule la moitié de l\'armée adverse.",ep:18,g:5,l:90,wd:true,fm:true,w:28,an:17,tw:true,e:[10,4,1,1,40,40,false,false],c:"CV>150&CW>50&CS>250",p:900};
I.u[2615] = {n:"Hache de Lookabeer",nn:"HACHE DE LOOKABEER",t:19,d:"Cette hache est réputée pour son efficacité à terminer définitivement et très rapidement une querelle entre Enutrofs ivres.",ep:18,g:8,l:96,wd:true,fm:true,w:23,an:17,e:[10,5,1,1,40,40,false,false],c:"CV>120",p:960};
I.u[2616] = {n:"Halle de Barde",nn:"HALLE DE BARDE",t:19,d:"Cette hache tire son nom du test qui fut utilisé pour prouver sa terrible efficacité : une hache géante, un mythophobiaque et une halle remplie de bardes.",ep:14,g:7,l:99,wd:true,fm:true,w:40,an:17,e:[15,5,1,1,40,40,false,false],c:"CV>100&CS>100",p:990};
I.u[2617] = {n:"Tranche de Nodkoko",nn:"TRANCHE DE NODKOKO",t:15,d:"Haaa... Enfin un élément qui vous fait penser aux vacances. Cette Agence de Voyages ne paie rien pour attendre.",ep:12,g:605,l:14,wd:true,fm:true,w:1,p:1};
I.u[2618] = {n:"Kokopaille",nn:"KOKOPAILLE",t:15,d:"Pour siroter quelque chose de frais, rien de mieux qu\'une paille colorée. Il n\'y a plus qu\'à trouver un trou dans la Nodkoko.",ep:12,g:433,l:14,wd:true,fm:true,w:1,p:1};
I.u[2619] = {n:"Kokolait",nn:"KOKOLAIT",t:15,d:"En voilà une bonne idée: avoir du Kokolait en bouteille, ça permet d\'en boire quand on veut et où que l\'on soit.",ep:12,g:423,l:14,wd:true,fm:true,w:1,p:1};
I.u[2620] = {n:"Rhum de Qualité",nn:"RHUM DE QUALITE",t:15,d:"Ce Rhum doit sortir d\'une cave très prisée. À moins qu\'il soit resté dans une poche pendant de bonnes générations.",ep:12,g:177,l:94,wd:true,fm:true,w:1,p:1};
I.u[2621] = {n:"Barbe du Chouque",nn:"BARBE DU CHOUQUE",t:15,d:"Vous venez d\'extraire du menton du Chouque sa Barbe. Bizarrement, les Dents sont venues avec. C\'est ça d\'être mort depuis longtemps, on perd des bouts.",ep:12,g:606,l:94,wd:true,fm:true,w:1,p:1};
I.u[2622] = {n:"Boucle du Chouque",nn:"BOUCLE DU CHOUQUE",t:15,d:"Du bout de cette Boucle, sept générations de pirates vous contemplent.",ep:12,g:607,l:94,wd:true,fm:true,w:2,p:1};
I.u[2623] = {n:"Bwak Squelettique",nn:"BWAK SQUELETTIQUE",t:15,d:"Les éleveurs ont toujours cru que les Bwaks s\'envolaient très loin à la fin de leur vie. Toutefois, ce Bwak est une exception à la règle.",ep:12,g:608,l:94,wd:true,fm:true,w:1,p:1};
I.u[2624] = {n:"Feuille de Kokoko",nn:"FEUILLE DE KOKOKO",t:36,d:"De hardis chasseurs veulent coûte que coûte et quoiqu\'il en coûte, récupérer en guise de souvenirs une Feuille de Kokoko, pour ensuite ne jamais revenir sur cette île maudite. Pourquoi pas ?",ep:12,g:609,l:42,wd:true,fm:true,w:1,p:1};
I.u[2625] = {n:"Poupée Vaudou Thierry",nn:"POUPEE VAUDOU THIERRY",t:15,d:"Une force se dégage de cette Poupée, mais vous avez l\'étrange sensation qu\'elle appelle quelqu\'un.",ep:12,g:610,l:51,wd:true,fm:true,w:2,p:1};
I.u[2626] = {n:"Poupée Vaudou Jav",nn:"POUPEE VAUDOU JAV",t:15,d:"Une force se dégage de cette Poupée, mais vous avez l\'étrange sensation qu\'elle cherche quelqu\'un.",ep:12,g:611,l:51,wd:true,fm:true,w:2,p:1};
I.u[2627] = {n:"Poupée Vaudou Sarbak",nn:"POUPEE VAUDOU SARBAK",t:15,d:"Une force se dégage de cette Poupée, mais vous avez l\'étrange sensation qu\'elle écoute quelqu\'un.",ep:12,g:612,l:51,wd:true,fm:true,w:2,p:1};
I.u[2628] = {n:"Poupée Vaudou Archer",nn:"POUPEE VAUDOU ARCHER",t:15,d:"Une force se dégage de cette Poupée, mais vous avez l\'étrange sensation qu\'elle attend quelqu\'un.",ep:12,g:613,l:51,wd:true,fm:true,w:2,p:1};
I.u[2629] = {n:"Caracape Minotoris",nn:"CARACAPE MINOTORIS",t:17,d:"Cette cape, d\'allure royale, constitue pourtant une incroyable protection. C\'est ce modèle qui a fait la réputation d\'une célèbre tailleuse, juste avant de mourir de son succès, écrasée par la foule.",ep:13,g:55,l:121,wd:true,fm:true,w:10,p:38500};
I.u[2630] = {n:"Pot de Gloutovore",nn:"POT DE GLOUTOVORE",t:15,d:"À la base, le Gloutovore n\'est pas très généreux. Avec ce Pot rempli à raz bord, vous voila bien chanceux. Reste plus qu\'à lui trouver une utilité.",ep:12,g:614,l:44,wd:true,fm:true,w:1,p:1};
I.u[2631] = {n:"Estomac de Gloutovore",nn:"ESTOMAC DE GLOUTOVORE",t:15,d:"Voici l\'estomac le plus résistant que vous puissiez voir. Extensible à souhait, vous pourriez même vous cacher dedans si vous le souhaitiez.",ep:12,g:615,l:44,wd:true,fm:true,w:1,p:1};
I.u[2632] = {n:"Glouto Rhum",nn:"GLOUTO RHUM",t:15,d:"Cette Bouteille a été laissée à l\'abandon à l\'intérieur d\'un Gloutovore. Quel gâchis ! Par contre, il a l\'air d\'avoir bien vieilli, il a gagné en arôme. Si les Paysans savaient ça, il y aurait des entrepôts remplis de Gloutovore.",ep:12,g:177,l:44,wd:true,fm:true,w:1,p:1};
I.u[2633] = {n:"Rhum Fourbe",nn:"RHUM FOURBE",t:15,d:"Alors là, c\'est le piège. La Fourbasse est tellement fourbe qu\'elle a rempli une bouteille de Rhum avec du... bah oui du Rhum, mais il a une odeur repérable à dix mille lieues : le Trukikol séché.",ep:12,g:177,l:24,wd:true,fm:true,w:1,p:1};
I.u[2634] = {n:"Lance Cassée",nn:"LANCE CASSEE",t:15,d:"Sans sa Lance, la Fourbasse fera moins la maligne. Reste à savoir ce que vous allez en faire.",ep:12,g:616,l:24,wd:true,fm:true,w:2,p:1};
I.u[2635] = {n:"Pain Gre",nn:"PAIN GRE",t:33,d:"Cette recette a été élaborée par un rival de Xavier le Boulanger, à savoir Danioul Garnioul le Boulanger. Son nom était si stupide que ses clients ne cessaient de se moquer de lui, il se vengeait en pétrissant avec ses pieds.",ep:1,g:13,l:80,wd:true,fm:true,w:1,u:true,ut:true,p:1};
I.u[2636] = {n:"Pain Tahde",nn:"PAIN TAHDE",t:33,d:"Ce pain renferme de délicieuses lichettes de viande, qui ont la saveur et l\'apparence de volaille, mais méfiance il n\'en n\'est rien. En y regardant de plus près, on peut sentir une légère odeur de Tofu.",ep:1,g:13,l:100,wd:true,fm:true,w:1,u:true,ut:true,p:1};
I.u[2637] = {n:"La Griffe",nn:"LA GRIFFE",t:6,d:"Cette lame recourbée, faite d\'os, de cartilage et de corne, est aussi tranchante qu\'une griffe d\'Ecaflip. En un peu plus gros et plus propre.",ep:13,g:33,l:35,wd:true,fm:true,w:10,an:14,e:[10,4,1,1,30,50,false,false],c:"CV>10&CS>10&CA>10",p:3500};
I.u[2638] = {n:"La Griffe Aiguisée",nn:"LA GRIFFE AIGUISEE",t:6,d:"Cent pour cent naturelle, cette épée est comme une sixième griffe pour une main Ecaflip. A ce propos, faites attention à ne pas vous grattouiller le museau avec cette griffe là !",ep:14,g:34,l:68,wd:true,fm:true,w:12,an:14,e:[12,4,1,1,30,50,false,false],c:"CV>30&CA>30&CS>30",p:6800};
I.u[2639] = {n:"Fausse Griffe de Ceangal",nn:"FAUSSE GRIFFE DE CEANGAL",t:6,d:"Une forgeuse écaflipette eut l\'idée de forger cette épée après son échec à maitriser la technique ancestrale du même nom. Elle se disait qu\'au moins cette griffe ne lui ferait pas faux bond.",ep:14,g:35,l:95,wd:true,fm:true,w:10,an:14,tw:true,e:[10,4,1,1,40,50,false,false],c:"CV>50&CA>50&CS>50",p:9500};
I.u[2640] = {n:"Bâton Carnivore",nn:"BATON CARNIVORE",t:4,d:"Etrangement sculpté, ce Bâton ne provient pas du coin. Il sent les vieux endroits humides et sombres de Brâkmar.",ep:1,g:24,l:30,wd:true,fm:true,w:56,an:12,e:[3,5,1,1,20,50,false,false],s:7,p:1000};
I.u[2641] = {n:"Toady",nn:"TOADY",t:16,d:"Rouge, blanc et surtout somptueusement classe, ce chapeau vous permettra d\'assurer un max lors des soirées de l\'ambassadeur qui sont toujours une réussite.",ep:1,g:51,l:30,wd:true,fm:true,w:10,s:7,p:2700};
I.u[2642] = {n:"Mâchoire de Rib",nn:"MACHOIRE DE RIB",t:47,d:"Déjà que le Rib est un tantinet maigrelet, alors s\'il lui manque sa Mâchoire, il va se retrouver aussi offensif qu\'une armée de tiques en hibernation.",ep:3,g:617,l:33,wd:true,fm:true,w:1,p:1};
I.u[2643] = {n:"Défense de Dragon Cochon",nn:"DEFENSE DE DRAGON COCHON",t:47,d:"Cette défense appartient à l\'un des plus célèbres prédateurs des montagnes d\'Amakna: le Dragon Cochon. Des chasseurs seraient prêts à revendre leur famille entière pour posséder ce que vous avez en main. C\'est pour vous l\'occasion rêvée d\'acheter une famille à moindre frais.",ep:2,g:618,l:70,wd:true,fm:true,w:1,p:1};
I.u[2644] = {n:"Queue de Dragon Cochon",nn:"QUEUE DE DRAGON COCHON",t:65,d:"Un régal que ce gros morceau de Queue de Dragon Cochon. Faites le sauter à la poële avec un peu de Graine de Sésame, le tout avec une Sauce hisson, et vos invités ne partiront plus de chez vous. N\'est-ce pas super chouette ? Non ? Bon...",ep:2,g:619,l:70,wd:true,fm:true,w:1,p:1};
I.u[2645] = {n:"Groin de Dragon Cochon",nn:"GROIN DE DRAGON COCHON",t:15,d:"À vue de Groin, le Dragon Cochon n\'a pas senti le coup venir. C\'est dommage pour lui.",ep:2,g:620,l:70,wd:true,fm:true,w:1,p:1};
I.u[2646] = {n:"Slip en cuir de Cochon de Lait",nn:"SLIP EN CUIR DE COCHON DE LAIT",t:56,d:"Ce slip bien large va à ravir au Cochon de Lait, les coutures sont élastiques et renforcées, pour une plus grande efficacité lors des combats rapprochés. Comme l\'exige la tradition, ce slip ne doit jamais être lavé, sous peine de perdre ses propriétés intrinsèques encore inconnues et redoutées de nos jours.",ep:1,g:621,l:20,wd:true,fm:true,w:1,p:1};
I.u[2647] = {n:"Oreille de Cochon de Lait",nn:"OREILLE DE COCHON DE LAIT",t:106,d:"Le Cochon de Lait n\'est pas chassé généralement pour ses Oreilles, mais plutôt pour son ivoire. Mais bon, à défaut d\'Ivoire....",ep:1,g:623,l:20,wd:true,fm:true,w:1,p:1};
I.u[2648] = {n:"Griffes de Kwak",nn:"GRIFFES DE KWAK",t:15,d:"Le Kwak n\'a rien d\'un charognard, mais il possède tout de même d\'étranges dépôts sous ses griffes qu\'il ne vaut mieux pas décrire.",ep:1,g:624,l:30,wd:true,fm:true,w:1,p:1};
I.u[2649] = {n:"Ver de Terre",nn:"VER DE TERRE",t:15,d:"Ce Ver est le plat préféré des Kwaks sauvages. Ce qui est plus étrange, c\'est qu\'il est resté vivant alors qu\'il a été extrait de l\'Estomac du Kwak.",ep:5,g:625,l:30,wd:true,fm:true,w:1,p:1};
I.u[2650] = {n:"Ver d\'Eau",nn:"VER D\'EAU",t:15,d:"Ce Ver est le plat préféré des Kwaks sauvages. Ce qui est plus étrange, c\'est qu\'il est resté vivant alors qu\'il a été extrait de l\'estomac du Kwak.",ep:1,g:626,l:30,wd:true,fm:true,w:1,p:1};
I.u[2651] = {n:"Ver de Feu",nn:"VER DE FEU",t:15,d:"Ce Ver est le plat préféré des Kwaks sauvages. Ce qui est plus étrange, c\'est qu\'il est resté vivant alors qu\'il a été extrait de l\'Estomac du Kwak.",ep:5,g:627,l:30,wd:true,fm:true,w:1,p:1};
I.u[2652] = {n:"Ver d\'Air",nn:"VER D\'AIR",t:15,d:"Ce Ver est le plat préféré des Kwaks sauvages. Ce qui est plus étrange, c\'est qu\'il est resté vivant alors qu\'il a été extrait de l\'Estomac du Kwak.",ep:5,g:628,l:30,wd:true,fm:true,w:1,p:1};
I.u[2653] = {n:"Arakne Majeure Morte",nn:"ARAKNE MAJEURE MORTE",t:15,d:"Vous avez bien fait de la ramasser, elle est intacte. Morte, mais intacte.",ep:1,g:629,l:43,wd:true,fm:true,w:10,p:1};
I.u[2654] = {n:"Oeuf d\'Arakne Majeure",nn:"OEUF D\'ARAKNE MAJEURE",t:105,d:"Le problème avec les Araknes, c\'est que l\'on peut se retrouver avec plusieurs générations en quelques jours avec la chaleur. Donc si le temps est caniculaire, ne vous promenez pas trop dans la Fôret des Abraknydes.",ep:1,g:630,l:43,wd:true,fm:true,w:1,p:1};
I.u[2655] = {n:"Ceinture Corbaquiale",nn:"CEINTURE CORBAQUIALE",t:10,d:"Cette ceinture a été confectionnée à partir des restes du Maître Corbac, soigneusement cousus entre eux. Vous devriez vous sentir respecté une fois équipé de celle-ci. Le Chouque a même accepté de prêter sa boucle pour l\'occasion.",ep:13,g:56,l:120,wd:true,fm:true,w:10,p:36938};
I.u[2656] = {n:"Fil Résistant",nn:"FIL RESISTANT",t:15,d:"Si les Cordonniers ainsi que les Tailleurs connaissaient l\'existence de ce Fil Résistant, il y aurait la Semaine du Fil Résistant, la Foire du Fil Résistant ainsi que le jour saint du Fil Résistant.",ep:1,g:631,l:43,wd:true,fm:true,w:1,p:1};
I.u[2659] = {n:"Pétale de Tournesol Sauvage",nn:"PETALE DE TOURNESOL SAUVAGE",t:35,d:"Ce Pétale bouge. Oui, il bouge en fonction du soleil. Mettez le dans votre poche droite le matin, il sera dans votre poche gauche le soir venu.",ep:1,g:632,l:12,wd:true,fm:true,w:1,p:1};
I.u[2661] = {n:"Feuille de Tournesol Sauvage",nn:"FEUILLE DE TOURNESOL SAUVAGE",t:36,d:"Cette partie du Tournesol n\'est pas que la Feuille de la plante, c\'est aussi sa voûte plantaire. Ne vous étonnez donc pas si elle sent des pieds.",ep:1,g:633,l:12,wd:true,fm:true,w:1,p:1};
I.u[2662] = {n:"Bave de Rose Démoniaque",nn:"BAVE DE ROSE DEMONIAQUE",t:15,d:"Une Rose enragée a la fâcheuse habitude de baver sur quiconque ose s\'approcher. Mais il paraît que c\'est un signe d\'affection. Une Rose Démoniaque enragée et affective qui plus est, on aura tout vu.",ep:1,g:634,l:8,wd:true,fm:true,w:1,p:1};
I.u[2663] = {n:"Engrais",nn:"ENGRAIS",t:15,d:"Toute belle plante a besoin d\'Engrais. Les Enutrofettes sur le déclin peuvent donc s\'en passer.",ep:1,g:635,l:8,wd:true,fm:true,w:1,p:1};
I.u[2664] = {n:"Os de Crocodaille",nn:"OS DE CROCODAILLE",t:47,d:"Cet os verdâtre et pour le moins étrange, provient d\'une patte de Crocodaille. Ce dernier a sûrement dû verser des larmes de Crocodaille quand il a compris qu\'il avait une patte en moins.",ep:4,g:636,l:36,wd:true,fm:true,w:2,p:100};
I.u[2665] = {n:"Herbe Folle",nn:"HERBE FOLLE",t:36,d:"Voici le genre d\'Herbe que l\'on met dans les enclos pour Bouftous. Cette Herbe est folle car de temps à autres, elle se lève et s\'échappe de l\'enclos, ce qui n\'est pas commun pour une touffe d\'herbe.",ep:1,g:637,l:9,wd:true,fm:true,w:1,p:1};
I.u[2666] = {n:"Sabot du Minotoror",nn:"SABOT DU MINOTOROR",t:47,d:"Il a dû écraser pas mal d\'aventuriers avec ce Sabot. Maintenant, il doit moins faire le malin à danser sur une seule patte. Quoique cela va lui donner une démarche encore plus inquiétante qu\'avant.",ep:2,g:800,l:125,wd:true,fm:true,w:1,p:1};
I.u[2667] = {n:"Queue de Minotoror",nn:"QUEUE DE MINOTOROR",t:65,d:"Cela relève tout de même de l\'exploit de voir une Queue de Minotoror, sans le Minotoror qui va avec. Le pauvre doit chercher partout après. A moins qu\'il ne s\'en soit toujours pas rendu compte, ce qui n\'est pas impossible.",ep:2,g:640,l:125,wd:true,fm:true,w:1,p:1};
I.u[2669] = {n:"Chauve souris",nn:"CHAUVE SOURIS",t:15,d:"Parfois, les Vampires se transforment en Chauve Souris, pour mieux fuir. Mais ils se sentent obligés de lâcher un petit rire avant de s\'échapper. Ca donne à peu près ceci : " + "\"" + "niap nioop niap niap" + "\"" + ". Ce rire peut mettre en déroute une armée... de tiques.",ep:3,g:641,l:21,wd:true,fm:true,w:1,p:1};
I.u[2673] = {n:"Oeuf pourri",nn:"OEUF POURRI",t:105,d:"C\'est tout simplement honteux. Comment peut-on pondre un Oeuf de Tofu en décomposition? Il faut vraiment être Maléfique pour avoir des idées pareils.",ep:1,g:642,l:3,wd:true,fm:true,w:1,p:1};
I.u[2674] = {n:"Citrouillette",nn:"CITROUILLETTE",t:46,d:"La citrouillette est délicieuse en cette saison. Pas trop petite pour avoir du goût, mais pas trop grosse non plus pour ne pas se transformer en Citwouille, succulente et remboursée par le sec Huh.",ep:4,g:643,l:15,wd:true,fm:true,w:1,p:1};
I.u[2675] = {n:"Plume de Tofu Maléfique",nn:"PLUME DE TOFU MALEFIQUE",t:53,d:"Le Tofu Maléfique n\'a vraiment plus beaucoup de Plumes, il va finir par attraper un rhume carabiné, il ne faudra donc pas s\'étonner si vous l\'entendez éternuer la nuit.",ep:1,g:644,l:3,wd:true,fm:true,w:1,p:1};
I.u[2676] = {n:"Noeul Mort",nn:"NOEUL MORT",t:15,d:"Vous avez bien fait de le ramasser, il est intact. Mort, mais intact.",ep:4,g:645,l:274,wd:true,fm:true,w:10,p:1};
I.u[2677] = {n:"Ceinture Chouqueuse",nn:"CEINTURE CHOUQUEUSE",t:10,d:"Cette ceinture a été confectionnée à partir des éléments les plus puissants du Chouque, notamment son célèbre caleçon. De quoi mettre en émoi toutes les flibustières du bar des pirates.",ep:12,g:41,l:98,wd:true,fm:true,w:10,p:25000};
I.u[2678] = {n:"Ceinture Kokokette",nn:"CEINTURE KOKOKETTE",t:10,d:"Cette ceinture ressemble presque à un pagne, elle conviendra parfaitement si vous voulez vous camoufler parmi les Kokokos de l\'île de Moon, ou tout simplement si vous désirez courtiser un Kokoko.",ep:12,g:40,l:42,wd:true,fm:true,w:10,p:4800};
I.u[2681] = {n:"Ceinture Tortue Jaune",nn:"CEINTURE TORTUE JAUNE",t:10,d:"Cette petite ceinture est idéale pour protéger les parties sensibles, des coups critiques. \nVérifiez tout de même que le cordonnier a bien retiré le propriétaire de la carapace avant que vous ne l\'essayez.",ep:7,g:43,l:60,wd:true,fm:true,w:10,p:10000};
