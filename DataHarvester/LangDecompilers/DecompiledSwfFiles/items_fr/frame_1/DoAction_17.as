I.u[7403] = {n:"Os de Fantôme Léopardo",nn:"OS DE FANTOME LEOPARDO",t:47,d:"Ce petit os pointu était autrefois utilisé par les Brigandins pour décorer leurs narines. Une façon élégante de mettre en valeur un nez crochu par exemple.",ep:19,g:3,l:100,wd:true,fm:true,w:1,p:10};
I.u[7404] = {n:"Os de Fantôme Maho Firefoux",nn:"OS DE FANTOME MAHO FIREFOUX",t:47,d:"Cet os est encore imprégné d\'une magie que l\'on peut qualifier d\'artisanale, ne vous étonnez donc pas, si cet os reprend vie et cherche à retrouver les restes de son propriétaire.",ep:19,g:8,l:100,wd:true,fm:true,w:1,p:10};
I.u[7405] = {n:"Os de Fantôme Pandikaze",nn:"OS DE FANTOME PANDIKAZE",t:47,d:"Cet os est tout ce qu\'il reste du fantôme que vous venez d\'exterminer. Pas de chance pour vous, aucune magie ne semble l\'habiter.",ep:19,g:4,l:100,wd:true,fm:true,w:1,p:10};
I.u[7406] = {n:"Os de Fantôme Pandore",nn:"OS DE FANTOME PANDORE",t:47,d:"Cet os ne sort pas de la boîte du Pandore et pourtant, tout porte à croire que si.",ep:19,g:5,l:100,wd:true,fm:true,w:1,p:10};
I.u[7407] = {n:"Os de Fantôme Pandule",nn:"OS DE FANTOME PANDULE",t:47,d:"Finement taillé, cet os peut sans problème, servir d\'aiguille pour une horloge géante. Tout simplement indispensable.",ep:19,g:6,l:100,wd:true,fm:true,w:1,p:10};
I.u[7408] = {n:"Os de Fantôme Soryo Firefoux",nn:"OS DE FANTOME SORYO FIREFOUX",t:47,d:"La première fois que le Soryo Firefoux s\'est cassé un os, il a tenté de se venger sur le premier venu. Au même moment, Francisque Kabroule, ménestrel de son état, se promenait en chantant et croisait le Soryo Firefoux. La suite de l\'histoire, personne ne la connaît, tout ce que l\'on sait, c\'est qu\'après ça, sa voix était devenue encore plus aiguë.",ep:19,g:7,l:100,wd:true,fm:true,w:1,p:10};
I.u[7410] = {n:"Os de Fantôme Tanukouï San",nn:"OS DE FANTOME TANUKOUI SAN",t:47,d:"A force de supporter le poids incommensurable de ses énormes testicules, les os du Tanukouï San ont fini par devenir incroyablement résistants.",ep:19,g:1,l:100,wd:true,fm:true,w:1,p:10};
I.u[7411] = {n:"Os de Fantôme Yokai Firefoux",nn:"OS DE FANTOME YOKAI FIREFOUX",t:47,d:"Cet os aurait pu servir de massue pour fendre les crânes des aventuriers insolents, ou même de marteau pour enfoncer les Enutrofs dans le sol comme de vulgaire asperges. Mais depuis qu\'il est entre vos mains, il semblerait que son destin soit différent...",ep:19,g:2,l:100,wd:true,fm:true,w:1,p:10};
I.u[7412] = {n:"Lot de cent parchemins vierges",nn:"LOT DE CENT PARCHEMINS VIERGES",t:87,d:"Ce lot permet de " + "\"" + "marquer" + "\"" + " <b>une seule cible</b> pour ensuite pouvoir la traquer plusieurs fois.",ep:16,g:3,l:10,wd:false,fm:true,w:100,c:"Ps!0&PP>0",u:true,ut:true,p:3500};
I.u[7413] = {n:"Parchemin " + "\"" + "Pointer du doigt" + "\"" + "",nn:"PARCHEMIN " + "\"" + "POINTER DU DOIGT" + "\"" + "",t:25,d:"(Double clic pour apprendre cette emote) \nCe parchemin vous permet d\'apprendre l\'attitude " + "\"" + "Pointer du doigt" + "\"" + ", pour indiquer une direction ou dénoncer ouvertement les tricheurs.",ep:18,g:9,l:1,wd:false,fm:true,w:1,u:true,p:1};
I.u[7414] = {n:"Petit Chacha Blanc",nn:"PETIT CHACHA BLANC",t:18,d:"Cette petite boule de poils griffe, mord et pince même la main qui la nourrit. C\'est donc une créature terrifiante, mais tout de même irrésistiblement attachante et fascinante. Toute ressemblance avec des personnes de la gent féminine ne serait pas fortuite. Attention, cette créature en plus de son caractère irascible est très difficile sur le plan alimentaire, puisqu\'elle ne consomme que de la viande issue de créatures chassées.",ep:3,g:9,l:1,wd:true,fm:true,w:10,c:"PZ=1",p:1};
I.u[7415] = {n:"Petit Chienchien Noir",nn:"PETIT CHIENCHIEN NOIR",t:18,d:"Cette étrange petite créature ne se nourrit que de poissons fraîchement pêchés. Mais ne vous inquiétez pas, sa compagnie agréable compense largement son haleine de poisson déshydraté.",ep:3,g:10,l:1,wd:true,fm:true,w:10,c:"PZ=1",p:1};
