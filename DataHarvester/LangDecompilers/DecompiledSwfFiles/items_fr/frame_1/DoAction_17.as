I.u[7401] = {n:"Parchemin vierge",nn:"PARCHEMIN VIERGE",t:87,d:"Ce parchemin permet de " + "\"" + "marquer" + "\"" + " une cible pour ensuite pouvoir la traquer.",ep:16,g:1,l:10,wd:false,fm:true,w:1,c:"Ps!0&PP>0",u:true,ut:true,p:100};
