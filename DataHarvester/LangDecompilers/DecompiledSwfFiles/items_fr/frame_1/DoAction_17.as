I.u[7250] = {n:"Amulette Akwadala",nn:"AMULETTE AKWADALA",t:1,d:"Seuls les habitants du village d\'Akwadala avaient l\'autorisation de porter cette amulette, qui est censée être un des éléments les plus importants de leur costume traditionnel. En y regardant de plus près, on se demande bien pourquoi.",ep:18,g:79,l:41,wd:true,fm:true,w:2,s:34,p:5050};
I.u[7251] = {n:"Amulette Terrdala",nn:"AMULETTE TERRDALA",t:1,d:"Seuls les habitants du village de Terrdala avaient l\'autorisation de porter cette amulette, qui est censée être un des éléments les plus importants de leur costume traditionnel. En y regardant de plus près, on se demande bien pourquoi.",ep:18,g:80,l:80,wd:true,fm:true,w:10,s:36,p:16550};
I.u[7252] = {n:"Amulette Feudala",nn:"AMULETTE FEUDALA",t:1,d:"Seuls les habitants du village de Feudala avaient l\'autorisation de porter cette amulette, qui est censée être un des éléments les plus importants de leur costume traditionnel. En y regardant de plus près, on se demande bien pourquoi.",ep:18,g:81,l:91,wd:true,fm:true,w:10,s:37,p:21225};
I.u[7253] = {n:"Amulette Aerdala",nn:"AMULETTE AERDALA",t:1,d:"Seuls les habitants du village d\'Aerdala avaient l\'autorisation de porter cette amulette, qui est censée être un des éléments les plus importants de leur costume traditionnel. En y regardant de plus près, on se demande bien pourquoi.",ep:18,g:82,l:62,wd:true,fm:true,w:10,s:35,p:10000};
I.u[7254] = {n:"Bâton Akwadala",nn:"BATON AKWADALA",t:4,d:"Ce bâton servait à corriger les jeunes Pandawas un peu trop turbulents. Trois coups sur les doigts, quinze sur le derrière, soixante sur la tête, deux dans les yeux, et le tour était joué.",ep:18,g:61,l:43,wd:true,fm:true,w:15,an:12,e:[5,3,1,1,50,50,false,false],c:"CC>50",s:34,p:5040};
I.u[7255] = {n:"Hache Terrdala",nn:"HACHE TERRDALA",t:19,d:"Cette hache a été dès le début, pensée pour couper des êtres vivants. Attention, une seule erreur de manipulation de votre part, et vous pourriez vous retrouver diminué de moitié.",ep:18,g:32,l:83,wd:true,fm:true,w:10,an:17,e:[5,6,1,1,50,50,false,false],s:36,p:17635};
I.u[7256] = {n:"Dagues Aerdala",nn:"DAGUES AERDALA",t:5,d:"Aiguisées comme des griffes de Meulou, ces dagues feront de vous un excellent apprenti assassin.",ep:18,g:51,l:62,wd:true,fm:true,w:10,an:13,e:[10,3,1,1,30,50,false,false],c:"CA>150&CS>50",s:35,p:10105};
I.u[7257] = {n:"Sabre Feudala",nn:"SABRE FEUDALA",t:6,d:"Ce sabre a été forgé par Hotari Honza en personne. Il parait que c\'est l\'une de ses dernières créations avant sa reconversation en cuisinier. Une véritable pièce de collection en tout cas.",ep:18,g:64,l:76,wd:true,fm:true,w:1,an:14,e:[5,6,1,1,50,50,false,false],c:"CI>200&CA>60",s:37,p:14900};
I.u[7258] = {n:"Poils de Pandikaze",nn:"POILS DE PANDIKAZE",t:54,d:"Ces poils très fins pourraient provenir de l\'arrière train d\'une Sadidette, mais ne vous y méprenez pas, il n\'en n\'est rien.",ep:16,g:556,l:40,wd:true,fm:true,w:1,p:10};
I.u[7259] = {n:"Etoffe de Pandikaze",nn:"ETOFFE DE PANDIKAZE",t:55,d:"Cet amas de poils est presque intact malgré la punition que vous avez dû infliger au pauvre Pandikaze, pour la lui arracher.",ep:18,g:124,l:40,wd:true,fm:true,w:1,p:10};
I.u[7260] = {n:"Peau de Pandikaze",nn:"PEAU DE PANDIKAZE",t:59,d:"Cette peau est encore recouverte d\'un léger duvet, aussi doux que le dessous des aisselles d\'une Eniripsa, mais sans l\'odeur de transpiration.",ep:18,g:40,l:40,wd:true,fm:true,w:2,p:10};
I.u[7261] = {n:"Bois de Bambou Magique",nn:"BOIS DE BAMBOU MAGIQUE",t:38,d:"Chose surprenante, ce morceau de Bambou est imprégné d\'une magie surprenante, à même de lui permettre de se déplacer. Une légende tribale raconte que toutes les nuits, il vient frapper aux portes des habitations dont les propriétaires auraient des activités libertines. Les Pandawas, prennent donc souvent la peine de l\'accrocher à une chaîne avant d\'aller se coucher, pour éviter les déconvenues.",ep:18,g:670,l:220,wd:true,fm:true,w:5,p:20};
I.u[7262] = {n:"Racine de Bulbiflore",nn:"RACINE DE BULBIFLORE",t:98,d:"Cette racine de Bulbiflore n\'est pas comme les autres racines, elle peut être mâchée longuement sans vous faire perdre toutes vos dents. Attention, contrairement aux vraies sucreries, elle a très mauvais goût.",ep:18,g:142,l:26,wd:true,fm:true,w:2,p:20};
I.u[7263] = {n:"Racine de Bulbuisson",nn:"RACINE DE BULBUISSON",t:98,d:"Cette racine de Bulbuisson n\'est pas comme les autres racines, elle peut être mâchée longuement sans vous faire perdre toutes vos dents. Attention, contrairement aux vraies sucreries, elle a très mauvais goût.",ep:18,g:143,l:42,wd:true,fm:true,w:2,p:20};
I.u[7264] = {n:"Racine de Bulbambou",nn:"RACINE DE BULBAMBOU",t:98,d:"Cette racine de Bulbambou n\'est pas comme les autres racines, elle peut être mâchée longuement sans vous faire perdre toutes vos dents. Attention, contrairement aux vraies sucreries, elle a très mauvais goût.",ep:18,g:145,l:67,wd:true,fm:true,w:2,p:20};
I.u[7265] = {n:"Racine de Bulbig",nn:"RACINE DE BULBIG",t:98,d:"Cette racine de Bulbig n\'est pas comme les autres racines, elle peut être mâchée longuement sans vous faire perdre toutes vos dents. Attention, contrairement aux vraies sucreries, elle a très mauvais goût.",ep:18,g:144,l:150,wd:true,fm:true,w:2,p:20};
I.u[7266] = {n:"Fleur de Bulbiflore",nn:"FLEUR DE BULBIFLORE",t:35,d:"Cette fleur ne pousse qu\'à un seul endroit, sur la tête du Bulbiflore, n\'essayez donc pas avec la vôtre, le substrat n\'est pas aussi riche.",ep:18,g:119,l:26,wd:true,fm:true,w:1,p:10};
I.u[7267] = {n:"Feuille de Bulbuisson",nn:"FEUILLE DE BULBUISSON",t:36,d:"Ces feuilles ne pousse qu\'à un seul endroit, sur la tête du Bulbuisson, n\'essayez donc pas de les faire pousser sur la vôtre, elles risqueraient de faner rapidement.",ep:18,g:55,l:42,wd:true,fm:true,w:2,p:10};
I.u[7268] = {n:"Pousses de Bulbambou",nn:"POUSSES DE BULBAMBOU",t:36,d:"Cette pousse ne vit qu\'à un seul endroit, sur la tête du Bulbambou, n\'essayez donc pas avec la vôtre, elle ne trouverait rien d\'intéressant à manger dessus.",ep:18,g:56,l:67,wd:true,fm:true,w:2,p:10};
I.u[7269] = {n:"Ecorce magique de Bulbig",nn:"ECORCE MAGIQUE DE BULBIG",t:96,d:"Cette écorce magique est aussi résistante qu\'un Bouclier du Captain Amakna, mais beaucoup moins belle tout de même.",ep:18,g:105,l:150,wd:true,fm:true,w:2,p:10};
I.u[7270] = {n:"Ecorce magique de Bulbiflore",nn:"ECORCE MAGIQUE DE BULBIFLORE",t:96,d:"Cette écorce est si fine et fragile, qu\'elle pourrait servir dans la confection de dentelle ou de sous-vêtements coquins.",ep:18,g:106,l:26,wd:true,fm:true,w:2,p:10};
I.u[7271] = {n:"Ecorce magique de Bulbuisson",nn:"ECORCE MAGIQUE DE BULBUISSON",t:96,d:"Cette écorce rugueuse est parfaite pour la confection des semelles anti-dérapantes.",ep:18,g:107,l:42,wd:true,fm:true,w:2,p:10};
I.u[7272] = {n:"Ecorce magique de Bulbambou",nn:"ECORCE MAGIQUE DE BULBAMBOU",t:96,d:"Cette écorce lisse est parfaite pour la confection des parquets ou des pistes de danse.",ep:18,g:108,l:67,wd:true,fm:true,w:2,p:10};
I.u[7273] = {n:"Peau de Cooleuvre",nn:"PEAU DE COOLEUVRE",t:59,d:"Cette peau de Cooleuvre vous glissera entre les doigts comme aucune peau ne l\'avait fait auparavant. Évitez d\'en faire des chaussures, vous n\'arriveriez jamais à les retirer.",ep:18,g:41,l:67,wd:true,fm:true,w:2,p:10};
I.u[7274] = {n:"Dent magique de Cooleuvre",nn:"DENT MAGIQUE DE COOLEUVRE",t:47,d:"Cette dent n\'est pas comme les autres, elle sent plus mauvais que les autres, comme si l\'horrible haleine de la Cooleuvre s\'y était incrustée.",ep:18,g:16,l:67,wd:true,fm:true,w:2,p:100};
I.u[7275] = {n:"Poils de Kitsou Nae",nn:"POILS DE KITSOU NAE",t:54,d:"Ces poils de Kitsou Nae sont doux comme la croupe d\'une femelle Trool. De quoi vous rappeler de bons souvenirs.",ep:16,g:560,l:29,wd:true,fm:true,w:1,p:10};
I.u[7276] = {n:"Poils de Kitsou Nufeu",nn:"POILS DE KITSOU NUFEU",t:54,d:"Ces poils de Kitsou Nufeu sont doux comme la croupe d\'une femelle Trool. De quoi vous rappeler de bons souvenirs.",ep:18,g:557,l:111,wd:true,fm:true,w:1,p:10};
I.u[7277] = {n:"Poils de Kitsou Nere",nn:"POILS DE KITSOU NERE",t:54,d:"Ces poils de Kitsou Nere sont doux comme la croupe d\'une femelle Trool. De quoi vous rappeler de bons souvenirs.",ep:16,g:558,l:80,wd:true,fm:true,w:1,p:10};
I.u[7278] = {n:"Poils de Kitsou Nakwa",nn:"POILS DE KITSOU NAKWA",t:54,d:"Ces poils de Kitsou Nakwa sont doux comme la croupe d\'une femelle Trool. De quoi vous rappeler de bons souvenirs.",ep:1,g:559,l:60,wd:true,fm:true,w:1,p:10};
I.u[7279] = {n:"Peau de Kitsou Nae",nn:"PEAU DE KITSOU NAE",t:59,d:"Cette peau de Kitsou Nae est d\'une qualité exceptionnelle, découpée en de multiples lanières, elle pourrait faire un excellent fouet. C\'est le cadeau idéal à offrir aux parents dont les enfants sont trop effrontés.",ep:18,g:46,l:19,wd:true,fm:true,w:2,p:10};
I.u[7280] = {n:"Peau de Kitsou Nufeu",nn:"PEAU DE KITSOU NUFEU",t:59,d:"Cette peau de Kitsou Nufeu est d\'une qualité exceptionnelle, découpée en de multiples lanières, elle pourrait faire un excellent fouet. C\'est le cadeau idéal à offrir aux parents dont les enfants sont trop effrontés.",ep:18,g:44,l:111,wd:true,fm:true,w:2,p:10};
I.u[7281] = {n:"Peau de Kitsou Nere",nn:"PEAU DE KITSOU NERE",t:59,d:"Cette peau de Kitsou Nere est d\'une qualité exceptionnelle, découpée en de multiples lanières, elle pourrait faire un excellent fouet. C\'est le cadeau idéal à offrir aux parents dont les enfants sont trop effrontés.",ep:18,g:45,l:80,wd:true,fm:true,w:2,p:10};
I.u[7282] = {n:"Peau de Kitsou Nakwa",nn:"PEAU DE KITSOU NAKWA",t:59,d:"Cette peau de Kitsou Nakwa est d\'une qualité exceptionnelle, découpée en de multiples lanières, elle pourrait faire un excellent fouet. C\'est le cadeau idéal à offrir aux parents dont les enfants sont trop effrontés.",ep:1,g:43,l:60,wd:true,fm:true,w:2,p:10};
I.u[7283] = {n:"Peau de Tanukouï San",nn:"PEAU DE TANUKOUI SAN",t:59,d:"Cette énorme peau distendue de Tanukouï San pourrait faire une excellente bourse pour ranger vos kamas.",ep:1,g:2,l:85,wd:true,fm:true,w:2,p:100};
I.u[7284] = {n:"Poils de Tanukouï San",nn:"POILS DE TANUKOUI SAN",t:54,d:"Ces poils de Tanukouï San ne sentent pas très bon, mais qu\'importe, ce sont tout de même des poils de Tanukouï San.",ep:1,g:3,l:85,wd:true,fm:true,w:2,p:100};
I.u[7285] = {n:"Patte d\'Araknawa",nn:"PATTE D\'ARAKNAWA",t:103,d:"Cette petite patte va continuer à frétiller encore longtemps. Glissez-là dans la culotte de vos ennemis, pour les déstabiliser en plein combat, vous gagnerez sans peine.",ep:18,g:716,l:43,wd:true,fm:true,w:2,p:100};
I.u[7286] = {n:"Bois de Bambouto",nn:"BOIS DE BAMBOUTO",t:38,d:"Le bois de Bambouto vaut presque aussi cher que celui de Culbuto, et pourtant, il est beaucoup plus courant. C\'est à n\'y rien comprendre.",ep:18,g:675,l:47,wd:true,fm:true,w:2,p:100};
I.u[7287] = {n:"Feuille de Bambouto",nn:"FEUILLE DE BAMBOUTO",t:36,d:"Les feuilles de Bambouto sont larges et épaisses, un peu comme les femelles Bwork, l\'agressivité en moins.",ep:18,g:69,l:47,wd:true,fm:true,w:2,p:1};
I.u[7288] = {n:"Feuille de Bambouto Sacré",nn:"FEUILLE DE BAMBOUTO SACRE",t:36,d:"Cette grosse feuille est si dure et lourde, qu\'elle pourrait vous écraser vivant si vous vous endormiez sous un Bambouto Sacré en automne.",ep:18,g:68,l:120,wd:true,fm:true,w:2,p:100};
I.u[7289] = {n:"Bois de Bambouto Sacré",nn:"BOIS DE BAMBOUTO SACRE",t:38,d:"Ce bois-là n\'est pas à mettre entre toutes les mains, mais personne ne sait encore pourquoi.",ep:18,g:674,l:120,wd:true,fm:true,w:10,p:100};
I.u[7290] = {n:"Racine Magique de Bambouto",nn:"RACINE MAGIQUE DE BAMBOUTO",t:98,d:"Cette racine permet au Bambouto de puiser toute sa magie dans le sol avant un combat.",ep:18,g:100,l:20,wd:true,fm:true,w:2,p:10};
I.u[7291] = {n:"Racine Magique de Bambouto Sacré",nn:"RACINE MAGIQUE DE BAMBOUTO SACRE",t:98,d:"Cette racine permet au Bambouto Sacré de puiser toute sa magie dans le sol avant un combat.",ep:18,g:99,l:87,wd:true,fm:true,w:2,p:100};
I.u[7292] = {n:"Poils de Maho Firefoux",nn:"POILS DE MAHO FIREFOUX",t:54,d:"Ces poils proviennent d\'un Maho Firefoux, une terrible créature à l\'apparence chétive, mais qui possède en réalité de terribles pouvoirs magiques.",ep:18,g:202,l:150,wd:true,fm:true,w:2,p:1};
I.u[7293] = {n:"Poils de Soryo Firefoux",nn:"POILS DE SORYO FIREFOUX",t:54,d:"Ces poils proviennent d\'une petite créature rousse bien malicieuse, comme tant d\'autres.",ep:18,g:203,l:130,wd:true,fm:true,w:10,p:1};
I.u[7294] = {n:"Poils de Yokai Firefoux",nn:"POILS DE YOKAI FIREFOUX",t:54,d:"Ces poils épais et malodorants ont été arrachés avec force et détermination du derrière d\'une horrible créature rousse.",ep:18,g:204,l:110,wd:true,fm:true,w:10,p:10};
I.u[7295] = {n:"Poils de Pandore",nn:"POILS DE PANDORE",t:54,d:"Ces poils mystérieux permettent en général de tisser des capes ou des chapeaux magiques aux effets inattendus.",ep:18,g:2,l:200,wd:true,fm:true,w:10,p:200};
I.u[7297] = {n:"Poils de Pandule",nn:"POILS DE PANDULE",t:54,d:"Ces poils magiques sont assez puissants pour redonner vie à une horloge cassée.",ep:18,g:206,l:150,wd:true,fm:true,w:1,p:1};
I.u[7298] = {n:"Peau de Maho Firefoux",nn:"PEAU DE MAHO FIREFOUX",t:59,d:"Cette peau lisse et fine offre une excellente protection face aux diverses attaques magiques que le Maho Firefoux rencontre lorsque ses sorts se retournent contre lui.",ep:18,g:42,l:150,wd:true,fm:true,w:1,p:100};
I.u[7299] = {n:"Peau de Soryo Firefoux",nn:"PEAU DE SORYO FIREFOUX",t:59,d:"Cette peau était autrefois utilisée pour coudre les slips des enfants à la peau sensible. Les slips en peau de Soyo Firefoux sont approuvés par la descendance du Captain Chafer.",ep:18,g:38,l:130,wd:true,fm:true,w:1,p:100};
I.u[7300] = {n:"Peau de Yokai Firefoux",nn:"PEAU DE YOKAI FIREFOUX",t:59,d:"Cette peau épaisse offre une excellente protection face aux coups de Bambou.",ep:18,g:37,l:110,wd:true,fm:true,w:1,p:100};
I.u[7301] = {n:"Peau de Léopardo",nn:"PEAU DE LEOPARDO",t:59,d:"Cette peau était autrefois utilisée par les Pandawas pour confectionner des sous-vêtements légers, et minimalistes.",ep:18,g:36,l:40,wd:true,fm:true,w:1,p:10};
I.u[7302] = {n:"Peau de Pandore",nn:"PEAU DE PANDORE",t:59,d:"La peau du Pandore vous réservera beaucoup de surprises si vous êtes cordonnier.",ep:18,g:1,l:200,wd:true,fm:true,w:10,p:100};
I.u[7303] = {n:"Peau de Pandule",nn:"PEAU DE PANDULE",t:59,d:"Cette peau rugueuse change de couleur au fil des heures.",ep:18,g:39,l:100,wd:true,fm:true,w:1,p:10};
I.u[7304] = {n:"Etoffe de Maho Firefoux",nn:"ETOFFE DE MAHO FIREFOUX",t:55,d:"Cette étoffe magique de couleur rousse est imprégnée d\'une magie surpuissante. On ne peut pas en dire autant de vous.",ep:18,g:11,l:150,wd:true,fm:true,w:1,p:100};
I.u[7305] = {n:"Etoffe de Soryo Firefoux",nn:"ETOFFE DE SORYO FIREFOUX",t:55,d:"Cette étoffe de taille modeste est parfaite pour boucher les trous sous les vieilles portes, lorsque l\'hiver approche.",ep:18,g:10,l:130,wd:true,fm:true,w:1,p:10};
I.u[7306] = {n:"Etoffe de Yokai Firefoux",nn:"ETOFFE DE YOKAI FIREFOUX",t:55,d:"Cette étoffe provient des parties les plus sensibles du Yokai Firefoux, qui sont également les plus malodorantes. Beaucoup d\'aventuriers pensent qu\'il s\'agit de ses moustaches, mais ils se trompent.",ep:18,g:9,l:110,wd:true,fm:true,w:1,p:10};
I.u[7307] = {n:"Etoffe de Pandore",nn:"ETOFFE DE PANDORE",t:55,d:"Cette étoffe est presque aussi rare qu\'une bonne surprise sortant de la boîte du Pandore.",ep:18,g:125,l:200,wd:true,fm:true,w:10,p:10};
I.u[7308] = {n:"Etoffe de Pandule",nn:"ETOFFE DE PANDULE",t:55,d:"Contrairement aux idées reçues, cette étoffe n\'est pas mangeable. Les derniers qui ont essayé de la cuisiner se sont transformés en Tanukouï San. Une aubaine pour certains d\'entre eux.",ep:18,g:12,l:100,wd:true,fm:true,w:1,p:10};
I.u[7309] = {n:"Clef des Pandikazes",nn:"CLEF DES PANDIKAZES",t:84,d:"Cette clef permet de rentrer dans le donjon des Pandikazes. Attention, elle sera détruite après utilisation.",ep:13,g:229,l:1,wd:true,fm:true,w:1,p:100};
I.u[7310] = {n:"Clef de la caverne des Bulbes",nn:"CLEF DE LA CAVERNE DES BULBES",t:84,d:"Cette clef permet de rentrer dans la caverne des Bulbes. Attention, elle sera détruite après utilisation.",ep:1,g:230,l:1,wd:true,fm:true,w:1,p:100};
I.u[7311] = {n:"Clef du Donjon des Kitsounes",nn:"CLEF DU DONJON DES KITSOUNES",t:84,d:"Cette clef permet de rentrer dans le Donjon des Kitsounes. Attention, elle sera détruite après utilisation.",ep:1,g:231,l:1,wd:true,fm:true,w:1,p:100};
I.u[7312] = {n:"Clef du Donjon des Firefoux",nn:"CLEF DU DONJON DES FIREFOUX",t:84,d:"Cette clef permet de rentrer dans le Donjon des Firefoux. Attention, elle sera détruite après utilisation.",ep:2,g:232,l:1,wd:true,fm:true,w:1,p:100};
I.u[7313] = {n:"L\'art Pandawushu",nn:"L\'ART PANDAWUSHU",t:25,d:"Un livre que tous les pratiquants du Pandawushu devraient connaître sur le bout des doigts ...",ep:18,g:19,l:1,wd:true,fm:true,w:2,u:true,p:1};
I.u[7314] = {n:"Potion d\'oubli Percepteur : Armure aqueuse",nn:"POTION D\'OUBLI PERCEPTEUR : ARMURE AQUEUSE",t:86,d:"Cette potion permettra au meneur d\'une guilde d\'oublier un niveau de sort pour son percepteur.",ep:5,g:1,l:6,wd:true,fm:true,w:5,u:true,p:100};
