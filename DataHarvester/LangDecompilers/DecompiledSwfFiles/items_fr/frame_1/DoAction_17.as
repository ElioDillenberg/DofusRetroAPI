I.u[7212] = {n:"La Trancheuse de Bambou",nn:"LA TRANCHEUSE DE BAMBOU",t:19,d:"Légère et élégante, cette hache vous permettra de progresser facilement dans les forêts de Bambou de Pandala,  et de vous défendre efficacement contre les Pandis les plus fourbes, toujours prêts à attaquer les bûcherons isolés pour de sombres histoires de bourses...",ep:18,g:33,l:26,wd:true,fm:true,w:10,an:17,e:[5,6,1,1,50,50,false,false],p:2000};
I.u[7213] = {n:"Pelle Rhon",nn:"PELLE RHON",t:8,d:"Cette pelle est censée fendre l\'air à une vitesse assez incroyable. Ceux qui savent correctement s\'en servir, fendent surtout les crânes de leurs ennemis avec.",ep:19,g:41,l:142,wd:true,fm:true,w:15,an:16,tw:true,e:[10,5,1,1,40,70,false,false],c:"CC>400&CV>250",p:46050};
I.u[7214] = {n:"Sandales Macien",nn:"SANDALES MACIEN",t:11,d:"Ces sandales appartenaient aux Maciens, une tribu d\'Ouginaks, disparue aujourd\'hui. Ces derniers étaient très complices avec les Pandawas, ce qui avait fini au fil des générations, par changer la couleur de leur pelage, qui était devenu blanc, tâcheté de noir. Ils furent tous transformés en manteau par une cruelle tailleuse.",ep:18,g:59,l:30,wd:true,fm:true,w:10,p:2650};
I.u[7215] = {n:"Sandales Humeuses",nn:"SANDALES HUMEUSES",t:11,d:"Ces sandales étaient utilisées par certaines femelles Pandits, pour séduire les aventuriers égarés, et les attirer dans d\'horribles pièges.",ep:19,g:60,l:121,wd:true,fm:true,w:20,p:37400};
I.u[7216] = {n:"Sandales Koliques",nn:"SANDALES KOLIQUES",t:11,d:"Ces sandales étaient données aux Pandawas malades, qui avaient besoin de courir très vite lorsqu\'ils avaient contracté le virus de la Pandrista, ou lorsqu\'ils avaient passé une nuit trop arrosée à la taverne.",ep:18,g:61,l:58,wd:true,fm:true,w:10,p:8975};
I.u[7217] = {n:"Sandales Hambic",nn:"SANDALES HAMBIC",t:11,d:"Les Pandawas ont toujours été attirés par les boissons alcoolisées, ils ont ainsi au fil du temps, appris à mieux les conserver et les aromatiser. Quant à ces sandales, elles ont été faites spécialement pour les Pandawas alcooliques, elles sont lestées au niveau de la semelle, pour que vous puissiez garder les pieds sur terre plus souvent.",ep:19,g:62,l:159,wd:true,fm:true,w:50,p:50800};
I.u[7218] = {n:"Sandales Adin",nn:"SANDALES ADIN",t:11,d:"Adin était un jeune Pandawa, grand collectionneur de lampes, mais très loin d\'être un génie.",ep:18,g:63,l:76,wd:true,fm:true,w:15,p:14900};
I.u[7219] = {n:"Sandales Ailuya",nn:"SANDALES AILUYA",t:11,d:"Ces sandales étaient confiées aux Pandits à la fin de rituels religieux. Elles sont de nos jours surtout utilisées dans les championnats de claquettes.",ep:18,g:64,l:56,wd:true,fm:true,w:15,p:8200};
I.u[7220] = {n:"Ticket de loterie",nn:"TICKET DE LOTERIE",t:15,d:"Vous avez entre les mains, un superbe ticket de loterie, gardez-le précieusement, il se pourrait que vous soyez l\'heureux élu du très grand tirage au sort !",ep:1,g:717,l:2,wd:true,fm:true,w:1,p:1};
I.u[7221] = {n:"Collier du Tanukouï San",nn:"COLLIER DU TANUKOUI SAN",t:1,d:"Ce collier est porté par le Tanukouï San depuis sa tendre enfance, ce qui lui confère une valeur ajoutée non négligeable lorsque l\'on sait de quoi il était capable, même durant son enfance.",ep:19,g:92,l:152,wd:true,fm:true,w:10,p:49030};
