I.u[270] = {n:"Xaveur",nn:"XAVEUR",t:2,d:"Le Xaveur est un arc efficace. Il appartenait au grand Xavier le Boulanger. Entre deux fournées, il partait distribuer des pains aux monstres entourant sa boulangerie.",ep:7,g:5,l:60,wd:true,fm:true,w:18,an:10,tw:true,e:[10,5,2,9,30,100,false,true],c:"CA>60&CV>15",p:9050};
I.u[271] = {n:"Le Marteau du Chafer",nn:"LE MARTEAU DU CHAFER",t:7,d:"Une belle arme étrange et fascinante, elle vous permettra de vous battre au corps à corps comme le ferait le meilleur des Chafers.",ep:1,g:9,l:21,wd:true,fm:true,w:1,an:15,e:[5,4,1,1,40,30,false,false],c:"CS>10&CI>11",p:1350};
I.u[272] = {n:"Nomoon",nn:"NOMOON",t:2,d:"Cet arc est assez puissant pour envoyer une flèche sur la lune.",ep:7,g:9,l:28,wd:true,fm:true,w:18,an:10,tw:true,e:[10,5,2,12,30,50,false,true],c:"CA>28&CV>5",p:2350};
I.u[273] = {n:"La Corde de Yamato",nn:"LA CORDE DE YAMATO",t:2,d:"La Corde de Yamato a fait vibrer plus d\'un coeur de monstre. Arme fine, elle est capable de diriger sa flèche dans les endroits les plus cachés.",ep:7,g:10,l:33,wd:true,fm:true,w:18,an:10,tw:true,e:[10,4,2,7,30,50,false,false],c:"CA>33&CS>10",p:3000};
I.u[274] = {n:"Pièce du Puzzle de la Griffe",nn:"PIECE DU PUZZLE DE LA GRIFFE",t:51,d:"Une pièce très rare du trésor de Dodge, l\'un des plus grands Ecaflip d\'Amakna.",ep:1,g:267,l:12,wd:true,fm:true,w:1,p:100};
I.u[276] = {n:"Pièce du Trésor du Badoul",nn:"PIECE DU TRESOR DU BADOUL",t:15,d:"Une pièce très rare d\'un trésor caché par le célèbre Dodge, l\'un des plus grands guerriers Ecaflip d\'Amakna.",ep:1,g:268,l:12,wd:true,fm:true,w:1,p:100};
I.u[278] = {n:"Anneau des Rocheuses",nn:"ANNEAU DES ROCHEUSES",t:9,d:"Anneau serti d\'une pierre rare. Il doit son nom aux différentes pierres nécessaires à sa confection.",ep:1,g:4,l:15,wd:true,fm:true,w:4,p:800};
I.u[279] = {n:"La Larme du Bouftou",nn:"LA LARME DU BOUFTOU",t:1,d:"Selon un illustre aventurier, cette amulette contient une larme de Bouftou, à l\'odeur et aux propriétés semblables à celles des décoctions de cuir de bouftou.",ep:1,g:7,l:2,wd:true,fm:true,w:5,p:200};
I.u[280] = {n:"Collier de l\'Arc en Ciel",nn:"COLLIER DE L\'ARC EN CIEL",t:1,d:"Ce collier est recherché par les collectionneurs, vous en tirerez un bon prix.",ep:1,g:11,l:6,wd:true,fm:true,w:6,p:300};
I.u[281] = {n:"Pomme",nn:"POMME",t:42,d:"Cette pomme restaure quelques points de vie perdus. Posée sur la tête d\'un ami, elle pourra vous servir de cible.",ep:1,g:1,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:1};
I.u[282] = {n:"Fiole de Soin supérieure",nn:"FIOLE DE SOIN SUPERIEURE",t:12,d:"Cette fiole restaure quelques points de vie perdus.",ep:1,g:39,l:20,wd:true,fm:true,w:1,u:true,ut:true,p:10};
I.u[283] = {n:"Fiole de Soin",nn:"FIOLE DE SOIN",t:12,d:"Cette fiole restaure quelques points de vie perdus.",ep:1,g:3,l:10,wd:true,fm:true,w:1,u:true,ut:true,p:10};
I.u[284] = {n:"Sel",nn:"SEL",t:48,d:"Le sel est un minéral, moulu, il vous permet de donner du goût à vos pains.",ep:2,g:260,l:1,wd:true,fm:true,w:1,p:10};
I.u[285] = {n:"Farine de Blé",nn:"FARINE DE BLE",t:52,d:"La farine s\'obtient en écrasant des grains de blé, elle est très utile pour faire du pain, des crêpes, des gaufres et de délicieux gâteaux, mais surtout pour enfariner vos ennemis d\'enfance.",ep:1,g:251,l:1,wd:true,fm:true,w:2,p:0};
I.u[286] = {n:"Levure de Boulanger",nn:"LEVURE DE BOULANGER",t:48,d:"C\'est un ingrédient indispensable pour la fermentation du pain.",ep:1,g:137,l:1,wd:true,fm:true,w:1,p:1};
I.u[287] = {n:"Graine de Sésame",nn:"GRAINE DE SESAME",t:58,d:"Cette graine extrêmement riche en protéines permet de réaliser de délicieux pains, mais la culture du sésame est délicate sous le climat d\'Amakna, ce qui en fait une graine rare et chère.",ep:1,g:3,l:1,wd:true,fm:true,w:1,p:1};
I.u[288] = {n:"Graine de Tournesol sauvage",nn:"GRAINE DE TOURNESOL SAUVAGE",t:58,d:"Cette graine riche en protéines permet de donner une saveur très particulière aux pains. La seule façon d\'en trouver c\'est de tuer un Tournesol sauvage ou de l\'acheter !",ep:1,g:6,l:12,wd:true,fm:true,w:1,p:10};
I.u[289] = {n:"Blé",nn:"BLE",t:34,d:"Le Blé est depuis la nuit des temps un ingrédient indispensable dans l\'alimentation des habitants d\'Amakna et des Tofus.",ep:1,g:9,l:1,wd:true,fm:true,w:2,p:0};
I.u[290] = {n:"Champignon",nn:"CHAMPIGNON",t:15,d:"Avec ou sans crème, les champignons sont toujours aussi bons. La manière la plus simple de s\'en procurer reste de tuer un Champ-Champ.",ep:1,g:1,l:5,wd:true,fm:true,w:1,p:10};
I.u[291] = {n:"Poils du Mulou",nn:"POILS DU MULOU",t:54,d:"Ces poils ne sont pas comme tous les poils, ils disposent de propriétés particulières qui les rendent très appréciés des alchimistes.",ep:1,g:8,l:70,wd:true,fm:true,w:1,p:1};
I.u[292] = {n:"Testicules du Mulou",nn:"TESTICULES DU MULOU",t:15,d:"Ces énormes testicules sont recouverts d\'un étrange duvet particulièrement doux et chaud. Il est presque impossible de les récupérer intacts après un combat face à un Mulou, ce qui explique en partie leur prix scandaleux.",ep:1,g:10,l:70,wd:true,fm:true,w:20,p:50};
I.u[293] = {n:"Petite Pelle du Craqueleur",nn:"PETITE PELLE DU CRAQUELEUR",t:8,d:"La seule chose qui soit aussi solide qu\'un Craqueleur, c\'est un Craqueleur, voilà pourquoi cette pelle, destinée à la chasse aux craqueleurs, est en Pierre de Craqueleur.",ep:1,g:7,l:20,wd:true,fm:true,w:35,an:16,e:[5,4,1,1,50,50,false,false],c:"CS>5&CC>15",p:1200};
I.u[294] = {n:"Pelle du Craqueleur",nn:"PELLE DU CRAQUELEUR",t:8,d:"La seule chose qui soit aussi solide qu\'un Craqueleur, c\'est un Craqueleur, voilà pourquoi cette pelle destinée à la chasse aux craqueleurs est en Pierre de Craqueleur.",ep:1,g:7,l:23,wd:true,fm:true,w:35,an:16,e:[5,4,1,1,50,50,false,false],c:"CS>8&CC>15",p:1600};
I.u[295] = {n:"Grande Pelle du Craqueleur",nn:"GRANDE PELLE DU CRAQUELEUR",t:8,d:"La seule chose qui soit aussi solide qu\'un Craqueleur, c\'est un Craqueleur, voilà pourquoi cette pelle destinée à la chasse aux craqueleurs est en Pierre de Craqueleur.",ep:1,g:7,l:26,wd:true,fm:true,w:35,an:16,e:[5,4,1,1,50,50,false,false],c:"CS>11&CC>15",p:2000};
I.u[296] = {n:"Puissante Pelle du craqueleur",nn:"PUISSANTE PELLE DU CRAQUELEUR",t:8,d:"La seule chose qui soit aussi solide qu\'un Craqueleur, c\'est un Craqueleur, voilà pourquoi cette pelle destinée à la chasse aux craqueleurs est en Pierre de Craqueleur.",ep:1,g:7,l:29,wd:true,fm:true,w:35,an:16,e:[5,4,1,1,50,50,false,false],c:"CS>14&CC>15",p:2400};
I.u[297] = {n:"Bottes du Petit Bouftou",nn:"BOTTES DU PETIT BOUFTOU",t:11,d:"Ces bottes sont confectionnées en cuir de Bouftou, elles permettent aux éleveurs de s\'approcher plus facilement des Bouftous pour les tuer, grâce à l\'odeur qu\'elles dégagent.",ep:1,g:3,l:1,wd:true,fm:true,w:2,p:100};
I.u[298] = {n:"Sandales Tofuesques",nn:"SANDALES TOFUESQUES",t:11,d:"Ces sandales sont faites à partir de plumes de Tofu bouilli, et la semelle est en Bec de Tofu inusable. Pour des raisons encore inexpliquées, elles rendent parfois leur porteur un peu débile...comme un Tofu.",ep:1,g:4,l:1,wd:true,fm:true,w:7,p:100};
I.u[299] = {n:"Bottes de Faillete",nn:"BOTTES DE FAILLETE",t:11,d:"La meilleure défense c\'est l\'attaque, ces bottes anti-agression vous permettront d\'exterminer un groupe d\'agresseurs sans aucune difficulté.",ep:1,g:8,l:24,wd:true,fm:true,w:30,p:1800};
I.u[300] = {n:"Graine de Pavot",nn:"GRAINE DE PAVOT",t:58,d:"Cette graine confère des vertus hallucinogènes aux pains prisés des aventuriers qui désirent se reposer après un combat difficile.",ep:1,g:11,l:5,wd:true,fm:true,w:1,p:1};
I.u[301] = {n:"Plumes de Tofu",nn:"PLUMES DE TOFU",t:53,d:"Les plumes de Tofu sont très utilisées en couture pour les rembourrages divers. Et surtout, n\'oubliez pas, un kilo de plumes de Tofu pèse autant qu\'un kilo de plomb.",ep:1,g:14,l:4,wd:true,fm:true,w:1,p:10};
I.u[302] = {n:"Citrouille",nn:"CITROUILLE",t:68,d:"Les citrouilles poussent dans les champs, à peine plus grosses que les testicules de mulou, elles ont en revanche bien meilleur goût.",ep:4,g:12,l:15,wd:true,fm:true,w:5,p:10};
I.u[303] = {n:"Bois de Frêne",nn:"BOIS DE FRENE",t:38,d:"Le Bois de Frêne n\'est pas un bois de qualité, mais il est abondant et pousse relativement vite. Attention tout de même à ne pas le stocker près d\'une forte source de chaleur, il est inflammable.",ep:1,g:17,l:1,wd:true,fm:true,w:5,p:0};
I.u[304] = {n:"Cuir de Bouftou",nn:"CUIR DE BOUFTOU",t:56,d:"Ce cuir très résistant constitue la principale ressource des tanneurs qui en font des bottes, des ceintures et même des lanières de fouet. Son seul inconvénient, c\'est d\'avoir l\'odeur de la sueur de Bouftou en rut... ce qui ne favorise pas les rencontres.",ep:1,g:16,l:9,wd:true,fm:true,w:2,p:1};
I.u[305] = {n:"Dent de Wabbit",nn:"DENT DE WABBIT",t:47,d:"Abîmées à force de ronger des cawottes, les dents de Wabbit sont tout de même très utiles lors de la confection de potions grâce à leurs propriétés revigorantes.",ep:8,g:13,l:20,wd:true,fm:true,w:1,p:1};
I.u[306] = {n:"Fleur de Pissenlit Diabolique",nn:"FLEUR DE PISSENLIT DIABOLIQUE",t:35,d:"Correctement bouillie puis séchée, cette fleur perd ses propriétés toxiques et peut se consommer en tisane, mais la fleur de Pissenlit Diabolique est surtout utilisée pour amuser la jeunesse en quête de romantisme. En effet, qui n\'a jamais soufflé sur une fleur de Pissenlit Diabolique pour envoyer dans le cou de l\'être tant convoité, ces magnifiques graines, douces comme la caresse d\'une éclaircie sur les feuilles d\'un charme encore imprégnées de rosée ? Peut-être les Iops, qui au lieu de souffler, ont tendance à aspirer les graines et à s\'étouffer lamentablement dans d\'atroces convulsions.",ep:1,g:18,l:5,wd:true,fm:true,w:1,p:10};
I.u[307] = {n:"Ailes de Moskito",nn:"AILES DE MOSKITO",t:104,d:"Ces ailes très fines ont la particularité de favoriser la réflexion des personnes qui osent les mâcher. Leur goût est assez proche des oeufs de Kwak pourris, c\'est pour cela qu\'elles sont essentiellement utilisées pour les potions de sagesse.",ep:1,g:19,l:2,wd:true,fm:true,w:1,p:1};
I.u[308] = {n:"Coquille de Dragoeuf Blanc",nn:"COQUILLE DE DRAGOEUF BLANC",t:111,d:"Cette coquille est presque incassable, cependant, en la polissant pendant des générations, les meilleurs Bijoutiers peuvent utiliser ses facultés magiques pour confectionner des amulettes aux pouvoirs exceptionnels.",ep:11,g:21,l:64,wd:true,fm:true,w:5,p:1};
I.u[309] = {n:"Pétale de Rose Démoniaque",nn:"PETALE DE ROSE DEMONIAQUE",t:35,d:"Ces pétales ont des vertus aphrodisiaques très puissantes, ce qui en fait le cadeau idéal pour conquérir l\'élu(e) de votre coeur. Éviter de les consommer en infusion est une sage décision dans la mesure où leurs effets se retrouvent être inversés.",ep:1,g:20,l:6,wd:true,fm:true,w:1,p:10};
I.u[310] = {n:"Os de Chafer",nn:"OS DE CHAFER",t:47,d:"Cet os est presque incassable, mais au contact de l\'eau, il devient tout mou, ce qui facilite son utilisation par les alchimistes.",ep:1,g:15,l:34,wd:true,fm:true,w:5,p:1};
I.u[311] = {n:"Eau",nn:"EAU",t:15,d:"L\'eau est utile pour de nombreuses choses, et plus elle est trouble, plus elle donnera de goût à la bière (méfiez-vous tout de même, l\'eau bue explose).",ep:1,g:26,l:1,wd:true,fm:true,w:1,p:1};
I.u[312] = {n:"Fer",nn:"FER",t:39,d:"Le fer est très utile pour les forgerons et pour ceux qui utilisent du métal, certains s\'en servent même pour repasser.",ep:1,g:24,l:1,wd:true,fm:true,w:5,p:0};
I.u[313] = {n:"Or",nn:"OR",t:39,d:"L\'or est un métal rare et précieux, ses qualités intrinsèques en font un métal de choix pour la conception d\'armes ou d\'objets particulièrement puissants et d\'esthétique raffinée.",ep:1,g:22,l:80,wd:true,fm:true,w:5,p:0};
I.u[315] = {n:"Diamant",nn:"DIAMANT",t:50,d:"Tous les bijoutiers d\'Amakna raffolent de cette pierre qui demande une excellente dextérité pour être travaillée. Il faut souvent s\'aventurer dans de dangereuses mines pour les trouver, mais heureusement, les diamants sont éternels.",ep:1,g:25,l:50,wd:true,fm:true,w:10,p:5000};
I.u[316] = {n:"Emeraude",nn:"EMERAUDE",t:50,d:"Avec son vert intense, cette magnifique pierre précieuse peut facilement transformer un thon en princesse, sans l\'utilisation d\'une quelconque magie occulte.",ep:1,g:23,l:50,wd:true,fm:true,w:10,p:5000};
I.u[317] = {n:"Bâton du Petit Tabi",nn:"BATON DU PETIT TABI",t:4,d:"Ce bâton servait à l\'origine aux éleveurs d\'Amakna qui voulaient contrôler plus facilement leurs troupeaux de Tabis, ce bâton est en effet si léger, qu\'il permet à son porteur de courir après les Tabis égarés sans se fatiguer.",ep:7,g:6,l:30,wd:true,fm:true,w:1,an:12,e:[5,3,1,1,40,50,false,false],c:"CA>20",p:2650};
I.u[318] = {n:"Bâton du Tabi",nn:"BATON DU TABI",t:4,d:"Ce bâton servait à l\'origine aux éleveurs d\'Amakna qui voulaient contrôler plus facilement leurs troupeaux de Tabis, ce bâton est en effet si léger, qu\'il permet à son porteur de courir après les Tabis égarés sans se fatiguer.",ep:7,g:6,l:34,wd:true,fm:true,w:1,an:12,e:[5,3,1,1,40,50,false,false],c:"CA>20",p:3200};
I.u[319] = {n:"Bâton du Grand Tabi",nn:"BATON DU GRAND TABI",t:4,d:"Ce bâton servait à l\'origine aux éleveurs d\'Amakna qui voulaient contrôler plus facilement leurs troupeaux de Tabis, ce bâton est en effet si léger, qu\'il permet à son porteur de courir après les Tabis égarés sans se fatiguer.",ep:7,g:6,l:37,wd:true,fm:true,w:1,an:12,e:[5,3,1,1,40,50,false,false],c:"CA>25",p:3750};
I.u[320] = {n:"Bâton du Puissant Tabi",nn:"BATON DU PUISSANT TABI",t:4,d:"Ce bâton servait à l\'origine aux éleveurs d\'Amakna qui voulaient contrôler plus facilement leurs troupeaux de Tabis, ce bâton est en effet si léger, qu\'il permet à son porteur de courir après les Tabis égarés sans se fatiguer.",ep:7,g:6,l:40,wd:true,fm:true,w:1,an:12,e:[5,3,1,1,40,50,false,false],c:"CA>30",p:4300};
I.u[323] = {n:"Amulette du Kam Assutra",nn:"AMULETTE DU KAM ASSUTRA",t:1,d:"Cette amulette donne des facultés très particulières à son porteur, en effet ce dernier se voit comme transporté par une aura mystérieuse parcourant son corps, ce qui généralement a pour effet de décupler ses capacités physiques.",ep:1,g:4,l:43,wd:true,fm:true,w:5,p:5000};
I.u[324] = {n:"Amulette de Krokette",nn:"AMULETTE DE KROKETTE",t:1,d:"Cette amulette a la faculté de devenir rose les jours où son porteur est d\'humeur guillerette. On raconte qu\'elle décuple également vos chances de trouver de bons objets.",ep:1,g:7,l:6,wd:true,fm:true,w:5,p:300};
I.u[325] = {n:"Amulette du Sram",nn:"AMULETTE DU SRAM",t:1,d:"Cette amulette confectionnée à partir de petits crânes taillés dans de l\'os, par un des plus grands bijoutiers Sram, est unique en son genre. Elle permet à son porteur de s\'évader plus facilement lors des situations périlleuses.",ep:1,g:3,l:2,wd:true,fm:true,w:5,p:200};
I.u[326] = {n:"Amulette Deulegarnoulle",nn:"AMULETTE DEULEGARNOULLE",t:1,d:"Cette amulette provient des régions nordiques, elle confère a son porteur la capacité d\'invoquer plus facilement des créatures.",ep:1,g:2,l:33,wd:true,fm:true,w:4,p:3000};
I.u[327] = {n:"Lame du Chevaucheur de Karne",nn:"LAME DU CHEVAUCHEUR DE KARNE",t:6,d:"Cette lame forgée à l\'origine pour les Chevaucheurs de Karne, était en fait bien trop lourde pour ces faibles gobelins. Elle n\'en reste pas moins très légère pour un individu normal, ce qui lui permet de frapper très vite.",ep:1,g:3,l:16,wd:true,fm:true,w:25,an:14,e:[5,5,1,1,50,20,false,false],c:"CS>32",p:900};
I.u[328] = {n:"Koupe-koupe",nn:"KOUPE-KOUPE",t:6,d:"Cette lame double à la forme bien étrange servait à l\'origine à élaguer les arbres pour progresser dans les forêts denses. Les bûcherons se sont vite aperçus qu\'elle constituait une arme de choix pour exterminer toute forme de menace.",ep:1,g:4,l:18,wd:true,fm:true,w:28,an:14,e:[6,5,1,1,20,60,false,false],c:"CS>36",p:1050};
I.u[329] = {n:"Arc de Triomphe",nn:"ARC DE TRIOMPHE",t:2,d:"Comme son nom l\'indique, cet arc permet de gagner facilement un combat. Un grand architecte Crâ sans imagination a même utilisé le nom de cet arc pour nommer une de ses constructions.",ep:7,g:11,l:30,wd:true,fm:true,w:15,an:10,tw:true,e:[10,4,2,10,30,50,false,true],c:"CA>30&CC>5&CS>10",p:1950};
I.u[331] = {n:"Arc Hancihaile",nn:"ARC HANCIHAILE",t:2,d:"C\'est l\'arc le plus recherché de tout Amakna, on raconte que là où ses flèches retombent, on peut trouver un trésor. Étrangement, il ne semble être efficace que lorsque pluie et rayons de soleil se mêlent.",ep:7,g:16,l:22,wd:true,fm:true,w:19,an:10,tw:true,e:[10,5,2,11,30,50,false,true],c:"CA>22&CC>15",p:1500};
I.u[332] = {n:"Arc de Jeanne",nn:"ARC DE JEANNE",t:2,d:"Jeanne était un peu folle, elle entendait des Tabis roter dans sa tête. Pour éviter qu\'elle ne se blesse avec son arc, il lui a été confisqué et on a décidé de la brûler vive pour réchauffer l\'ambiance lors de l\'inauguration de la taverne.",ep:7,g:15,l:30,wd:true,fm:true,w:23,an:10,tw:true,e:[10,4,2,9,30,50,false,true],c:"CS>10&CA>30",p:2550};
I.u[333] = {n:"Baguette Deuh-Pain",nn:"BAGUETTE DEUH-PAIN",t:3,d:"A l\'origine destinée à être mangée, cette baguette est devenue une arme redoutée. En effet, Xavier le Boulanger l\'a oubliée dans son four pendant 234 ans alors qu\'il était parti distribuer des pains aux monstres autour de sa boulangerie.",ep:3,g:9,l:50,wd:true,fm:true,w:1,an:11,e:[5,3,3,5,50,50,false,true],c:"CW>40&CI>100",p:6650};
I.u[334] = {n:"Baguette de Liriel",nn:"BAGUETTE DE LIRIEL",t:3,d:"Cette baguette appartenait à l\'Eniripsa Liriel qui mourût d\'une overdose de fraises dans d\'atroces souffrances, perdant ses ailes dans un râle de douleur à vous glacer le sang. Les collectionneurs s\'arrachent cette baguette réputée maudite.",ep:7,g:5,l:20,wd:true,fm:true,w:13,an:11,e:[5,5,2,3,50,50,false,true],c:"CI>40",p:1300};
I.u[335] = {n:"Bâton du Bwork Mage",nn:"BATON DU BWORK MAGE",t:4,d:"Sans leur bâton, les Bwork Mages sont aussi inoffensifs que les Tofus sans leur Bécos. Voilà qui en dit long sur la puissance de ce bâton.",ep:7,g:1,l:58,wd:true,fm:true,w:28,an:12,e:[5,3,1,1,30,50,false,false],c:"CI>58&CA>20",p:8900};
I.u[336] = {n:"Bâton du Grand Pa Wabbit",nn:"BATON DU GRAND PA WABBIT",t:4,d:"Ce bâton a la faculté de pouvoir faire léviter les Cawottes, on se demande bien à quoi cela peut servir en combat.",ep:8,g:1,l:32,wd:true,fm:true,w:1,an:12,e:[5,2,1,1,30,50,false,false],c:"CI>32&CA>10",p:2900};
I.u[337] = {n:"Arc du Bwork Archer",nn:"ARC DU BWORK ARCHER",t:2,d:"C\'est un arc très simple à utiliser, il a été conçu pour s\'adapter à l\'intelligence d\'un Bwork Archer, ce qui le rend presque utilisable par un Tofu. En revanche, il nécessite une force de Craqueleur pour être bandé.",ep:7,g:4,l:20,wd:true,fm:true,w:20,an:10,tw:true,e:[10,5,2,7,30,50,false,true],c:"CS>20&CA>20",p:1300};
I.u[338] = {n:"Lame du Petit Bwork",nn:"LAME DU PETIT BWORK",t:6,d:"Cette épée d\'apparence rudimentaire est très appréciée des Bworks les plus débiles, qui ne savent pas se servir d\'un arc ou d\'un bâton, certains confondent même la lame et le manche ce qui ne manque pas de faire rire leurs adversaires.",ep:1,g:13,l:21,wd:true,fm:true,w:30,an:14,e:[5,5,1,1,50,30,false,false],c:"CS>42",p:1400};
I.u[339] = {n:"Lame du Chafer",nn:"LAME DU CHAFER",t:6,d:"Cette lame appartenait à Metzger le boucher, elle avait été forgée pour découper les os. Le Chafer a cependant réussi à lui dérober son outil, devenu maintenant une arme très redoutée qui est à l\'origine de nombreuses amputations.",ep:1,g:5,l:27,wd:true,fm:true,w:20,an:14,e:[20,6,1,1,15,60,false,false],c:"CS>54&CC>10",p:2100};
I.u[340] = {n:"Leurnettes",nn:"LEURNETTES",t:5,d:"Ces dagues permettent selon la légende de voler tout et n\'importe quoi, mais surtout n\'importe quoi à n\'importe qui, où l\'inverse, mais plus personne ne se souvient de la légende avec exactitude.",ep:1,g:9,l:12,wd:true,fm:true,w:15,an:13,e:[10,3,1,1,30,50,false,false],c:"CA>12",p:600};
I.u[341] = {n:"Dagues Eurfolles",nn:"DAGUES EURFOLLES",t:5,d:"Ces dagues légendaires constituent de vraies pièces de collection pour tout Sram qui se respecte, elles sont tellement vénérées, que certains Srams se refusent à les voler.",ep:7,g:10,l:46,wd:true,fm:true,w:10,an:13,e:[10,2,1,1,30,50,false,false],c:"CA>46",p:5600};
I.u[342] = {n:"Marteau Pickeur",nn:"MARTEAU PICKEUR",t:7,d:"Ce marteau très particulier a été confectionné à partir de becs de Kwaks, ce qui explique pourquoi il pique très fort lorsque l\'on se prend un coup de ce marteau.",ep:5,g:12,l:18,wd:true,fm:true,w:1,an:15,e:[5,4,1,1,50,30,false,false],c:"CI>12&CS>6",p:1100};
I.u[345] = {n:"Petite Pelle Teuze",nn:"PETITE PELLE TEUZE",t:8,d:"Après une soirée un peu trop arrosée à la taverne qui a débouché sur une bagarre, un Enutrof s\'est aperçu que cette pelle conçue à l\'origine pour les grands travaux de construction était une excellente arme.",ep:1,g:8,l:25,wd:true,fm:true,w:40,an:16,e:[5,4,1,1,40,40,false,false],c:"CS>5&CC>20",p:1900};
I.u[346] = {n:"Anneau Rak",nn:"ANNEAU RAK",t:9,d:"Selon certaines légendes, cet anneau protège son porteur des intempéries. Mais ceci n\'a jamais encore pu être prouvé, puisqu\'il ne pleut jamais en Amakna.",ep:1,g:34,l:17,wd:true,fm:true,w:4,p:1000};
I.u[347] = {n:"Rhizome Doré",nn:"RHIZOME DORE",t:8,d:"Cette pelle de jardinier est particulièrement efficace pour couper les rhizomes, des mauvaises herbes. C\'est également une excellente arme dès qu\'il s\'agit de couper autre chose que des rhizomes.",ep:1,g:10,l:40,wd:true,fm:true,w:30,an:16,e:[10,4,1,1,50,50,false,false],c:"CS>10&CC>30",p:4400};
I.u[348] = {n:"Bottes de Céleri-Thé",nn:"BOTTES DE CELERI-THE",t:11,d:"Ces bottes appartenaient à un cuisinier très pressé, qui allait chercher son céleri et son thé à toute vitesse à travers la campagne d\'Amakna.",ep:2,g:5,l:53,wd:true,fm:true,w:20,p:6700};
I.u[350] = {n:"Argent",nn:"ARGENT",t:39,d:"L\'argent est un métal brillant très apprécié des bijoutiers, contrairement à ce que certaines personnes essaient de vous faire croire, il n\'a aucun effet particulier sur les vampires.",ep:1,g:28,l:60,wd:true,fm:true,w:5,p:0};
I.u[351] = {n:"Bière d\'Amakna",nn:"BIERE D\'AMAKNA",t:37,d:"Cette bière vous permettra de restaurer vos points de vie. Et comme le dit un brillant dicton paternel. Il vaut mieux la bière dans le corps que le corps dans la bière.",ep:2,g:4,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:2};
I.u[352] = {n:"Petite Epée Sacrée",nn:"PETITE EPEE SACREE",t:6,d:"Une épée sainte distribuée aux jeunes guerriers durant leur formation.",ep:1,g:1,l:13,wd:true,fm:true,w:30,e:[5,4,1,1,50,50,false,false],c:"CS>26&CV>5",p:700};
I.u[353] = {n:"Epée du Petit Chevalier",nn:"EPEE DU PETIT CHEVALIER",t:6,d:"L\'épée du  Petit Chevalier est une épée solide et efficace, bien utilisée elle peut causer de gros dommages à vos ennemis.",ep:7,g:9,l:21,wd:true,fm:true,w:30,an:14,e:[4,5,1,1,30,30,false,false],c:"CS>42",p:1400};
I.u[354] = {n:"La Klebik",nn:"LA KLEBIK",t:6,d:"Ce type d\'épée était forgé pour les gardes d\'un commerçant aussi riche que réputé. Plutôt destinée à effrayer les voleurs, elle n\'en reste pas moins redoutable pour achever les voleurs ou les clients honnêtes, mais un peu usants.",ep:1,g:11,l:25,wd:true,fm:true,w:20,e:[12,4,1,1,30,60,false,false],c:"CS>50&CW>5",p:1850};
I.u[355] = {n:"L\'Anneau de Valeur",nn:"L\'ANNEAU DE VALEUR",t:9,d:"Cet anneau apporte force et courage à son porteur.",ep:1,g:7,l:12,wd:true,fm:true,w:4,p:600};
I.u[356] = {n:"Ceinture d\'Endurance",nn:"CEINTURE D\'ENDURANCE",t:10,d:"Cette ceinture rend son porteur plus résistant et endurant.",ep:1,g:3,l:27,wd:true,fm:true,w:16,p:2200};
