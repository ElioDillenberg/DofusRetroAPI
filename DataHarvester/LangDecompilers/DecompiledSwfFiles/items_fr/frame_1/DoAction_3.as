I.u[924] = {n:"Puissantes Bottes de Klime",nn:"PUISSANTES BOTTES DE KLIME",t:11,d:"L\'agilité que confère ces bottes permettait à Klime le cordonnier de monter facilement sur le dos de n\'importe quel Bouftou. Il est mort en confondant un Bouftou avec une Gelée, après une soirée trop arrosée passée à la taverne.",ep:1,g:9,l:19,wd:true,fm:true,w:20,p:1200};
I.u[926] = {n:"Bois Envoûté",nn:"BOIS ENVOUTE",t:38,d:"Ce bois a été envoûté par un Mage Abraknyde, pour donner vie à un petit Abraknyde une fois trempé dans l\'eau. Il va de soi que ce bois est extrêmement magique.",ep:1,g:140,l:45,wd:true,fm:true,w:30,p:0};
I.u[927] = {n:"Fendeuse d\'Abraknyde Ancestral",nn:"FENDEUSE D\'ABRAKNYDE ANCESTRAL",t:19,d:"Cette hache de guerre a été forgée par Kademlia Boulgoure, pour combattre l\'Abraknyde Ancestral. Seuls les meilleurs guerriers sont à même de pouvoir l\'utiliser.",ep:3,g:13,l:93,wd:true,fm:true,w:100,an:17,tw:true,e:[10,4,1,1,40,50,false,false],c:"CS>200&CI>100",p:22400};
I.u[928] = {n:"Lame d\'Ogralimde",nn:"LAME D\'OGRALIMDE",t:6,d:"Ogralimde était un grand guerrier qui devait sa force à Adyol son maître, un puissant Bwork Mage d\'une intelligence surprenante. Mais l\'élève n\'a jamais dépassé le maître, contrairement à ce que raconte la légende.",ep:1,g:12,l:45,wd:true,fm:true,w:10,an:14,e:[2,4,1,1,25,25,false,false],c:"CS>100",p:4500};
I.u[929] = {n:"Kristalite",nn:"KRISTALITE",t:50,d:"Cette pierre magique au nom particulier n\'est pas à mettre en toutes les mains étant donné la puissance qu\'elle peut conférer aux armes ou aux bijoux.",ep:22,g:705,l:120,wd:true,fm:true,w:20,p:500};
I.u[930] = {n:"Carte au trésor des Mineurs Sombres",nn:"CARTE AU TRESOR DES MINEURS SOMBRES",t:24,d:"Cette carte indique l\'emplacement du coffre des Mineurs Sombres : \n" + "\"" + "Le coffre est situé sur une île, il faut passer derrière une fonderie pour y accéder" + "\"" + ".",ep:1,g:6,l:1,wd:true,fm:true,w:0,p:1};
I.u[931] = {n:"Carte au trésor des Boulangers Sombres",nn:"CARTE AU TRESOR DES BOULANGERS SOMBRES",t:24,d:"Cette carte indique l\'emplacement du coffre des Boulangers Sombres: \n" + "\"" + "Le coffre est situé sur une île au sud d\'Amakna, il faut passer derrière des rochers entre deux petits étangs" + "\"" + ".",ep:1,g:10,l:1,wd:true,fm:true,w:0,p:1};
I.u[932] = {n:"Petite Cape de Rougeur",nn:"PETITE CAPE DE ROUGEUR",t:17,d:"Cette cape augmente la vie de son porteur.",ep:1,g:13,l:8,wd:true,fm:true,w:5,p:400};
I.u[933] = {n:"Cape de Rougeur",nn:"CAPE DE ROUGEUR",t:17,d:"Cette cape augmente la vie de son porteur.",ep:1,g:13,l:13,wd:true,fm:true,w:5,p:650};
I.u[934] = {n:"Grande Cape de Rougeur",nn:"GRANDE CAPE DE ROUGEUR",t:17,d:"Cette cape augmente la vie de son porteur.",ep:1,g:13,l:16,wd:true,fm:true,w:5,p:900};
I.u[935] = {n:"Petite Cape Bleutée",nn:"PETITE CAPE BLEUTEE",t:17,d:"Cette cape augmente la chance de son porteur.",ep:1,g:18,l:1,wd:true,fm:true,w:5,p:560};
I.u[936] = {n:"Cape Bleutée",nn:"CAPE BLEUTEE",t:17,d:"Cette cape augmente la chance de son porteur.",ep:1,g:18,l:3,wd:true,fm:true,w:5,p:1020};
I.u[937] = {n:"Grande Cape Bleutée",nn:"GRANDE CAPE BLEUTEE",t:17,d:"Cette cape augmente la chance de son porteur.",ep:1,g:18,l:6,wd:true,fm:true,w:5,p:1560};
I.u[938] = {n:"Carte au trésor des Forgerons Sombres",nn:"CARTE AU TRESOR DES FORGERONS SOMBRES",t:24,d:"Cette carte indique l\'emplacement du coffre des Forgerons Sombres: \n-- a faire --",ep:1,g:9,l:1,wd:true,fm:true,w:0,p:1};
I.u[939] = {n:"Carte au trésor des Bworks",nn:"CARTE AU TRESOR DES BWORKS",t:24,d:"Cette carte indique l\'emplacement du coffre des Bworks : \n" + "\"" + "L\'accès au coffre est situé dans une impasse, dans le Sud-Est de la forêt sombre. Il sera certainement protégé par un groupe de Bworks" + "\"" + ".",ep:3,g:7,l:1,wd:true,fm:true,w:0,p:1};
I.u[940] = {n:"Louffeur",nn:"LOUFFEUR",t:16,d:"Le Louffeur prévient des dégâts physiques.",ep:1,g:20,l:1,wd:true,fm:true,w:1,p:1500};
I.u[941] = {n:"Bandeau Komintot",nn:"BANDEAU KOMINTOT",t:16,d:"Le Komintot permet d\'augmenter la sagesse de son porteur.",ep:1,g:22,l:1,wd:true,fm:true,w:1,p:100};
I.u[942] = {n:"Lisseur",nn:"LISSEUR",t:16,d:"Le Lisseur prévient des dégâts magiques.",ep:4,g:23,l:1,wd:true,fm:true,w:1,p:150};
I.u[943] = {n:"Petit Bandeau de Vitalité",nn:"PETIT BANDEAU DE VITALITE",t:16,d:"Ce Bandeau permet d\'augmenter la vitalité de son porteur.",ep:1,g:20,l:1,wd:true,fm:true,w:1,p:1500};
I.u[944] = {n:"Bandeau de Vitalité",nn:"BANDEAU DE VITALITE",t:16,d:"Ce Bandeau permet d\'augmenter la vitalité de son porteur.",ep:1,g:20,l:1,wd:true,fm:true,w:2,p:3000};
I.u[945] = {n:"Petite Cape Sombre",nn:"PETITE CAPE SOMBRE",t:17,d:"Cette cape augmente la sagesse de son porteur.",ep:1,g:15,l:8,wd:true,fm:true,w:5,p:375};
I.u[946] = {n:"Cape Sombre",nn:"CAPE SOMBRE",t:17,d:"Cette cape augmente la sagesse de son porteur.",ep:1,g:15,l:12,wd:true,fm:true,w:5,p:600};
I.u[947] = {n:"Grande Cape Sombre",nn:"GRANDE CAPE SOMBRE",t:17,d:"Cette cape augmente la sagesse de son porteur.",ep:1,g:15,l:19,wd:true,fm:true,w:5,p:1200};
I.u[948] = {n:"Abracape",nn:"ABRACAPE",t:17,d:"Cette magnifique cape est un brillant mélange entre l\'essence d\'Abraknyde et le bon Bouftou des familles.",ep:1,g:16,l:10,wd:true,fm:true,w:5,p:10000};
I.u[949] = {n:"Grand Bandeau de Vitalité",nn:"GRAND BANDEAU DE VITALITE",t:16,d:"Ce Bandeau permet d\'augmenter la vitalité de son porteur.",ep:1,g:20,l:3,wd:true,fm:true,w:2,p:4500};
I.u[951] = {n:"Aiguilles et Fil",nn:"AIGUILLES ET FIL",t:20,d:"Ces aiguilles et ce fil vous permettront d\'exercer le métier de tailleur.",ep:2,g:4,l:1,wd:true,fm:true,w:1,an:4,e:[1,4,1,1,100,100,false,false],p:100};
I.u[952] = {n:"Cape du Mulou Fou",nn:"CAPE DU MULOU FOU",t:17,d:"Cette cape a été confectionné grâce à des poils et autres élèments organiques de diverses Mulous... On peut dire qu\'elle respire la puissance.",ep:1,g:3,l:33,wd:true,fm:true,w:5,p:3075};
I.u[953] = {n:"Cape du Tofu Fou",nn:"CAPE DU TOFU FOU",t:17,d:"Cette cape, cousue main avec des plumes de Tofus, permet au héros qui la porte de courir plus vite et de s\'échapper plus facilement des zones de tacle.",ep:1,g:19,l:36,wd:true,fm:true,w:10,p:3075};
I.u[954] = {n:"La Samoulaille",nn:"LA SAMOULAILLE",t:17,d:"La Samoulaille est une cape légendaire, brodée par les tailleurs pour les guerriers préférant les combats au corps à corps.",ep:7,g:2,l:22,wd:true,fm:true,w:10,p:1500};
I.u[955] = {n:"Cape Hilère",nn:"CAPE HILERE",t:17,d:"La cape Hilère est un type de cape très demandée par les coiffeurs. Ils adorent se rouler dedans. Elle rend les cheveux de son porteur plus résistants aux chocs.",ep:8,g:5,l:34,wd:true,fm:true,w:10,p:3275};
I.u[956] = {n:"Cape Rice",nn:"CAPE RICE",t:17,d:"Cette cape augmente la portée de son porteur, lui permettant d\'atteindre des cibles très éloignées, ou d\'impressionner les aventuriers un peu naïfs.",ep:8,g:7,l:33,wd:true,fm:true,w:10,p:3000};
I.u[957] = {n:"La Boufteuse",nn:"LA BOUFTEUSE",t:17,d:"La Boufteuse augmente les dégâts et la vitalité de son porteur. Cependant elle dégage une forte odeur, et il n\'est pas rare qu\'un bouftou vous prenne pour l\'un des siens.",ep:5,g:12,l:23,wd:true,fm:true,w:15,p:1575};
I.u[958] = {n:"Dofusteuse",nn:"DOFUSTEUSE",t:17,d:"Cette cape est certainement l\'une des plus puissantes au monde. Elle augmente toutes les caractéristiques de son porteur.",ep:1,g:8,l:6,wd:true,fm:true,w:15,p:100000};
I.u[959] = {n:"Carte au trésor 5",nn:"CARTE AU TRESOR 5",t:24,d:"-- a faire --",ep:1,g:10,l:1,wd:true,fm:true,w:0,p:1};
I.u[960] = {n:"Potion Wabbit",nn:"POTION WABBIT",t:24,d:"Cette potion est un remède contre la myxomawose. Une gorgée de ce breuvage vous permet de visiter l\'île des wabbits sans craindre pour votre vie.",ep:5,g:2,l:1,wd:true,fm:true,w:0,p:1};
I.u[961] = {n:"Clé Wabbit",nn:"CLE WABBIT",t:24,d:"Une des trois clés permettant d\'ouvrir la porte du château wabbit.",ep:8,g:3,l:1,wd:true,fm:true,w:0,p:1};
I.u[962] = {n:"Clé Wabbit",nn:"CLE WABBIT",t:24,d:"Une des trois clés permettant d\'ouvrir la porte du château wabbit.",ep:8,g:4,l:1,wd:true,fm:true,w:0,p:1};
I.u[963] = {n:"Clé Wabbit",nn:"CLE WABBIT",t:24,d:"Une des trois clés permettant d\'ouvrir la porte du château wabbit.",ep:8,g:5,l:1,wd:true,fm:true,w:0,p:1};
I.u[965] = {n:"Pierre de Dopeul",nn:"PIERRE DE DOPEUL",t:50,d:"Cette pierre vient du monde des Dopeuls. Elle a une très grande valeur auprès de certains habitants d\'Amakna.",ep:2,g:663,l:41,wd:true,fm:true,w:15,p:10000};
I.u[966] = {n:"Manuel du Tailleur",nn:"MANUEL DU TAILLEUR",t:25,d:"(Double clic pour apprendre le métier de Tailleur) \nCe manuel de tailleur vous permet d\'apprendre les ficelles du métier. Pour ceux dont l\'imagination ferait défaut, les premières pages proposent des patrons simples à découper.",ep:1,g:13,l:1,wd:true,fm:true,w:1,u:true,p:100};
I.u[967] = {n:"Cape Fulgurante",nn:"CAPE FULGURANTE",t:17,d:"Cette magnifique cape augmente les points d\'action de son porteur. Elle augmente également sa vitalité et sa sagesse.",ep:13,g:78,l:55,wd:true,fm:true,w:20,p:100000};
I.u[968] = {n:"Ceinture Fulgurante",nn:"CEINTURE FULGURANTE",t:10,d:"Cette magnifique ceinture augmente les points d\'action de son porteur. Elle augmente également sa vitalité et sa sagesse.",ep:1,g:10,l:30,wd:true,fm:true,w:200,p:100000};
I.u[969] = {n:"Couronne du Wa Wabbit",nn:"COURONNE DU WA WABBIT",t:16,d:"Cette magnifique couronne a appartenu au tristement célèbre Wa Wabbit.",ep:8,g:21,l:30,wd:true,fm:true,w:30,s:40,p:3000};
I.u[970] = {n:"Bâton du Wa Wabbit",nn:"BATON DU WA WABBIT",t:4,d:"Ce bâton appartenait au tristement célèbre Wa Wabbit.",ep:8,g:21,l:25,wd:true,fm:true,w:20,an:12,tw:true,e:[5,5,1,1,30,20,false,false],s:40,p:2500};
I.u[971] = {n:"Cape du Wa Wabbit",nn:"CAPE DU WA WABBIT",t:17,d:"Cette cape appartenait au tristement célèbre Wa Wabbit. Elle est encore imprégnée de sa transpiration, ce qui vous permettra de vous remémorer ce magnifique combat à chaque fois que vous la porterez.",ep:8,g:21,l:30,wd:true,fm:true,w:25,s:40,p:2525};
I.u[972] = {n:"Dofus Cawotte",nn:"DOFUS CAWOTTE",t:23,d:"Le Dofus Cawotte augmente la sagesse de son porteur.",ep:1,g:6,l:6,wd:true,fm:true,w:5,p:100000};
I.u[973] = {n:"Patte d\'Araknelle",nn:"PATTE D\'ARAKNELLE",t:24,d:"Une patte très fine, mais très poilue. Les meilleurs Alchimistes arrivent à créer de puissants poisons avec le jus qu\'ils arrivent à extraire de ces pattes.",ep:2,g:11,l:1,wd:true,fm:true,w:0,p:1};
I.u[974] = {n:"Tête du Paysan en détresse",nn:"TETE DU PAYSAN EN DETRESSE",t:24,d:"Ce qui devait arriver est arrivé, le Paysan en détresse a fini par perdre la tête.",ep:1,g:12,l:1,wd:true,fm:true,w:0,p:1};
I.u[975] = {n:"Canine du Vampire Captif",nn:"CANINE DU VAMPIRE CAPTIF",t:24,d:"Cette dent très pointue, provient d\'un Vampire. La posséder dans son inventaire ou la porter autour du cou vous permettra d\'impressionner vos amis en leur faisant croire que vous êtes un chasseur de Vampires.",ep:1,g:13,l:1,wd:true,fm:true,w:0,p:1};
I.u[976] = {n:"Citwouille",nn:"CITWOUILLE",t:16,d:"La citwouille a la particularité d\'empêcher la dispersion des particules de magie, cela en fait un très bon isolant magique. C\'est d\'ailleurs pour cela que certains guerriers se servent de ce légume comme couvre-chef.",ep:4,g:24,l:20,wd:true,fm:true,w:10,p:500};
I.u[989] = {n:"Shigekax Citron",nn:"SHIGEKAX CITRON",t:42,d:"Retrouvez la saveur et tout le charme des sucreries d\'antan... avec ce bonbon au citron préparé de façon traditionnelle.",ep:1,g:23,l:1,wd:true,fm:true,w:1,u:true,p:200};
I.u[990] = {n:"Shigekax Fraise",nn:"SHIGEKAX FRAISE",t:42,d:"Retrouvez la saveur et tout le charme des sucreries d\'antan... avec ce bonbon à la fraise préparé de façon traditionnelle.",ep:13,g:25,l:1,wd:true,fm:true,w:1,u:true,p:140};
I.u[991] = {n:"Shigekax Menthe",nn:"SHIGEKAX MENTHE",t:42,d:"Retrouvez la saveur et tout le charme des sucreries d\'antan... avec ce bonbon à la menthe préparé de façon traditionnelle.",ep:2,g:24,l:1,wd:true,fm:true,w:1,u:true,p:400};
I.u[992] = {n:"Shigekax Cerise",nn:"SHIGEKAX CERISE",t:42,d:"Retrouvez la saveur et tout le charme des sucreries d\'antan... avec ce bonbon à la cerise préparé de façon traditionnelle.",ep:2,g:19,l:1,wd:true,fm:true,w:1,u:true,p:100};
I.u[993] = {n:"Vertgely",nn:"VERTGELY",t:42,d:"Préparé à base de gelée menthe, même si il a rebuté plus d\'un végétarien, il n\'en reste pas moins une des spécialités d\'Amakna.",ep:5,g:21,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:1};
I.u[994] = {n:"Rougely",nn:"ROUGELY",t:42,d:"Préparé à base de gelée à la fraise, même si il a rebuté plus d\'un végétarien, il n\'en reste pas moins une des spécialités d\'Amakna.",ep:5,g:20,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:1};
I.u[995] = {n:"Blugely",nn:"BLUGELY",t:42,d:"Préparé à base de gelée bleue, même s\'il a rebuté plus d\'un végétarien, il n\'en reste pas moins une des spécialités d\'Amakna.",ep:5,g:22,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:1};
I.u[996] = {n:"Multygely",nn:"MULTYGELY",t:42,d:"La multygely a été interdite à la vente, car elle a provoqué la disparition de nombreux enfants. \nIl parait qu\'elle envoie la personne qui la mange dans ses pires cauchemars.",ep:5,g:26,l:1,wd:false,fm:true,w:1,c:"Sc!1056",u:true,p:100};
I.u[997] = {n:"Noix de Kokoko",nn:"NOIX DE KOKOKO",t:46,d:"Aussi ronde qu\'un boulet de canon, cette noix possède en plus 3 trous très pratiques pour la soulever et la lancer au loin.",ep:12,g:159,l:42,wd:true,fm:true,w:2,p:1};
I.u[998] = {n:"Caleçon Mignon du Capitaine Pirate",nn:"CALECON MIGNON DU CAPITAINE PIRATE",t:15,d:"Le caleçon du Capitaine Pirate vérifie que le ridicule tue quand même de temps en temps. Mais il semblerait pourtant qu\'il porte chance.",ep:12,g:163,l:82,wd:true,fm:true,w:2,p:1};
I.u[999] = {n:"Toile peu ragoutante",nn:"TOILE PEU RAGOUTANTE",t:55,d:"Gluant et malodorant, ce large tissu n\'en reste pas moins de qualité. Dommage qu\'il soit percé.",ep:12,g:164,l:1,wd:true,fm:true,w:5,p:1};
I.u[1000] = {n:"Tonneau vide",nn:"TONNEAU VIDE",t:15,d:"Ce gros tonneau est vide depuis fort longtemps, on peut quand même deviner tout ce qu\'il a contenu : rhum, kamas, pirates ivres... un peu de tout, mais vraisemblablement jamais d\'eau.",ep:12,g:165,l:1,wd:true,fm:true,w:5,p:1};
I.u[1001] = {n:"Planche en bois de Kokoko",nn:"PLANCHE EN BOIS DE KOKOKO",t:95,d:"Futur fragment de bateau ? Futur sol de maison ? Dans tous les cas, ça n\'est pour l\'instant qu\'une planche comme les autres.",ep:12,g:166,l:42,wd:true,fm:true,w:10,p:10};
I.u[1002] = {n:"Tronc de Kokoko",nn:"TRONC DE KOKOKO",t:38,d:"Comme tous les troncs, celui du Kokotier est droit et solide, mais un peu plus fin que la moyenne.",ep:12,g:167,l:42,wd:true,fm:true,w:1,p:1};
I.u[1003] = {n:"Poutre en Bois de Kokoko",nn:"POUTRE EN BOIS DE KOKOKO",t:38,d:"Une poutre de bois solide à ne pas mettre dans l\'oeil.",ep:12,g:168,l:42,wd:true,fm:true,w:1,p:1};
I.u[1004] = {n:"Indispensable Corde",nn:"INDISPENSABLE CORDE",t:15,d:"Tous les aventuriers savent qu\'il faut toujours avoir une corde comme celle-ci sur soi... Pourtant, personne n\'en emporte jamais.",ep:12,g:170,l:1,wd:true,fm:true,w:1,p:1};
I.u[1005] = {n:"Reçu pour le radeau",nn:"RECU POUR LE RADEAU",t:24,d:"Un reçu signé de la main de Kib Roche, indiquant que votre radeau sera prêt d\'ici quelque temps.",ep:1,g:176,l:1,wd:true,fm:true,w:0,p:1};
I.u[1006] = {n:"Chapeau Pirate",nn:"CHAPEAU PIRATE",t:24,d:"Chapeau aux formes imposantes, insigne de mort brodé sur le devant, fourrure pour faire joli, ce chapeau de pirate est un classique parmi les capitaines.",ep:12,g:14,l:1,wd:true,fm:true,w:0,p:1};
I.u[1007] = {n:"Crochet",nn:"CROCHET",t:24,d:"Accessoire indispensable aux pirates en manque de chance, ce crochet remplace avantageusement une main. Sa couleur lui donne en plus un aspect assez chic.",ep:12,g:15,l:1,wd:true,fm:true,w:0,p:1};
I.u[1008] = {n:"Mâchoire tombante",nn:"MACHOIRE TOMBANTE",t:24,d:"Cette mâchoire en collier provient certainement d\'une colère d\'un capitaine envers un matelot trop hilare à la vue du caleçon de son chef.",ep:12,g:16,l:1,wd:true,fm:true,w:0,p:1};
I.u[1009] = {n:"Crâne de pirate",nn:"CRANE DE PIRATE",t:24,d:"Ce crâne coiffé d\'un bandeau montre bien qu\'un pirate, même mort, peut encore se cacher d\'être chauve.",ep:12,g:17,l:1,wd:true,fm:true,w:0,p:1};
I.u[1010] = {n:"Carapace verte",nn:"CARAPACE VERTE",t:24,d:"La carapace d\'une tortue folle qui se prenait pour un ninja.",ep:12,g:18,l:1,wd:true,fm:true,w:0,p:1};
I.u[1011] = {n:"Carapace jaune",nn:"CARAPACE JAUNE",t:24,d:"La carapace d\'une tortue folle qui avait abusé sans doute de substances dangereuses.",ep:12,g:19,l:1,wd:true,fm:true,w:0,p:1};
I.u[1012] = {n:"Carapace Rouge",nn:"CARAPACE ROUGE",t:24,d:"La carapace d\'une tortue folle qui mangeait des tomates et du fromage sur du pain chaud.",ep:12,g:20,l:1,wd:true,fm:true,w:0,p:1};
I.u[1013] = {n:"Carapace Bleue",nn:"CARAPACE BLEUE",t:24,d:"La carapace d\'une tortue folle qui voulait imiter Zowwo, le Wenard masqué.",ep:12,g:21,l:1,wd:true,fm:true,w:0,p:1};
I.u[1014] = {n:"Masque Vert",nn:"MASQUE VERT",t:24,d:"Masque de Kanniboul rieur, à l\'image de la Verdure, élément d\'assaisonnement de la viande.",ep:12,g:22,l:1,wd:true,fm:true,w:0,p:1};
I.u[1015] = {n:"Masque Jaune",nn:"MASQUE JAUNE",t:24,d:"Masque de kanniboul pleureur, à l\'image de la Foudre, élément qui effraye souvent la jeune viande.",ep:12,g:23,l:1,wd:true,fm:true,w:0,p:1};
I.u[1016] = {n:"Masque Rouge",nn:"MASQUE ROUGE",t:24,d:"Masque de kanniboul colérique, à l\'image du Feu, élément de cuisson de la viande.",ep:12,g:24,l:1,wd:true,fm:true,w:0,p:1};
I.u[1017] = {n:"Masque Bleu",nn:"MASQUE BLEU",t:24,d:"Masque de Kanniboul surpris, à l\'image de l\'Eau, élément désaltérant entre deux bouchées de viande.",ep:12,g:25,l:1,wd:true,fm:true,w:0,p:1};
I.u[1018] = {n:"Kole",nn:"KOLE",t:15,d:"Ce pot de gelée rose à la bonne odeur ne doit surtout pas être goûté. Sinon vous ne pourriez plus jamais parler tant ce liquide est fixant.",ep:12,g:171,l:1,wd:true,fm:true,w:1,p:1};
