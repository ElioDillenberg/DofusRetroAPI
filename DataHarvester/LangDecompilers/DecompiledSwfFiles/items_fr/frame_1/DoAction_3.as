I.u[1324] = {n:"Herbe à chat",nn:"HERBE A CHAT",t:36,d:"Cette plante fait saliver n\'importe quel Ecaflip.",ep:2,g:46,l:1,wd:true,fm:true,w:1,p:5000};
I.u[1325] = {n:"Coque de Noix",nn:"COQUE DE NOIX",t:46,d:"Cette coque, naturellement solide, résiste à tout type d\'agression et représente bien l\'idéologie Féca.",ep:2,g:52,l:1,wd:true,fm:true,w:1,p:5000};
I.u[1326] = {n:"Silex Sanglant",nn:"SILEX SANGLANT",t:51,d:"Cette pierre aussi résistante que coupante semble avoir été utilisé contre quelque chose ou quelqu\'un. Cet aspect brut et dangereux semble illustrer parfaitement la définition d\'un Iop.",ep:2,g:47,l:1,wd:true,fm:true,w:1,p:5000};
I.u[1327] = {n:"Os taillé",nn:"OS TAILLE",t:47,d:"Cet objet de mort devient redoutable une fois travaillé, exactement comme un Sram.",ep:2,g:48,l:1,wd:true,fm:true,w:1,p:5000};
I.u[1328] = {n:"Kama énigmatique",nn:"KAMA ENIGMATIQUE",t:15,d:"Ce Kama est orné d\'une tête de tofu coiffé d\'un chapeau étrange.",ep:1,g:182,l:1,wd:true,fm:true,w:10,p:1};
I.u[1329] = {n:"Bois élastique",nn:"BOIS ELASTIQUE",t:38,d:"Cette grande branche de bois, par son retour élastique et sa forme curviligne, fait immédiatement penser aux Crâ.",ep:2,g:49,l:1,wd:true,fm:true,w:1,p:5000};
I.u[1330] = {n:"Amulette Itby",nn:"AMULETTE ITBY",t:1,d:"Création originale de l\'orfèvre John Les Nonnes, c\'est aujourd\'hui un des bijoux les plus classiques.",ep:1,g:31,l:10,wd:true,fm:true,w:5,p:500};
I.u[1331] = {n:"Médaille Oly",nn:"MEDAILLE OLY",t:1,d:"Oly était une disciple de Iop dont le seul talent était la cuisine. On rapporte que Iop en personne lui aurait donné cette médaille, pour compenser ses trop grandes faiblesses en combat. Cependant seuls les coeurs purs peuvent la porter.",ep:7,g:23,l:51,wd:true,fm:true,w:10,c:"Ps=1&Pa>10",p:7000};
I.u[1332] = {n:"Vieux Trèfle à 4 feuilles",nn:"VIEUX TREFLE A 4 FEUILLES",t:36,d:"Les trèfles à quatre feuilles sont bien plus rares que ceux à cinq feuilles. Lorsque vous en trouvez-un, il est conseillé de le faire sécher entre deux parchemins de sorts.",ep:2,g:50,l:1,wd:true,fm:true,w:1,p:5000};
I.u[1333] = {n:"Potion d\'Etincelle",nn:"POTION D\'ETINCELLE",t:26,d:"Cette potion chauffante, grâce à la faible magie de feu qu\'elle emprisonne, est généralement utilisée pour enchanter un peu toutes sortes d\'objets.",ep:3,g:19,l:1,wd:true,fm:true,w:6,c:"Pj=48|Pj=49|Pj=50|Pj=46|Pj=44|Pj=45|Pj=43|Pj=47",p:250};
I.u[1334] = {n:"Blague de Soufre",nn:"BLAGUE DE SOUFRE",t:48,d:"Cette petite poche pleine de soufre dégage une odeur insupportable... La même que celle des Osamodas.",ep:2,g:51,l:1,wd:true,fm:true,w:1,p:5000};
I.u[1335] = {n:"Potion de Crachin",nn:"POTION DE CRACHIN",t:26,d:"Cette potion frémissante, grâce à la faible magie d\'eau qu\'elle emprisonne, est généralement utilisée pour enchanter un peu toutes sortes d\'objets.",ep:3,g:22,l:1,wd:true,fm:true,w:6,c:"Pj=48|Pj=49|Pj=50|Pj=46|Pj=44|Pj=45|Pj=43|Pj=47",p:250};
I.u[1336] = {n:"Orties",nn:"ORTIES",t:36,d:"En frottant vivement cette plante contre votre peau, vous souffrirez le martyr. Pourtant, la stimulation provoquée est bénéfique, exactement la philosophie des Sacrieurs",ep:2,g:53,l:1,wd:true,fm:true,w:1,p:5000};
I.u[1337] = {n:"Potion de Courant d\'Air",nn:"POTION DE COURANT D\'AIR",t:26,d:"Cette potion soupirante, grâce à la faible magie d\'air qu\'elle emprisonne, est généralement utilisée pour enchanter un peu toutes sortes d\'objets.",ep:3,g:25,l:1,wd:true,fm:true,w:6,c:"Pj=48|Pj=49|Pj=50|Pj=46|Pj=44|Pj=45|Pj=43|Pj=47",p:250};
I.u[1338] = {n:"Potion de Secousse",nn:"POTION DE SECOUSSE",t:26,d:"Cette potion vibrante, grâce à la faible magie de terre qu\'elle emprisonne, est généralement utilisée pour enchanter un peu toutes sortes d\'objets.",ep:3,g:28,l:1,wd:true,fm:true,w:6,c:"Pj=48|Pj=49|Pj=50|Pj=46|Pj=44|Pj=45|Pj=43|Pj=47",p:250};
I.u[1339] = {n:"Clochettes",nn:"CLOCHETTES",t:35,d:"Ces fleurs de muguet sont vraiment toutes petites, fragiles et mignonnes. On dit de plus qu\'elles ont le pouvoir de calmer l\'esprit et de guérir certains maux. Elles ont sûrement été conçues par une divinité qui souhaitait voir une plante à l\'image des Eniripsas.",ep:2,g:46,l:1,wd:true,fm:true,w:1,p:5000};
I.u[1340] = {n:"Potion d\'Eboulement",nn:"POTION D\'EBOULEMENT",t:26,d:"Cette potion tremblante, grâce à la magie de terre qu\'elle emprisonne, est généralement utilisée pour enchanter correctement toutes sortes d\'objets.",ep:9,g:29,l:25,wd:true,fm:true,w:6,c:"Pj=48|Pj=49|Pj=50|Pj=46|Pj=44|Pj=45|Pj=43|Pj=47",p:500};
I.u[1341] = {n:"Potion d\'Averse",nn:"POTION D\'AVERSE",t:26,d:"Cette potion clapotante, grâce à la magie d\'eau qu\'elle emprisonne, est généralement utilisée pour enchanter correctement toutes sortes d\'objets.",ep:9,g:23,l:25,wd:true,fm:true,w:6,c:"Pj=48|Pj=49|Pj=50|Pj=46|Pj=44|Pj=45|Pj=43|Pj=47",p:500};
I.u[1342] = {n:"Potion de Rafale",nn:"POTION DE RAFALE",t:26,d:"Cette potion sifflante, grâce à la magie d\'air qu\'elle emprisonne, est généralement utilisée pour enchanter correctement toutes sortes d\'objets.",ep:9,g:26,l:25,wd:true,fm:true,w:6,c:"Pj=48|Pj=49|Pj=50|Pj=46|Pj=44|Pj=45|Pj=43|Pj=47",p:500};
I.u[1343] = {n:"Potion de Flambée",nn:"POTION DE FLAMBEE",t:26,d:"Cette potion crépitante, grâce à la magie de feu qu\'elle emprisonne, est généralement utilisée pour enchanter correctement toutes sortes d\'objets.",ep:9,g:20,l:25,wd:true,fm:true,w:1,c:"Pj=48|Pj=49|Pj=50|Pj=46|Pj=44|Pj=45|Pj=43|Pj=47",p:500};
I.u[1344] = {n:"Verveine",nn:"VERVEINE",t:36,d:"Cette plante spéciale permet de faire dormir n\'importe qui bien plus facilement. Cela doit probablement être l\'emblême des Sadida.",ep:2,g:53,l:1,wd:true,fm:true,w:1,p:5000};
I.u[1345] = {n:"Potion d\'Incendie",nn:"POTION D\'INCENDIE",t:26,d:"Cette potion fulminante, grâce à la puissante magie de feu qu\'elle emprisonne, est généralement utilisée pour enchanter puissamment toutes sortes d\'objets.",ep:4,g:21,l:50,wd:true,fm:true,w:6,c:"Pj=48|Pj=49|Pj=50|Pj=46|Pj=44|Pj=45|Pj=43|Pj=47",p:800};
I.u[1346] = {n:"Potion de Tsunami",nn:"POTION DE TSUNAMI",t:26,d:"Cette potion inondante, grâce à la puissante magie d\'eau qu\'elle emprisonne, est généralement utilisée pour enchanter puissamment toutes sortes d\'objets.",ep:4,g:24,l:50,wd:true,fm:true,w:6,c:"Pj=48|Pj=49|Pj=50|Pj=46|Pj=44|Pj=45|Pj=43|Pj=47",p:800};
I.u[1347] = {n:"Potion d\'Ouragan",nn:"POTION D\'OURAGAN",t:26,d:"Cette potion hurlante, grâce à la puissante magie d\'air qu\'elle emprisonne, est généralement utilisée pour enchanter puissamment toutes sortes d\'objets.",ep:4,g:27,l:50,wd:true,fm:true,w:6,c:"Pj=48|Pj=49|Pj=50|Pj=46|Pj=44|Pj=45|Pj=43|Pj=47",p:800};
I.u[1348] = {n:"Potion de Séisme",nn:"POTION DE SEISME",t:26,d:"Cette potion embourbante, grâce à la puissante magie de terre qu\'elle emprisonne, est généralement utilisée pour enchanter puissamment toutes sortes d\'objets.",ep:4,g:30,l:50,wd:true,fm:true,w:6,c:"Pj=48|Pj=49|Pj=50|Pj=46|Pj=44|Pj=45|Pj=43|Pj=47",p:800};
I.u[1350] = {n:"Arc Holic",nn:"ARC HOLIC",t:2,d:"Cet arc a été créé par un magicien porté sur la bouteille. Afin de résoudre son problème, il a inventé cet arc spécialisé dans l\'eau.",ep:3,g:4,l:15,wd:true,fm:true,w:10,an:10,tw:true,e:[5,5,3,6,30,50,false,true],p:250};
I.u[1351] = {n:"Arc du Xueluom",nn:"ARC DU XUELUOM",t:2,d:"" + "\"" + "Avec un arc, ça passe ou ça casse" + "\"" + ", dit le proverbe. Les sages éviteront cet arc, il serait soi-disant porteur du mauvais oeil...",ep:1,g:51,l:30,wd:true,fm:true,w:10,an:10,tw:true,e:[10,5,2,6,10,10,false,true],c:"CC>30",p:1600};
I.u[1352] = {n:"Arc de Boo",nn:"ARC DE BOO",t:2,d:"Cet arc prend sa force aussi bien de la terre que de l\'eau. Malheur à ceux qui ne maîtrisent ni l\'un ni l\'autre...",ep:2,g:6,l:50,wd:true,fm:true,w:10,an:10,tw:true,e:[10,5,2,8,30,50,false,true],p:5000};
I.u[1353] = {n:"Arc à Poulie Sciée",nn:"ARC A POULIE SCIEE",t:2,d:"Cet arc, dont toutes les poulies ont été sciées, était, il y a fort longtemps, le symbole de la lutte du bien contre le mal. \nEncore aujourd\'hui, on peut sentir en lui la flamme des anciens justiciers.",ep:12,g:10,l:41,wd:true,fm:true,w:10,an:10,tw:true,e:[10,5,2,8,30,50,false,true],c:"Ps=1&Pa>10&CI>30&CW>30",p:200};
I.u[1354] = {n:"L\'Arc à Hick",nn:"L\'ARC A HICK",t:2,d:"Hick était un Crâ maudit : il n\'arrivait jamais à se débarrasser de son hoquet. Cependant, il finit un jour par jeter son arc au feu pour le remplacer par une épée, et son mal disparu mystérieusement...",ep:13,g:11,l:50,wd:true,fm:true,w:10,an:10,tw:true,e:[5,5,2,6,40,40,false,true],p:1000};
I.u[1355] = {n:"Arc Hidsad",nn:"ARC HIDSAD",t:2,d:"Le créateur de cet arc, un certain Marc Hidsad, n\'a jamais très bien su s\'en servir. Il pouvait tout autant infliger de sévères blessures à ses ennemis que de se blesser lui-même.",ep:14,g:12,l:40,wd:true,fm:true,w:10,an:10,tw:true,e:[5,5,2,7,30,50,false,true],p:4000};
I.u[1356] = {n:"Demi-Baguette",nn:"DEMI-BAGUETTE",t:3,d:"Rika Zarouil la boulangère fut connue pour avoir été l\'eniripsa la plus petite de la contrée. Elle faisait son pain dans des petits-fours en carton et se battait avec des demi-baguettes.",ep:1,g:13,l:11,wd:true,fm:true,w:10,an:11,e:[5,4,2,2,30,50,false,true],p:1000};
I.u[1357] = {n:"Daguette",nn:"DAGUETTE",t:3,d:"Un sculpteur un peu farceur tenta de faire passer ses baguettes pour des dagues. Son stratagème n\'eu aucun succès mais ses baguettes, elles, en eurent.",ep:7,g:17,l:35,wd:true,fm:true,w:10,an:11,e:[5,4,2,2,40,40,false,true],c:"CI>25",p:3500};
I.u[1359] = {n:"Le Tison Fûté",nn:"LE TISON FUTE",t:3,d:"Les origines de cette baguette restent obscures. Cependant, elle a toujours aidé ses heureux possesseurs à trouver les chemins les moins encombrés d\'Amakna, ce qui peut servir les jours de grande influence sur le domaine du bouftou royal.",ep:9,g:12,l:59,wd:true,fm:true,w:10,an:11,e:[7,3,2,3,30,50,false,true],c:"CI>70&CS>30",p:5000};
