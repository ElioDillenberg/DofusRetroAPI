I.u[11808] = {n:"Certificat de mise en chanil : Estok",nn:"CERTIFICAT DE MISE EN CHANIL : ESTOK",t:77,d:"Ce certificat décrit les caractéristiques du familier que vous avez placé au Chanil.",ep:0,g:84,l:1,wd:false,fm:true,w:1,p:100};
I.u[11809] = {n:"Estok",nn:"ESTOK",t:18,d:"N\'est-il pas mignon ? Armé jusqu\'au dents, certes, mais pouvez-vous en attendre autrement d\'un petit allié de Iop ? En attendant, vous en êtes l\'heureux propriétaire maintenant, alors faites lui plaisir : montrez lui ce que c\'est de la vraie baston !",ep:0,g:65,l:1,wd:true,fm:true,w:1,p:1};
I.u[11811] = {n:"Tempotion d\'Expérience",nn:"TEMPOTION D\'EXPERIENCE",t:12,d:"Fabriquée à partir de tempotons, cette potion vous donnera beaucoup d\'expérience.",ep:1,g:43,l:1,wd:true,fm:false,w:1,u:true,p:1};
I.u[11812] = {n:"Écu Cornu",nn:"ECU CORNU",t:82,d:"S\'il lui arrive de sortir les griffes ou les crocs, il semble tout naturel qu\'Osamodas sorte aussi les défenses... pour se défendre. Certains voient en ce bouclier un furieux porkass, d\'autres un minotoror en train de charger. D\'autres encore font volte-face sans se poser de questions.",ep:0,g:111,l:1,wd:false,fm:false,w:5,ce:true,p:0};
