I.u[11621] = {n:"Bouclier Draconique Polykrome",nn:"BOUCLIER DRACONIQUE POLYKROME",t:82,d:"Façonné à partir d\'une écaille de Grozilla, ce lourd bouclier est d\'une prestance exceptionnelle. Seuls de rares et redoutables combattants arborent fièrement ce pavois, en plus, celui-ci est particulièrement coloré.",ep:35,g:93,l:1,wd:false,fm:false,w:10,ce:true,p:0};
I.u[11622] = {n:"Coquille de Krokille",nn:"COQUILLE DE KROKILLE",t:24,d:"Cette coquille est lourde, encombrante et dégage une forte odeur nauséabonde. Pas de doute : il faut s\'en débarasser au plus vite.",ep:35,g:820,l:1,wd:false,fm:false,w:0,p:0};
I.u[11623] = {n:"Cadeau Coloré - Dantgoule",nn:"CADEAU COLORE - DANTGOULE",t:89,d:"Ce cadeau comprend une coiffe Polykrome et un Mimibiote. Attention car l\'encre sur le paquet peut déteindre... ça vous apprendra à fourrer vos mains partout.",ep:1,g:43,l:1,wd:false,fm:true,w:10,u:true,p:1};
I.u[11700] = {n:"Potion de protection enténébrée",nn:"POTION DE PROTECTION ENTENEBREE",t:24,d:"Cette potion vous permet de ne pas être affecté par l\'écrasante puissance d\'Ilyzaelle. Un objet indispensable, donc.",ep:35,g:821,l:200,wd:true,fm:false,w:0,p:1};
I.u[11701] = {n:"Potion de protection opalescente",nn:"POTION DE PROTECTION OPALESCENTE",t:24,d:"Cette potion vous permet de ne pas être affecté par l\'écrasante perfidie de Qu\'Tan. Un objet indispensable, donc.",ep:35,g:822,l:200,wd:true,fm:false,w:0,p:1};
I.u[11702] = {n:"Dragoune Rouge",nn:"DRAGOUNE ROUGE",t:18,d:"On raconte que cette Dragoune en avait marre d\'être comparé aux fesses roses d\'une femelle Bwork et voulait se différencier de ses confrères. C\'est pour cela qu\'elle s\'est enfuit sur l\'Île de Moon pour prendre un bon bain de soleil loin de tout. Regardez-là maintenant avec son nouveau teint de vrai dragon flamboyant !",ep:3,g:64,l:1,wd:true,fm:true,w:10,c:"PZ=1",p:1};
I.u[11703] = {n:"Chapeau d\'Ankanniversaire",nn:"CHAPEAU D\'ANKANNIVERSAIRE",t:16,d:"Ce chapeau festif vous rappelle un rêve très étrange que vous aviez fait il y a déjà deux ans. Comme quoi, vous avez bonne mémoire quand vous y mettez du vôtre !",ep:3,g:284,l:1,wd:true,fm:false,w:10,ce:true,p:100};
I.u[11704] = {n:"Fantôme de Dragoune Rouge",nn:"FANTOME DE DRAGOUNE ROUGE",t:90,d:"Ce Fantôme de Dragoune Rouge n\'a pas encore quitté le monde des vivants, il ne tient qu\'à vous de trouver une façon de le ressusciter, ou de l\'abandonner lâchement une bonne fois pour toutes.",ep:3,g:64,l:1,wd:true,fm:true,w:1,p:1};
I.u[11705] = {n:"Certificat de Mise en Chanil : Dragoune Rouge",nn:"CERTIFICAT DE MISE EN CHANIL : DRAGOUNE ROUGE",t:77,d:"Ce certificat décrit les caractéristiques de la Dragoune Rouge que vous avez placé au chanil.",ep:3,g:76,l:1,wd:false,fm:true,w:1,p:100};
I.u[11706] = {n:"Eupéoh pour Dragoune Rouge",nn:"EUPEOH POUR DRAGOUNE ROUGE",t:116,d:"En utilisant ce comprimé puissant, vous augmenterez les capacités de votre Dragoune Rouge de façon définitive. Attention, une seule dose sera prise en compte par votre familier, plusieurs tentatives provoqueront des résultats décevants.",ep:1,g:61,l:1,wd:true,fm:true,w:1,c:"PO=11702",u:true,p:1};
I.u[11707] = {n:"Cadeau Coloré - Champcoiffe",nn:"CADEAU COLORE - CHAMPCOIFFE",t:89,d:"Ce cadeau comprend une coiffe Polykrome et un Mimibiote. Attention car l\'encre sur le paquet peut déteindre... ça vous apprendra à fourrer vos mains partout.",ep:1,g:43,l:1,wd:false,fm:true,w:10,u:true,p:1};
I.u[11708] = {n:"Coiffe du Cherchogrine",nn:"COIFFE DU CHERCHOGRINE",t:16,d:"Elles vous font tourner la tête, occupent vos pensées nuit et jour. Les ogrines ! Aussi déterminé(e) que le familier qui les renifle à des kilokamètres à la ronde, vous avez à présent l\'honneur de lui ressembler.  ",ep:0,g:288,l:1,wd:false,fm:false,w:20,ce:true,p:0};
I.u[11709] = {n:"Cape du Cherchogrine",nn:"CAPE DU CHERCHOGRINE",t:17,d:"Aussi transparente que vos intentions (à savoir dénicher un max d\'ogrines !), cet accessoire du plus bel effet reflète votre passion pour les larmes d\'ogre précieuses. En plus, elle a des grelots.",ep:0,g:180,l:1,wd:false,fm:false,w:10,ce:true,p:0};
I.u[11710] = {n:"Bouclier du Cherchogrine",nn:"BOUCLIER DU CHERCHOGRINE",t:82,d:"Fait de véritables larmes d\'ogre naturelles, ce bouclier ne sera néanmoins pas suffisant pour vous sauver face à l\'une de ses créatures. Cela dit, il vous protégera de ses gigantesques postillons en cas d\'éternuement.",ep:0,g:94,l:1,wd:false,fm:false,w:5,ce:true,p:0};
I.u[11711] = {n:"Miniduk",nn:"MINIDUK",t:18,d:"Un problème, un bug ? Un aventurier malin vous a donné 10% de vos kamas ? Pas de panique ! Vous n\'avez pas besoin de contacter le support Ankama car le Miniduk sera là pour vous guider mais aussi sécher vos larmes tout au long de votre aventure dans le Monde des Douze.",ep:36,g:68,l:1,wd:true,fm:true,w:1,c:"PZ=1",p:1};
I.u[11712] = {n:"Tricorne du Pirate",nn:"TRICORNE DU PIRATE",t:16,d:"Aucun pirate n\'a réussi à toucher ce chapeau lors d\'un combat à l\'épée dû à l\'agilité et la souplesse des porteurs vêtus de ce magnifique tricorne. Si par malheur quelqu\'un arrive à lui poser ne serait-ce qu\'une simple égratignure, le porteur s\'en verra maudit jusqu\'à la fin de ses jours et ne sera plus digne de porter ce dernier.",ep:0,g:290,l:1,wd:false,fm:false,w:20,ce:true,p:0};
I.u[11713] = {n:"Manteau du Pirate",nn:"MANTEAU DU PIRATE",t:17,d:"Le Manteau de Pirate aussi appelé " + "\"" + "Le trophée des milles batailles" + "\"" + " est le symbole d\'une grande prouesse, celle d\'avoir triomphé d\'au moins autant de batailles en pleine mer. Il vaut mieux y réfléchir à deux fois avant de s\'attaquer à ce genre de pirate ! On raconte que porter ce manteau alors que la prouesse n\'a pas été conquise, provoque un mal de mer plutôt désagréable... À vos risques et périls matelots !",ep:0,g:181,l:1,wd:false,fm:false,w:10,ce:true,p:0};
I.u[11714] = {n:"Écu du Pirate",nn:"ECU DU PIRATE",t:82,d:"Est-ce un bouclier ? ... Un fourreau ? ... Ou tout simplement les deux ! L\'utilisation multiple de ce resplendissant Écu de Pirate vous permettra de non seulement bloquer les coups les plus redoutables de vos adversaires mais aussi de ranger vos armes les plus tranchantes.",ep:0,g:95,l:1,wd:false,fm:false,w:5,ce:true,p:0};
I.u[11715] = {n:"Flibalak",nn:"FLIBALAK",t:18,d:"La pilosité de cette adorable créature semble lui causer des problèmes de vue. Obnubilée par les flibustiers en tout genre, elle voit en vous l\'un(e) des ses plus éminent(e)s représentant(e)s. On vous avait bien dit qu\'elle voyait mal. Ce familier dévoreur d\'âmes ne mange que des Pirates.",ep:36,g:69,l:1,wd:true,fm:true,w:1,c:"PZ=1",p:1};
I.u[11716] = {n:"Hormone de Flibalak",nn:"HORMONE DE FLIBALAK",t:48,d:"Cette hormone de croissance déshydratée peut être utilisée dans la confection d\'une puissante potion pour améliorer un familier.",ep:36,g:266,l:1,wd:true,fm:true,w:1,p:10};
I.u[11717] = {n:"Saponnet de Nowel 651",nn:"SAPONNET DE NOWEL 651",t:16,d:"Quand vous portez ce bonnet, on ne peut pas vraiment dire que ce qu\'il y a sous ce sapin là soit un cadeau.",ep:3,g:289,l:1,wd:true,fm:false,w:10,ce:true,p:100};
I.u[11718] = {n:"Hormone de Miniduk",nn:"HORMONE DE MINIDUK",t:48,d:"Cette hormone de croissance déshydratée peut être utilisée dans la confection d\'une puissante potion pour améliorer un familier.",ep:36,g:265,l:1,wd:true,fm:true,w:1,p:10};
I.u[11719] = {n:"Rennowel",nn:"RENNOWEL",t:18,d:"Vous entendez cette clochette ? Vous entendez les bruissements des pas dans la neige ? C\'est nowel, et ce familier est là pour vous le rappeler !",ep:0,g:67,l:1,wd:true,fm:true,w:1,c:"PZ=1",p:1};
I.u[11720] = {n:"Hormone de Rennowel",nn:"HORMONE DE RENNOWEL",t:48,d:"Cette hormone de croissance déshydratée peut être utilisée dans la confection d\'une puissante potion pour améliorer un familier.",ep:0,g:264,l:1,wd:true,fm:true,w:1,p:10};
