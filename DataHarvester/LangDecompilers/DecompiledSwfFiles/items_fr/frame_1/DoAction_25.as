I.u[12150] = {n:"Bouclier du Valeureux",nn:"BOUCLIER DU VALEUREUX",t:82,d:"Ce bouclier vous protège autant qu\'il vous couvre de gloire. Même si la défense n\'est pas la stratégie préférée de Iop, il admet qu\'elle peut être utile, parfois.",ep:0,g:104,l:1,wd:false,fm:true,w:10,ce:true,p:1};
I.u[12151] = {n:"Cape du Valeureux",nn:"CAPE DU VALEUREUX",t:113,d:"À l\'apparence changeante, cette cape protège votre dos des fourbes qui voudraient l\'égratigner. Peu importe son look, elle vous permet d\'avoir la classe, même de dos.",ep:1,g:177,l:1,wd:true,fm:false,w:1,p:1000};
I.u[12152] = {n:"Heaume du Valeureux",nn:"HEAUME DU VALEUREUX",t:113,d:"Lourd ou léger, chacun des aspects de ce heaume fait de vous un valeureux guerrier. Équipez le avec fierté : vous êtes unique ! (Offre soumise à conditions)",ep:1,g:285,l:1,wd:true,fm:false,w:1,p:1000};
I.u[12650] = {n:"Épée en bronze",nn:"EPEE EN BRONZE",t:6,d:"Une épée à la hauteur de vos faits d\'armes lors du banquet de Iop, elle est certainement plus affutée que vos amis Iop, quoiqu\'en pense leur Dieu.",ep:1,g:100,l:60,wd:true,fm:true,w:5,an:14,e:[8,5,1,1,40,50,false,false],s:200,p:1};
I.u[12651] = {n:"Médaille en bronze",nn:"MEDAILLE EN BRONZE",t:1,d:"Cette médaille vous récompense pour vos efforts. Iop a bien remarqué que la castagne, c\'était votre truc, et a tenu à vous l\'offrir pour vous encourager à continuer dans cette voie qui lui est chère.",ep:1,g:170,l:60,wd:true,fm:true,w:1,s:200,p:1};
I.u[12652] = {n:"Anneau en bronze",nn:"ANNEAU EN BRONZE",t:9,d:"Grâce à cette anneau, votre poing est aussi une arme. Pour Iop, il faut toujours être prêt à vous battre, même si vous avez laissé votre épée à la taverne.",ep:20,g:190,l:60,wd:true,fm:true,w:1,s:200,p:1};
