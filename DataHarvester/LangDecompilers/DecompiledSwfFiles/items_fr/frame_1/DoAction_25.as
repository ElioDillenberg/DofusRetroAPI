I.u[12732] = {n:"Bandanam",nn:"BANDANAM",t:16,d:"Avec cette sombre coiffe proche du bandana, même la plus mimi des Eniripsas a l\'air menaçant. Alors, vous, on n\'en parle même pas !",ep:0,g:296,l:1,wd:false,fm:false,w:20,ce:true,p:0};
I.u[12733] = {n:"Caparnam",nn:"CAPARNAM",t:17,d:"La légendaire cape des Chafers Blancs a l\'avantage d\'effrayer vos adversaires même quand vous êtes de dos !",ep:0,g:186,l:1,wd:false,fm:false,w:10,ce:true,p:0};
I.u[12734] = {n:"Boucarnam",nn:"BOUCARNAM",t:82,d:"Quand ils fixent trop longtemps ce bouclier, les boufbowlers du camp adverse sont comme hypnotisés par sa troublante beauté... et en oublient de jouer.",ep:0,g:112,l:1,wd:false,fm:false,w:5,ce:true,p:0};
