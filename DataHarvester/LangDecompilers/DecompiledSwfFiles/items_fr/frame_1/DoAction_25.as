I.u[11724] = {n:"Floconclier Tooshuss",nn:"FLOCONCLIER TOOSHUSS",t:82,d:"Tout droit revenu des Champs de glace en chasse-neige, ce bouclier montre vos prouesses d\'apprenti skieur de l\'Île de Nowel.",ep:0,g:100,l:1,wd:false,fm:false,w:5,ce:true,p:0};
I.u[11725] = {n:"Porte-skis Tooshuss",nn:"PORTE-SKIS TOOSHUSS",t:17,d:"En portant ce sac à dos, vous ne portez pas qu\'un simple sac à dos, vous portez un simple sac à dos qui porte une simple paire de skis.",ep:0,g:182,l:1,wd:false,fm:false,w:10,ce:true,p:0};
I.u[11726] = {n:"Bonnet Tooshuss",nn:"BONNET TOOSHUSS",t:16,d:"" + "\"" + "Aventurier, en ville ou en sport d\'hiver, vous n\'aurez jamais froids aux oreilles grâce à ce bonnet." + "\"" + " Ça reste difficile de croire à ce slogan avec les oreilles qui dépasse, mais bon... On raconte que c\'est la vérité, satisfait ou pas remboursé !",ep:0,g:291,l:1,wd:false,fm:false,w:20,ce:true,p:0};
I.u[11727] = {n:"Oursolaire",nn:"OURSOLAIRE",t:18,d:"L\'Oursolaire à toujours été fidèle au Papa Nowel et vous sera fidèle aussi. Grâce à sa fourrure si duveteuse, vous n\'aurez jamais froid même sur les îles les plus glaciales. Grattez-lui le ventre de temps en temps pour réchauffer son corps. ",ep:0,g:71,l:1,wd:true,fm:true,w:1,p:1};
I.u[11728] = {n:"Hormone de Oursolaire",nn:"HORMONE DE OURSOLAIRE",t:48,d:"Cette hormone de croissance déshydratée peut être utilisée dans la confection d\'une puissante potion pour améliorer un familier.",ep:0,g:268,l:1,wd:true,fm:true,w:1,p:10};
I.u[11729] = {n:"Tibilax",nn:"TIBILAX",t:18,d:"Ne vous fiez pas à cette bouille si mignonne, les Shushus sont généralement des êtres sanguinaires, effrayants et cruels. Il serait donc préférable pour vous de ne pas utiliser ce familier comme objet de réconfort lors de vos longues nuits d\'hiver. On ne sait pas de quelle partie du corps il pourrait vouloir vous amputer. \n",ep:0,g:73,l:1,wd:true,fm:true,w:1,c:"PZ=1",p:1};
I.u[11732] = {n:"Écu qui pète",nn:"ECU QUI PETE",t:82,d:"L\'écu qui pète, s\'ils s\'y mettent tous, ça va faire beaucoup. Telles furent les paroles d\'Halden le Sram lorsqu\'il terrassa le Minotoboule de Nowel qui terrorisait le village d\'Amakna.",ep:36,g:96,l:1,wd:true,fm:false,w:1,ce:true,p:1};
I.u[11733] = {n:"Bouclier qui pète du Sapik",nn:"BOUCLIER QUI PETE DU SAPIK",t:82,d:"Brandissez-le haut et fort, car même après votre mort, c\'est le seul souvenir qui témoignera de tous vos efforts.",ep:36,g:97,l:1,wd:true,fm:false,w:1,ce:true,p:1};
