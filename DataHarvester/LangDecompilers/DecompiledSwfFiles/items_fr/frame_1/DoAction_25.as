I.u[11801] = {n:"Hormone de Glouton",nn:"HORMONE DE GLOUTON",t:48,d:"Cette hormone de croissance déshydratée peut être utilisée dans la confection d\'une puissante potion pour améliorer un familier.",ep:0,g:273,l:1,wd:true,fm:true,w:1,p:10};
I.u[11802] = {n:"Potion d\'Amélioration de Glouton",nn:"POTION D\'AMELIORATION DE GLOUTON",t:116,d:"En utilisant cette puissante potion, vous augmenterez les capacités de votre familier de façon définitive. Attention, une seule dose sera prise en compte par votre familier, plusieurs tentatives provoqueront des résultats décevants.",ep:0,g:73,l:1,wd:true,fm:true,w:1,c:"PO=11804",u:true,p:10};
I.u[11803] = {n:"Certificat de mise en chanil : Glouton",nn:"CERTIFICAT DE MISE EN CHANIL : GLOUTON",t:77,d:"Ce certificat décrit les caractéristiques du familier que vous avez placé au Chanil.",ep:0,g:86,l:1,wd:false,fm:true,w:1,p:100};
I.u[11804] = {n:"Glouton",nn:"GLOUTON",t:18,d:"Après des années à voir ses enfants engloutis sur l\'île de Moon, Iop a décidé de renverser la tendance et d\'apprivoiser le Gloutovore. Malgré son air mignon, il se nourrit de ses congénères... C\'est tout de même mieux que se nourrir de Iops ! Ce dévoreur d\'âmes mange les dépouilles des créatures de l\'île de Moon.",ep:0,g:66,l:1,wd:true,fm:true,w:1,p:1};
I.u[11805] = {n:"Fantôme d\'Estok",nn:"FANTOME D\'ESTOK",t:90,d:"Ce Fantôme n\'a pas encore quitté le monde des vivants, il ne tient qu\'à vous de trouver une façon de le ressusciter, ou de l\'abandonner lâchement une bonne fois pour toutes.",ep:0,g:65,l:1,wd:true,fm:true,w:1,p:1};
