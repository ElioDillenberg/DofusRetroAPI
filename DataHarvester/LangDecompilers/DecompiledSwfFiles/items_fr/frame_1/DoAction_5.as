I.u[1862] = {n:"Le Bâton d\'Amour",nn:"LE BATON D\'AMOUR",t:20,d:"Le bois de cette canne dégage une résine odorante qui imprègne le fil et l\'hameçon. Cette odeur suave doit ressembler au parfum d\'une Kralamourette car tous les Kralamoures viennent s\'y coller.",ep:2,g:8,l:1,wd:true,fm:true,w:5,an:18,e:[1,6,1,6,100,100,false,false],c:"PJ>36,59",p:10000};
I.u[1863] = {n:"Canne à Chô",nn:"CANNE A CHO",t:20,d:"Chô était le plus fameux cuisinier de la table ronde. Sa particularité était de dénicher les aliments frais en face du client. Cette canne lui permettait de pêcher les poissons indispensables pour son cèlebre plat de carpes à Chô.",ep:2,g:8,l:1,wd:true,fm:true,w:5,an:18,e:[1,6,1,6,100,100,false,false],c:"PJ>36,59",p:10000};
I.u[1864] = {n:"L\'aiguille à Tricoter",nn:"L\'AIGUILLE A TRICOTER",t:20,d:"Le fil de cette canne à pêche est exclusivement composé de laine dont la pelote remplace l\'appât. Très peu de poisson-chaton arrivent à y résister.",ep:2,g:8,l:1,wd:true,fm:true,w:4,an:18,e:[1,6,1,4,100,100,false,false],c:"PJ>36,29",p:5000};
