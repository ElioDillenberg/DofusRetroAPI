I.u[1901] = {n:"Cuisse de Wabbit ****",nn:"CUISSE DE WABBIT ****",t:63,d:"Cette cuisse est simplement parfaite, on ne peut s\'empêcher de la dévorer des yeux, puis de la bouche.",ep:2,g:368,l:80,wd:true,fm:true,w:2,p:1};
I.u[1902] = {n:"Cuisse de Wabbit",nn:"CUISSE DE WABBIT",t:63,d:"Cette cuisse de Wabbit semble être parfumée à la cawotte. Idéal pour les pot-au-feu.",ep:8,g:365,l:20,wd:true,fm:true,w:2,p:1};
I.u[1903] = {n:"Cuisse de Wabbit **",nn:"CUISSE DE WABBIT **",t:63,d:"De la bonne cuisse bien musclée qui promet d\'être un régal.",ep:8,g:366,l:40,wd:true,fm:true,w:2,p:1};
I.u[1904] = {n:"Dragocoiffe Blanche",nn:"DRAGOCOIFFE BLANCHE",t:16,d:"Une coquille de Dragoeuf Blanc rembourrée de cuir et de laine, rien de tel pour se protéger des attaques classiques et des éclairs d\'un Bwork Mage ivre. En revanche, vous avez l\'air de sortir de l\'oeuf avec un pareil chapeau.",ep:11,g:28,l:39,wd:true,fm:true,w:50,p:4200};
I.u[1905] = {n:"Cuisse de Wabbit ***",nn:"CUISSE DE WABBIT ***",t:63,d:"Une cuisse bien grosse qui devrait remplir la plupart des estomacs.",ep:8,g:367,l:60,wd:true,fm:true,w:2,p:1};
I.u[1907] = {n:"Oreilles de Lapineupe",nn:"OREILLES DE LAPINEUPE",t:16,d:"Ces oreilles mettront en valeur votre sourire éclatant, tout en vous permettant d\'amplifier les ragots et rumeurs qui circuleront dans votre dos.",ep:1,g:30,l:30,wd:true,fm:true,w:9,p:2600};
I.u[1908] = {n:"Casque du Chafer",nn:"CASQUE DU CHAFER",t:16,d:"Même Spartapine le grand moissonneur, n\'eut pas la chance de porter un tel casque, qui doit sa résistance aux métaux et pierres de qualité utilisés pour sa confection.",ep:13,g:31,l:50,wd:true,fm:true,w:18,p:6500};
I.u[1910] = {n:"Cape Ouginak",nn:"CAPE OUGINAK",t:17,d:"Cette cape provient d\'un Ouginak, elle sent encore l\'odeur de sa truffe, chaude et humide, un peu comme les mamelles d\'une femelle Bouftou en rût.",ep:14,g:33,l:50,wd:true,fm:true,w:10,p:6650};
I.u[1911] = {n:"Cuisse de Boufton",nn:"CUISSE DE BOUFTON",t:63,d:"L\'odeur de cette petite cuisse disparaît à la cuisson.",ep:1,g:334,l:10,wd:true,fm:true,w:2,p:1};
I.u[1912] = {n:"Cuisse de Bouftou **",nn:"CUISSE DE BOUFTOU **",t:63,d:"Cette cuisse pue la mort bien que cela ne soit pas lié avec le décès prématuré de son propriétaire.",ep:1,g:335,l:20,wd:true,fm:true,w:2,p:1};
I.u[1913] = {n:"Cuisse de Bouftou ***",nn:"CUISSE DE BOUFTOU ***",t:63,d:"Cette cuisse provient d\'un chef de guerre bouftou, dure comme un craqueleur elle pourrait être une massue très puissante s\'il n\'y avait pas l\'odeur.",ep:1,g:336,l:40,wd:true,fm:true,w:2,p:1};
I.u[1914] = {n:"Cuisse de Bouftou ****",nn:"CUISSE DE BOUFTOU ****",t:63,d:"Tendre, mais ferme, pas trop grasse ni trop sèche, cette cuisse est comme un bon vin qui s\'est enrichi et affiné au court de toutes ces années.",ep:1,g:337,l:50,wd:true,fm:true,w:2,p:1};
I.u[1915] = {n:"Chair d\'Insecte",nn:"CHAIR D\'INSECTE",t:63,d:"Il ne faut pas sous-estimer la valeur nutritionnelle de cette chair gluante et asphyxiante.",ep:1,g:370,l:1,wd:true,fm:true,w:2,p:1};
I.u[1916] = {n:"Chair d\'Insecte **",nn:"CHAIR D\'INSECTE **",t:63,d:"Une chair identique à celle des mosquitos, mais en quantité 10 fois supérieure. Heureusement que certains aiment.",ep:3,g:370,l:50,wd:true,fm:true,w:4,p:1};
I.u[1917] = {n:"Carré de Porc",nn:"CARRE DE PORC",t:63,d:"Ce morceau de choix de sanglier est un peu sanguinolent pour le moment, mais promet de bons moments.",ep:1,g:331,l:30,wd:true,fm:true,w:3,p:1};
I.u[1918] = {n:"Carré de Porc **",nn:"CARRE DE PORC **",t:63,d:"Un peu plus gros pour un peu plus de bonheur, ce découpage improvisé de Sanglier des plaines ravira tous les carnivores.",ep:13,g:331,l:50,wd:true,fm:true,w:3,p:1};
I.u[1919] = {n:"Carré de Porc ****",nn:"CARRE DE PORC ****",t:63,d:"Ce morceau de DragonCochon est si doux et si moelleux qu\'on pourrait en faire un coussin royal. Mais on préfère le manger.",ep:2,g:331,l:80,wd:true,fm:true,w:4,p:1};
I.u[1920] = {n:"Badge Bontarien",nn:"BADGE BONTARIEN",t:24,d:"Ce badge permet d\'identifier un milicien lorsqu\'il périt sur un champ de bataille. Il paraît que certains en font collection, à Brâkmar.",ep:2,g:49,l:1,wd:false,fm:true,w:0,p:1};
I.u[1921] = {n:"Carré de Porc ***",nn:"CARRE DE PORC ***",t:63,d:"Ferme à souhait, le cochon, à qui appartenait ce morceau, devait faire beaucoup d\'exercice.",ep:1,g:331,l:60,wd:true,fm:true,w:3,p:1};
I.u[1922] = {n:"DragoViande",nn:"DRAGOVIANDE",t:63,d:"Viande rare de qualité supérieure, elle garde la chaleur de nombreuses heures après sa cuisson, faisant d\'elle une viande particulièrement pratique pour les voyages.",ep:2,g:371,l:40,wd:true,fm:true,w:2,p:1};
I.u[1923] = {n:"DragoViande **",nn:"DRAGOVIANDE **",t:63,d:"Cette dragoviande, en plus de ses qualités thermiques, possède un petit goût naturel de fumé.",ep:2,g:371,l:60,wd:true,fm:true,w:2,p:1};
I.u[1924] = {n:"DragoViande ***",nn:"DRAGOVIANDE ***",t:63,d:"La dragoviande de dragoeuf est un peu dure à macher, mais bien plus nourrissante que la Dragoviande traditionnelle. Certains pensent même qu\'elle aurait des vertus magiques.",ep:11,g:371,l:70,wd:true,fm:true,w:2,p:1};
I.u[1925] = {n:"Permission de 24 heures",nn:"PERMISSION DE 24 HEURES",t:24,d:"Cette permission de la milice indique que vous avez quartier libre pendant une journée.",ep:2,g:50,l:1,wd:true,fm:true,w:0,p:1};
I.u[1926] = {n:"DragoViande ****",nn:"DRAGOVIANDE ****",t:63,d:"Cette viande issue des parties dragoniques du dragoncochon (que personne n\'a envie de localiser précisément) \nest un cadeau du ciel. Ce qui est plutôt logique puisque le dragoncochon vole.",ep:2,g:371,l:80,wd:true,fm:true,w:2,p:1};
I.u[1927] = {n:"Museau",nn:"MUSEAU",t:63,d:"Ce museau de milimulou, avec ses petites babines fermes, accompagnera avec plaisir toutes vos chalades.",ep:1,g:372,l:30,wd:true,fm:true,w:1,p:1};
I.u[1928] = {n:"Défense de sanglier agressif",nn:"DEFENSE DE SANGLIER AGRESSIF",t:24,d:"Une bonne dextérité a dû être nécessaire pour extraire cette défense à un Sanglier des plaines.",ep:2,g:51,l:1,wd:true,fm:true,w:0,p:1};
I.u[1929] = {n:"Museau **",nn:"MUSEAU **",t:63,d:"Ce museau au goût délicat vous rendra placide. A consommer sans modération.",ep:1,g:373,l:50,wd:true,fm:true,w:2,p:1};
I.u[1930] = {n:"Museau ***",nn:"MUSEAU ***",t:63,d:"Cette viande est délicieuse, mais attention à ne pas en abuser. Ce n\'est pas pour rien que le Meulou pue de la gueule.",ep:2,g:374,l:70,wd:true,fm:true,w:2,p:1};
I.u[1931] = {n:"Katana ébréché",nn:"KATANA EBRECHE",t:24,d:"C\'est l\'arme de Amayiro. Elle a une prise en main étonnante, mais elle semble être légèrement fendue.",ep:2,g:52,l:1,wd:true,fm:true,w:0,p:1};
I.u[1932] = {n:"Katana rénové",nn:"KATANA RENOVE",t:24,d:"Cette arme, autrefois ébréchée, est à nouveau prête à couper n\'importe quoi. Amayiro ne sera pas déçu.",ep:2,g:53,l:1,wd:true,fm:true,w:0,p:1};
I.u[1933] = {n:"Viande d\'Oiseau ****",nn:"VIANDE D\'OISEAU ****",t:63,d:"Ce petit morceau de corbac est imprégné d\'une magie puissante, probablement celle appliquée par le maître corbac lui-même.",ep:13,g:350,l:30,wd:true,fm:true,w:2,p:1};
I.u[1934] = {n:"Couteau de Chasse",nn:"COUTEAU DE CHASSE",t:20,d:"Ce couteau est la marque typique de reconnaissance des chasseurs. Plus il est gros, coupant, et brillant plus ils se trouvent beaux, du moins selon leurs critères.",ep:1,g:14,l:1,wd:true,fm:true,w:2,an:13,e:[10,3,1,1,25,30,false,false],c:"PJ>41,0",p:1};
I.u[1935] = {n:"Chèque de 100000 kamas",nn:"CHEQUE DE 100000 KAMAS",t:24,d:"Qui paye ses dettes s\'enrichit, mais c\'est tout de même une sacrée somme. Mieux vaut éviter les dettes de cette taille, votre porte-feuille vous remerciera.",ep:1,g:54,l:1,wd:true,fm:true,w:0,p:1};
I.u[1936] = {n:"Hache de chasse",nn:"HACHE DE CHASSE",t:20,d:"Cette hache est particulièrement adaptée pour trancher rapidement et précisément n\'importe quelle partie d\'un animal même encore vivant.",ep:1,g:15,l:1,wd:true,fm:true,w:5,an:17,e:[10,4,1,1,30,40,false,false],c:"PJ>41,9",p:1000};
I.u[1937] = {n:"Rapport d\'espionnage",nn:"RAPPORT D\'ESPIONNAGE",t:24,d:"Ce rapport de Bishom Pot doit contenir pas mal de faits et gestes sur les habitants de Brâkmar.",ep:2,g:55,l:1,wd:true,fm:true,w:0,p:1};
I.u[1938] = {n:"Lame de chasse",nn:"LAME DE CHASSE",t:20,d:"Cette lame, au nom controversé, fut conçue pour la chasse exclusivement. Un peu délicate à manier, elle sera cependant d\'un grand secours au chasseur habile.",ep:3,g:16,l:1,wd:true,fm:true,w:2,an:14,e:[5,4,1,1,40,40,false,false],c:"PJ>41,29",p:5000};
I.u[1939] = {n:"Lance de Chasse",nn:"LANCE DE CHASSE",t:20,d:"Pratique contre les groupes de monstres, cette lance demande un certain talent pour être maniée avec dextérité.",ep:3,g:17,l:1,wd:true,fm:true,w:2,an:12,e:[7,5,1,1,30,50,false,false],c:"PJ>41,49",p:10000};
I.u[1940] = {n:"Marteau de chasse",nn:"MARTEAU DE CHASSE",t:20,d:"Ce marteau n\'a pas une conception classique, il est équilibré de telle sorte qu\'il assome plutôt qu\'il n\'écrase. Très pratique pour chasser, pas du tout pour survivre.",ep:1,g:18,l:1,wd:true,fm:true,w:2,an:15,e:[5,6,1,1,50,50,false,false],c:"PJ>41,69",p:20000};
I.u[1941] = {n:"Pelle Chasseuse",nn:"PELLE CHASSEUSE",t:20,d:"Cette pelle est particulièrement indiquée pour faucher du monstre sans l\'abîmer. Un accessoire pour chasseur confirmé uniquement.",ep:3,g:9,l:1,wd:true,fm:true,w:2,an:16,e:[7,4,1,1,30,50,false,false],c:"PJ>41,29",p:5000};
I.u[1942] = {n:"Arc de Chasse",nn:"ARC DE CHASSE",t:20,d:"L\'arc de chasse est considéré comme l\'arme de chasse la plus sure. Un maximum de dégâts sans s\'exposer pour un minimum de traces visibles.",ep:3,g:10,l:1,wd:true,fm:true,w:2,an:10,tw:true,e:[7,5,1,6,35,40,false,false],c:"PJ>41,29",p:5000};
I.u[1943] = {n:"Baguette de Chasse",nn:"BAGUETTE DE CHASSE",t:20,d:"Cette baguette brûlera l\'animal visé mais ne déchirera pas ses chairs. En plus de conserver la viande elle permet sa pré-cuisson. Indispensable.",ep:3,g:11,l:1,wd:true,fm:true,w:2,an:11,e:[5,3,2,4,30,50,false,false],c:"PJ>41,29",p:5000};
I.u[1944] = {n:"Crache Boulette",nn:"CRACHE BOULETTE",t:20,d:"Cette arme projette une petite boule de cuivre dans la direction visée. Son efficacité est effrayante mais son bruit ne la destine pas à un autre milieu que celui de la chasse.",ep:1,g:21,l:1,wd:false,fm:true,w:2,an:10,e:[5,3,1,10,40,40,false,false],c:"PJ>41,99",p:50000};
I.u[1945] = {n:"Hachoir de Boucher",nn:"HACHOIR DE BOUCHER",t:20,d:"Cet énorme couteau permet de découper n\'importe quelle viande.",ep:1,g:12,l:1,wd:true,fm:true,w:2,an:14,e:[10,4,1,1,25,50,false,false],p:200};
