I.u[2217] = {n:"Costume Fecaesque",nn:"COSTUME FECAESQUE",t:14,d:"Cette moumoute sort tout droit d\'un vieux placard, cela se voit à l\'aspect un tantinet " + "\"" + "rétro" + "\"" + " de cette coiffe. Par contre, c\'est affolant ce que vous pouvez ressemblez à un féca en la mettant sur votre tête (parce que vous le voulez bien).",ep:1,g:17,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:100};
I.u[2218] = {n:"Féca attitude",nn:"FECA ATTITUDE",t:31,d:"Vous avez la Féca attitude, jusqu\'à ce que vous rentriez en combat.",ep:1,g:7,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2219] = {n:"Costume Eniripsesque",nn:"COSTUME ENIRIPSESQUE",t:14,d:"Ces ailes d\'eniripsa ne vous permettent pas de voler. Ne faites donc pas le fanfaron devant vos camarades avec cet accoutrement, vous risquez d\'avoir l\'air ridicule à l\'atterrissage.",ep:1,g:18,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:100};
I.u[2220] = {n:"Eniripsa attitude",nn:"ENIRIPSA ATTITUDE",t:31,d:"Vous avez l\'Eniripsa attitude, jusqu\'à ce que vous rentriez en combat.",ep:1,g:8,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2221] = {n:"Costume Enutrofesque",nn:"COSTUME ENUTROFESQUE",t:14,d:"Cette fausse barbe vous fait ressembler à un Enutrof, certes, mais elle vous donne aussi une haleine digne d\'un Enu de sortie le samedi soir. De quoi donner une sacré fièvre.",ep:1,g:19,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:100};
I.u[2222] = {n:"Enutrof attitude",nn:"ENUTROF ATTITUDE",t:31,d:"Vous avez l\'Enutrof attitude, jusqu\'à ce que vous rentriez en combat.",ep:1,g:9,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2223] = {n:"Costume Sacriesque",nn:"COSTUME SACRIESQUE",t:14,d:"Un peu de sang de tofu ainsi que de fausses cicatrices, un regard de pirate psychotique et le tour est joué. Vous pourrez participer aux réunions de sacrieurs en vous faisant passer pour l\'un d\'eux sans être suspecté.",ep:1,g:20,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:1};
I.u[2224] = {n:"Sacrieur attitude",nn:"SACRIEUR ATTITUDE",t:31,d:"Vous avez la Sacrieur attitude, jusqu\'à ce que vous rentriez en combat.",ep:1,g:10,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2225] = {n:"Costume Sramesque",nn:"COSTUME SRAMESQUE",t:14,d:"Un Kwane, une cape sur la tête, l\'illusion est parfaite. Si vous portez ce costume, vous aurez l\'air d\'un affreux et abject personnage sortant du fin fond d\'un cimetière: un Sram, tout simplement.",ep:1,g:21,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:1};
I.u[2226] = {n:"Sram attitude",nn:"SRAM ATTITUDE",t:31,d:"Vous avez la Sram attitude, jusqu\'à ce que vous rentriez en combat.",ep:1,g:11,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2227] = {n:"Costume Osamodesque",nn:"COSTUME OSAMODESQUE",t:14,d:"N\'en déplaise à certains, vous pouvez vous munir de cette queue fourchue et de ces cornes. Une fois équipé, ce costume vous ira comme un gant et vous serez prêt à vous inviter parmis les fêtes très connues " + "\"" + "vie-ail-pie" + "\"" + " que font les Osamodas en cachette.",ep:1,g:22,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:1};
I.u[2228] = {n:"Osamodas attitude",nn:"OSAMODAS ATTITUDE",t:31,d:"Vous avez l\'Osamodas attitude, jusqu\'à ce que vous rentriez en combat.",ep:1,g:12,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2229] = {n:"Costume Ecaflipesque",nn:"COSTUME ECAFLIPESQUE",t:14,d:"On peut y trouver des oreilles en poil de wabbit très doux, une queue tigrée et une boîte de puces excitées. Pas de doute, quand vous refilerez ces sales petites bêtes à vos voisins, on ne pourra pas douter de vos talents de transformiste.",ep:1,g:23,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:1};
I.u[2230] = {n:"Ecaflip attitude",nn:"ECAFLIP ATTITUDE",t:31,d:"Vous avez l\'Ecaflip attitude, jusqu\'à ce que vous rentriez en combat.",ep:1,g:13,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2231] = {n:"Costume Craesque",nn:"COSTUME CRAESQUE",t:14,d:"Le Cra, vous l\'aurez remarqué, se promène souvent avec son carquois. Pas étonnant que muni de celui-ci, les gens vous prennent pour un des leurs. Mais ce n\'est qu\'un leurre, quel bonheur de les berner à toute heure.",ep:1,g:24,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:1};
