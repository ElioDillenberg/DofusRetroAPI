I.u[2070] = {n:"Shako",nn:"SHAKO",t:16,d:"Ce casque en os a longtemps séjourné dans les poches d\'un Kanigrou. Usé, rayé, il garde tout de même de belles propriétés.",ep:9,g:58,l:34,wd:true,fm:true,w:15,p:3200};
I.u[2071] = {n:"Potion d\'oubli : \'Libération\'",nn:"POTION D\'OUBLI : \'LIBERATION\'",t:43,d:"Cette potion permet d\'oublier un niveau du sort élémentaire \'Libération\'",ep:5,g:36,l:1,wd:true,fm:true,w:1,u:true,p:1000};
I.u[2072] = {n:"Potion d \'oubli : \'Foudroiement\'",nn:"POTION D \'OUBLI : \'FOUDROIEMENT\'",t:43,d:"Cette potion permet d\'oublier un niveau du sort élémentaire \'Foudroiement\'",ep:5,g:36,l:1,wd:true,fm:true,w:1,u:true,p:1000};
I.u[2073] = {n:"Potion d\'oubli : \'Brokle\'",nn:"POTION D\'OUBLI : \'BROKLE\'",t:43,d:"Cette potion permet d\'oublier un niveau du sort spécial \'Brokle\'",ep:2,g:36,l:1,wd:true,fm:true,w:1,u:true,p:1000};
I.u[2074] = {n:"Bwak de Feu",nn:"BWAK DE FEU",t:18,d:"Ce mignon petit bwak deviendra, plus tard, un affreux kwak. Jusque là n\'ayez pas peur de le garder à vos cotés.",ep:1,g:3,l:1,wd:true,fm:true,w:10,c:"PZ=1",s:2,p:1};
I.u[2075] = {n:"Bwak d\'Eau",nn:"BWAK D\'EAU",t:18,d:"Ce mignon petit bwak deviendra, plus tard, un affreux kwak. Jusque là n\'ayez pas peur de le garder à vos cotés.",ep:1,g:2,l:1,wd:true,fm:true,w:10,c:"PZ=1",s:13,p:1};
I.u[2076] = {n:"Bwak d\'Air",nn:"BWAK D\'AIR",t:18,d:"Ce mignon petit bwak deviendra, plus tard, un affreux kwak. Jusque là n\'ayez pas peur de le garder à vos cotés.",ep:1,g:4,l:1,wd:true,fm:true,w:10,c:"PZ=1",s:15,p:1};
I.u[2077] = {n:"Bwak de Terre",nn:"BWAK DE TERRE",t:18,d:"Ce mignon petit bwak deviendra, plus tard, un affreux kwak. Jusque là n\'ayez pas peur de le garder à vos cotés.",ep:1,g:5,l:1,wd:true,fm:true,w:10,c:"PZ=1",s:14,p:1};
I.u[2078] = {n:"Testament d\'Ixa Mustam",nn:"TESTAMENT D\'IXA MUSTAM",t:24,d:"Le testament du père d\'Oto Mustam, il est écrit en ancien Brâkmarien, dommage pour vous, votre connaissance dans cette langue ce résume au mot : " + "\"" + "Guo\'rhalys !" + "\"" + " .",ep:2,g:26,l:1,wd:true,fm:true,w:0,p:1};
I.u[2079] = {n:"Vin de Gloutovore",nn:"VIN DE GLOUTOVORE",t:24,d:"Il paraît qu\'il faut 20 ans pour arriver à distiller du jus de Gloutovore sans créer un vin toxique.",ep:2,g:62,l:1,wd:true,fm:true,w:1,p:1};
I.u[2080] = {n:"Jus de Gloutovore",nn:"JUS DE GLOUTOVORE",t:24,d:"Le jus de Gloutovore est aussi toxique que du jus de scorbute. Mais bien distillé il donne un vin au goût incomparable. Cela dit rare sont gens sachant distiller ce jus. Et la plupart du temps se sont des pirates.",ep:2,g:63,l:1,wd:true,fm:true,w:1,p:1};
I.u[2081] = {n:"Rapport de Finche Aticus",nn:"RAPPORT DE FINCHE ATICUS",t:24,d:"Le rapport de l\'espion Finche Aticus indique une liste exhaustive des armes de Bonta et de ses effectifs.",ep:2,g:33,l:1,wd:true,fm:true,w:0,p:1};
I.u[2082] = {n:"Marcassin",nn:"MARCASSIN",t:32,d:"Lorsqu\'un jeune marcassin voit ses parents mourir sous ses yeux, il suit l\'assassin pour que ce dernier s\'occupe de lui.",ep:13,g:1,l:1,wd:true,fm:true,w:0,m:true,p:10};
I.u[2083] = {n:"Lettre d\'Oto Mustam",nn:"LETTRE D\'OTO MUSTAM",t:24,d:"Une lettre de votre maître adressée à Vil Smisse. Elle est écrite dans un dialecte inconnu pour éviter que les curieux dans votre genre y jettent un oeil.",ep:2,g:26,l:1,wd:true,fm:true,w:0,p:1};
I.u[2084] = {n:"Réponse de Vil Smisse",nn:"REPONSE DE VIL SMISSE",t:24,d:"Une réponse de Vil Smisse à Oto Mustam, complètement incompréhensible pour le commun des mortels.",ep:2,g:26,l:1,wd:true,fm:true,w:0,p:1};
I.u[2085] = {n:"Bière de la Tabasse",nn:"BIERE DE LA TABASSE",t:37,d:"Non seulement elle n\'a aucun goût, mais en plus cette bière est hors de prix. Encore un point commun avec la gente féminine.",ep:16,g:4,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:5};
I.u[2086] = {n:"Bière du Alakarte",nn:"BIERE DU ALAKARTE",t:37,d:"Cette bière aromatisée au Blop Griotte est aussi parfois appelée Manoca.",ep:16,g:4,l:1,wd:true,fm:true,w:1,u:true,p:3};
I.u[2087] = {n:"Bière de la Bagrutte",nn:"BIERE DE LA BAGRUTTE",t:37,d:"Cette bière vous rend plus vaillant l\'espace d\'un instant... attention cependant à la chute, car l\'instant est vraiment très bref.",ep:16,g:4,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:5};
I.u[2088] = {n:"Bière de la Chopenbois",nn:"BIERE DE LA CHOPENBOIS",t:37,d:"Une bière classique, mais la choppe en revanche est superbement sculptée. Pour une fois, le contenant a plus de valeur et d\'importance que le contenu. Une caractéristique partagée avec le crâne des Iops et la presse populaire.",ep:16,g:4,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:2};
I.u[2089] = {n:"Bière du Pinchaut",nn:"BIERE DU PINCHAUT",t:37,d:"Cette bière est chaude ce qui la rend difficilement buvable en été. En revanche, elle dégage une agréable odeur de pain grillé, ce qui en fait une bière parfaite pour les petits déjeuners sans pain.",ep:16,g:4,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:2};
I.u[2090] = {n:"Bière du Ferayeur",nn:"BIERE DU FERAYEUR",t:37,d:"Cette bière a un arrière-goût de ferraille très désagréable, mais est fortement revitalisante, ce qui n\'est pas sans rappeler les bisous de la Momie Nova.",ep:16,g:4,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:7};
I.u[2091] = {n:"Le Bwak : Bien l\'élever",nn:"LE BWAK : BIEN L\'ELEVER",t:25,d:"Ce livre explique comment élever un Bwak.",ep:3,g:4,l:1,wd:true,fm:true,w:1,u:true,p:200};
I.u[2092] = {n:"Le Chacha : Bien l\'élever",nn:"LE CHACHA : BIEN L\'ELEVER",t:25,d:"Ce livre explique comment élever un Chacha.",ep:3,g:3,l:1,wd:true,fm:true,w:1,u:true,p:200};
I.u[2093] = {n:"Le Wabbit : Bien l\'élever",nn:"LE WABBIT : BIEN L\'ELEVER",t:25,d:"Ce livre explique comment élever un Wabbit.",ep:3,g:5,l:1,wd:true,fm:true,w:1,u:true,p:200};
I.u[2094] = {n:"Makroute Sombre",nn:"MAKROUTE SOMBRE",t:16,d:"Le Makroute Sombre est un chapeau qui provient d\'un Makroute Déchiqueté recousu à la main et sans anesthésie. \nMaintenant, il n\'obéit plus qu\'à son maître et adore mordre les inconnus.",ep:14,g:33,l:50,wd:true,fm:true,w:5,p:6600};
I.u[2095] = {n:"Le Ours",nn:"LE OURS",t:16,d:"Le Ours est un chapeau confectionné dans la tête d\'un ours mort. Il a cependant gardé un poil soyeux. Le tailleur qui l\'a confectionné devait être très habile pour avoir réussi a garder l\'expression de mécontentement de l\'ours d\'origine.",ep:14,g:34,l:35,wd:true,fm:true,w:5,p:10000};
I.u[2096] = {n:"Dora Sombre de Brave",nn:"DORA SOMBRE DE BRAVE",t:16,d:"C\'est le fantôme Brave qui le premier comprit comment utiliser correctement de l\'Osier Sombre à des fins autres que gastronomiques.",ep:14,g:35,l:37,wd:true,fm:true,w:5,p:3800};
I.u[2097] = {n:"Champo",nn:"CHAMPO",t:16,d:"Le Champo appartenait à l\'un des plus grands Maîtres Bolet d\'Amakna. Vaillant et brillant, Gnion le Bolet était le seul de sa race à avoir réussi à se faire respecter des êtres humains.",ep:1,g:36,l:43,wd:true,fm:true,w:25,p:5000};
I.u[2098] = {n:"Certificat de Mise en Chanil : Bwak de Terre",nn:"CERTIFICAT DE MISE EN CHANIL : BWAK DE TERRE",t:77,d:"Ce certificat décrit les caractéristiques du Bwak de Terre que vous avez placé au chanil.",ep:3,g:21,l:1,wd:false,fm:true,w:1,p:100};
I.u[2099] = {n:"Certificat de Mise en Chanil : Bwak de Feu",nn:"CERTIFICAT DE MISE EN CHANIL : BWAK DE FEU",t:77,d:"Ce certificat décrit les caractéristiques du Bwak de Feu que vous avez placé au chanil.",ep:3,g:23,l:1,wd:false,fm:true,w:1,p:100};
I.u[2100] = {n:"Certificat de Mise en Chanil : Bwak d\'Eau",nn:"CERTIFICAT DE MISE EN CHANIL : BWAK D\'EAU",t:77,d:"Ce certificat décrit les caractéristiques du Bwak d\'Eau que vous avez placé au chanil.",ep:3,g:22,l:1,wd:false,fm:true,w:1,p:100};
I.u[2101] = {n:"Certificat de Mise en Chanil : Bwak d\'Air",nn:"CERTIFICAT DE MISE EN CHANIL : BWAK D\'AIR",t:77,d:"Ce certificat décrit les caractéristiques du Bwak d\'Air que vous avez placé au chanil.",ep:3,g:20,l:1,wd:false,fm:true,w:1,p:100};
I.u[2102] = {n:"Certificat de Mise en Chanil : Wabbit",nn:"CERTIFICAT DE MISE EN CHANIL : WABBIT",t:77,d:"Ce certificat décrit les caractéristiques du Wabbit que vous avez placé au chanil.",ep:3,g:25,l:1,wd:false,fm:true,w:1,p:100};
I.u[2103] = {n:"Formulaire de Welloy Cab",nn:"FORMULAIRE DE WELLOY CAB",t:24,d:"Écrit à la patte par Welloy Cab, ce rapport ravira tous les amateurs de chiffres et de phrases creuses.",ep:1,g:74,l:1,wd:true,fm:true,w:0,p:1};
I.u[2104] = {n:"Kraignosse",nn:"KRAIGNOSSE",t:16,d:"Le Kraignosse est un casque inquiétant confectionné à partir de restes d\'Ouginak et de divers détritus. Longtemps porté par Gourlo le Terrible, le Kraignosse fût l\'image de la désolation.",ep:14,g:37,l:48,wd:true,fm:true,w:30,p:6000};
I.u[2105] = {n:"Formulaire de rapport vierge",nn:"FORMULAIRE DE RAPPORT VIERGE",t:24,d:"Formulaire de rapport vide de toute annotation. \nPrêt à être rempli par Welloy Cab",ep:1,g:41,l:1,wd:true,fm:true,w:0,p:1};
I.u[2106] = {n:"Liste d\'Appâts pour pêcheur",nn:"LISTE D\'APPATS POUR PECHEUR",t:24,d:"Vous devez aller chercher les appâts le plus rapidement possible.",ep:1,g:55,l:1,wd:true,fm:true,w:0,p:1};
I.u[2107] = {n:"Appâts spéciaux",nn:"APPATS SPECIAUX",t:24,d:"Cette boîte d\'appâts spéciaux ressemble à une vulgaire boîte de Chabon.",ep:1,g:45,l:1,wd:true,fm:true,w:0,p:1};
I.u[2108] = {n:"Badge de Tabach",nn:"BADGE DE TABACH",t:24,d:"Badge de Tabach, Greffier de l\'Ordre du Coeur Vaillant.",ep:2,g:49,l:1,wd:true,fm:true,w:0,p:1};
I.u[2109] = {n:"Badge de Ybaul",nn:"BADGE DE YBAUL",t:24,d:"Badge de Ybaul, Greffière de l\'Ordre de l\'Esprit Salvateur.",ep:2,g:49,l:1,wd:true,fm:true,w:0,p:1};
I.u[2110] = {n:"Badge d\'Aigurraf",nn:"BADGE D\'AIGURRAF",t:24,d:"Badge d\'Aigurraf, Greffier de l\'Ordre de l\'Oeil Attentif",ep:2,g:49,l:1,wd:true,fm:true,w:0,p:1};
I.u[2111] = {n:"L\'âme du pêcheur",nn:"L\'AME DU PECHEUR",t:25,d:"Ce livre explique les ficelles pour devenir un bon pêcheur",ep:2,g:1,l:1,wd:true,fm:true,w:1,u:true,p:250};
I.u[2112] = {n:"L\'âme du chasseur",nn:"L\'AME DU CHASSEUR",t:25,d:"Ce livre explique les ficelles pour devenir un bon chasseur",ep:3,g:1,l:1,wd:true,fm:true,w:1,u:true,p:250};
