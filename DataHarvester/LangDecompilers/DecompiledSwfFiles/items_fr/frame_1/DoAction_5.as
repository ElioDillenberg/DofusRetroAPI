I.u[2020] = {n:"Pain Doré",nn:"PAIN DORE",t:33,d:"Ce pain fut pétri à la farine de blé magique. Celui qui en vend le plus se retrouve plus riche qu\'auparavant.",ep:1,g:69,l:10,wd:true,fm:true,w:2,c:"Cv<10",u:true,ut:true,p:1};
I.u[2021] = {n:"Houblon Brillant",nn:"HOUBLON BRILLANT",t:34,d:"Ce houblon possède une aura brillante très discrète, révélant la magie que la terre mère lui a prêtée.",ep:1,g:475,l:30,wd:true,fm:true,w:2,p:0};
I.u[2022] = {n:"Farine de Houblon Magique",nn:"FARINE DE HOUBLON MAGIQUE",t:52,d:"Cette farine fut fabriquée en utilisant un houblon brillant. Nul doute qu\'elle renferme des capacités magiques.",ep:1,g:253,l:1,wd:true,fm:true,w:2,p:0};
I.u[2024] = {n:"Briochette",nn:"BRIOCHETTE",t:33,d:"Cette petite brioche est fabriquée à partir de houblon. Ce qui explique l\'impression d\'afflux d\'énergie quand on la mange.",ep:1,g:8,l:20,wd:true,fm:true,w:1,u:true,ut:true,p:1};
I.u[2025] = {n:"Briochette magique",nn:"BRIOCHETTE MAGIQUE",t:33,d:"Cette petite brioche est fabriquée à partir de houblon magique. Ce qui explique l\'impression d\'un énorme afflux d\'énergie quand on la mange",ep:1,g:8,l:20,wd:true,fm:true,w:1,c:"Cv<20",u:true,p:1};
I.u[2026] = {n:"Lin Tempête",nn:"LIN TEMPETE",t:34,d:"Ce Lin a subi les pires tempêtes, et s\'est relevé plus de sept fois, ce qui en soi est assez extraordinaire pour du lin, il faut le rappeler. Après avoir rouï à la rosée magique des champs du Grand Pahti pendant plus de 148 années, sa fibre finira avec un peu de chance par être enfin utilisable.",ep:1,g:476,l:40,wd:true,fm:true,w:1,p:0};
I.u[2027] = {n:"Farine de Lin magique",nn:"FARINE DE LIN MAGIQUE",t:52,d:"Cette farine a été préparée avec du Lin tempête. Elle possède probablement les mêmes vertus de résistance.",ep:1,g:254,l:1,wd:true,fm:true,w:1,p:0};
I.u[2028] = {n:"Pain consistant magique",nn:"PAIN CONSISTANT MAGIQUE",t:33,d:"Ce pain est fait à partir de Farine de Lin Tempête, ce qui explique pourquoi il n\'est pas gras et a bon goût.",ep:1,g:12,l:1,wd:true,fm:true,w:1,c:"Cv<30",u:true,ut:true,p:1};
I.u[2029] = {n:"Seigle Résistant",nn:"SEIGLE RESISTANT",t:34,d:"Cette céréale sombre a réussi à pousser dans des conditions impossibles.",ep:1,g:124,l:50,wd:true,fm:true,w:2,p:0};
I.u[2030] = {n:"Farine de Seigle Magique",nn:"FARINE DE SEIGLE MAGIQUE",t:52,d:"Cette farine hérite de tous les bienfaits du Seigle Résistant. Elle provoque donc d\'importants effets laxatifs en cas de consommation excessive.",ep:1,g:257,l:1,wd:true,fm:true,w:2,p:0};
I.u[2031] = {n:"Pain de Seigle Résistant",nn:"PAIN DE SEIGLE RESISTANT",t:33,d:"Ce pain est fait à partir de farine de Seigle Résistant, ce qui lui donne une saveur exécrable, mais possède des qualités nutritionnelles uniques.",ep:1,g:9,l:10,wd:true,fm:true,w:1,c:"Cv<40",u:true,ut:true,p:1};
I.u[2032] = {n:"Orge en Sucre",nn:"ORGE EN SUCRE",t:34,d:"Cette Orge très rare, a absorbé de l\'eau de pluie sucrée lors de sa germination, ce qui lui a conféré des propriétés magiques inestimables.",ep:1,g:477,l:10,wd:true,fm:true,w:2,p:0};
I.u[2033] = {n:"Farine d\'Orge en Sucre",nn:"FARINE D\'ORGE EN SUCRE",t:52,d:"Cette farine a été obtenue avec de l\'Orge en Sucre, elle permet de donner un pain si léger qu\'il pourrait s\'envoler à la moindre bourrasque de vent. Ce n\'est pas une raison pour en manger plus que de raison.",ep:1,g:256,l:1,wd:true,fm:true,w:1,p:0};
I.u[2034] = {n:"Sucre d\'Orge",nn:"SUCRE D\'ORGE",t:42,d:"En extrayant le sucre de la farine d\'orge sucré on parvient à créer ce bonbon unique au goût magique.",ep:1,g:128,l:1,wd:true,fm:true,w:1,c:"Cv<50",u:true,p:50000};
I.u[2035] = {n:"Chanvre Euphorique",nn:"CHANVRE EUPHORIQUE",t:34,d:"Cette tige de chanvre semble avoir des effets inhibant extrêmement puissants. Manipulée par un spécialiste, elle pourrait révéler un certain intérêt.",ep:1,g:478,l:70,wd:true,fm:true,w:1,p:0};
I.u[2036] = {n:"Avoine Aurifère",nn:"AVOINE AURIFERE",t:34,d:"Quand on regarde cette avoine, on a l\'impression qu\'elle pleure de l\'Or, signe de la grande puissance magique qu\'elle a emmagasinée pendant sa pousse.",ep:1,g:551,l:20,wd:true,fm:true,w:2,p:0};
I.u[2037] = {n:"Farine d\'Avoine Aurifère",nn:"FARINE D\'AVOINE AURIFERE",t:52,d:"Cette farine a été obtenue avec de l\'avoine aurifère, ce qui lui donne une douce couleur dorée et un pouvoir magique indéniable. Elle ne fera cependant pas de vous un pâtissier magicien.",ep:1,g:250,l:1,wd:true,fm:true,w:1,p:0};
I.u[2038] = {n:"Pain aux Flocons d\'Avoine Aurifère",nn:"PAIN AUX FLOCONS D\'AVOINE AURIFERE",t:33,d:"Ce pain à la mie ultra légère est tout simplement parfait.",ep:1,g:5,l:10,wd:true,fm:true,w:2,c:"Cv<70",u:true,ut:true,p:1};
I.u[2039] = {n:"Marque de test du chasseur",nn:"MARQUE DE TEST DU CHASSEUR",t:24,d:"Vous avez essayé de passer le test pour devenir chasseur.",ep:1,g:55,l:1,wd:true,fm:true,w:0,p:1};
I.u[2040] = {n:"Coffre caché de Tsog",nn:"COFFRE CACHE DE TSOG",t:24,d:"Vous avez retrouvé le coffre de Tsog dans une petite boîte. C\'est étrange comme cette boîte sent la même chose que Tsog, une odeur indescriptible.",ep:2,g:56,l:1,wd:true,fm:true,w:0,p:1};
I.u[2041] = {n:"Cuisse malodorante de Bouftou",nn:"CUISSE MALODORANTE DE BOUFTOU",t:24,d:"Cette cuisse pue la mort bien que cela ne soit pas lié avec le décès prématuré de son propriétaire.",ep:1,g:64,l:20,wd:true,fm:true,w:0,p:1};
I.u[2042] = {n:"Os de Mak Gahan",nn:"OS DE MAK GAHAN",t:24,d:"Vous avez pris cet os après avoir éparpillé le corps de Mak Gahan le chafer dans les quatre coins de la pièce.",ep:1,g:57,l:1,wd:true,fm:true,w:0,p:1};
I.u[2043] = {n:"Nom de l\'assassin griffoné",nn:"NOM DE L\'ASSASSIN GRIFFONE",t:24,d:"Sur ce bout de papier gluant et puant se trouve le nom de l\'assassin du tenancier du Chabrulé.",ep:2,g:58,l:1,wd:true,fm:true,w:0,p:1};
I.u[2044] = {n:"Bière du Chabrulé",nn:"BIERE DU CHABRULE",t:37,d:"Cette bière ressemble trop à de la pisse de chat... Ou pas assez.",ep:16,g:4,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:6};
I.u[2045] = {n:"Reçu de caisse",nn:"RECU DE CAISSE",t:24,d:"Ce document prouve que vous avez rapporté une caisse.",ep:2,g:50,l:1,wd:true,fm:true,w:0,p:1};
I.u[2046] = {n:"Clef de Brâkmar",nn:"CLEF DE BRAKMAR",t:24,d:"Cette clef a été volée par un écureuil pour d\'obscures raisons.",ep:2,g:61,l:1,wd:true,fm:true,w:0,p:1};
I.u[2047] = {n:"Liste des soldats prenant leur pause",nn:"LISTE DES SOLDATS PRENANT LEUR PAUSE",t:24,d:"Cette liste vous a été donnée par le milicien dans la taverne. Les noms suivants y apparaissent : \nBrank Thoof, Haco Norss, Jack Rottance, Kad Maxy",ep:2,g:55,l:1,wd:true,fm:true,w:0,p:1};
I.u[2048] = {n:"Liste des soldats prenant leur pause",nn:"LISTE DES SOLDATS PRENANT LEUR PAUSE",t:24,d:"Cette liste vous a été donnée par le milicien dans la taverne. Les noms suivants y apparaissent : \nHaco Norss, Tink Berval, Brank Thoof, Kad Maxy, Seta Bnamron",ep:2,g:58,l:1,wd:true,fm:true,w:0,p:1};
I.u[2049] = {n:"Aveux du Guerrier Xale DuGros",nn:"AVEUX DU GUERRIER XALE DUGROS",t:24,d:"Xale DuGros avoue ne pas aller à la taverne, cependant il prend des pauses régulièrement.",ep:2,g:55,l:1,wd:true,fm:true,w:0,p:1};
I.u[2050] = {n:"Aveux de Guerrier Nager Nacmeil",nn:"AVEUX DE GUERRIER NAGER NACMEIL",t:24,d:"Le guerrier Nager Nacmeil avoue ne pas prendre de pause, car il ne va pas très bien en ce moment.",ep:2,g:55,l:1,wd:true,fm:true,w:0,p:1};
I.u[2051] = {n:"Aveux Bishom Pot",nn:"AVEUX BISHOM POT",t:24,d:"Bishom Pot avoue tout, son comportement est impardonnable.",ep:2,g:55,l:1,wd:true,fm:true,w:0,p:1};
I.u[2052] = {n:"Permission signée et datée",nn:"PERMISSION SIGNEE ET DATEE",t:24,d:"Ce document indique que vous avez une journée de libre.",ep:2,g:50,l:1,wd:true,fm:true,w:0,p:1};
I.u[2053] = {n:"Badge de milicien Allen Tokar",nn:"BADGE DE MILICIEN ALLEN TOKAR",t:24,d:"Badge du milicien Allen Tokar.",ep:2,g:49,l:1,wd:true,fm:true,w:0,p:1};
I.u[2054] = {n:"Badge de milicien Tek Apout",nn:"BADGE DE MILICIEN TEK APOUT",t:24,d:"Badge du milicien Tek Apout.",ep:2,g:49,l:1,wd:true,fm:true,w:0,p:1};
I.u[2055] = {n:"Badge de milicien Bill Iédebar",nn:"BADGE DE MILICIEN BILL IEDEBAR",t:24,d:"Badge du milicien Bill Iédebar.",ep:2,g:49,l:1,wd:true,fm:true,w:0,p:1};
I.u[2056] = {n:"Oeil de Corbac",nn:"OEIL DE CORBAC",t:109,d:"Un oeil de corbac est toujours utile, mais attention à ce que le mauvais oeil ne tombe pas sur vous.",ep:1,g:326,l:20,wd:true,fm:true,w:1,p:100};
I.u[2058] = {n:"Coeur de Brandon",nn:"COEUR DE BRANDON",t:15,d:"Ce coeur humain vient sûrement du célèbre Lhi, qui était passionné par les corbacs.",ep:1,g:328,l:20,wd:true,fm:true,w:5,p:500};
I.u[2059] = {n:"Corbac Mort",nn:"CORBAC MORT",t:15,d:"Vous avez bien fait de le ramasser, il est intact. Mort, mais intact.",ep:1,g:329,l:20,wd:true,fm:true,w:10,p:100};
I.u[2060] = {n:"Patte de Corbac",nn:"PATTE DE CORBAC",t:103,d:"Pas de quoi casser une patte à un corbac. Surtout qu\'à l\'inverse de la patte de wabbit, la patte de corbac risque de vous porter malheur.",ep:1,g:330,l:20,wd:true,fm:true,w:1,p:10};
I.u[2061] = {n:"Cape du Corbac",nn:"CAPE DU CORBAC",t:17,d:"Cette cape, pas très jolie au premier abord est pourtant très efficace, une fois que l\'on s\'habitue à son odeur de Corbac en décomposition.",ep:1,g:15,l:45,wd:true,fm:true,w:5,p:5600};
I.u[2062] = {n:"Camouflage",nn:"CAMOUFLAGE",t:31,d:"Vous êtes camouflé au sein du décor, jusqu\'à ce que vous rentriez en combat.",ep:1,g:1,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[2063] = {n:"Bottes Volatiles",nn:"BOTTES VOLATILES",t:11,d:"Ces bottes, confectionnées avec de nombreuses parties de cadavre de Corbac, donnent à leur porteur une étrange sensation de légèreté, comparable à celle apportée par la consommation de Bolets grillés.",ep:1,g:17,l:41,wd:true,fm:true,w:15,p:4500};
I.u[2064] = {n:"Parchemin de Camouflage",nn:"PARCHEMIN DE CAMOUFLAGE",t:14,d:"Ce parchemin vous permet de vous camoufler au sein du décor, jusqu\'à ce que vous rentriez en combat. Évitez les mouvements brusques et les bruits suspects si vous ne voulez pas être découvert...",ep:16,g:1,l:10,wd:true,fm:true,w:1,c:"Pg=10&PM=B",u:true,p:250};
I.u[2065] = {n:"Casque de chafer d\'élite",nn:"CASQUE DE CHAFER D\'ELITE",t:24,d:"Ce casque de Chafer est endommagé, mais toujours aussi efficace lorsque l\'on veut faire partie de l\'élite.",ep:2,g:59,l:1,wd:false,fm:true,w:0,p:1};
I.u[2066] = {n:"Bâton du Kanigrou",nn:"BATON DU KANIGROU",t:4,d:"Ce bâton, est une réplique approximative de celui d\'un Kanigrou. Il réserve quelques surprises, mais pas uniquement de bonnes surprises...",ep:13,g:22,l:54,wd:true,fm:true,w:10,an:12,e:[5,4,1,1,30,50,false,false],c:"CS>50&CA>40&CC>30",p:5400};
I.u[2067] = {n:"Potion d\'oubli : Flamiche",nn:"POTION D\'OUBLI : FLAMICHE",t:43,d:"Cette potion permet d\'oublier un niveau du sort élémentaire \'Flamiche\'",ep:5,g:36,l:1,wd:true,fm:true,w:1,u:true,p:1000};
I.u[2068] = {n:"Potion d\'oubli : Cawotte",nn:"POTION D\'OUBLI : CAWOTTE",t:43,d:"Cette potion permet d\'oublier un niveau du sort élémentaire \'Cawotte\'",ep:5,g:36,l:1,wd:true,fm:true,w:1,u:true,p:1000};
