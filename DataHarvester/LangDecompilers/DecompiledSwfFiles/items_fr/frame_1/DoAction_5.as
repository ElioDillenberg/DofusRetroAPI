I.u[1879] = {n:"Caisse d\'armes blanches",nn:"CAISSE D\'ARMES BLANCHES",t:24,d:"Une caisse d\'armes blanches, remplie d\'objets contondants en tout genre.",ep:2,g:44,l:1,wd:true,fm:true,w:500,p:1};
I.u[1880] = {n:"Boîte de Chabon",nn:"BOITE DE CHABON",t:24,d:"Hmm... du chabon, c\'est votre Kanigrou qui va être content.",ep:2,g:45,l:1,wd:true,fm:true,w:0,p:1};
I.u[1881] = {n:"Boue",nn:"BOUE",t:24,d:"Voila, ça, c\'est fait.",ep:2,g:46,l:1,wd:true,fm:true,w:2,p:1};
I.u[1882] = {n:"Fleurs de Blop concassé",nn:"FLEURS DE BLOP CONCASSE",t:24,d:"Vous avez récupéré une bien belle bouillie.",ep:2,g:47,l:1,wd:true,fm:true,w:0,p:1};
I.u[1883] = {n:"Collier réparé",nn:"COLLIER REPARE",t:24,d:"Le collier que la fille du maire a perdu, presque comme neuf...",ep:2,g:48,l:1,wd:true,fm:true,w:0,p:1};
I.u[1889] = {n:"Plume de Corbac",nn:"PLUME DE CORBAC",t:53,d:"Autrefois utilisée pour l\'écriture, cette plume est désormais essentiellement utilisée en haute couture. En effet, les Corbacs sont de moins en moins généreux et n\'offrent plus leurs plumes au premier venu. Tout se perd décidément.",ep:1,g:327,l:20,wd:true,fm:true,w:1,p:1};
I.u[1890] = {n:"Poil de Kanigrou",nn:"POIL DE KANIGROU",t:54,d:"Le poil de Kanigrou réfléchit la lumière du soleil d\'une manière assez étrange, comme si il en absorbait une partie avant de la renvoyer.",ep:13,g:359,l:70,wd:true,fm:true,w:1,p:1};
I.u[1891] = {n:"Peau de serpentin",nn:"PEAU DE SERPENTIN",t:59,d:"N\'avez-vous jamais rêvé d\'un magnifique petit sac en peau de Serpentin ? Non ? Vous avez bien raison, car sa résistance laisse à désirer.",ep:2,g:361,l:10,wd:true,fm:true,w:1,p:1};
I.u[1892] = {n:"Plume du Maître Corbac",nn:"PLUME DU MAITRE CORBAC",t:53,d:"Pris de panique, le Maître Corbac ouvrit de larges ailes et laissa tomber cette plume. Pas de quoi casser trois pattes à un Koin-koin, et pourtant...",ep:13,g:362,l:160,wd:true,fm:true,w:1,p:1};
I.u[1893] = {n:"Queue de scorbute",nn:"QUEUE DE SCORBUTE",t:65,d:"Un formidable trophée, dommage qu\'il suinte un liquide nauséabond.",ep:14,g:363,l:36,wd:true,fm:true,w:1,p:1};
I.u[1894] = {n:"Poil de Sanglier des Plaines",nn:"POIL DE SANGLIER DES PLAINES",t:54,d:"Ce poil est vraiment répugnant. En regardant de plus près, il y a tout un microcosme qui vit dedans. Quelle chance vous avez.",ep:13,g:364,l:44,wd:true,fm:true,w:1,p:1};
I.u[1895] = {n:"Bière du Feubuk",nn:"BIERE DU FEUBUK",t:37,d:"Cette bière fortement alcoolisée vous permettra de restaurer vos points de vie.",ep:16,g:4,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:4};
I.u[1896] = {n:"Viande d\'Oiseau",nn:"VIANDE D\'OISEAU",t:63,d:"Cette cuisse de tofu prouve que les chasseurs savent faire dans la finesse de temps en temps.",ep:1,g:348,l:1,wd:true,fm:true,w:2,p:1};
