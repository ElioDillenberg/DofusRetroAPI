I.u[2211] = {n:"Costume Iopesque",nn:"COSTUME IOPESQUE",t:14,d:"Grâce à cet accoutrement, vous allez briller dans les bals mondains. Le seul souci, c\'est que l\'on va souvent vous dire que vous êtes malin comme un Iop.",ep:1,g:14,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:100};
I.u[2212] = {n:"Costume Xeloresque",nn:"COSTUME XELORESQUE",t:14,d:"Muni de ce chapeau et de quelques ustensiles fournis, la ressemblance est flagrante. Vous êtes un Xelor, cela ne fait aucun doute.",ep:1,g:15,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:100};
I.u[2213] = {n:"Costume Sadidesque",nn:"COSTUME SADIDESQUE",t:14,d:"Il suffit de très peu de choses pour ressembler à un sadida: une descente de lit, avoir le regard dans le brouillard et la tête coincée dans le rectum. Effet garanti.",ep:1,g:16,l:1,wd:true,fm:true,w:10,c:"Pg=10",u:true,p:100};
I.u[2214] = {n:"Iop attitude",nn:"IOP ATTITUDE",t:31,d:"Vous avez la Iop attitude, jusqu\'à ce que vous rentriez en combat.",ep:1,g:4,l:1,wd:true,fm:true,w:0,m:true,p:1};
