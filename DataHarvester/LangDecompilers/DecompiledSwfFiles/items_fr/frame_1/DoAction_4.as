I.u[1648] = {n:"Potion d\'oubli de métier : Sculptemage de Baguettes",nn:"POTION D\'OUBLI DE METIER : SCULPTEMAGE DE BAGUETTES",t:44,d:"Cette potion vous fera oublier le métier de Sculptemage de Baguettes.",ep:1,g:9,l:1,wd:true,fm:true,w:1,u:true,p:10};
I.u[1649] = {n:"Potion d\'oubli de métier : Sculptemage de Bâtons",nn:"POTION D\'OUBLI DE METIER : SCULPTEMAGE DE BATONS",t:44,d:"Cette potion vous fera oublier le métier de Sculptemage de Bâtons.",ep:1,g:10,l:1,wd:true,fm:true,w:1,u:true,p:10};
I.u[1650] = {n:"Le Grand Cutter",nn:"LE GRAND CUTTER",t:6,d:"Cette épée est totalement appropriée pour agresser de gentilles personnes. Rapide, maniable et surtout extrêmement coupante. Elle est, de ce fait, interdite à la vente depuis longtemps. Mais tolérée aujourd\'hui comme défense anti-Bwork.",ep:14,g:23,l:58,wd:true,fm:true,w:12,an:14,e:[10,5,1,1,30,50,false,false],p:5200};
I.u[1651] = {n:"Le Cutter Sanglant",nn:"LE CUTTER SANGLANT",t:6,d:"Cette épée est totalement appropriée pour agresser de gentilles personnes. Rapide, maniable et surtout extrêmement coupante. Elle est, de ce fait, interdite à la vente depuis longtemps. Mais tolérée aujourd\'hui comme défense anti-bwork.",ep:14,g:23,l:64,wd:true,fm:true,w:12,an:14,e:[10,5,1,1,30,50,false,false],p:5200};
I.u[1652] = {n:"Patte d\'Arakne Majeure",nn:"PATTE D\'ARAKNE MAJEURE",t:103,d:"Une patte très fine, mais très poilue. Elle était autrefois utilisée lors de la confection des balais et des perruques au caractère un peu exotique.",ep:1,g:246,l:39,wd:true,fm:true,w:1,p:1};
I.u[1653] = {n:"Foi de Don Rouann",nn:"FOI DE DON ROUANN",t:24,d:"Cette bague symbolise le fait que vous êtes à la recherche du secret de Don Rouann. Vous avez appris actuellement le courage.",ep:2,g:27,l:1,wd:true,fm:true,w:0,p:1};
I.u[1654] = {n:"Foi de Don Rouann",nn:"FOI DE DON ROUANN",t:24,d:"Cette bague symbolise le fait que vous êtes à la recherche du secret de Don Rouann. Vous avez appris actuellement le courage, la communication et devez à présent être patient.",ep:2,g:27,l:1,wd:true,fm:true,w:0,p:1};
I.u[1655] = {n:"Foi de Don Rouann",nn:"FOI DE DON ROUANN",t:24,d:"Cette bague symbolise le fait que vous êtes à la recherche du secret de Don Rouann.",ep:2,g:27,l:1,wd:true,fm:true,w:0,p:1};
I.u[1656] = {n:"Fécaliseur du Magicien",nn:"FECALISEUR DU MAGICIEN",t:9,d:"Cet anneau est à même de protéger son porteur des petites attaques magiques et donc par extension, des petits monstres qui usent de la magie pour faire les malins et se rendre intéressants.",ep:1,g:40,l:26,wd:true,fm:true,w:2,p:2000};
I.u[1657] = {n:"Fécaliseur du Guerrier",nn:"FECALISEUR DU GUERRIER",t:9,d:"Cet anneau est à même de protéger son porteur des petites attaques physiques et donc par extension, des petits monstres qui jouent sur leur physique pour vous surprendre.",ep:1,g:41,l:26,wd:true,fm:true,w:2,p:2000};
I.u[1660] = {n:"Ambre d\'Abraknyde Sombre",nn:"AMBRE D\'ABRAKNYDE SOMBRE",t:50,d:"Cet ambre splendide contient une vilaine bestiole prisonnière dans la sève de son maître. Une sorte de sarcophage, qui permet à l\'Abraknyde Sombre de garder de bons souvenirs de ses invocations, une fois celles-ci mortes.",ep:9,g:638,l:78,wd:true,fm:true,w:2,p:6000};
I.u[1661] = {n:"Ambramulette Moskito",nn:"AMBRAMULETTE MOSKITO",t:1,d:"Porter autour du cou un Moskito pris dans un ambre, c\'est un peu montrer à vos adversaires, que vous êtes un redoutable chasseur... de Moskitos. De quoi vous rendre ridicule en dehors du Berceau.",ep:1,g:17,l:40,wd:true,fm:true,w:5,p:4400};
I.u[1662] = {n:"Ambramulette Arackne",nn:"AMBRAMULETTE ARACKNE",t:1,d:"Porter autour du coup une Arackne prise dans l\'ambre, lorsque vous attaquez une de ses soeurs, peut suffire à la décontenancer. Pauvre bête, on appelle cela l\'ironie du sort.",ep:1,g:18,l:34,wd:true,fm:true,w:5,p:3225};
I.u[1663] = {n:"Ecaille de Crocodaille",nn:"ECAILLE DE CROCODAILLE",t:59,d:"Cette écaille de Crocodaille constitue, pour tout chasseur débutant de Crocodaille qui se respecte, un petit trophée. N\'hésitez pas à la montrer aux ivrognes de la taverne, ils seront de toute évidence impressionnés et vous paieront des coups à boire.",ep:4,g:233,l:36,wd:true,fm:true,w:5,p:50};
I.u[1664] = {n:"Dent de Crocodaille",nn:"DENT DE CROCODAILLE",t:47,d:"Quand un Crocodaille a une dent contre vous, la meilleure solution consiste à la lui arracher avec vigueur et détermination, tel un dentiste rigoureux.",ep:4,g:234,l:36,wd:true,fm:true,w:1,p:50};
I.u[1665] = {n:"Bottes d\'Apprentissage",nn:"BOTTES D\'APPRENTISSAGE",t:11,d:"Ces bottes bien étranges étaient utilisées sur les jeunes disciples un peu limités intellectuellement, pour leur permettre de rattraper leur retard. Elles conviendront particulièrement aux Iops.",ep:4,g:68,l:50,wd:true,fm:true,w:10,p:6675};
I.u[1666] = {n:"Bottes de l\'Apprenti Invocateur",nn:"BOTTES DE L\'APPRENTI INVOCATEUR",t:11,d:"Ces bottes ont été spécialement conçues pour ceux qui aiment laisser à leurs créatures le plaisir de faire tout le travail pendant les combats.",ep:12,g:14,l:51,wd:true,fm:true,w:10,p:7000};
I.u[1667] = {n:"Ceinture Luthuthu",nn:"CEINTURE LUTHUTHU",t:10,d:"Cette ceinture porte le nom ridicule de son créateur, Luthuthu, un cordonnier beaucoup moins connu que Bowisse, mais encore très créatif malgré son âge avancé.",ep:8,g:13,l:50,wd:true,fm:true,w:10,p:6700};
I.u[1668] = {n:"Ceinture du Dodu Trembleur",nn:"CEINTURE DU DODU TREMBLEUR",t:10,d:"Le Dodu était un Sadida qui s\'obstinait à faire trembler ses ennemis, il a commandé à Bowisse cette ceinture, assortie à la couleur de ses poils, car ne l\'oublions pas, le Dodu en plus d\'être un trembleur était un charmeur.",ep:4,g:14,l:55,wd:true,fm:true,w:8,p:8000};
I.u[1669] = {n:"Ceinture des Vents",nn:"CEINTURE DES VENTS",t:10,d:"Cette ceinture a été créée pour la plus populaire des Crâette, qui aimait donner de son temps pour organiser tout un tas de fêtes et d\'évènements hors du commun, histoire de divertir le peuple entre deux repassages de Slip en Cuir du Captain Chafer.",ep:8,g:15,l:84,wd:true,fm:true,w:10,p:18300};
I.u[1670] = {n:"Plume Pointue de Kwak du Vent",nn:"PLUME POINTUE DE KWAK DU VENT",t:53,d:"Parfois, un Kwak du Vent dispose d\'une plume pointue différente de toutes les autres, elle est en règle générale magique et peut-être utilisée pour se déguiser en chef indien.",ep:5,g:393,l:24,wd:true,fm:true,w:10,p:100};
I.u[1671] = {n:"Avoine Magique",nn:"AVOINE MAGIQUE",t:34,d:"Ce fétu de paille d\'Avoine Magique est issu d\'un modeste paysan ensorcelé par un apprenti Bwork Mage, qui tenait son livre à l\'envers.",ep:1,g:550,l:20,wd:true,fm:true,w:10,p:0};
I.u[1672] = {n:"Poils Darits",nn:"POILS DARITS",t:54,d:"Ces poils constituent les derniers restes de Darit, un jeune magicien très prétentieux, qui avait oublié de vérifier si le Prespic avait lancé Contre, avant de lancer son sort Foudre Dépoteuse.",ep:1,g:555,l:18,wd:true,fm:true,w:1,p:100};
I.u[1673] = {n:"Patte d\'Arakne Magique",nn:"PATTE D\'ARAKNE MAGIQUE",t:103,d:"Cette patte d\'Arakne, n\'est pas comme toutes les autres, elle bouge toute seule et semble pleine de vie, même une fois arrachée à sa maîtresse. Une nouvelle vie pourrait commencer pour cette patte qui semble attirée par la couture.",ep:1,g:392,l:2,wd:true,fm:true,w:10,p:100};
I.u[1674] = {n:"Champignon Luidegît",nn:"CHAMPIGNON LUIDEGIT",t:15,d:"Ce héros, a été envoûté par le Chêne Mou, mais la magie un peu aléatoire de ce dernier, a fait qu\'il s\'est transformé en champignon. Les Champs étant assez idiots, ils le vénèrent et le protègent jusqu\'à la mort.",ep:1,g:390,l:46,wd:true,fm:true,w:10,p:100};
I.u[1675] = {n:"Os Surprise du Chafer Fantassin",nn:"OS SURPRISE DU CHAFER FANTASSIN",t:47,d:"Cet Os magique, permet aux chafers fantassins de faire des coups surprenants, comme d\'enlever et de mettre leurs slips sur la tête, sans les mains, en plein combat et en réussissant leurs jets d\'esquive. On ne se lasse jamais de cette prestation.",ep:3,g:15,l:35,wd:true,fm:true,w:1,p:100};
I.u[1676] = {n:"Osier Enchanté",nn:"OSIER ENCHANTE",t:15,d:"Ce morceau d\'Osier enchanté devrait permettre de fabriquer une magnifique corbeille à fruits magique, alliant élégance et puissance, tout le contraire d\'une méchante Liche.",ep:2,g:123,l:69,wd:true,fm:true,w:10,p:100};
I.u[1677] = {n:"Potion Dantegoulesque",nn:"POTION DANTEGOULESQUE",t:12,d:"Cette potion magique est la seule à même de rendre magiques des Slips en Cuir de Captain Chafer tressés entre eux, voilà qui en dit long sur la puissance d\'une telle potion.",ep:2,g:139,l:50,wd:true,fm:true,w:10,p:1000};
I.u[1678] = {n:"Noix de Kokoko Traumatisée",nn:"NOIX DE KOKOKO TRAUMATISEE",t:46,d:"Cette noix de Kokoko a été traumatisée durant son enfance, elle a accumulé une haine incroyable durant toutes ces années, ce qui en fait une noix de Kokoko très puissante.",ep:12,g:552,l:42,wd:true,fm:true,w:2,p:100};
I.u[1679] = {n:"Pierre de Crystaloboule",nn:"PIERRE DE CRYSTALOBOULE",t:50,d:"Avec un peu de patience et de travail, cette pierre précieuse peut devenir toute ronde et brillante comme un pompon de chapeau magique. Sa forme lui permettra en outre de se glisser presque n\'importe où.",ep:4,g:240,l:16,wd:true,fm:true,w:10,p:100};
I.u[1680] = {n:"Corne Brûlée du Minotoror",nn:"CORNE BRULEE DU MINOTOROR",t:47,d:"Cette Corne de Minotoror a été brûlée par une magie très puissante, ce qui en fait un artéfact magique de choix. Malheureusement, comme beaucoup de choses, elles ne fonctionnent correctement que par paires...",ep:2,g:245,l:125,wd:true,fm:true,w:10,p:100};
I.u[1681] = {n:"Peau de Larve Verte Magique",nn:"PEAU DE LARVE VERTE MAGIQUE",t:59,d:"Ne vous y trompez pas, derrière son apparence visqueuse, se cache en fait, un artefact qui s\'il est porté près de la tête, peut vous rendre aussi chanceux, qu\'un Chafer cocu.",ep:1,g:391,l:7,wd:true,fm:true,w:10,p:100};
I.u[1682] = {n:"Ecorce de Liroye Merline",nn:"ECORCE DE LIROYE MERLINE",t:96,d:"Ce morceau d\'écorce provient d\'un arbre de la clairière enchantée de Liroye Merline. \nCe morceau s\'est décroché lorsque Brouce Boulgoure le bûcheron a cassé sa Boulgourde en tentant d\'abattre un arbre enchanté.",ep:9,g:542,l:78,wd:true,fm:true,w:1,p:100};
I.u[1683] = {n:"Sac à Patates Clair",nn:"SAC A PATATES CLAIR",t:15,d:"Ce sac magique permettait à un Sadida Roublard de transporter des quantités incroyables de patates déjà épluchées, pour les revendre aux prisonniers. Une fois mort, son sac est transmis de Roublard en Roublard comme une sorte de rituel.",ep:1,g:243,l:12,wd:true,fm:true,w:10,p:100};
I.u[1684] = {n:"Sac à Patates Sombre",nn:"SAC A PATATES SOMBRE",t:15,d:"C\'est à peu près le même sac que le Sac à Patates Clair, si ce n\'est que celui-ci a été trempé dans de la sève d\'Abraknyde Sombre, ce qui lui confère des pouvoirs supplémentaires.",ep:1,g:244,l:12,wd:true,fm:true,w:10,p:100};
I.u[1685] = {n:"Plume du Dernier Poolay",nn:"PLUME DU DERNIER POOLAY",t:53,d:"Attention, vous avez en votre possession, la dernière plume arrachée au derrière, du dernier Poolay de tout Amakna. Cette plume constituera un très beau cadeau pour vos amis Tailleurs, ou pour les fanatiques du Poolay de plus en plus nombreux au fil du temps.",ep:3,g:247,l:32,wd:true,fm:true,w:1,p:100};
I.u[1686] = {n:"Teinture Magique de Rougeur",nn:"TEINTURE MAGIQUE DE ROUGEUR",t:70,d:"Cette petite potion magique donne une couleur rouge magnifique aux poils sur lesquels elle est appliquée. Accessoirement, il semblerait qu\'elle puisse apporter quelques effets magiques.",ep:7,g:63,l:10,wd:true,fm:true,w:1,p:500};
I.u[1687] = {n:"Teinture Magique Verdâtre",nn:"TEINTURE MAGIQUE VERDATRE",t:70,d:"Cette petite potion magique donne une couleur verte magnifique aux poils sur lesquels elle est appliquée. Accessoirement, il sembleraît qu\'elle puisse apporter quelques effets magiques.",ep:1,g:64,l:10,wd:true,fm:true,w:1,p:500};
I.u[1688] = {n:"Teinture Magique Bleutée",nn:"TEINTURE MAGIQUE BLEUTEE",t:70,d:"Cette petite potion magique donne une couleur bleue magnifique aux poils sur lesquels elle est appliquée. Accessoirement, il sembleraît qu\'elle puisse apporter quelques effets magiques.",ep:5,g:65,l:10,wd:true,fm:true,w:1,p:500};
I.u[1689] = {n:"Teinture Magique Orange",nn:"TEINTURE MAGIQUE ORANGE",t:70,d:"Cette petite potion magique donne une couleur orange magnifique aux poils sur lesquels elle est appliquée. Accessoirement, il semblerait qu\'elle puisse apporter quelques effets magiques.",ep:1,g:66,l:10,wd:true,fm:true,w:1,p:500};
I.u[1690] = {n:"Poils du Milimulou",nn:"POILS DU MILIMULOU",t:54,d:"Ces poils ressemblent à ceux de Mulou, en un peu plus courts, moins solides, mais plus doux. Malheureusement, l\'odeur elle est la même.",ep:1,g:554,l:22,wd:true,fm:true,w:1,p:1};
I.u[1691] = {n:"Poils du Meulou",nn:"POILS DU MEULOU",t:54,d:"Rares sont les aventuriers à avoir la chance de toucher ces poils vivants. \nÇa ne fait pas de vous un héros pour autant.",ep:2,g:553,l:198,wd:true,fm:true,w:10,p:200};
I.u[1692] = {n:"Teinture Magique Sombre",nn:"TEINTURE MAGIQUE SOMBRE",t:70,d:"Cette petite potion magique donne une couleur noire magnifique aux poils sur lesquels elle est appliquée. Accessoirement, il sembleraît qu\'elle puisse apporter quelques effets magiques.",ep:1,g:67,l:10,wd:true,fm:true,w:1,p:500};
I.u[1693] = {n:"Cape de Jules Yanos",nn:"CAPE DE JULES YANOS",t:17,d:"Jules Yanos, célèbre tailleur, était surtout célèbre pour avoir échangé sa femme contre un dé à coudre magique. Comme quoi, il existe encore des gens qui ont le sens des affaires.",ep:8,g:8,l:54,wd:true,fm:true,w:1,p:7800};
I.u[1694] = {n:"Laine du Trool Furieux",nn:"LAINE DU TROOL FURIEUX",t:57,d:"Ce morceau de laine provient du Trool le plus dangereux d\'Amakna, qui aurait été transformé en couvre-lit, par un tailleur sans scrupules, le même qui aurait soi-disant échangé sa femme contre un dé à coudre magique. Le mystère reste entier.",ep:4,g:27,l:115,wd:true,fm:true,w:10,p:1};
I.u[1695] = {n:"Cape d\'Elya Wood",nn:"CAPE D\'ELYA WOOD",t:17,d:"On raconte que le Captain Chafer a tenté de voler cette cape pour remplacer ses célèbres sous-vêtements en cuir. Mais elle ne lui allait pas, elle n\'incarnait pas assez la perversité et la perfidie pour lui.",ep:8,g:9,l:80,wd:true,fm:true,w:10,s:39,p:16500};
I.u[1696] = {n:"Etoffe du Meulou",nn:"ETOFFE DU MEULOU",t:55,d:"Les tailleurs ont trouvé il y a des lustres un moyen de déterminer la valeur d\'une étoffe, plus cette dernière sent mauvais, plus elle est puissante. Celle-ci sent plus mauvais qu\'un pédiluve d\'Enutrofs dans lequel un Chafer aurait lavé son slip.",ep:2,g:14,l:198,wd:true,fm:true,w:20,p:300};
I.u[1697] = {n:"Petit Sac en Laine de Boufton",nn:"PETIT SAC EN LAINE DE BOUFTON",t:81,d:"Ce petit sac ne fera pas de vous une célébrité, mais il aura au moins le mérite de vous permettre de ranger quelques effets personnels.",ep:1,g:25,l:1,wd:true,fm:true,w:5,p:100};
I.u[1698] = {n:"Sac du Petit Récolteur",nn:"SAC DU PETIT RECOLTEUR",t:81,d:"Ce sac est spécialement étudié pour contenir quelques ressources collectables. \nSeuls les vrais collecteurs sont à même de pouvoir le porter. (niveau 20 de collecteur requis).",ep:1,g:26,l:5,wd:true,fm:true,w:5,c:"PJ>2,20|PJ>24,20|PJ>36,20|PJ>28,20|PJ>26,20|PJ>41,20",p:2000};
I.u[1699] = {n:"Sac du Petit Aventurier",nn:"SAC DU PETIT AVENTURIER",t:81,d:"Ce sac est destiné à ceux qui ne partent pas trop loin de chez eux, et ont besoin d\'emmener des choses pas importantes du tout, comme des Planches de Frêne par exemple.",ep:1,g:27,l:10,wd:true,fm:true,w:5,p:3000};
I.u[1700] = {n:"Ceinture de Célérité",nn:"CEINTURE DE CELERITE",t:10,d:"Cette ceinture aérodynamique conçue par Tengra Reufrus vous rend agile comme l\'air, et libre comme un Tofu. A moins que ça ne soit l\'inverse.",ep:4,g:17,l:27,wd:true,fm:true,w:15,p:2000};
I.u[1701] = {n:"Ceinture de Poche",nn:"CEINTURE DE POCHE",t:10,d:"Cette ceinture possède de petites poches permettant de stocker davantage d\'objets dans votre inventaire.",ep:1,g:16,l:20,wd:true,fm:true,w:10,p:1300};
I.u[1702] = {n:"Sac du Récolteur",nn:"SAC DU RECOLTEUR",t:81,d:"Ce sac est spécialement étudié pour contenir des ressources collectables. \nSeuls les vrais collecteurs sont à même de pouvoir le porter. (niveau 40 de collecteur requis).",ep:8,g:28,l:15,wd:true,fm:true,w:10,c:"PJ>2,40|PJ>24,40|PJ>36,40|PJ>28,40|PJ>26,40|PJ>41,40",p:3000};
I.u[1703] = {n:"Sac du Grand Récolteur",nn:"SAC DU GRAND RECOLTEUR",t:81,d:"Ce sac est spécialement étudié pour contenir de nombreuses ressources collectables. \nSeuls les vrais collecteurs sont à même de pouvoir le porter. (niveau 60 de collecteur requis).",ep:8,g:29,l:20,wd:true,fm:true,w:10,c:"PJ>2,60|PJ>24,60|PJ>36,60|PJ>28,60|PJ>26,60|PJ>41,60",p:2000};
I.u[1704] = {n:"Sac du Staca Noviste",nn:"SAC DU STACA NOVISTE",t:81,d:"On raconte qu\'un Mineur célèbre était capable de miner sans arrêt pendant des mois dans 4 mines différentes en même temps. Il s\'agit en fait d\'une légende inventée par la guilde du Marteau-Faucille pour inciter ses mineurs à récolter plus.",ep:8,g:30,l:20,wd:true,fm:true,w:10,c:"PJ>2,90|PJ>24,90|PJ>36,90|PJ>28,90|PJ>26,90|PJ>41,90",p:10000};
I.u[1705] = {n:"Sac de l\'Aventurier",nn:"SAC DE L\'AVENTURIER",t:81,d:"C\'est le sac idéal pour ranger tous vos pains et potions lorsque vous partez pour plusieurs mois, dans le labyrinthe par exemple.",ep:8,g:31,l:40,wd:true,fm:true,w:4,p:4000};
I.u[1707] = {n:"Sac du grand aventurier",nn:"SAC DU GRAND AVENTURIER",t:81,d:"C\'est le sac idéal pour ranger tous vos pains et potions lorsque vous partez en voyage vers des destinations exotiques, comme l\'île de Moon.",ep:13,g:32,l:60,wd:true,fm:true,w:5,p:6000};
I.u[1709] = {n:"Nonos",nn:"NONOS",t:47,d:"" + "\"" + "Il est pour qui le nonos à sa mémère ?" + "\"" + " Tel fût les derniers mots d\'une Enutrof qui confondit un Meulou avec son animal de compagnie.",ep:1,g:15,l:1,wd:true,fm:true,w:2,p:1};
I.u[1710] = {n:"Croissant de Lune",nn:"CROISSANT DE LUNE",t:1,d:"Les origines de cette amulette, représentant un quartier de lune, sont inconnues de tout le monde...ou presque. Qui sait donc quels secrets ce bijou renferme...",ep:1,g:19,l:1,wd:false,fm:true,w:10,p:1000};
I.u[1711] = {n:"Chienchien",nn:"CHIENCHIEN",t:18,d:"Avec ce petit toutou, vous goûterez aux joies des petites promenades pipi. Fort de ses racines sauvages, il est déconseillé de lui donner à manger, sous peine de perdre une main.  \n(CE FAMILIER N\'AIME AUCUNE RESSOURCE)",ep:1,g:6,l:1,wd:true,fm:true,w:10,c:"PZ=1",p:1};
I.u[1712] = {n:"Potion Pahoa Raide",nn:"POTION PAHOA RAIDE",t:12,d:"Cette potion permet de retrouver quelques points d\'énergie.",ep:1,g:140,l:20,wd:true,fm:true,w:1,u:true,p:50};
I.u[1713] = {n:"Potion Raide Boule",nn:"POTION RAIDE BOULE",t:12,d:"Cette potion permet de retrouver quelques points d\'énergie.",ep:1,g:141,l:40,wd:true,fm:true,w:1,u:true,p:100};
I.u[1715] = {n:"Le don de Nouwel",nn:"LE DON DE NOUWEL",t:25,d:"Best seller de la fin d\'année 634, ce livre se doit d\'être lu. Un classique !",ep:2,g:19,l:1,wd:true,fm:true,w:1,u:true,p:50};
I.u[1716] = {n:"La famille DuCiel",nn:"LA FAMILLE DUCIEL",t:25,d:"Deuxième meilleure vente de l\'hiver 634, ce livre est déjà une référence.",ep:2,g:19,l:1,wd:true,fm:true,w:1,u:true,p:50};
I.u[1717] = {n:"Premier Noël",nn:"PREMIER NOEL",t:25,d:"L\'outsider du début d\'année 635, ce livre mérite vraiment qu\'on s\'y intéresse.",ep:2,g:19,l:1,wd:true,fm:true,w:1,u:true,p:50};
I.u[1718] = {n:"Force de Crocoburio",nn:"FORCE DE CROCOBURIO",t:9,d:"Cet anneau fut enchanté par le souffle noir de Grougalorasalar, lui conférant un immense pouvoir, hélas aujourd\'hui amoindri. Cette relique était destinée à Crocoburio car lui seul était à même d\'équiper cet anneau sans subir sa domination.",ep:4,g:38,l:40,wd:true,fm:true,w:2,c:"CS>59",p:4000};
I.u[1719] = {n:"Esprit de Crocoburio",nn:"ESPRIT DE CROCOBURIO",t:11,d:"Cette paire de bottes appartiendrait à Crocoburio. Le peu de personnes aptes à les chausser se sentent soudain envahis de pensées machiavéliques et ingénieuses. Ce maléfice étant, selon la légende, dû aux milliers de crânes qu\'elles ont écrasés.",ep:4,g:12,l:42,wd:true,fm:true,w:10,c:"CI>49",p:5000};
I.u[1720] = {n:"Puissance de Crocoburio",nn:"PUISSANCE DE CROCOBURIO",t:10,d:"Grougalorasalar aimait garder des fragments d\'os appartenant aux ennemis vaincus les plus forts. Son affection pour Crocoburio le poussa à les utiliser pour fabriquer cette ceinture surpuissante.",ep:4,g:18,l:34,wd:true,fm:true,w:15,c:"CV>119",p:3000};
I.u[1721] = {n:"Potion d\'oubli : \'Horloge\'",nn:"POTION D\'OUBLI : \'HORLOGE\'",t:43,d:"Cette potion permet de désapprendre un niveau du sort des Xélor : \'Horloge\'.",ep:2,g:37,l:1,wd:true,fm:true,w:1,u:true,p:1000};
I.u[1722] = {n:"Transformation Abraknyde",nn:"TRANSFORMATION ABRAKNYDE",t:27,d:"La malédiction du Chêne Mou vous a transformé en abraknyde. \nPour rompre le charme, vous devez vaincre un aventurier égaré dans la forêt des Abraknydes.",ep:1,g:1,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[1723] = {n:"Bonbon rouge surpuissant",nn:"BONBON ROUGE SURPUISSANT",t:28,d:"Ce bonbon vous booste temporairement.",ep:1,g:1,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[1724] = {n:"Courage",nn:"COURAGE",t:29,d:"Cette bénédiction permet de gagner de la Force pour les prochains combats.",ep:18,g:1,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[1725] = {n:"Découragé",nn:"DECOURAGE",t:30,d:"Cette malédiction vous enlève de la Force pour le prochain combat.",ep:1,g:1,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[1726] = {n:"Touffe de cheveux de Tarche",nn:"TOUFFE DE CHEVEUX DE TARCHE",t:24,d:"Cette touffe de cheveux prouve votre victoire face à Tarche. Quel exploit !",ep:1,g:30,l:1,wd:true,fm:true,w:0,p:1};
I.u[1727] = {n:"Lettre de Menaces",nn:"LETTRE DE MENACES",t:24,d:"Une lettre de menaces adressée à Dame Cloude.",ep:2,g:26,l:1,wd:true,fm:true,w:0,p:1};
I.u[1728] = {n:"Chacha",nn:"CHACHA",t:18,d:"Cette petite boule de poil friponne et câline fait craquer la gente féminine toutes classes confondues. Un animal redoutable donc.",ep:1,g:1,l:1,wd:true,fm:true,w:10,c:"PZ=1",p:1};
I.u[1729] = {n:"Anneau Mèr",nn:"ANNEAU MER",t:9,d:"Cet anneau appartenait à Mèr, le plus gros fainéant de toute la contrée d\'Amakna. Il vouait un culte aux doneuths, mais aussi aux côtelettes de ribs.",ep:7,g:28,l:25,wd:true,fm:true,w:25,c:"CI<100&CC<100",p:1900};
I.u[1730] = {n:"Mesure de sel",nn:"MESURE DE SEL",t:48,d:"Cette mesure de sel est la dose idéale pour assaisonner moult plats savoureux. Elle est indispensable pour un repas réussi.",ep:1,g:261,l:1,wd:true,fm:true,w:1,p:1};
I.u[1731] = {n:"Dose de jus goûtu",nn:"DOSE DE JUS GOUTU",t:15,d:"Ce jus vous apportera les saveurs élémentaires pour avoir un plat parfumé.",ep:1,g:262,l:1,wd:true,fm:true,w:1,p:4};
I.u[1732] = {n:"Bol",nn:"BOL",t:15,d:"Rien de plus pratique qu\'un bol pour boire plus proprement de la soupe, bien que boire proprement n\'altère pas le goût. À moins que vos doigts soient très sales.",ep:16,g:263,l:1,wd:true,fm:true,w:1,p:6};
I.u[1733] = {n:"Terrine",nn:"TERRINE",t:15,d:"" + "\"" + "La terrine, c\'est de la viandasse, ça fait du bien, par ou que qu\'ça passe. Vive la terrine, vive la terrine !" + "\"" + " (Extrait de l\'hymne du boucher couplet 4).",ep:16,g:264,l:1,wd:true,fm:true,w:1,p:6};
I.u[1734] = {n:"Cerise",nn:"CERISE",t:46,d:"Ce fruit ressemble à s\'y méprendre à une grosse paire de boules rebondissantes.",ep:1,g:265,l:1,wd:true,fm:true,w:1,p:1};
I.u[1735] = {n:"Certificat de Mise en Chanil : Chacha",nn:"CERTIFICAT DE MISE EN CHANIL : CHACHA",t:77,d:"Ce certificat décrit les caractéristiques du Chacha que vous avez placé au chanil.",ep:3,g:24,l:1,wd:false,fm:true,w:1,p:100};
I.u[1736] = {n:"Citron",nn:"CITRON",t:46,d:"Le jus de ce fruit est parfait pour cacher les odeurs de chair en putréfaction. Idéal pour servir des restes de poissons avariés et variés à vos invités.",ep:1,g:266,l:1,wd:true,fm:true,w:1,p:2};
I.u[1737] = {n:"Pain des Champs",nn:"PAIN DES CHAMPS",t:33,d:"Dans la poche ou dans le gosier, ce pain des champs s\'avère un pain très lourd. Mais il faut avouer que son goût a le côté rustique que l\'on attend d\'un bon pain.",ep:1,g:68,l:40,wd:true,fm:true,w:3,u:true,ut:true,p:1};
I.u[1738] = {n:"Pain des Villes",nn:"PAIN DES VILLES",t:33,d:"Sans hésitation, on peut dire que ce pain donne envie de se bricoler un graillou. Il procure aussi une sensation de satiété assez rapidement.",ep:1,g:69,l:10,wd:true,fm:true,w:2,u:true,ut:true,p:1};
I.u[1739] = {n:"Baguette Affeulante",nn:"BAGUETTE AFFEULANTE",t:3,d:"Cette baguette a d\'étranges propriétés : au toucher, elle est brûlante. Mais le plus énigmatique reste son odeur de cendre, qui laisse à penser que le bois de cette baguette aurait survécu à un feu de forêt.",ep:13,g:24,l:80,wd:true,fm:true,w:1,an:11,e:[5,4,2,3,40,50,false,true],c:"CI>60&CV>40",p:1};
I.u[1740] = {n:"Transformation Crocoburio",nn:"TRANSFORMATION CROCOBURIO",t:27,d:"La Crocobur enferme l\'essence de Crocoburio et ne peut être volée à son maître qu\'en lui ôtant la vie.",ep:4,g:2,l:1,wd:true,fm:true,w:0,m:true,p:1};
I.u[1741] = {n:"Sang de Phénix",nn:"SANG DE PHENIX",t:14,d:"Ce sang de phénix permet de ramener un fantôme à la vie et de lui rendre quelques points d\'énergie.",ep:18,g:3,l:10,wd:true,fm:true,w:1,c:"Pg=23",ut:true,p:1000};
I.u[1742] = {n:"Sang de Phénix de Grand Cru",nn:"SANG DE PHENIX DE GRAND CRU",t:14,d:"Ce sang de phénix permet de ramener un fantôme à la vie et de lui rendre une grande quantité d\'énergie.",ep:18,g:3,l:50,wd:true,fm:true,w:1,c:"Pg=23",ut:true,p:2500};
I.u[1744] = {n:"Touffe de cheveux de Torche",nn:"TOUFFE DE CHEVEUX DE TORCHE",t:24,d:"Cette touffe de cheveux prouve votre victoire face à Torche. De toute beauté.",ep:1,g:30,l:1,wd:true,fm:true,w:0,p:1};
I.u[1745] = {n:"Seringue de vétérinaire",nn:"SERINGUE DE VETERINAIRE",t:24,d:"Bout pointu, réservoir, pas de doute, c\'est une seringue... Vous voilà prévenu.",ep:1,g:31,l:1,wd:true,fm:true,w:0,p:1};
I.u[1746] = {n:"Sang de Wabbit GM",nn:"SANG DE WABBIT GM",t:24,d:"Qui aurait pu imaginer arriver chez les wabbits avec une seringue, dans le but d\'extraire du sang sur la créature la plus modifiée génétiquement de l\'île ? Vous, évidemment.",ep:8,g:32,l:1,wd:true,fm:true,w:0,p:1};
I.u[1747] = {n:"Analyse de sang de Wabbit GM",nn:"ANALYSE DE SANG DE WABBIT GM",t:24,d:"Les analyses sont formelles, le sang identifié provient d\'un postérieur de Wabbit GM. Les tests ont été si poussés que l\'alchimiste a même su vous dire quelle fesse avait été piquée, en l\'occurrence la droite.",ep:5,g:33,l:1,wd:true,fm:true,w:0,p:1};
I.u[1748] = {n:"Wabbit",nn:"WABBIT",t:18,d:"Ce petit Wabbit est domestiqué. Il ne vous mordra donc pas. En revanche pour les autres...",ep:8,g:7,l:1,wd:true,fm:true,w:10,c:"PZ=1",s:11,p:1};
I.u[1749] = {n:"Jeton de la Foire du Trool",nn:"JETON DE LA FOIRE DU TROOL",t:15,d:"Vous avez acheté ce jeton à Leonzi Trool qui rêve un jour d\'ouvrir un lieu d\'amusement ouvert à tous (Plaine de Cania).",ep:13,g:737,l:1,wd:true,fm:true,w:1,p:1};
I.u[1750] = {n:"Poisson Pané",nn:"POISSON PANE",t:41,d:"Ce poisson cubique est connu pour avoir l\'esprit particulièrement carré. Il est même carrément stupide ce qui en fait un poisson facile à pêcher.",ep:2,g:269,l:1,wd:true,fm:true,w:3,p:10};
I.u[1751] = {n:"Poisson pané vidé",nn:"POISSON PANE VIDE",t:62,d:"Ce poisson pané peut maintenant être utilisé comme ingrédient.",ep:2,g:270,l:1,wd:true,fm:true,w:2,p:1};
I.u[1752] = {n:"Poisson pané frit **",nn:"POISSON PANE FRIT **",t:49,d:"Ce rectangle savoureux est composé des plus légers et meilleurs morceaux de plusieurs anciens poissons cubiques. Son goût plaît beaucoup aux enfants, mais ses valeurs nutritionnelles ne sont pas grandes.",ep:2,g:71,l:1,wd:true,fm:true,w:3,u:true,ut:true,p:60};
I.u[1753] = {n:"Poisson pané frit ***",nn:"POISSON PANE FRIT ***",t:49,d:"Ce rectangle savoureux est composé des plus légers et meilleurs morceaux de plusieurs anciens poissons cubiques. Son goût plaît beaucoup aux enfants, mais ses valeurs nutritionnelles ne sont pas grandes.",ep:2,g:72,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:100};
I.u[1754] = {n:"Poisson Igloo",nn:"POISSON IGLOO",t:41,d:"Cette espèce rare de poisson pané vit habituellement dans les eaux glacées du Grand Nord d\'où son nom. Du fait de son alimentation polaire, sa chair est riche en fibre.",ep:1,g:271,l:1,wd:true,fm:true,w:3,p:100};
I.u[1755] = {n:"Poisson Igloo vidé",nn:"POISSON IGLOO VIDE",t:62,d:"Ce poisson Igloo peut maintenant être utilisé comme ingrédient.",ep:1,g:272,l:1,wd:true,fm:true,w:2,p:100};
I.u[1756] = {n:"Poisson pané frit ****",nn:"POISSON PANE FRIT ****",t:49,d:"Ce poisson Igloo frit, en plus d\'être savoureux, possède d\'énormes valeurs nutritionnelles.",ep:1,g:73,l:1,wd:true,fm:true,w:1,c:"Ci<10",u:true,ut:true,p:10000};
I.u[1757] = {n:"Crabe Sourimi",nn:"CRABE SOURIMI",t:41,d:"Ce crabe, à la forme si particulière, est très apprécié pour la grande quantité de chair qu\'il contient. Il est aussi le seul crabe pêchable à la canne, car il possède l\'instinct de couper les fils avec ses pinces... qui elles, ne coupent pas.",ep:2,g:273,l:1,wd:true,fm:true,w:3,p:1};
I.u[1758] = {n:"Crabe Sourimi vidé",nn:"CRABE SOURIMI VIDE",t:62,d:"Ce crabe sourimi peut maintenant être utilisé comme ingrédient.",ep:2,g:274,l:1,wd:true,fm:true,w:2,p:1};
I.u[1759] = {n:"Crabe Sourimi Exotique",nn:"CRABE SOURIMI EXOTIQUE",t:41,d:"Cousin du crabe sourimi, celui-ci est bien plus intelligent. Cela explique sa rareté. Il arrive cependant que parfois, après des soirées arrosées, il laisse à nouveau parler son instinct.",ep:1,g:275,l:1,wd:true,fm:true,w:3,p:20};
I.u[1760] = {n:"Crabe Sourimi exotique vidé",nn:"CRABE SOURIMI EXOTIQUE VIDE",t:62,d:"Ce crabe sourimi exotique peut maintenant être utilisé comme ingrédient.",ep:1,g:276,l:1,wd:true,fm:true,w:2,p:20};
