I.u[1403] = {n:"Marteau Tau",nn:"MARTEAU TAU",t:7,d:"Ce marteau a appartenu à Tau, un jeune disciple d\'Osamodas qui passait son temps à raconter des blagues stupides. Son marteau, tout comme lui, ne rend pas très intelligent, mais peut parfois détendre l\'atmosphère...",ep:1,g:3,l:23,wd:true,fm:true,w:20,an:15,e:[4,4,1,1,30,30,false,true],c:"CA>20",p:1700};
I.u[1404] = {n:"Marteau Toh\'Lo",nn:"MARTEAU TOH\'LO",t:7,d:"Vous aimez les animaux ? Toh\'Lo aussi. Il leur avait même voué sa vie. Ce marteau raconte un peu son histoire , une grande communion avec les bêtes...",ep:5,g:15,l:35,wd:true,fm:true,w:35,an:15,e:[6,4,1,1,30,30,false,true],c:"CI>80&CW>30",p:3400};
I.u[1405] = {n:"Potion Eau de fée",nn:"POTION EAU DE FEE",t:12,d:"Cette potion parvient à reproduire les mêmes pouvoirs de guérison que le sang d\'une Eniripsa. Plus besoin de les saigner à blanc maintenant.",ep:3,g:41,l:60,wd:true,fm:true,w:1,u:true,ut:true,p:100};
