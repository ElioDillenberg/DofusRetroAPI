I.u[1762] = {n:"Poisson Tigre",nn:"POISSON TIGRE",t:41,d:"Ce poisson est comme un poisson chaton, mais en beaucoup, beaucoup plus dangereux.",ep:1,g:279,l:1,wd:true,fm:true,w:3,p:30};
