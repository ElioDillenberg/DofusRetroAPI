I.u[1438] = {n:"Bonne Pioche",nn:"BONNE PIOCHE",t:21,d:"Cette pioche est considérée dans de nombreux clans comme un talisman portant chance.",ep:1,g:1,l:25,wd:false,fm:true,w:20,an:17,e:[4,4,1,1,50,50,false,true],c:"CC>30",p:2500};
I.u[1439] = {n:"Mauvaise Pioche",nn:"MAUVAISE PIOCHE",t:21,d:"Cette pioche a tendance à attirer le mauvais oeil. Cependant, il parait qu\'il n\'y manquerait pas grand chose pour qu\'elle devienne une grande pioche",ep:1,g:1,l:25,wd:false,fm:true,w:20,an:17,e:[4,6,1,1,50,10,false,true],p:250};
