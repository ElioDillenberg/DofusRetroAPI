I.u[1468] = {n:"Bocal",nn:"BOCAL",t:71,d:"Ce bocal peut servir à tout, de la fabrication de potion, à la noyade de poisson.",ep:1,g:58,l:1,wd:true,fm:true,w:1,p:100};
I.u[1469] = {n:"Parchemin de recommandation",nn:"PARCHEMIN DE RECOMMANDATION",t:24,d:"Ce parchemin vous a été remis par Uba Tessé. Il vous recommande chaudement pour devenir alchimiste.",ep:1,g:26,l:1,wd:true,fm:true,w:0,p:1};
I.u[1470] = {n:"Parchemin de recommandation",nn:"PARCHEMIN DE RECOMMANDATION",t:24,d:"Ce parchemin vous a été remis par Uba Tessé. Il vous recommande chaudement pour devenir alchimiste. Une petite note a été ajouté par Boff Chalon.",ep:2,g:26,l:1,wd:true,fm:true,w:0,p:1};
I.u[1471] = {n:"Parchemin de recommandation et fleur",nn:"PARCHEMIN DE RECOMMANDATION ET FLEUR",t:24,d:"Ce parchemin vous a été remis par Uba Tessé. Il vous recommande chaudement pour devenir alchimiste. Une petite note a été ajoutée par Boff Chalon. Une fleur attachée prouve que vous avez appris la première leçon.",ep:3,g:26,l:1,wd:true,fm:true,w:0,p:1};
I.u[1472] = {n:"Parchemin de demande d\'examen",nn:"PARCHEMIN DE DEMANDE D\'EXAMEN",t:24,d:"Ce parchemin indique que vous avez passé votre formation d\'alchimiste et que vous êtes apte à tenter l\'examen.",ep:3,g:26,l:1,wd:true,fm:true,w:0,p:1};
I.u[1473] = {n:"Mains Vertes",nn:"MAINS VERTES",t:20,d:"Avec ces gants, vous pourrez facilement et sans risque cueillir n\'importe quelle plante ou fleur.",ep:1,g:5,l:1,wd:true,fm:true,w:5,e:[5,3,1,1,30,50,false,true],p:100};
I.u[1474] = {n:"Amulette de Xélor",nn:"AMULETTE DE XELOR",t:1,d:"Cette amulette surpuissante est le fruit du travail sur la maîtrise du temps d\'un très vieux Xélor.",ep:1,g:28,l:52,wd:true,fm:true,w:10,p:7300};
I.u[1475] = {n:"La Zulie Lame",nn:"LA ZULIE LAME",t:6,d:"La Zulie Lame a été forgée par le célèbre forgeron Dave-Lol avant de mourir, pour la plus jolie des iopettes du royaume. On dit que cette lame n\'a d\'égal que la beauté de son porteur.",ep:4,g:15,l:60,wd:true,fm:true,w:30,an:14,e:[10,4,1,1,30,50,false,false],c:"CS>100&CV>50",p:3000};
I.u[1476] = {n:"Amulette du Craqueleur",nn:"AMULETTE DU CRAQUELEUR",t:1,d:"Cette puissante amulette contient toute la vie du Craqueleur, tous les objets qu\'il a récoltés avant de mourir sous les coups de butoir d\'aventuriers attirés par ses richesses.",ep:2,g:25,l:47,wd:true,fm:true,w:5,p:5800};
I.u[1477] = {n:"Chance à Mulette",nn:"CHANCE A MULETTE",t:1,d:"Mulette était une petite Ecaflipette qui n\'a jamais eu de chance dans sa vie, jusqu\'au jour où sa grand-mère lui offrit cette amulette.",ep:1,g:26,l:44,wd:true,fm:true,w:5,p:5000};
I.u[1478] = {n:"Harmonie",nn:"HARMONIE",t:1,d:"Ce pendentif aide son porteur à se retrouver en paix avec le monde qui l\'entoure.",ep:1,g:122,l:47,wd:true,fm:true,w:1,p:6000};
I.u[1479] = {n:"Le Twote en Carton",nn:"LE TWOTE EN CARTON",t:7,d:"Ce marteau légendaire a déjà fait ses preuves en Amakna, appartenant au puissant Twote en Carton, qui n\'hésita jamais à s\'en servir, pour réduire ses adversaires en patte à mâcher et confettis.",ep:13,g:6,l:46,wd:true,fm:true,w:20,an:15,e:[5,4,1,1,50,30,false,false],c:"CI>50&CV>30",p:5600};
I.u[1480] = {n:"Accord",nn:"ACCORD",t:1,d:"Ce pendentif aide son porteur à se retrouver en paix avec le monde qui l\'entoure.",ep:1,g:121,l:38,wd:true,fm:true,w:1,p:4000};
I.u[1481] = {n:"Entente",nn:"ENTENTE",t:1,d:"Ce pendentif aide son porteur à se retrouver en paix avec le monde qui l\'entoure.",ep:1,g:11,l:26,wd:true,fm:true,w:1,p:2000};
I.u[1482] = {n:"Emblème Livide",nn:"EMBLEME LIVIDE",t:1,d:"Cette amulette est extrêmement utile, mais rend son porteur blanc comme un bouftou.",ep:1,g:32,l:41,wd:true,fm:true,w:2,p:4500};
I.u[1483] = {n:"Kabale",nn:"KABALE",t:1,d:"Symbole mystérieux d\'un organisme mystérieux. D\'ailleurs il n\'existe pas, et vous n\'en avez jamais entendu parler !",ep:11,g:13,l:29,wd:true,fm:true,w:2,p:2500};
I.u[1484] = {n:"Amulette Itby remaniée",nn:"AMULETTE ITBY REMANIEE",t:1,d:"Excellent travail de l\'apprenti-orfèvre Polma Kartnez, cette copie surpasse l\'originale.",ep:1,g:31,l:17,wd:true,fm:true,w:5,p:1000};
I.u[1485] = {n:"La Reinette",nn:"LA REINETTE",t:1,d:"Cette amulette, est une amélioration de La Destinée Dorée. Étrangement, elles n\'ont rien en commun, excepté leur aspect assez ridicule, il faut le reconnaître.",ep:1,g:27,l:22,wd:true,fm:true,w:5,c:"CC>10",p:1500};
I.u[1486] = {n:"Médaille Holy",nn:"MEDAILLE HOLY",t:1,d:"Holy était une disciple de Iop qui avait juré de terrasser le mal où qu\'il soit. D\'un calme redoutable, elle ne s\'énervait jamais sauf quand on la comparait à une certaine Oly.",ep:1,g:23,l:31,wd:true,fm:true,w:3,p:2800};
I.u[1487] = {n:"L\'Adelus",nn:"L\'ADELUS",t:10,d:"Cette ceinture, en plus d\'habiller d\'une manière élégante, procure à son porteur un effet massant vraiment agréable. L\'heureux propriétaire, moins stressé, est alors plus apte à réfléchir.",ep:1,g:12,l:48,wd:true,fm:true,w:30,p:6075};
I.u[1488] = {n:"Plimclik",nn:"PLIMCLIK",t:1,d:"Sous ce nom étrange se cache un objet tout aussi étrange, né on ne sait trop comment de précédentes amulettes. On ignore tout des circonstances dans lesquelles elle a été découverte.",ep:11,g:123,l:130,wd:true,fm:true,w:24,p:1000};
I.u[1489] = {n:"La Broche Hète",nn:"LA BROCHE HETE",t:1,d:"Hète était l\'un des plus grands guerriers d\'Amakna. La légende dit qu\'il tirait sa sagesse de la broche qu\'il portait toujours sur lui.",ep:11,g:15,l:28,wd:true,fm:true,w:1,p:2300};
I.u[1490] = {n:"La Chaîne Nhanor Kibrill",nn:"LA CHAINE NHANOR KIBRILL",t:1,d:"Son créateur, un certain Ken Aton, était très célèbre pour ses habitudes assez étranges. Il passait ses journées à chasser le Minotoror pour se fabriquer une moquette pectorale.",ep:1,g:12,l:13,wd:true,fm:true,w:2,p:650};
I.u[1491] = {n:"Chaîne de la Liche",nn:"CHAINE DE LA LICHE",t:1,d:"La Liche était une sale créature connue pour s\'incruster dans les groupes de valeureux guerriers afin de subtiliser une part du butin. Un jour, son petit manège fut découvert et elle dut prendre la fuite.",ep:13,g:12,l:45,wd:true,fm:true,w:2,p:5400};
I.u[1493] = {n:"Blessure du Sacrieur",nn:"BLESSURE DU SACRIEUR",t:9,d:"On dit que cet anneau serait issu d\'une des nombreuses blessures reçues par un Sacrieur.",ep:1,g:13,l:15,wd:true,fm:true,w:2,p:800};
I.u[1494] = {n:"Le Bagueur",nn:"LE BAGUEUR",t:9,d:"Ce joli anneau avait comme fonction première de permettre aux Osamodas de reconnaître leur tofu dans les combats.",ep:1,g:14,l:15,wd:true,fm:true,w:4,p:800};
I.u[1495] = {n:"L\'Anneau Dhyn",nn:"L\'ANNEAU DHYN",t:9,d:"Ce petit anneau qui ne paye pas de mine a été crée par Degun. Ce bijoutier était plus connu pour ses coups de gueule envers tout le monde et sa cruelle aversion pour la Liche, que pour sa créativité en matière d\'orfévrerie.",ep:1,g:37,l:36,wd:true,fm:true,w:3,p:3575};
I.u[1496] = {n:"Chance d\'Ecaflip",nn:"CHANCE D\'ECAFLIP",t:9,d:"Cet anneau a été créé pour augmenter la chance de son porteur lors de parties de cartes.",ep:1,g:32,l:20,wd:true,fm:true,w:4,p:1300};
I.u[1497] = {n:"Passé de Xélor",nn:"PASSE DE XELOR",t:9,d:"Anneaux engendrés lors des voyages temporels de Xélor.",ep:1,g:16,l:30,wd:true,fm:true,w:4,p:2600};
I.u[1498] = {n:"Souvenir d\'Enutrof",nn:"SOUVENIR D\'ENUTROF",t:9,d:"Anneaux créés à partir de larmes d\'Enutrof ayant perdu sa pelle. Cela donne une idée du désespoir dans lequel peut être plongé le bonhomme.",ep:1,g:15,l:30,wd:true,fm:true,w:4,p:2600};
I.u[1499] = {n:"L\'Enutrofion",nn:"L\'ENUTROFION",t:9,d:"Cet anneau est destiné aux seuls Enutrofs. Leur dieu l\'a enchanté pour qu\'il ne soit porté que par ses disciples. Il permettra à tout Enutrof qui se respecte, d\'avoir beaucoup plus de chance pour découvrir des trésors.",ep:1,g:39,l:59,wd:true,fm:true,w:1,c:"PG=3",p:10000};
I.u[1500] = {n:"La Mokette",nn:"LA MOKETTE",t:17,d:"Cette splendide cape, tissée à partir de divers éléments fibreux de toute nature, est une pièce unique de par sa couleur et ses reflets.",ep:8,g:8,l:43,wd:true,fm:true,w:16,p:5000};
I.u[1501] = {n:"Alliance",nn:"ALLIANCE",t:9,d:"Cette alliance symbolise l\'union entre deux âmes follement amoureuses l\'une de l\'autre. Inutile de conter fleurette caché derrière les buissons, cette fois c\'est pour de bon, vous allez pouvoir vous afficher sans scrupules.",ep:2,g:27,l:1,wd:true,fm:true,w:10,p:1};
I.u[1503] = {n:"Baguette de Feu Follesque",nn:"BAGUETTE DE FEU FOLLESQUE",t:3,d:"Pour la fabrication de cette baguette lugubre, on dit qu\'il faut mettre la baguette dans le corps d\'un cadavre frais, et l\'enterrer peu après. Mais personne n\'ose imaginer comment faire.",ep:3,g:14,l:20,wd:true,fm:true,w:15,an:11,e:[5,3,2,3,30,50,false,true],c:"CI>30",p:1000};
I.u[1504] = {n:"Dagues O\'Bert",nn:"DAGUES O\'BERT",t:5,d:"O\'Bert cou rouge était célèbre dans la campagne profonde. Un jour où il chantait à tue-tête il fut tué, la tête tranchée. D\'où son surnom. La dague coupable fut retrouvée, mais personne ne chercha son propriétaire. O\'Bert chantait vraiment trop mal.",ep:1,g:1,l:30,wd:true,fm:true,w:10,an:13,e:[10,3,1,1,30,50,false,false],c:"CA>30",p:3000};
I.u[1505] = {n:"Flûte",nn:"FLUTE",t:20,d:"Cette flûte très rare en bois d\'abraknyde vous permettra de divertir et émouvoir vos amis.",ep:1,g:6,l:1,wd:true,fm:true,w:1,an:4,e:[0,5,1,1,0,0,false,false],p:50000};
I.u[1506] = {n:"Symbole de l\'étrange",nn:"SYMBOLE DE L\'ETRANGE",t:15,d:"Cette carte à jouer représente...quelque chose. En fait, on ne sait pas trop quoi, mais on est persuadé que c\'est important.",ep:1,g:224,l:1,wd:true,fm:true,w:10,p:1};
