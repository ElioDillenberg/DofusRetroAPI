I.u[1435] = {n:"La Pelle du Large",nn:"LA PELLE DU LARGE",t:8,d:"Pelle très lourde, donc réservée à l\'usage des aventuriers les plus gr... larges.",ep:9,g:11,l:80,wd:true,fm:true,w:150,an:16,e:[10,6,1,1,50,50,false,true],c:"CS>100",p:40000};
I.u[1436] = {n:"Pelle Ikan",nn:"PELLE IKAN",t:8,d:"Ikan etait le petit poisson de Chourros le guerrier. Il est important de préciser que le nom de cette pelle n\'a aucun rapport avec ce poisson, mais plutôt avec son odeur...",ep:7,g:8,l:50,wd:true,fm:true,w:50,an:16,e:[10,5,1,1,50,50,false,true],c:"CC>30&CA>10",p:7800};
