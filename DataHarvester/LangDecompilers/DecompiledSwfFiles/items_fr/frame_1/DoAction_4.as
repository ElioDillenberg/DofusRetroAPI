I.u[1779] = {n:"Bar Rikain",nn:"BAR RIKAIN",t:41,d:"On rencontre ce poisson le plus souvent dans les lacs et cours d\'eau qui descendent des glaciers. Sa grosseur et sa chaire fraîche en font un animal très apprécié des pêcheurs.",ep:2,g:290,l:40,wd:true,fm:true,w:3,p:1};
I.u[1780] = {n:"Bar Rikain vidé",nn:"BAR RIKAIN VIDE",t:62,d:"Ce bar rikain peut maintenant être utilisé comme ingrédient.",ep:2,g:291,l:40,wd:true,fm:true,w:2,p:1};
I.u[1781] = {n:"Greu-vette vidée",nn:"GREU-VETTE VIDEE",t:62,d:"Cette greu-vette peut maintenant être utilisée comme ingrédient.",ep:1,g:293,l:1,wd:true,fm:true,w:1,p:1};
I.u[1782] = {n:"Goujon",nn:"GOUJON",t:41,d:"Petit poisson rapide d\'eau douce, le goujon est le plus souvent pêché par les enfants et par les pêcheurs débutants. Mais il ne nourrira pas votre famille.",ep:1,g:294,l:1,wd:true,fm:true,w:3,p:1};
I.u[1783] = {n:"Goujon vidé",nn:"GOUJON VIDE",t:62,d:"Ce goujon peut maintenant être utilisé comme ingrédient.",ep:1,g:295,l:1,wd:true,fm:true,w:2,p:1};
I.u[1784] = {n:"Raie Bleue",nn:"RAIE BLEUE",t:41,d:"La Raie Bleue domine les sols marins où elle passe son temps à creuser de minuscules trous. Nul ne sait encore pourquoi. Elle remonte à la surface une fois de temps en temps. Juste assez souvent pour la prendre dans vos filets.",ep:2,g:296,l:50,wd:true,fm:true,w:3,p:1};
I.u[1785] = {n:"Raie bleue vidée",nn:"RAIE BLEUE VIDEE",t:62,d:"Cette raie bleue peut maintenant être utilisée comme ingrédient.",ep:2,g:297,l:50,wd:true,fm:true,w:2,p:1};
I.u[1786] = {n:"Greu-vette Horreur",nn:"GREU-VETTE HORREUR",t:41,d:"Mutation de la greu-vette, cette espèce très rare a la particularité d\'émettre d\'horribles sons lorsqu\'elle ondule son corps.",ep:1,g:298,l:1,wd:true,fm:true,w:3,p:100};
I.u[1787] = {n:"Greu-vette Horreur vidé",nn:"GREU-VETTE HORREUR VIDE",t:62,d:"Cette greu-vette horreur peut maintenant être utilisée comme ingrédient.",ep:1,g:299,l:1,wd:true,fm:true,w:2,p:100};
I.u[1788] = {n:"Raie de Farle",nn:"RAIE DE FARLE",t:41,d:"Cette espèce de raie a été découverte la première fois par Farle Ingalsse en personne lors d\'une de ses grandes aventures. Cette raie aurait la capacité d\'envoûter tous les poissons des profondeurs par son cri grave unique.",ep:1,g:300,l:50,wd:true,fm:true,w:3,p:20};
I.u[1789] = {n:"Raie de Farle vidée",nn:"RAIE DE FARLE VIDEE",t:62,d:"Cette raie de Farle peut maintenant être utilisée comme ingrédient.",ep:1,g:301,l:50,wd:true,fm:true,w:2,p:20};
I.u[1790] = {n:"Goujon Kiye",nn:"GOUJON KIYE",t:41,d:"Ce goujon rare vient tout droit de l\'île de Kiye, située au large de l\'île de Moon. Habitué au courant très chaud, il effectue cependant une migration une fois par an.",ep:1,g:302,l:1,wd:true,fm:true,w:3,p:1};
