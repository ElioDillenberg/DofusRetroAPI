I.u[1360] = {n:"L\'Hyldegarde",nn:"L\'HYLDEGARDE",t:3,d:"Cette baguette fut créée il y a de nombreuses années dans les flammes de la forge de Mort Dur. Cette arme nécessite d\'avoir toujours de l\'eau à côté de soi pour pouvoir maitriser son feu intérieur...",ep:13,g:8,l:48,wd:true,fm:true,w:10,an:11,e:[5,4,2,4,50,50,false,true],p:4800};
I.u[1361] = {n:"Baguette Sylvien",nn:"BAGUETTE SYLVIEN",t:3,d:"La Baguette Sylvien a l\'étrange pouvoir de conférer une vue perçante à son porteur. Charge ensuite à celui ci d\'être suffisamment agile pour pouvoir fuir le danger.",ep:1,g:15,l:43,wd:true,fm:true,w:10,an:11,e:[4,5,2,2,40,40,false,true],c:"CA>80",p:4000};
I.u[1362] = {n:"Pieu Vampirique",nn:"PIEU VAMPIRIQUE",t:3,d:"Le pieu vampirique est en fait la baguette d\'un vampire depuis longtemps retiré de la vie sociale et de la boucherie. Cet étrange bout de bois a cependant gardé quelques uns des avantages de son ancien maître...",ep:7,g:14,l:65,wd:true,fm:true,w:10,an:11,e:[5,6,2,3,50,20,false,true],c:"CA>30&CW<60",p:4000};
I.u[1363] = {n:"Bâton Tont\'Ata",nn:"BATON TONT\'ATA",t:4,d:"Outre son nom insupportable, ce bâton possède bien d\'autres moyens de rendre son propriétaire fou de rage.",ep:1,g:5,l:40,wd:true,fm:true,w:10,an:12,e:[5,5,1,1,5,5,false,true],c:"CI<100",p:5000};
I.u[1364] = {n:"Bâton d\'Oubli",nn:"BATON D\'OUBLI",t:4,d:"Ce bâton fait oublier tous les soucis et les mauvaises pensées de son propriétaire. Malheureusement, il perd aussi parfois une partie de sa mémoire et devient sujet à des pertes de concentration de plus en plus fréquentes. A réserver aux sages...",ep:13,g:11,l:30,wd:true,fm:true,w:15,an:12,e:[5,3,1,1,50,50,false,true],c:"CW>50",p:3000};
I.u[1365] = {n:"La Canne Hête",nn:"LA CANNE HETE",t:4,d:"Hète était vraiment un guerrier redoutable avec son bâton. Il était tellement efficace qu\'on prétendait que son bâton était magique et qu\'il lançait des flammes...",ep:1,g:68,l:20,wd:true,fm:true,w:15,an:12,e:[8,5,1,1,50,40,false,true],p:2000};
I.u[1366] = {n:"Bâton de Bouftier",nn:"BATON DE BOUFTIER",t:4,d:"Bâton couramment utilisé lors de la transhumance par les bouftiers. Les bworks aiment déguster ces troupeaux de bouftous en fines tranches lors de l\'apéritif (le plat principal étant constitué du bouftier lui-même).",ep:9,g:9,l:12,wd:true,fm:true,w:10,an:12,e:[5,4,1,1,50,50,false,true],p:1200};
I.u[1367] = {n:"Bâton de Dina, dit \'le Mythe\'",nn:"BATON DE DINA, DIT \'LE MYTHE\'",t:4,d:"Ce bâton a appartenu à la très belle Dina \'Mythe\', qui était surtout connue pour ses formes avantageuses et son caractère explosif... Bref c\'était une vraie bombe.",ep:13,g:6,l:40,wd:true,fm:true,w:15,an:12,e:[5,5,1,1,30,50,false,true],c:"CI>50&CA>40",p:4000};
I.u[1368] = {n:"La Lance à un " + "\"" + "110" + "\"" + "",nn:"LA LANCE A UN " + "\"" + "110" + "\"" + "",t:4,d:"" + "\"" + "Les 110" + "\"" + " est le surnom donné aux 110 valeureux pompiers Amakniens, qui ont notamment combattu le feu d\'un Dragonnet en mal de sensations fortes. Cette lance aurait appartenu à l\'un d\'eux. S\'en servir demande courage et force.",ep:1,g:18,l:29,wd:true,fm:true,w:15,an:12,e:[5,4,1,1,40,50,false,true],c:"CC>10",p:2500};
I.u[1369] = {n:"Les Lames en Table",nn:"LES LAMES EN TABLE",t:5,d:"Ces couteaux étaient à l\'origine censés servir dans un restaurant de luxe. Mais devant leur look plutôt... étrange, le patron du restaurant a préféré les revendre à un lanceur de couteaux ambulant.",ep:1,g:13,l:33,wd:true,fm:true,w:10,an:13,e:[10,3,1,1,30,50,false,true],c:"CA>55&CC>5",p:3000};
I.u[1370] = {n:"L\'Ergot Mina",nn:"L\'ERGOT MINA",t:5,d:"La poignée de cette arme est grasse et glissante, mais elle a vraiment un look génial.",ep:1,g:14,l:20,wd:true,fm:true,w:10,an:13,e:[8,3,1,1,50,50,false,true],p:2000};
I.u[1371] = {n:"Dagues Eulasse",nn:"DAGUES EULASSE",t:5,d:"Eulasse était un bouftier du nord d\'Amakna qui trouva un jour, au milieu du fumier de ses bouftous, une paire de dagues légèrement salies. \nQue celui qui les porte fasse attention : le manche glisse, il est facile de manquer son coup.",ep:1,g:10,l:40,wd:true,fm:true,w:20,an:13,e:[10,3,1,1,30,50,false,true],c:"CA>40",p:4000};
I.u[1372] = {n:"Dagues Aj\'Deh\'Là",nn:"DAGUES AJ\'DEH\'LA",t:5,d:"Qui ne connait pas Aj\'Deh\'Là, l\'ermite, légendaire disciple de Sram qui avait passé sa vie à refuser tout contact avec les autres. À sa mort, on retrouva ses dagues fétiches à côté de son corps, et seuls les plus agiles purent les apprivoiser.",ep:9,g:19,l:44,wd:true,fm:true,w:15,an:13,e:[10,3,1,1,30,50,false,false],c:"CA>60",p:5300};
I.u[1373] = {n:"Le Rasoir Infernal",nn:"LE RASOIR INFERNAL",t:5,d:"Krys Razepa était un vieux disciple d\'Enutrof qui portait la barbe, comme tous les disciples d\'Enutrof dignes de ce nom. Lorsque madame Razepa, sa femme, lui offrit un rasoir, il préféra l\'utiliser pour combattre...",ep:1,g:17,l:30,wd:true,fm:true,w:15,an:13,e:[10,3,1,1,30,50,false,false],c:"CA>30",p:3000};
I.u[1374] = {n:"Dagues Tylo",nn:"DAGUES TYLO",t:5,d:"Tylo était un scribe amaknéen qui vécut il y a des siècles de cela. Il avait la particularité de graver ses cahiers de pierre non pas avec des burins mais avec des dagues. Nous reste de lui cette superbe dague, impressionnante de résistance aux chocs.",ep:1,g:12,l:30,wd:true,fm:true,w:13,an:13,e:[10,3,1,1,30,50,false,false],c:"CS>50",p:3000};
I.u[1375] = {n:"Hache Térophyle",nn:"HACHE TEROPHYLE",t:19,d:"Térophyle était un disciple de Iop capable de soulever des masses impressionnantes. Mais malheureusement pour lui, comme tous les Iop trop musclés, l\'intelligence n\'était pas son fort...",ep:1,g:14,l:15,wd:true,fm:true,w:25,an:17,e:[10,5,1,1,50,50,false,true],c:"CS>70&CI<10",p:800};
I.u[1376] = {n:"Hache Heuvante",nn:"HACHE HEUVANTE",t:19,d:"Cette hache, parfois lourde à manier, est parfaite pour terrasser les Trools : les runes magiques qu\'elle contient permettent en effet de brûler les ennemis.",ep:7,g:15,l:35,wd:true,fm:true,w:23,an:17,e:[10,5,1,1,40,50,false,true],c:"CS>50",p:3000};
I.u[1377] = {n:"Hache Huré",nn:"HACHE HURE",t:19,d:"Huré était un jeune prêtre qui ne reculait pas devant les armes pour faire progresser la foi. Son arme de prédilection était une hache sur laquelle il avait dessiné de nombreux traits, dont la signification est perdue à jamais...",ep:1,g:16,l:22,wd:true,fm:true,w:22,an:17,e:[10,5,1,1,50,50,false,true],c:"CW>30",p:2200};
I.u[1378] = {n:"Francisque à Brêles",nn:"FRANCISQUE A BRELES",t:19,d:"Cette hache de combat, réservée aux jeunes guerriers, comme son nom l\'indique, a la particularité de produire un son strident particulièrement désagréable lorsqu\'elle entrechoque une autre arme.",ep:1,g:17,l:28,wd:true,fm:true,w:22,an:17,e:[8,5,1,1,40,40,false,true],c:"CA>30",p:2300};
I.u[1379] = {n:"Le Scrounnchhh",nn:"LE SCROUNNCHHH",t:7,d:"D\'après la légende, ce marteau tirerait son nom du son qu\'il produit lorsqu\'il s\'écrase sur un crâne ennemi. Malheureusement, nul ennemi n\'est là pour confirmer ces dires.",ep:7,g:9,l:35,wd:true,fm:true,w:35,an:15,e:[8,5,1,1,30,40,false,true],c:"CS>50&CA>20",p:35000};
