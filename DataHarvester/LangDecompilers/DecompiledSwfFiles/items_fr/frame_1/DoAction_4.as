I.u[1459] = {n:"Tube longiforme",nn:"TUBE LONGIFORME",t:71,d:"Ce tube est spécialement étudié pour pouvoir être débouché d\'une seule main.",ep:1,g:55,l:1,wd:true,fm:true,w:1,p:5};
I.u[1460] = {n:"Grande fiole",nn:"GRANDE FIOLE",t:71,d:"Cette fiole est spécialement étudiée pour pouvoir être débouchée simplement même sans les mains.",ep:3,g:56,l:1,wd:true,fm:true,w:1,p:10};
I.u[1461] = {n:"Poison cinglant",nn:"POISON CINGLANT",t:15,d:"Ce poison à base de pattes d\'araknes vous fera perdre la tête. Au propre comme au figuré puisque qu\'après une période de folie la boîte crânienne explose sous la pression sanguine.",ep:1,g:189,l:20,wd:true,fm:true,w:1,p:100};
I.u[1462] = {n:"Liquide vampiresque",nn:"LIQUIDE VAMPIRESQUE",t:15,d:"Ce liquide fait d\'un croisement de plusieurs sangs a la mauvaise habitude de sauter au cou.",ep:2,g:188,l:30,wd:true,fm:true,w:1,p:100};
I.u[1463] = {n:"Fiole à élément",nn:"FIOLE A ELEMENT",t:71,d:"Cette fiole est tout naturellement destinée à emprisonner de l\'essence élémentaire.",ep:1,g:57,l:1,wd:true,fm:true,w:1,p:50};
I.u[1464] = {n:"Antenne du Scarafeuille bleu",nn:"ANTENNE DU SCARAFEUILLE BLEU",t:15,d:"Nul ne sait à quoi servent les antennes d\'un Scarafeuille. La seule certitude que l\'on ait c\'est qu\'elles ne sentent pas très bon.",ep:3,g:216,l:32,wd:true,fm:true,w:1,p:100};
