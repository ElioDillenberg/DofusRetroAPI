I.u[1764] = {n:"Bâton de crabe **",nn:"BATON DE CRABE **",t:49,d:"À déguster partout avec les doigts. Mais gare à vous si vous en abusez.",ep:2,g:74,l:1,wd:true,fm:true,w:3,u:true,ut:true,p:30};
I.u[1765] = {n:"Bâton de crabe ***",nn:"BATON DE CRABE ***",t:49,d:"À déguster lentement, très lentement, avec une petite sauce pour relever le tout.",ep:2,g:75,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:60};
I.u[1766] = {n:"Bâton de crabe ****",nn:"BATON DE CRABE ****",t:49,d:"Produit à partir de la chair de crabe sourimi exotique ce bâton de crabe a un goût tout simplement horrible. Par contre, il est excessivement nourrissant.",ep:1,g:76,l:1,wd:true,fm:true,w:1,c:"Ci<30",u:true,ut:true,p:30000};
I.u[1767] = {n:"Poisson-Chaton fumé **",nn:"POISSON-CHATON FUME **",t:49,d:"Délicatement fumé, ce poisson-chaton est un merveilleux repas. Mais attention, son fumet reste agrippé à vos vêtements pendant plusieurs semaines.",ep:1,g:78,l:1,wd:true,fm:true,w:3,u:true,ut:true,p:60};
