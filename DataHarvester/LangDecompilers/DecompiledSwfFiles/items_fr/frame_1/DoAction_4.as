I.u[1769] = {n:"Poisson-Tigre fumé ****",nn:"POISSON-TIGRE FUME ****",t:49,d:"Un peu plus ferme que le poisson-chaton fumé, et surtout beaucoup plus raffiné. Ce poisson-tigre fumé ravira les amateurs de grands mets.",ep:1,g:77,l:1,wd:true,fm:true,w:1,c:"Cw<40",u:true,ut:true,p:40000};
I.u[1770] = {n:"Bout de Blop Coco",nn:"BOUT DE BLOP COCO",t:15,d:"Cet extrait de blop sent à plein nez la coco. Comment un résidu de monstres peut-il sentir autant ?",ep:5,g:281,l:34,wd:true,fm:true,w:1,p:1};
I.u[1771] = {n:"Souris verte",nn:"SOURIS VERTE",t:15,d:"Une souris verte qui courait dans les plaines... n\'essayez pas de l\'attraper par la queue.",ep:13,g:282,l:1,wd:true,fm:true,w:2,p:1};
