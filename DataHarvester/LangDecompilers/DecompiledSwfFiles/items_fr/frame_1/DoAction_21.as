I.u[9060] = {n:"Morceau de papier déchiré",nn:"MORCEAU DE PAPIER DECHIRE",t:24,d:"C\'est une partie de lettre que quelqu\'un a déchirée. On peut encore y lire : \'... Comme convenu, je m\'arrangerai pour que l\'enquête sur ce marchand n\'aboutisse pas. Venez me verser la somme prévue à l\'endroit habituel où les femmes sont de charmante compagnie.\' \nLa lettre est signée X.",ep:16,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9061] = {n:"Exoram",nn:"EXORAM",t:32,d:"Exoram doit être escorté hors du camp gobelin et jusqu\'au temple Sram.",ep:1,g:9,l:1,wd:false,fm:true,w:0,m:true,p:1};
I.u[9062] = {n:"Document sur Xephirès",nn:"DOCUMENT SUR XEPHIRES",t:24,d:"Ce document contient de nombreuses informations intéressantes sur l\'individu nommé Xephirès. Notamment, on y apprend qu\'il passe beaucoup de temps à la taverne du Pandawa Ivre.",ep:2,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9063] = {n:"Liste de Milone",nn:"LISTE DE MILONE",t:24,d:"Cette liste absolument impressionnante vous a été remise par Milone le bijoutier. Elle décrit les objets nécessaires pour confectionner un somptueux joyaux.",ep:16,g:176,l:1,wd:false,fm:true,w:0,p:1};
I.u[9064] = {n:"Bave du Bouftou Royal",nn:"BAVE DU BOUFTOU ROYAL",t:24,d:"Est-elle vraiment différente d\'une bave ordinaire ? En tout cas, ça n\'en a pas l\'air.",ep:1,g:32,l:1,wd:false,fm:true,w:0,p:1};
I.u[9065] = {n:"Lettre pour Milone",nn:"LETTRE POUR MILONE",t:24,d:"Cette lettre, écrite à la hâte, explique à Milone que le bijou est en danger. D\'ailleurs, pourquoi perdez-vous du temps à la lire au lieu de vous précipiter chez lui ?",ep:16,g:176,l:1,wd:false,fm:true,w:0,p:1};
I.u[9066] = {n:"Lettre du chef des voleurs",nn:"LETTRE DU CHEF DES VOLEURS",t:24,d:"Retrouvée sur un des voleurs que vous avez vaincus chez Milone : \nMilone Corichel le bijoutier vient de créer un joyau de grande valeur. Dérobez-le par tous les moyens possibles. \nVisiblement, les voleurs ont échoué et en plus au dos du message se trouve l\'emplacement de leur repaire. Une petite visite de courtoisie s\'impose, mais mieux vaut s\'y rendre accompagné.",ep:16,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9067] = {n:"Lettre pour Xephirès",nn:"LETTRE POUR XEPHIRES",t:24,d:"Cette lettre d\'Oto Mustam explique en détails le plan pour Xephirès.",ep:1,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9068] = {n:"Joyau de Milone",nn:"JOYAU DE MILONE",t:24,d:"Ce joyau est le résultat de vos efforts et de ceux de Milone le bijoutier. Il vaut assurément une petite fortune... pour ceux que ça peut intéresser.",ep:16,g:717,l:1,wd:false,fm:true,w:0,p:1};
I.u[9069] = {n:"Nouvelle lettre pour Xephirès",nn:"NOUVELLE LETTRE POUR XEPHIRES",t:24,d:"Oto y décrit ses conditions pour la corruption de Xephirès.",ep:1,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9070] = {n:"Epée étrange",nn:"EPEE ETRANGE",t:24,d:"Cette épée est vraiment inhabituelle. Vous n\'en avez jamais vu de telle dans Bonta.",ep:1,g:718,l:1,wd:false,fm:true,w:0,p:1};
I.u[9071] = {n:"Notes du tavernier Feubuk",nn:"NOTES DU TAVERNIER FEUBUK",t:24,d:"Le tavernier Feubuk a reçu des Ecaflips louches qui ont prononcé le nom de Loudac.",ep:16,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9072] = {n:"Notes du tavernier Pinchaut",nn:"NOTES DU TAVERNIER PINCHAUT",t:24,d:"Le tavernier Pinchaut a entendu des gardes parler de leurs problèmes avec une bande d\'Ecaflips.",ep:16,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9073] = {n:"Insigne de Bandit Ecaflip",nn:"INSIGNE DE BANDIT ECAFLIP",t:24,d:"Vous avez récupéré cet insigne sur les bandits Ecaflips des plaines de Cania.",ep:16,g:49,l:1,wd:false,fm:true,w:0,p:1};
I.u[9074] = {n:"Bijou d\'Amayiro",nn:"BIJOU D\'AMAYIRO",t:24,d:"Ce bijou servira à appâter le chef des bandits afin de le capturer.",ep:1,g:111,l:1,wd:false,fm:true,w:0,p:1};
I.u[9075] = {n:"Lettre du Xélor",nn:"LETTRE DU XELOR",t:24,d:"Cette lettre vous a été remise par l\'intermédiaire de Marge la bijoutière : \nAmenez le bijou à Ekusoyk Ubnan à la Taverne de la Chopenbois. Ce sera plus rapide pour nous et beaucoup plus lucratif pour vous.",ep:1,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9076] = {n:"Bijou d\'Amayiro (faux)",nn:"BIJOU D\'AMAYIRO (FAUX)",t:24,d:"Ce bijou ressemble à s\'y méprendre à celui qu\'Amayiro vous a envoyé chercher. D\'ailleurs, il vaudrait mieux qu\'il s\'y méprenne, sinon...",ep:1,g:110,l:1,wd:false,fm:true,w:0,p:1};
I.u[9077] = {n:"Crâne de Feca",nn:"CRANE DE FECA",t:47,d:"Le crâne de Feca fait un excellent presse-livres.",ep:1,g:34,l:1,wd:true,fm:false,w:1,p:1};
I.u[9078] = {n:"Crâne d\'Osamodas",nn:"CRANE D\'OSAMODAS",t:47,d:"Un Tofu par-ci, un Bwork mage par-là : ce disciple d\'Osamodas en a perdu la tête.",ep:1,g:33,l:1,wd:true,fm:false,w:2,p:1};
I.u[9079] = {n:"Crâne d\'Enutrof",nn:"CRANE D\'ENUTROF",t:47,d:"Ce trophée était déjà vieux avant de devenir un trophée.",ep:1,g:32,l:1,wd:true,fm:false,w:2,p:1};
I.u[9080] = {n:"Crâne de Sram",nn:"CRANE DE SRAM",t:47,d:"Ce trophée était déjà morbide avant de devenir un trophée.",ep:1,g:31,l:1,wd:true,fm:false,w:2,p:1};
I.u[9081] = {n:"Crâne de Xélor",nn:"CRANE DE XELOR",t:47,d:"Ce crâne est souvent confondu avec celui de la célèbre momie de Tut Enkarthon. Ne vous y trompez pas : ces bandelettes-ci ne sont pas en carton, mais en papier mâché.",ep:1,g:30,l:1,wd:true,fm:false,w:2,p:1};
I.u[9082] = {n:"Crâne d\'Ecaflip",nn:"CRANE D\'ECAFLIP",t:47,d:"À défaut de jouer à pile ou face, on pourra jouer aux osselets.",ep:1,g:29,l:1,wd:true,fm:false,w:2,p:1};
I.u[9083] = {n:"Crâne d\'Eniripsa",nn:"CRANE D\'ENIRIPSA",t:47,d:"Ne soyez pas mauvaise langue : Non, Il ne s\'agit pas d\'un crâne de Piaf !",ep:1,g:28,l:1,wd:true,fm:false,w:2,p:1};
I.u[9084] = {n:"Crâne de Iop",nn:"CRANE DE IOP",t:47,d:"Ce trophée sonne aussi creux que lorsqu\'il était plein.",ep:1,g:27,l:1,wd:true,fm:false,w:1,p:1};
I.u[9085] = {n:"Crâne de Crâ",nn:"CRANE DE CRA",t:47,d:"On pourrait le croire sorti d\'une fouille archéologique, ce crâne de Crâ mignon !",ep:1,g:26,l:1,wd:true,fm:false,w:2,p:1};
I.u[9086] = {n:"Crâne de Sadida",nn:"CRANE DE SADIDA",t:47,d:"Qui eût cru qu\'il y eut un crâne sous la touffe de cheveux ?",ep:1,g:25,l:1,wd:true,fm:false,w:2,p:1};
I.u[9087] = {n:"Crâne de Sacrieur",nn:"CRANE DE SACRIEUR",t:47,d:"Ce sacrieur fait moins le fier sans ses milliers de points de vie.",ep:1,g:24,l:1,wd:true,fm:false,w:2,p:1};
I.u[9088] = {n:"Crâne de Pandawa",nn:"CRANE DE PANDAWA",t:47,d:"On aurait pu croire qu\'après la mise en bière le Pandawa aurait une gueule de bois.",ep:1,g:23,l:1,wd:true,fm:false,w:2,p:1};
I.u[9089] = {n:"Insigne de la patrouille de Bonta",nn:"INSIGNE DE LA PATROUILLE DE BONTA",t:24,d:"Vous avez récupéré cet insigne lors de la (très facile) bataille d\'escarmouche contre Bonta.",ep:16,g:49,l:1,wd:false,fm:true,w:0,p:1};
I.u[9090] = {n:"Insigne de la patrouille de Brâkmar",nn:"INSIGNE DE LA PATROUILLE DE BRAKMAR",t:24,d:"Vous avez récupéré cet insigne à grand peine lors de la bataille d\'escarmouche contre Brâkmar.",ep:16,g:76,l:1,wd:false,fm:true,w:0,p:1};
I.u[9094] = {n:"Poison Bwork",nn:"POISON BWORK",t:24,d:"Ca ressemble davantage à de la gelée de menthe qu\'à du poison. Les Bworks ne sont assurément pas très doués pour ça : ce qui est plus inquiétant, c\'est que les espions de Brâkmar non plus, apparemment.",ep:4,g:32,l:1,wd:false,fm:true,w:0,p:1};
I.u[9096] = {n:"Tacticien",nn:"TACTICIEN",t:32,d:"Le tacticien a accepté de vous suivre jusqu\'à votre chef.",ep:16,g:22,l:1,wd:false,fm:true,w:0,m:true,p:1};
I.u[9097] = {n:"Badge du puissant milicien",nn:"BADGE DU PUISSANT MILICIEN",t:24,d:"Vous avez trouvé ce badge sur le milicien particulièrement redoutable qui a vaincu plusieurs guerriers de Brâkmar. Son équipement semblait très particulier.",ep:16,g:49,l:1,wd:false,fm:true,w:0,p:1};
I.u[9098] = {n:"Marteau de Nara",nn:"MARTEAU DE NARA",t:24,d:"Ce marteau indiquera à Oto que vous avez vaincu le forgeron mercenaire de Bonta.",ep:16,g:8,l:1,wd:false,fm:true,w:0,p:1};
I.u[9099] = {n:"Demande d\'aide d\'Amayiro",nn:"DEMANDE D\'AIDE D\'AMAYIRO",t:24,d:"Cette lettre, signée par Amayiro, est une demande d\'aide officielle.",ep:3,g:26,l:1,wd:false,fm:true,w:0,p:1};
I.u[9100] = {n:"Caleçon de Lenglad",nn:"CALECON DE LENGLAD",t:24,d:"Là, c\'est sûr : Lenglad ne fera plus parler de lui avant un moment.",ep:1,g:84,l:1,wd:false,fm:true,w:0,p:1};
I.u[9101] = {n:"Caisse d\'outils de Nara",nn:"CAISSE D\'OUTILS DE NARA",t:24,d:"Cette caisse contient les outils que Nara le forgeron a oubliés.",ep:3,g:44,l:1,wd:false,fm:true,w:0,p:1};
I.u[9103] = {n:"Plan d\'attaque",nn:"PLAN D\'ATTAQUE",t:24,d:"Cette lettre, datée d\'il y a une semaine, explique en détails le plan d\'attaque contre Bonta. Il faut toutefois se méfier, car une note en fin de page indique que ce plan n\'est pas encore confirmé et sujet à modification dans les prochains jours par l\'officier Kapi Taigne. Par conséquent, vous devez découvrir ce qu\'il en est à l\'heure actuelle.",ep:16,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9104] = {n:"Insigne de la patrouille du Cimetière",nn:"INSIGNE DE LA PATROUILLE DU CIMETIERE",t:24,d:"Vous avez récupéré cet insigne sur la patrouille brâkmarienne que vous avez attaquée au Cimetière des Torturés.",ep:16,g:76,l:1,wd:false,fm:true,w:0,p:1};
I.u[9105] = {n:"Lettre de Danathor",nn:"LETTRE DE DANATHOR",t:24,d:"Quelqu\'un a glissé cette lettre dans votre sac pendant qu\'Amayiro vous donnait la mission : \nRécupérez les documents, mais apportez-les moi plutôt qu\'à Amayiro. Vous serez récompensé. Danathor",ep:16,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9106] = {n:"Documents secrets de Bonta",nn:"DOCUMENTS SECRETS DE BONTA",t:24,d:"Ces documents contiennent un bon nombre d\'informations concernant les défenses et le système de Bonta. Mais à qui allez-vous les donner ?",ep:16,g:26,l:1,wd:false,fm:true,w:0,p:1};
I.u[9107] = {n:"Consignes pour l\'espion de Bonta",nn:"CONSIGNES POUR L\'ESPION DE BONTA",t:24,d:"Pénètre dans la tour de Brâkmar à minuit et récupère tous les documents que tu trouveras au deuxième étage. Reviens ensuite à Bonta en passant par l\'Est de la route. \nNon, ce n\'est pas VOUS qui devez le faire, c\'était la consigne pour l\'espion que vous devez sauver.",ep:16,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9108] = {n:"Documents brâkmariens secrets",nn:"DOCUMENTS BRAKMARIENS SECRETS",t:24,d:"Ces documents contiennent de nombreuses informations sur la Corne Edfrit.",ep:14,g:26,l:1,wd:false,fm:true,w:0,p:1};
I.u[9109] = {n:"Bandana Troué",nn:"BANDANA TROUE",t:24,d:"Ce bandana en mauvais état est couvert de glaire, comme s\'il avait servi de mouchoir à plusieurs générations de Wabbits. Il a sans nul doute appartenu à Turorte, en témoigne son odeur de hareng pourri subtilement camouflée par une odeur de cawotte. Pour sentir aussi mauvais après toutes ces années, il est indubitablement imprégné d\'une puissante magie.",ep:8,g:748,l:1,wd:false,fm:true,w:0,p:1};
I.u[9110] = {n:"Corne Edfrit",nn:"CORNE EDFRIT",t:24,d:"Vous avez déjà essayé de souffler dedans, mais il ne se passe rien de spécial...",ep:16,g:720,l:1,wd:false,fm:true,w:0,p:1};
I.u[9112] = {n:"Consigne pour l\'espion brâkmarien",nn:"CONSIGNE POUR L\'ESPION BRAKMARIEN",t:24,d:"Pénètre dans la tour de Bonta à minuit, égorge tous ceux qui sont sur ton chemin, et récupère tous les documents que tu trouveras au premier étage. Reviens ensuite à Brâkmar passant par la route. \nNon, ce n\'est pas VOUS qui devez le faire, c\'était la consigne pour l\'espion que vous devez retrouver.",ep:16,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9113] = {n:"Documents secrets volés",nn:"DOCUMENTS SECRETS VOLES",t:24,d:"Les documents que l\'espion blessé a récupéré dans la tour de Bonta.",ep:13,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9114] = {n:"Documents secrets froissés",nn:"DOCUMENTS SECRETS FROISSES",t:24,d:"Les documents que l\'espion blessé a récupéré dans la tour de Bonta. Ils ont été un peu abimés pendant votre combat contre les forces de Bonta.",ep:16,g:55,l:1,wd:false,fm:true,w:0,p:1};
I.u[9115] = {n:"Salace fourrée au pollen",nn:"SALACE FOURREE AU POLLEN",t:24,d:"Cette salace a été préparée dans le chaudron primordial ce qui lui confère des vertus particulières. Pour savoir lesquelles, il faudrait pouvoir y goûter, malheureusement c\'est à Turorte qu\'elle est destinée. La vie est parfois injuste.",ep:12,g:719,l:1,wd:false,fm:true,w:0,p:1};
I.u[9116] = {n:"Caisse d\'équipement",nn:"CAISSE D\'EQUIPEMENT",t:24,d:"Une arme, un casque, un bouclier : cette caisse contient un équipement rudimentaire mais efficace pour les guerriers.",ep:16,g:44,l:1,wd:false,fm:true,w:0,p:1};
I.u[9117] = {n:"Ougarteau",nn:"OUGARTEAU",t:7,d:"Ougah a la tête dure, plus dure que n\'importe qui, c\'est pourquoi un marteau créé avec son crâne est idéal pour fracasser celui des autres.",ep:23,g:67,l:180,wd:true,fm:true,w:15,an:15,e:[10,4,1,1,50,50,false,false],c:"CV>1200",s:118,p:50000};
I.u[9118] = {n:"Cadeau Event Shika",nn:"CADEAU EVENT SHIKA",t:89,d:"(cadeau offert aux joueurs de Shika pour l\'event, ne pas traduire)",ep:0,g:42,l:1,wd:false,fm:true,w:0,u:true,p:0};
I.u[9119] = {n:"Cadeau Shika",nn:"CADEAU SHIKA",t:89,d:"(cadeau pour les joueurs abonnés de Shika, ne pas traduire)",ep:0,g:42,l:1,wd:false,fm:true,w:0,u:true,p:0};
I.u[9120] = {n:"Chapeau Shika",nn:"CHAPEAU SHIKA",t:16,d:"(Chapeau pour les abonnés du serveur Shika, ne pas traduire)",ep:0,g:210,l:30,wd:false,fm:true,w:10,p:3000};
I.u[9121] = {n:"Cape Shika",nn:"CAPE SHIKA",t:17,d:"(cape destinée aux joueurs abonnés du serveur Shika, ne pas traduire)",ep:0,g:148,l:30,wd:false,fm:true,w:10,p:3000};
I.u[9122] = {n:"Blopanneau Griotte",nn:"BLOPANNEAU GRIOTTE",t:9,d:"Une fois enfilé au bout d\'un doigt, il n\'est plus possible de se débarrasser de cet anneau. Plus vous y pensez, et plus il vous rappelle quelqu\'un.",ep:5,g:149,l:50,wd:true,fm:true,w:10,s:98,p:4000};
