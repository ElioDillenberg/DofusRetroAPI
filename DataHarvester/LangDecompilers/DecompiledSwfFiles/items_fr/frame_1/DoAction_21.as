I.u[9366] = {n:"Tutu Rose",nn:"TUTU ROSE",t:10,d:"Ce tutu rose sera parfait pour la représentation de fin d\'année de votre petite dernière. Fabriqué en coton ancestral et en mousse de Mob l\'éponge extensible, doux et confortable avec une jupe en froufrou, ce tutu sera peut-être le premier d\'une future danseuse étoile.",ep:22,g:148,l:49,wd:true,fm:true,w:5,p:300};
I.u[9379] = {n:"Ballotin de chocolat",nn:"BALLOTIN DE CHOCOLAT",t:42,d:"Ce petit paquet est à offrir à celle ou celui que vous aimez. Si vous n\'aimez personne, vous êtes autorisé à le manger.",ep:1,g:3,l:1,wd:false,fm:true,w:1,u:true,ut:true,p:1};
I.u[9380] = {n:"Fée d\'artifice invocatrice de coeurs",nn:"FEE D\'ARTIFICE INVOCATRICE DE COEURS",t:74,d:"Cette Fée d\'artifice originale vous permettra de répandre un peu d\'amour dans ce monde de brutes, et un peu de rut dans ce monde d\'amour.",ep:1,g:32,l:1,wd:false,fm:true,w:1,ut:true,p:1};
I.u[9381] = {n:"Fleur de Gloutoblop",nn:"FLEUR DE GLOUTOBLOP",t:35,d:"Le Gloutoblop est tellement affamé, qu\'il serait capable de manger ses propres fleurs. Heureusement, il trouve toujours un aventuriers imprudent à portée de dents.",ep:5,g:7,l:50,wd:true,fm:true,w:1,p:1};
I.u[9382] = {n:"Fleur de Blopignon",nn:"FLEUR DE BLOPIGNON",t:35,d:"Cette fleur vient d\'être arrachée de la tête d\'un Blopignon. Etant donné l\'allure ridicule qu\'elle lui donnait, il ne va pas s\'en plaindre.",ep:5,g:8,l:40,wd:true,fm:true,w:1,p:1};
I.u[9383] = {n:"Racine de Tronkoblop",nn:"RACINE DE TRONKOBLOP",t:98,d:"Le Tronkoblop possède de longues racines, qui vont puiser dans le sol tout ce que les aventuriers peuvent y rejeter.",ep:5,g:1,l:40,wd:true,fm:true,w:1,p:1};
I.u[9384] = {n:"Fleur de Blop Griotte Royal",nn:"FLEUR DE BLOP GRIOTTE ROYAL",t:35,d:"Si cette fleur n\'était pas royale, vous l\'auriez sûrement jetée.",ep:5,g:12,l:100,wd:true,fm:true,w:1,p:1};
I.u[9385] = {n:"Fleur de Blop Coco Royal",nn:"FLEUR DE BLOP COCO ROYAL",t:35,d:"Si cette fleur n\'était pas royale, vous l\'auriez sûrement jetée.",ep:5,g:9,l:100,wd:true,fm:true,w:1,p:1};
I.u[9386] = {n:"Fleur de Blop Indigo Royal",nn:"FLEUR DE BLOP INDIGO ROYAL",t:35,d:"Si cette fleur n\'était pas royale, vous l\'auriez sûrement jetée.",ep:5,g:11,l:100,wd:true,fm:true,w:1,p:1};
I.u[9387] = {n:"Fleur de Blop Reinette Royal",nn:"FLEUR DE BLOP REINETTE ROYAL",t:35,d:"Si cette fleur n\'était pas royale, vous l\'auriez sûrement jetée.",ep:5,g:10,l:100,wd:true,fm:true,w:1,p:1};
I.u[9388] = {n:"Feuille de Blop Royal",nn:"FEUILLE DE BLOP ROYAL",t:36,d:"Une fois séchée, roulée, puis fumée, cette feuille peut déclencher chez les plus faibles, d\'importantes hallucinations, et une incroyable envie de se soulager. Une consommation excessive entraîne une mort subite.",ep:5,g:13,l:100,wd:true,fm:true,w:1,p:1};
I.u[9389] = {n:"Feuille de Blop Multicolore Royal",nn:"FEUILLE DE BLOP MULTICOLORE ROYAL",t:36,d:"Cette feuille se nourrit de la matière grise du Blop Multicolore Royal. Heureusement qu\'elle n\'est pas trop gourmande.",ep:5,g:14,l:150,wd:true,fm:true,w:1,p:1};
I.u[9391] = {n:"Fleur de Blop Multicolore Royal",nn:"FLEUR DE BLOP MULTICOLORE ROYAL",t:35,d:"Cette fleur était cachée à l\'intérieur de la boîte crânienne du Blop Multicolore Royal. Elle lui servait à boucher ses trous de mémoire.",ep:5,g:13,l:150,wd:true,fm:true,w:1,p:1};
I.u[9394] = {n:"Chapblopignon",nn:"CHAPBLOPIGNON",t:16,d:"Cette coiffe stylée donne la Mitcho classe à n\'importe qui, ainsi qu\'une odeur de sous-bois dont vos amis se passeraient bien.",ep:22,g:222,l:152,wd:true,fm:true,w:15,p:45782};
I.u[9395] = {n:"Blopronne Royale",nn:"BLOPRONNE ROYALE",t:16,d:"Ce couvre-chef a été porté par Kuri en personne pendant plusieurs années, jusqu\'à ce qu\'elle décide d\'arrêter de porter des coiffes démodées.",ep:22,g:223,l:161,wd:true,fm:true,w:10,p:47750};
I.u[9396] = {n:"Le sceau des Zewouan",nn:"LE SCEAU DES ZEWOUAN",t:9,d:"En ancien langage Lenald, il se nomme « Zewouan Ling ».  \nLa rumeur prétend qu\'il aurait été forgé dans un volcan situé sous Pandala Feu, par Zauwon, le Wabbit obscur.",ep:1,g:7,l:1,wd:false,fm:true,w:1,c:"PX=3|PX=5",p:1};
I.u[9401] = {n:"Peau de Bworker",nn:"PEAU DE BWORKER",t:59,d:"Cette peau a traversé toutes les guerres, elle en est sortie indemne. Les épreuves du temps l\'ont à peine écorchée. Ca ne vous a pas empêché de lui faire la peau pour autant.",ep:14,g:9,l:400,wd:true,fm:true,w:10,p:1};
I.u[9402] = {n:"Pantoufrelons",nn:"PANTOUFRELONS",t:11,d:"Les petites ailes accrochées à ces pantoufles permettent d\'aérer les pieds malodorants. Attention, il est déconseillé d\'utiliser ces pantoufles pour jouer au Boufball, le Bouftou pourrait ne pas apprécier de se faire piquer les fesses.",ep:1,g:128,l:34,wd:true,fm:true,w:5,s:112,p:3200};
I.u[9403] = {n:"Masse O\'dard",nn:"MASSE O\'DARD",t:7,d:"Les petites ailes accrochées à cette masse permettent de la rendre plus légère et plus facile à manier. Parfois, elle s\'envolera même toute seule et vous frappera pendant votre sommeil, pour rire un peu.",ep:1,g:68,l:27,wd:true,fm:true,w:12,an:15,e:[10,4,1,1,45,50,false,false],c:"CI>17&CC>5",s:112,p:2100};
I.u[9404] = {n:"Cape Kifébzz",nn:"CAPE KIFEBZZ",t:17,d:"Le dard accroché au bout de cette cape, vous empêchera probablement de vous asseoir sans vous faire très mal au postérieur, mais personne n\'osera vous botter les fesses.",ep:1,g:155,l:33,wd:true,fm:true,w:5,s:112,p:3000};
I.u[9405] = {n:"Caskabeille",nn:"CASKABEILLE",t:16,d:"En équipant ce chapeau vous prenez le risque de recevoir des coups de tapette à Moskitos sur les antennes.",ep:1,g:224,l:37,wd:true,fm:true,w:9,s:112,p:3825};
I.u[9406] = {n:"Cochobits",nn:"COCHOBITS",t:16,d:"Ces oreilles de Porkass encore fraîches vous permettront de séduire facilement les Porkass que vous croiserez. Ne rigolez pas, ça pourrait finir par vous servir, on peut parfois se sentir bien seul dans la campagne d\'Amakna.",ep:1,g:225,l:36,wd:true,fm:true,w:2,s:113,p:3600};
I.u[9407] = {n:"Cape Bouchon",nn:"CAPE BOUCHON",t:17,d:"Certains nomment cette cape la Tire-boucape, et vouent une haine farouche à l\'encontre de ceux qui l\'appellent autrement. Qu\'importe, quel que soit le nom que vous donnerez à cette cape, elle ne vous sera d\'aucune utilité pour déboucher les bouteilles.",ep:1,g:156,l:37,wd:true,fm:true,w:3,s:113,p:3800};
I.u[9408] = {n:"Jambanneau",nn:"JAMBANNEAU",t:9,d:"Avec cet anneau, votre doigt fera de vous un véritable petit Porkass, ce qui ne devrait pas déplaire à vos amis les plus proches.",ep:1,g:162,l:40,wd:true,fm:true,w:3,s:113,p:500};
I.u[9409] = {n:"Ceinture Lardière",nn:"CEINTURE LARDIERE",t:10,d:"Votre petite queue en tire-bouchon fera plus d\'un jaloux parmi votre entourage.",ep:1,g:149,l:39,wd:true,fm:true,w:1,s:113,p:4225};
I.u[9410] = {n:"Chapeau du bois de l\'est",nn:"CHAPEAU DU BOIS DE L\'EST",t:16,d:"Moins à la mode que le Clint, mais beaucoup plus raffiné, ce chapeau vous permettra d\'exacerber votre coquetterie sans limite.",ep:1,g:226,l:40,wd:true,fm:true,w:25,s:114,p:1000};
I.u[9411] = {n:"Cape du bois de l\'est",nn:"CAPE DU BOIS DE L\'EST",t:17,d:"Cette cape protégera vos dos des coups de soleil. Attention elle ne descend pas jusqu\'aux fesses chez tous les aventuriers.",ep:1,g:157,l:36,wd:true,fm:true,w:10,s:114,p:6500};
I.u[9412] = {n:"Ceinture du bois de l\'est",nn:"CEINTURE DU BOIS DE L\'EST",t:10,d:"La fronde accrochée à cette ceinture est très bien accrochée. Tellement bien accrochée que vous ne pourrez pas la décrocher. Elle ne sert qu\'à impressionner les petits bandits de grand chemin que vous croiserez.",ep:1,g:150,l:32,wd:true,fm:true,w:10,s:114,p:3000};
I.u[9413] = {n:"Bottes du bois de l\'est",nn:"BOTTES DU BOIS DE L\'EST",t:11,d:"Les éperons accrochés à ces bottes, permettent de lacérer le cuir de votre Dragodinde, pour pouvoir la dépecer plus rapidement lorsque vous aurez décidé de la manger.",ep:1,g:129,l:30,wd:true,fm:true,w:10,s:114,p:2700};
I.u[9414] = {n:"Borsitrono",nn:"BORSITRONO",t:16,d:"Cet énorme citron vidé accueillera sans problème votre tête et le petit pépin qui se loge dans votre crâne.",ep:1,g:227,l:30,wd:true,fm:true,w:9,s:115,p:2600};
I.u[9415] = {n:"Cape Grume",nn:"CAPE GRUME",t:17,d:"Cette cape a été confectionnée à partir d\'une multitude d\'écorces d\'agrumes diverses et variées. Elle est jolie, elle rafraîchit, mais elle ne brille pas dans le noir.",ep:1,g:158,l:30,wd:true,fm:true,w:8,s:115,p:2525};
I.u[9416] = {n:"Anneau Zest",nn:"ANNEAU ZEST",t:9,d:"Si votre eau n\'a plus assez de goût, trempez-votre doigt avec cet anneau dans votre verre, agitez-le frénétiquement jusqu\'à ce qu\'il se décroche. Et voilà vous avez un délicieux cocktail à l\'orange sanguine et au citron, ainsi qu\'un doigt en moins. On ne peut pas tout avoir.",ep:1,g:163,l:20,wd:true,fm:true,w:4,s:115,p:1300};
I.u[9417] = {n:"Pailleton",nn:"PAILLETON",t:4,d:"Si vous avez rêvé de boire le sang de vos adversaires tout en les frappant vigoureusement, ce bâton est fait pour vous.",ep:1,g:81,l:32,wd:true,fm:true,w:1,an:12,e:[5,2,1,1,30,50,false,false],c:"CI>32&CA>10",s:115,p:2900};
I.u[9418] = {n:"Coiffe Cristaline",nn:"COIFFE CRISTALINE",t:16,d:"Le prochain qui tentera de vous coiffer les cheveux, risque de casser son peigne.",ep:1,g:228,l:22,wd:false,fm:true,w:12,s:125,p:1525};
I.u[9451] = {n:"Cape Cristaline",nn:"CAPE CRISTALINE",t:17,d:"Cet impressionnant morceau de cristal peut servir de cape rigide. Elle ne vous rendra pas nécessairement plus fort, mais vous pouvez être certain que le prochain monstre qui tentera de vous dévorer, risque de s\'étouffer en l\'avalant.",ep:1,g:159,l:21,wd:false,fm:true,w:10,s:125,p:1400};
I.u[9452] = {n:"Anneau Cristalin",nn:"ANNEAU CRISTALIN",t:9,d:"Avec cet anneau, votre doigt n\'aura jamais été aussi éclatant.",ep:1,g:164,l:30,wd:false,fm:true,w:4,s:125,p:2600};
I.u[9453] = {n:"Ceinture Cristaline",nn:"CEINTURE CRISTALINE",t:10,d:"Avec cette ceinture autour des hanches, plus personne n\'osera vous les pincer de peur de se trancher les doigts.",ep:1,g:151,l:48,wd:false,fm:true,w:30,s:125,p:6075};
I.u[9454] = {n:"Pompamulette",nn:"POMPAMULETTE",t:1,d:"Cette amulette pourra servir de bavoir pour les plus jeunes et les plus vieux.",ep:1,g:140,l:27,wd:false,fm:true,w:5,s:124,p:2200};
I.u[9455] = {n:"Pompanneau",nn:"POMPANNEAU",t:9,d:"Cet anneau peut être porté à un orteil si vous avez de mauvaises odeurs de pieds à cacher.",ep:1,g:165,l:30,wd:false,fm:true,w:4,s:124,p:2600};
I.u[9456] = {n:"Pompocape",nn:"POMPOCAPE",t:17,d:"Si vous estimez que votre postérieur n\'a jamais eu l\'attention qu\'il mérite, il n\'est pas trop tard pour revêtir cette longue cape qui le recouvrira et attirera tous les regards.",ep:1,g:160,l:33,wd:false,fm:true,w:5,s:124,p:3075};
I.u[9457] = {n:"Pompocoiffe",nn:"POMPOCOIFFE",t:16,d:"Cette coiffe dégage une odeur de rose démoniaque suffisamment forte pour faire fuir tous les poux qui vivent sur votre tête, et tous les parasites qui se font passer pour vos amis.",ep:1,g:229,l:33,wd:false,fm:true,w:5,s:124,p:3000};
I.u[9459] = {n:"Liste de jouets du papa Nowel",nn:"LISTE DE JOUETS DU PAPA NOWEL",t:24,d:"Le papa Nowel prend toujours soin des listes de jouets qui sont amenées par les enfants qu\'il dévore.  \nVous n\'aviez pas encore écrit la vôtre, en voilà une.",ep:1,g:41,l:1,wd:false,fm:true,w:0,p:0};
I.u[9460] = {n:"Pomme de pik",nn:"POMME DE PIK",t:24,d:"Voilà un Pokipik qui ne poussera jamais, vous n\'avez pas honte ?",ep:1,g:123,l:1,wd:false,fm:true,w:0,p:0};
I.u[9461] = {n:"Le Kim",nn:"LE KIM",t:16,d:"On dit que le Kimbo porte cette perruque en hommage à un bel éphèbe bronzé rencontré un jour sur son île, dont il appréciait le sourire aéré. Il la cache néanmoins sous sa coco parce qu\'il la trouve quand même ridicule.",ep:22,g:230,l:196,wd:true,fm:true,w:10,p:60000};
I.u[9463] = {n:"Amunite",nn:"AMUNITE",t:1,d:"Ce bijou dégage une telle puissance qu\'elle tuerait un Moskito à 30 pas, si son odeur n\'empêchait pas ce dernier d\'approcher à moins d\'un kilomètre.",ep:23,g:141,l:189,wd:true,fm:true,w:10,s:107,p:58000};
I.u[9464] = {n:"Kralamansion",nn:"KRALAMANSION",t:1,d:"Cette effrayante amulette est longtemps restée cachée dans un manoir perdu. Autrefois vivante, elle voulait conquérir le monde. Finalement elle n\'aura que votre cou. La pauvre.",ep:22,g:142,l:194,wd:true,fm:true,w:30,s:97,p:58411};
I.u[9468] = {n:"Pelle Emélaka",nn:"PELLE EMELAKA",t:8,d:"Cette pelle est dangereuse, elle volera votre vitalité, votre jeunesse, l\'eau de votre corps et vos précieux gâtrools.",ep:22,g:51,l:191,wd:true,fm:true,w:10,an:16,e:[5,4,1,1,35,50,false,false],c:"CC>450",p:58401};
I.u[9469] = {n:"Epée Loponèze",nn:"EPEE LOPONEZE",t:6,d:"Cette épée construite à partir de fragments de mycoses, parasitera à merveille les intestins de vos ennemis.",ep:23,g:85,l:184,wd:true,fm:true,w:10,an:14,e:[10,4,1,1,50,50,false,false],c:"CC>300&CA>300",p:58456};
I.u[9471] = {n:"Chapeau de la Sain Ballotin",nn:"CHAPEAU DE LA SAIN BALLOTIN",t:16,d:"Ballotins, Ballotines, vous qui cherchez l\'âme sœur, ce couvre-chef élégant et sobre sera votre signe de reconnaissance. En effet, ce chapeau magique ne peut être porté que par les aventuriers et les aventurières qui ne sont pas encore mariés. Vous pourrez ainsi facilement distinguer plus facilement celles et ceux dont le cœur n\'est pas encore pris.",ep:1,g:231,l:1,wd:false,fm:true,w:1,c:"PR=0",p:1};
I.u[9472] = {n:"Shigekax Passion",nn:"SHIGEKAX PASSION",t:42,d:"Réputé pour ses vertus aphrodisiaques, ce bonbon doux et sucré est idéal comme cadeau à offrir à l\'être aimé.",ep:1,g:4,l:1,wd:false,fm:true,w:1,u:true,ut:true,p:1};
I.u[9474] = {n:"Sublime Pierre d\'âme hasardeuse",nn:"SUBLIME PIERRE D\'AME HASARDEUSE",t:83,d:"Cette rune offre une faible chance de capturer des âmes d\'une puissance tout simplement incroyable.",ep:0,g:17,l:1,wd:false,fm:true,w:1,p:23000};
I.u[9475] = {n:"Sublime Pierre d\'âme",nn:"SUBLIME PIERRE D\'AME",t:83,d:"Cette rune offre une chance correcte de capturer des âmes d\'une puissance tout simplement incroyable.",ep:0,g:16,l:1,wd:false,fm:true,w:1,p:50000};
I.u[9476] = {n:"Sublime Pierre d\'âme heureuse",nn:"SUBLIME PIERRE D\'AME HEUREUSE",t:83,d:"Cette rune offre une bonne chance de capturer des âmes d\'une puissance tout simplement incroyable.",ep:0,g:18,l:1,wd:false,fm:true,w:1,p:75000};
I.u[9479] = {n:"Poshocoiffe",nn:"POSHOCOIFFE",t:16,d:"Le clou planté dans cette coiffe permet de dégonfler la tête de ceux qui l\'ont un peu trop grosse.",ep:1,g:232,l:45,wd:true,fm:true,w:23,s:123,p:1000};
I.u[9480] = {n:"Poshocape",nn:"POSHOCAPE",t:17,d:"Avec cette jolie cible brodée sur votre cape, vos ennemis n\'auront aucune excuse s\'ils vous ratent.",ep:1,g:161,l:33,wd:true,fm:true,w:10,s:123,p:3000};
I.u[9481] = {n:"Poshoclier",nn:"POSHOCLIER",t:82,d:"Le clou planté dans ce bouclier peut servir d\'arme de corps à corps si vous voulez crever votre adversaire pour qu\'il se dégonfle.",ep:1,g:36,l:1,wd:true,fm:true,w:1,s:123,p:1};
I.u[9482] = {n:"Cigaloclier",nn:"CIGALOCLIER",t:82,d:"Si vous avez peur d\'effrayer vos amis avec votre tête de monstre, vous pourrez toujours vous cacher derrière ce bouclier pour ressembler à quelqu\'un de sympathique.",ep:1,g:37,l:1,wd:true,fm:true,w:1,s:122,p:1};
I.u[9483] = {n:"Bouclier Boh-Bihn",nn:"BOUCLIER BOH-BIHN",t:82,d:"Ce bouclier était utilisé par les jeunes Iops qui y rangeaient les images offertes par leurs instituteurs pour les récompenser de leurs efforts. De temps en temps, ils s\'amusaient à les faire défiler rapidement pour épater leurs camarades de classe.",ep:1,g:38,l:1,wd:true,fm:true,w:1,p:1};
I.u[9484] = {n:"Bouclier Tigré",nn:"BOUCLIER TIGRE",t:82,d:"Ce bouclier ne protège pas totalement son propriétaire, mais il pourra vous défendre efficacement contre les fantômes.",ep:1,g:39,l:1,wd:true,fm:true,w:1,p:1};
I.u[9485] = {n:"Cigalocape",nn:"CIGALOCAPE",t:17,d:"Les trois petites boules qui ornent cette cape permettent de donner de jolis coups de boules si vous savez faire la toupie.",ep:1,g:162,l:30,wd:true,fm:true,w:10,s:122,p:2500};
I.u[9486] = {n:"Cigalocoiffe",nn:"CIGALOCOIFFE",t:16,d:"Avec ou sans cette coiffe, vous ressemblerez toujours à un bouffon, mais avec cette coiffe vous aurez au moins une bonne excuse.",ep:1,g:233,l:33,wd:true,fm:true,w:10,s:122,p:3075};
I.u[9487] = {n:"Certificat de changement d\'alignement",nn:"CERTIFICAT DE CHANGEMENT D\'ALIGNEMENT",t:24,d:"Ce certificat vous a été remis par Morrie Gane. Il contient plusieurs informations vous concernant et atteste de votre changement d\'alignement.",ep:0,g:26,l:1,wd:false,fm:true,w:0,p:1};
I.u[9488] = {n:"Registre de Bonta",nn:"REGISTRE DE BONTA",t:24,d:"Ce registre appartient à Morrie Gane et contient de nombreux documents dont comment devenir Bontarien. Il y a une page à remplir par vos soins.",ep:0,g:95,l:1,wd:false,fm:true,w:0,p:1};
I.u[9489] = {n:"Registre de Brâkmar",nn:"REGISTRE DE BRAKMAR",t:24,d:"Ce registre appartient à Morrie Gane et contient de nombreux documents dont comment devenir Brâkmarien. Il y a une page à remplir par vos soins.",ep:0,g:95,l:1,wd:false,fm:true,w:0,p:1};
I.u[9490] = {n:"Certificat des Ordres numéro 5",nn:"CERTIFICAT DES ORDRES NUMERO 5",t:24,d:"Ce certificat atteste que vous appartenez à un ordre. Pour en changer, il vous faut attendre sept jours à compter de la date de remise de ce certificat. Vous avez en effet le droit de faire la girouette, mais il ne faut pas exagérer tout de même.",ep:16,g:33,l:1,wd:false,fm:true,w:0,p:1};
I.u[9491] = {n:"Malédiction du Ballotin",nn:"MALEDICTION DU BALLOTIN",t:30,d:"Vous voyez désormais la vie en rose, et vous êtes heureux.",ep:1,g:2,l:1,wd:false,fm:true,w:0,m:true,p:1};
I.u[9492] = {n:"Cadeau Pantoufrelons",nn:"CADEAU PANTOUFRELONS",t:89,d:"En secouant vigoureusement ce cadeau, vous comprenez qu\'il n\'est pas vide. Peut-être que pour une fois, les dieux ne se sont pas moqués de vous.",ep:1,g:42,l:1,wd:false,fm:true,w:1,u:true,p:1};
