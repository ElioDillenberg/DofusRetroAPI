I.u[8964] = {n:"Patte d\'Ecaflip",nn:"PATTE D\'ECAFLIP",t:6,d:"" + "\"" + "La chance sourit aux audacieux !" + "\"" + " soufflent les adeptes du Dieu Ecaflip. Vous voilà armé comme un as de pique, vous allez pouvoir abattre votre jeu. Cette épée de prestige est d\'une rareté et d\'une pureté exceptionnelle.",ep:1,g:82,l:100,wd:true,fm:true,w:20,an:14,tw:true,e:[10,5,1,1,50,40,false,false],c:"CS>150",p:10000};
I.u[8965] = {n:"Patte d\'Ecaflip",nn:"PATTE D\'ECAFLIP",t:6,d:"" + "\"" + "La chance sourit aux audacieux !" + "\"" + " soufflent les adeptes du Dieu Ecaflip. Vous voilà armé comme un as de pique, vous allez pouvoir abattre votre jeu. Cette épée de prestige est d\'une rareté et d\'une pureté exceptionnelle.",ep:1,g:82,l:100,wd:true,fm:true,w:20,an:14,tw:true,e:[10,5,1,1,50,40,false,false],c:"CS>150",p:10000};
I.u[8966] = {n:"Patte d\'Ecaflip Parfaite",nn:"PATTE D\'ECAFLIP PARFAITE",t:6,d:"Cette épée constitue l\'incarnation même de la chance du Dieu Ecaflip. Vous tenez entre vos mains la plus pure version de la Patte d\'Ecaflip, qui témoigne de votre chance aussi incroyable qu\'insolente (et donc de votre célibat définitif).",ep:1,g:81,l:100,wd:true,fm:true,w:20,an:14,tw:true,e:[10,5,1,1,50,40,false,false],c:"CS>150",p:10000};
