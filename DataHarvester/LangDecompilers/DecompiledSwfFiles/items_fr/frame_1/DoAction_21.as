I.u[8961] = {n:"Patte d\'Ecaflip",nn:"PATTE D\'ECAFLIP",t:6,d:"Certains superstitieux prétendent que les pattes de lapin apportent chance et fortune. Grâce à cette légendaire épée, vous pourrez briller à leurs yeux. Par ailleurs, son tranchant est très affuté et sa coupe vous rappelle légèrement celle d\'une épée que vous avez déjà connue.",ep:1,g:82,l:100,wd:true,fm:true,w:20,an:14,tw:true,e:[10,5,1,1,50,40,false,false],c:"CS>150",p:10000};
I.u[8962] = {n:"Patte d\'Ecaflip",nn:"PATTE D\'ECAFLIP",t:6,d:"Certains superstitieux prétendent que les pattes de lapin apportent chance et fortune. Grâce à cette légendaire épée, vous pourrez briller à leurs yeux. Par ailleurs, son tranchant est très affuté et sa coupe vous rappelle légèrement celle d\'une épée que vous avez déjà connue.",ep:1,g:82,l:100,wd:true,fm:true,w:20,an:14,tw:true,e:[10,5,1,1,50,40,false,false],c:"CS>150",p:10000};
