I.u[8913] = {n:"Potion Bulbique",nn:"POTION BULBIQUE",t:12,d:"Cette potion est composée d\'extraits de bulbes macérés dans de la purée de Kloug. Les alchimistes les plus audacieux utilisent de la farine de riz à la place de la purée de Kloug devenue introuvable depuis longtemps. Aussi improbable que cela puisse paraître, vous vous habituerez à son goût.",ep:22,g:23,l:110,wd:true,fm:true,w:1,u:true,ut:true,p:10};
I.u[8914] = {n:"Fiche descriptive d\'un Bricoleur",nn:"FICHE DESCRIPTIVE D\'UN BRICOLEUR",t:24,d:"Cette fiche contient toutes les informations nécessaires pour retrouver le Bricoleur qui y est décrit.",ep:0,g:176,l:1,wd:false,fm:true,w:0,c:"PJ>65,50",p:1};
I.u[8915] = {n:"Fiche du Bricoleur à remplir",nn:"FICHE DU BRICOLEUR A REMPLIR",t:80,d:"Cette fiche est faite pour y remplir toutes les informations nécessaires pour identifier un Bricoleur.",ep:0,g:6,l:1,wd:false,fm:true,w:1,ut:true,p:1};
I.u[8916] = {n:"Ambre du Tynril",nn:"AMBRE DU TYNRIL",t:50,d:"Cette ambre n\'est pas comme les autres. Si vous l\'agitez de haut en bas, en fermant les yeux, et en dansant un tango rapide du Captain Chafer, vous pourrez peut-être la sentir vibrer et à briller, comme si une âme mystérieuse l\'habitait. Accessoirement, vous aurez l\'air d\'un fou, mais d\'un fou qui brille et qui vibre.",ep:22,g:1,l:100,wd:true,fm:true,w:10,p:1};
I.u[8917] = {n:"Clef de la Grotte Hesque",nn:"CLEF DE LA GROTTE HESQUE",t:84,d:"Cette clef étrange vous permettra de rentrer dans la Grotte Hesque en un seul morceau.",ep:22,g:244,l:1,wd:true,fm:true,w:1,p:1};
I.u[8918] = {n:"Casque amélioré",nn:"CASQUE AMELIORE",t:16,d:"Ce casque amélioré permet en théorie de chevaucher les Scarafeuilles les plus rapides sans perdre sa perruque. Tout simplement inestimable.",ep:22,g:25,l:1,wd:true,fm:true,w:10,p:1};
I.u[8919] = {n:"Ailes en bois améliorées",nn:"AILES EN BOIS AMELIOREES",t:17,d:"Ces ailes en bois améliorées ont été rembourrées et rendues plus légères grâce à des plumes et du tissu de qualité. L\'armature étant toujours en bois, vous aurez tout de même du mal à ne pas vous écraser.",ep:22,g:22,l:30,wd:true,fm:true,w:10,p:2633};
I.u[8922] = {n:"Bourse de kamas",nn:"BOURSE DE KAMAS",t:24,d:"Cette bourse contient une importante somme de kamas. Elle est surement destinée à quelque pot-de-vin.",ep:2,g:243,l:1,wd:true,fm:true,w:0,p:1};
I.u[8923] = {n:"Sac de patates",nn:"SAC DE PATATES",t:24,d:"Un sac de patates, il faut vraiment ne pas savoir quoi faire de son temps pour se balader avec ça",ep:2,g:243,l:1,wd:true,fm:true,w:0,p:1};
I.u[8924] = {n:"Arc à Chon",nn:"ARC A CHON",t:2,d:"C\'est l\'histoire d\'un homme appelé Chon, il avait un phare. C\'était le Phare à Chon.",ep:22,g:59,l:151,wd:true,fm:true,w:10,an:10,e:[10,3,2,7,30,50,false,true],c:"CA>200",p:46105};
I.u[8925] = {n:"Arc Oleptik",nn:"ARC OLEPTIK",t:2,d:"Cet arc étrange a été sculpté par une Sadida pendant une phase de somnambulisme créatrice. Il en émane une envie de sommeil qui affecte autant le porteur que ses cibles.",ep:22,g:60,l:185,wd:true,fm:true,w:15,an:10,tw:true,e:[10,4,3,8,30,50,false,true],c:"CA>300&CI>300",p:51125};
I.u[8926] = {n:"Dague Adou",nn:"DAGUE ADOU",t:5,d:"Ces dagues sont parfaites pour pousser les ananas ou moudre le café. Certains s\'en servent même pour taper des pommes et des poires.",ep:22,g:65,l:175,wd:true,fm:true,w:10,an:13,tw:true,e:[7,3,1,1,30,30,false,false],c:"CA>250&CC>250&CS>150",p:49775};
I.u[8927] = {n:"Dagues Eudin",nn:"DAGUES EUDIN",t:5,d:"Eudin était un Sram complètement fou, il volait les pauvres pour donner aux riches.",ep:22,g:66,l:171,wd:true,fm:true,w:10,an:13,tw:true,e:[10,3,1,1,30,50,false,false],c:"CS>350&CA>200",p:49280};
I.u[8928] = {n:"Dagues Ruik",nn:"DAGUES RUIK",t:5,d:"Ruik est meilleur buveur que combattant, et ça ne s\'améliore pas au fil du temps. Il est quand même déconseillé de ressembler à un bouchon quand il porte ses dagues.",ep:22,g:67,l:158,wd:true,fm:true,w:10,an:13,e:[15,2,1,1,30,50,false,false],c:"CA>300",p:47275};
I.u[8929] = {n:"Kriss Toubal",nn:"KRISS TOUBAL",t:5,d:"A ne pas confondre avec le célèbre bâton au nom semblable, ces lames provoquent des carnages bien plus violents.",ep:22,g:68,l:149,wd:true,fm:true,w:10,an:13,tw:true,e:[10,3,1,1,30,50,false,false],c:"CI>250&CC>250",p:45875};
I.u[8930] = {n:"Fuschia",nn:"FUSCHIA",t:6,d:"Plus d\'un fier guerrier a refusé cette arme à cause de sa couleur, la majorité d\'entre eux est morte sous sa lame.",ep:22,g:78,l:163,wd:true,fm:true,w:10,an:14,e:[7,5,1,1,30,50,false,false],c:"CA>300&CI>300",p:48245};
I.u[8931] = {n:"Az\'tech",nn:"AZ\'TECH",t:6,d:"Cette épée appartenait à un vieil Enutrof mégalomane, elle est en or massif, comme l\'était son égo.",ep:22,g:79,l:186,wd:true,fm:true,w:10,an:14,tw:true,e:[7,5,1,1,45,50,false,false],c:"CA>200&CC>200&CS>200",p:51275};
I.u[8932] = {n:"Kukri Kura",nn:"KUKRI KURA",t:6,d:"Souriez ! Dites cheese ! Voilà, vous êtes coupé en deux.",ep:22,g:80,l:179,wd:true,fm:true,w:10,an:14,e:[7,4,1,1,40,50,false,false],c:"CS>300&CA>150",p:50330};
I.u[8933] = {n:"Hache Sueur Rance",nn:"HACHE SUEUR RANCE",t:19,d:"Ourag\'han était un vieux Pandawa au derme si doux qu\'on l\'appelait Peau Lisse. Son odeur en revanche était si forte qu\'il a hérité d\'un autre surnom. Il l\'a transmis à sa fidèle hache.",ep:22,g:49,l:162,wd:true,fm:true,w:10,an:17,tw:true,e:[10,5,1,1,50,50,false,false],c:"CW>250&CS>300",p:47900};
I.u[8934] = {n:"Hache Harnée",nn:"HACHE HARNEE",t:19,d:"Fâché de s\'être fait voler un grain de riz dans son assiette par un écureuil chenapan pendant ses vacances touristiques sur l\'île de Pandala, Brouce Boulgour s\'est servi de cette hache pour abattre chaque bambou au sommet duquel le voleur se réfugiait, afin de l\'attraper. Les Pandawas du village ont dû le calmer avec la moitié de la réserve d\'alcool de bambou pour éviter qu\'il ne rase la forêt. Sacré Brouce.",ep:22,g:50,l:158,wd:true,fm:true,w:10,an:17,e:[7,5,1,1,50,50,false,false],c:"CC>150&CA>150&CI>150",p:47542};
I.u[8935] = {n:"la Bêche à Mel",nn:"LA BECHE A MEL",t:8,d:"Jeune Iopette, Mel se voyait déjà vaincre des dragons. Elle jouait à combattre les rochers du jardin de sa mère avec une pelle en guise d\'épée. La rocaille a appris à redouter l\'ustensile.",ep:22,g:48,l:176,wd:true,fm:true,w:10,an:16,e:[7,4,1,1,35,50,false,false],c:"CC>250&CI>250",p:49985};
I.u[8936] = {n:"Pelle Shattkitou",nn:"PELLE SHATTKITOU",t:8,d:"Cette pelle est sauvage et tente de mordre son propriétaire à chaque utilisation. Elle émet des sons bizarres quand elle est contente et ne se calme qu\'en présence de grelots.",ep:22,g:49,l:137,wd:true,fm:true,w:10,an:16,tw:true,e:[5,4,1,1,40,50,false,false],c:"CC>150&CA>200",p:43945};
I.u[8937] = {n:"Pelle Aigante",nn:"PELLE AIGANTE",t:8,d:"Un Enutrof futé s\'était fait pirate après avoir fait le constat qu\'on ramasse plus de trésor sur un cadavre frais que dans une tombe moisie. Sa pelle rouge sang était son symbole et faisait trembler l\'équipage des bateaux de toutes les mers.",ep:22,g:50,l:156,wd:true,fm:true,w:10,an:16,e:[7,5,1,1,20,50,false,false],c:"CS>250&CI>250",p:49455};
