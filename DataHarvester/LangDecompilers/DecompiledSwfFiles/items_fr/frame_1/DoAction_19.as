I.u[8352] = {n:"Ecaille de Dragoss Noir",nn:"ECAILLE DE DRAGOSS NOIR",t:59,d:"Cette écaille de Dragoss pourrait servir à confectionner de belles bottines, ou des sous-vêtements de combat.",ep:11,g:606,l:50,wd:true,fm:true,w:1,p:10};
I.u[8353] = {n:"Ecaille de Dragoss de Saphire",nn:"ECAILLE DE DRAGOSS DE SAPHIRE",t:59,d:"Cette écaille de Dragoss pourrait servir à confectionner de belles bottines, ou des sous-vêtements de combat.",ep:11,g:608,l:50,wd:true,fm:true,w:1,p:10};
I.u[8354] = {n:"Ecaille de Dragoss Blanc",nn:"ECAILLE DE DRAGOSS BLANC",t:59,d:"Cette écaille de Dragoss pourrait servir à confectionner de belles bottines, ou des sous-vêtements de combat.",ep:11,g:607,l:50,wd:true,fm:true,w:1,p:10};
I.u[8355] = {n:"Ecaille de Dragoss Doré",nn:"ECAILLE DE DRAGOSS DORE",t:59,d:"Cette écaille de Dragoss pourrait servir à confectionner de belles bottines, ou des sous-vêtements de combat.",ep:11,g:605,l:50,wd:true,fm:true,w:1,p:10};
I.u[8356] = {n:"Peau de Dragueuse",nn:"PEAU DE DRAGUEUSE",t:59,d:"Lisse et parfumée avec du sang de jeunes aventuriers, cette peau rend de toute évidence les Dragueuses irrésistibles. Utilisée pour des bottes, ou des ceintures, cette peau a l\'avantage d\'être lavable en machine, à n\'importe quelle température.",ep:11,g:609,l:70,wd:true,fm:true,w:1,p:1};
I.u[8357] = {n:"Corne de Dragueuse",nn:"CORNE DE DRAGUEUSE",t:47,d:"La Dragueuse dispose de deux petites cornes à l\'arrière de son crâne, qui lui permettent de capter plus facilement les ondes amoureuses émises par les autres Dragoeufs pendant la saison des amours, qui rappelons-le, ne s\'arrête jamais chez les Dragoeufs.",ep:11,g:649,l:70,wd:true,fm:true,w:1,p:10};
I.u[8358] = {n:"Protection de la Dragueuse",nn:"PROTECTION DE LA DRAGUEUSE",t:111,d:"Cette paire de coquilles de Dragoeuf permet à la Dragueuse de se protéger de ses propres coups de foudre. Cette protection est également utilisable par les mâles, si elle est portée sous la ceinture.",ep:11,g:742,l:70,wd:true,fm:true,w:1,p:10};
I.u[8359] = {n:"Aile de Dragoeuf Volant",nn:"AILE DE DRAGOEUF VOLANT",t:104,d:"Même abimée, déchirée et brûlée, cette aile permet au Dragoeuf Volant de se mouvoir librement au dessus du vide, comme un jeune Moskito insouciant.",ep:11,g:743,l:80,wd:true,fm:true,w:1,p:10};
I.u[8360] = {n:"Peau de Dragoeuf Volant",nn:"PEAU DE DRAGOEUF VOLANT",t:59,d:"Cette peau sombre est recouverte d\'une multitude de petites écailles censées améliorer l\'aérodynamisme du Dragoeuf Volant. En réalité, elles servent juste à impressionner les badauds.",ep:11,g:610,l:70,wd:true,fm:true,w:1,p:10};
I.u[8361] = {n:"Corne de Dragoeuf Volant",nn:"CORNE DE DRAGOEUF VOLANT",t:47,d:"Cette corne sert de viseur au Dragoeuf Volant afin de ne pas râter ses cibles, lorsqu\'il vole en état d\'ébriété.",ep:11,g:650,l:70,wd:true,fm:true,w:1,p:10};
I.u[8362] = {n:"Peau de Dragoeuf Guerrier",nn:"PEAU DE DRAGOEUF GUERRIER",t:59,d:"Assez solide pour résister aux coups de sa propre lame, la peau du Dragoeuf Guerrier est surtout réputée pour être comestible, une fois bouillie pendant plusieurs années.",ep:11,g:611,l:80,wd:true,fm:true,w:1,p:10};
I.u[8363] = {n:"Corne de Dragoeuf Guerrier",nn:"CORNE DE DRAGOEUF GUERRIER",t:47,d:"Le Dragoeuf Guerrier dispose de trois cornes différentes, deux derrières la tête, et une sur le bout du museau. Généralement, après une attaque, une corne se retrouve plantée dans le derrière de ses victimes, et les deux dernières enfoncées profondément dans les narines d\'un imprudent. Mais parfois le Dragoeuf Guerrier confond le derrière et les narines de ses victimes, ce qui pour le coup, leur cause beaucoup plus de tort.",ep:11,g:651,l:80,wd:true,fm:true,w:1,p:10};
I.u[8364] = {n:"Lanière en cuir du Dragoeuf Guerrier",nn:"LANIERE EN CUIR DU DRAGOEUF GUERRIER",t:15,d:"Cette lanière en cuir permet au Dragoeuf Guerrier d\'accrocher son épaulette, ou de remplacer vos bretelles.",ep:11,g:744,l:80,wd:true,fm:true,w:1,p:10};
I.u[8365] = {n:"Corne brisée de Crocabulia",nn:"CORNE BRISEE DE CROCABULIA",t:47,d:"Crocabulia a cassé sa propre corne lors d\'un terrible combat dans lequel elle tentait de protéger l\'oeuf qui donna naissance 15 années plus tard, au Crocoburio.",ep:11,g:652,l:400,wd:true,fm:true,w:1,p:10};
I.u[8366] = {n:"Cape du Petit Chapon Rouge",nn:"CAPE DU PETIT CHAPON ROUGE",t:17,d:"Cette cape appartenait à un célèbre Chapon, ancien volatile aux longues plumes qui aimait se pavoiser devant le Zaap de Bonta devant sa basse-cour composée de volailles crédules et de poulardes immatures.",ep:22,g:121,l:171,wd:true,fm:true,w:10,p:49200};
I.u[8367] = {n:"Peau de Crocabulia",nn:"PEAU DE CROCABULIA",t:59,d:"Cette peau rouge vous permettra peut-être de confectionner une cape du petit Chapon Rouge, si vous ne redoutez pas les railleries de vos alliés.",ep:11,g:612,l:400,wd:true,fm:true,w:1,p:10};
I.u[8368] = {n:"Dent de Crocabulia",nn:"DENT DE CROCABULIA",t:47,d:"Cette énorme dent a été arrachée in extremis avant que Crocabulia ne referme sa gueule pour toujours.",ep:11,g:653,l:400,wd:true,fm:true,w:1,p:10};
I.u[8369] = {n:"Fragment de Coquille de Terrakoubiak le Guerrier",nn:"FRAGMENT DE COQUILLE DE TERRAKOUBIAK LE GUERRIER",t:111,d:"Ce fragment de coquille appartenant à Terrakoubiak le Guerrier, devrait vous aider à fabriquer une clef pour le sanctuaire des Dragoeufs.",ep:10,g:750,l:80,wd:true,fm:true,w:1,p:10};
I.u[8370] = {n:"Fragment de Coquille d\'Ignelicrobur le Guerrier",nn:"FRAGMENT DE COQUILLE D\'IGNELICROBUR LE GUERRIER",t:111,d:"Ce fragment de coquille appartenant à Ignelicrobur le Guerrier, devrait vous aider à fabriquer une clef pour le sanctuaire des Dragoeufs.",ep:10,g:749,l:80,wd:true,fm:true,w:1,p:10};
I.u[8371] = {n:"Fragment de Coquille d\'Aerohouctor le Guerrier",nn:"FRAGMENT DE COQUILLE D\'AEROHOUCTOR LE GUERRIER",t:111,d:"Ce fragment de coquille appartenant à Aerohouctor le Guerrier, devrait vous aider à fabriquer une clef pour le sanctuaire des Dragoeufs.",ep:10,g:748,l:80,wd:true,fm:true,w:1,p:10};
I.u[8372] = {n:"Fragment de Coquille d\'Aquabralak le Guerrier",nn:"FRAGMENT DE COQUILLE D\'AQUABRALAK LE GUERRIER",t:111,d:"Ce fragment de coquille appartenant à Aquabralak le Guerrier, devrait vous aider à fabriquer une clef pour le sanctuaire des Dragoeufs.",ep:10,g:747,l:80,wd:true,fm:true,w:1,p:10};
I.u[8373] = {n:"Pierre d\'Ignerkocropos l\'Affamé",nn:"PIERRE D\'IGNERKOCROPOS L\'AFFAME",t:51,d:"Cette pierre, si elle est combinée avec les trois autres manquantes, devrait libérer une onde magique assez puissante pour activer et ouvrir une porte magique.",ep:10,g:463,l:100,wd:true,fm:true,w:1,p:10};
I.u[8374] = {n:"Pierre d\'Aqualikros l\'Impitoyable",nn:"PIERRE D\'AQUALIKROS L\'IMPITOYABLE",t:51,d:"Cette pierre, si elle est combinée avec les trois autres manquantes, devrait libérer une onde magique assez puissante pour activer et ouvrir une porte magique.",ep:10,g:460,l:100,wd:true,fm:true,w:1,p:10};
I.u[8375] = {n:"Pierre de Terraburkal le Perfide",nn:"PIERRE DE TERRABURKAL LE PERFIDE",t:51,d:"Cette pierre, si elle est combinée avec les trois autres manquantes, devrait libérer une onde magique assez puissante pour activer et ouvrir une porte magique.",ep:10,g:461,l:100,wd:true,fm:true,w:1,p:10};
I.u[8376] = {n:"Pierre d\'Aerotrugobur le Malveillant",nn:"PIERRE D\'AEROTRUGOBUR LE MALVEILLANT",t:51,d:"Cette pierre, si elle est combinée avec les trois autres manquantes, devrait libérer une onde magique assez puissante pour activer et ouvrir une porte magique.",ep:10,g:462,l:100,wd:true,fm:true,w:1,p:10};
I.u[8377] = {n:"Rune de Bricoleur",nn:"RUNE DE BRICOLEUR",t:88,d:"Cette pierre magique permet aux Bricoleurs de donner leur position aux clients.",ep:18,g:12,l:1,wd:true,fm:true,w:1,u:true,p:1};
I.u[8378] = {n:"Fragment Magique",nn:"FRAGMENT MAGIQUE",t:88,d:"Ce fragment magique, contient une partie de la magie autrefois contenue dans un objet. Avec un peu de chance, vous devriez pouvoir récupérer quelques runes magiques.",ep:1,g:44,l:1,wd:false,fm:true,w:1,u:true,p:1};
I.u[8379] = {n:"Rune Vie",nn:"RUNE VIE",t:78,d:"Cette petite rune a le pouvoir de rendre plus résistant.",ep:1,g:59,l:20,wd:true,fm:true,w:1,p:10};
I.u[8380] = {n:"Etoffe de Péki",nn:"ETOFFE DE PEKI",t:55,d:"Cette étoffe recouvrait les fesses poilues du Péki avant que vous ne décidiez de le caresser à rebrousse-poils.",ep:18,g:721,l:400,wd:true,fm:true,w:10,p:100};
I.u[8381] = {n:"Queue Magique du Péki",nn:"QUEUE MAGIQUE DU PEKI",t:65,d:"Cette queue est si magique, qu\'elle frétille encore, même après avoir été coupée, séchée, et empaillée. En cherchant bien, vous pourrez lui trouver une utilité autre que décorative.",ep:18,g:751,l:400,wd:true,fm:true,w:10,p:100};
I.u[8383] = {n:"Peau de Moon",nn:"PEAU DE MOON",t:59,d:"Privé de sa petite peau douce, le pauvre petit Moon s\'est retrouvé bien au dépourvu et à préféré fuir lâchement dans la jungle en hurlant de douleur. Une scène réjouissante que vous ne manquerez pas de raconter à vos enfants le soir, pour les aider à trouver le sommeil.",ep:12,g:613,l:200,wd:true,fm:true,w:1,p:10};
I.u[8384] = {n:"Etoffe Magique de Moon",nn:"ETOFFE MAGIQUE DE MOON",t:55,d:"Cette étoffe ressemble à une étoffe magique, et vous avez du mal à l\'accepter. Pourtant il faudra bien que vous reconnaissiez un jour ou l\'autre, que vous êtes trop stupide pour déterminer vous-même ce qui est magique ou non. Ne suivez pas votre instinct de pourfendeur d\'Astrubiens, faites confiance aux descriptions.",ep:12,g:722,l:200,wd:true,fm:true,w:1,p:100};
I.u[8385] = {n:"Peau de Shin Larve",nn:"PEAU DE SHIN LARVE",t:59,d:"Cette peau de Shin Larve est encore plus gluante et collante que vous ne l\'imaginiez. Elle vous rappelle la dernière princesse que vous avez délivrée.",ep:5,g:614,l:100,wd:true,fm:true,w:1,p:10};
I.u[8386] = {n:"Dent de Shin Larve",nn:"DENT DE SHIN LARVE",t:47,d:"Personne n\'aurait pu croire qu\'une Shin Larve avait la bouche remplie de dents. C\'est peut-être parce que la Shin Larve ne dispose que d\'une seule dent.",ep:5,g:654,l:100,wd:true,fm:true,w:1,p:10};
I.u[8387] = {n:"Plume Vibrante du Tofu Royal",nn:"PLUME VIBRANTE DU TOFU ROYAL",t:53,d:"Plus royale que la plume du Tofu Royal, plus vibrante que la plume d\'un Kirboule Vibreur des mines de Kruup, cette plume saura chatouiller qui vous voudrez, et où vous voudrez. Il est expliqué sur la notice, que son entretien est relativement simple.",ep:3,g:645,l:200,wd:true,fm:true,w:1,p:10};
I.u[8388] = {n:"Peau de Bworkette",nn:"PEAU DE BWORKETTE",t:59,d:"La Bworkette vous a laissé lui arracher un morceau de peau en échange de votre clémence. Mais elle a oublié de vous dire qu\'elle a prévenu son grand frère le Bworker, qui se fera un plaisir de vous arracher la votre.",ep:4,g:615,l:100,wd:true,fm:true,w:1,p:10};
I.u[8389] = {n:"Poil de Bworkette",nn:"POIL DE BWORKETTE",t:54,d:"La Bworkette a la peau lisse, sauf sous les aisselles. Il faut bien que les gâteaux qu\'elle roule sous ses aisselles soient assaisonnés de temps en temps.",ep:4,g:665,l:100,wd:true,fm:true,w:1,p:10};
I.u[8390] = {n:"Peau de Cochon de Farle",nn:"PEAU DE COCHON DE FARLE",t:59,d:"Farle était le plus grand éleveur de cochons et peut-être même de cochonnes, d\'après les éleveurs qui jalousaient Shika. Ce qui est certain, c\'est qu\'il est à l\'origine des Cochons de Farle, à qui il a donné son nom, car ils les avaient créés à son image.",ep:15,g:616,l:50,wd:true,fm:true,w:1,p:10};
I.u[8391] = {n:"Peau de Don Dorgan",nn:"PEAU DE DON DORGAN",t:59,d:"Le Don Dorgan a été une fois de plus très généreux avec vous, malgré son terrible caractère. Il vous a fait don de sa peau avant de partir en grognant comme un gros Porkass. Quel culot !",ep:15,g:617,l:60,wd:true,fm:true,w:1,p:10};
I.u[8392] = {n:"Peau de Don Duss Ang",nn:"PEAU DE DON DUSS ANG",t:59,d:"Le Don Duss Ang que vous avez dû peler avec une râpe à fromage pour obtenir cette peau très fine, s\'en souviendra toute sa vie. On peut supposer qu\'il vous en voudra longtemps. Ne comptez plus sur lui pour vous indiquer le bon chemin la prochaine fois que vous vous égarerez dans son labyrinthe.",ep:15,g:618,l:60,wd:true,fm:true,w:1,p:10};
I.u[8393] = {n:"Poil de Dragon Cochon",nn:"POIL DE DRAGON COCHON",t:54,d:"Vous avez beau chercher, vous n\'arrivez pas à trouver la partie du corps du Dragon Cochon sur laquelle ce poil était planté. Cherchez-mieux, l\'anatomie du Dragon Cochon semble révéler de nombreuses surprises...",ep:2,g:666,l:100,wd:true,fm:true,w:1,p:10};
I.u[8394] = {n:"Méga Pierre du Craqueleur Légendaire",nn:"MEGA PIERRE DU CRAQUELEUR LEGENDAIRE",t:51,d:"Cette énorme pierre ressemble à une pierre du Craqueleur Légendaire, et pourtant il n\'en n\'est rien, c\'est bien plus encore.",ep:5,g:464,l:100,wd:true,fm:true,w:1,p:10};
I.u[8396] = {n:"Mojo du Mulou",nn:"MOJO DU MULOU",t:15,d:"Cette petite fiole contient toute la puissance virile du Mulou. Attention, son contenu n\'est pas comestible.",ep:1,g:752,l:1,wd:true,fm:true,w:1,p:1};
I.u[8397] = {n:"Poil du Wa Wabbit",nn:"POIL DU WA WABBIT",t:54,d:"Le Wa Wabbit est en règle générale assez avare et rechigne à partager ses poils. Exceptionnellement, pour récompenser votre bravoure (ou acheter votre clémence), il a décidé de vous faire don d\'une partie de son corps. Touchant.",ep:2,g:667,l:100,wd:true,fm:true,w:1,p:10};
I.u[8398] = {n:"Etoffe du Wa Wabbit",nn:"ETOFFE DU WA WABBIT",t:55,d:"Personne ne sait réellement d\'où le Wa Wabbit a sorti cette étoffe, et il semblerait que personne ne cherche à savoir également. A la fois inquiétante et troublante, l\'origine de cette étoffe reste un des plus grands mystères de tout Amakna.",ep:2,g:723,l:100,wd:true,fm:true,w:1,p:10};
I.u[8399] = {n:"Poil de Maître Pandore",nn:"POIL DE MAITRE PANDORE",t:54,d:"Une fois de plus le Maître Pandore a voulu faire le malin, et s\'est pris une bonne leçon qu\'il n\'oubliera pas de si tôt. Il aurait mieux fait de rester au fond de sa boîte.",ep:18,g:668,l:100,wd:true,fm:true,w:1,p:10};
I.u[8400] = {n:"Etoffe de Maître Pandore",nn:"ETOFFE DE MAITRE PANDORE",t:55,d:"Protégeant tantôt sa nuque, tantôt son arrière train, cette étoffe a sauvé la vie du Maître Pandore plus d\'une fois. Mais pas cette fois-ci.",ep:18,g:724,l:100,wd:true,fm:true,w:1,p:10};
I.u[8401] = {n:"Peau de Maître Pandore",nn:"PEAU DE MAITRE PANDORE",t:59,d:"Usée par les attaques magiques, desséchée par le vent de l\'île de Pandala, cette peau est désormais d\'une piètre qualité, et pourtant vous semblez vous y attacher, comme si vous cherchiez à garder un précieux souvenir de ce brave Maître Pandore, qui a tout de même failli vous ôter la vie plusieurs fois sans aucun remord. C\'est dans ce genre de situations que l\'on reconnaît un véritable monstre d\'une petite nature.",ep:18,g:619,l:100,wd:true,fm:true,w:1,p:10};
I.u[8402] = {n:"Pierre du Koulosse",nn:"PIERRE DU KOULOSSE",t:51,d:"Le Koulosse a beau être généreux, il ne vous a pas donné sa grosse pierre de gaieté de cœur, et on peut le comprendre lorsque l\'on sait ce que vous allez en faire.",ep:20,g:465,l:100,wd:true,fm:true,w:10,p:10};
I.u[8403] = {n:"Tresse du Koulosse",nn:"TRESSE DU KOULOSSE",t:15,d:"Cette énorme tresse a été confectionnée par le Koulosse en personne il y a plusieurs centaines d\'années. Elle est devenue avec le temps, presque aussi rigide qu\'un bloc de pierre. Elle est cependant encore gorgée d\'un liquide gluant et nauséabond qui attire de temps en temps les Moskitos. Le plus surprenant dans tout cela c\'est que cette tresse allait très bien au Koulosse. Comme un gros Ploukosse, vous avez eu l\'idée saugrenue de la lui voler.",ep:20,g:753,l:100,wd:true,fm:true,w:10,p:10};
I.u[8404] = {n:"Peau de Skeunk",nn:"PEAU DE SKEUNK",t:59,d:"Ce morceau de peau semble provenir des talons de Skeunk, ce qui explique sa couleur jaunâtre du plus bel effet. A défaut de vous confectionner des vêtements à la mode avec cette vieille peau, vous pourrez vous confectionner des vêtements d\'une autre époque, et adopter une apparence " + "\"" + "rétro" + "\"" + " qui fera fureur auprès des plus jeunes.",ep:20,g:620,l:400,wd:true,fm:true,w:1,p:100};
I.u[8405] = {n:"Poil de Skeunk",nn:"POIL DE SKEUNK",t:54,d:"Qui à part vous, aurait pu croire que Skeunk ne s\'épilait pas entièrement le corps depuis sa tendre enfance ?",ep:20,g:669,l:400,wd:true,fm:true,w:10,p:100};
I.u[8406] = {n:"Laine du Minotot",nn:"LAINE DU MINOTOT",t:57,d:"Cette laine a la particularité d\'être très fine, et permet au Minotot de ne pas avoir de bouclettes le long du corps. Personne ne sait à quoi cela lui sert, vous devriez peut-être essayer de vous confectionner des implants pilaires avec cette laine, vous pourrez ainsi arborer une imposante toison de poils bruns sur votre torse. Même les rêves d\'enfance peuvent se réaliser.",ep:21,g:656,l:400,wd:true,fm:true,w:10,p:100};
I.u[8407] = {n:"Cuir du Minotot",nn:"CUIR DU MINOTOT",t:56,d:"A peine plus rare que le cuir de Bouftou, ce cuir est idéal pour les créations qui nécessitent des coutures souples, comme les masques en cuir par exemple.",ep:21,g:658,l:100,wd:true,fm:true,w:1,p:10};
I.u[8408] = {n:"Sabot du Minotot",nn:"SABOT DU MINOTOT",t:47,d:"Ce sabot a foulé énormément d\'aventuriers, mais aussi de grosses bouses égarées dans le labyrinthe du Minotoror. A la décharge du Minotot, il faut reconnaître qu\'il n\'est parfois pas évident de faire la différence au premier coup d\'oeil.",ep:21,g:754,l:400,wd:true,fm:true,w:10,p:100};
I.u[8409] = {n:"Corne du Minotot",nn:"CORNE DU MINOTOT",t:47,d:"Cette corne ressemble de loin à une corne de Bouftou géante et extrêmement magique. Fait surprenant, de près, elle ressemble à une corne de Minotot.",ep:21,g:655,l:400,wd:true,fm:true,w:1,p:10};
I.u[8410] = {n:"Fémur du Minotot",nn:"FEMUR DU MINOTOT",t:47,d:"Cet os imposant a réussi à supporter pendant plusieurs siècles, la surcharge pondérale du Minotot. Un exploit qui méritait d\'être signalé.",ep:21,g:656,l:400,wd:true,fm:true,w:10,p:100};
I.u[8411] = {n:"Amulette du Mominotor",nn:"AMULETTE DU MOMINOTOR",t:1,d:"Cette amulette maudite recouverte de bandelettes vous permettra peut-être de survivre à la malédiction du labyrinthe du Minotoror. Mais rien n\'est moins sûr, peut-être n\'est-ce qu\'un bibelot destiné à épater les touristes.",ep:0,g:114,l:111,wd:false,fm:true,w:5,s:59,p:31300};
I.u[8412] = {n:"Anneau du Mominotor",nn:"ANNEAU DU MOMINOTOR",t:9,d:"Cet anneau confectionné à partir de bandelettes de Mominotor enroulées sur elles-mêmes, sent plutôt mauvais,  et est assez laid, mais fera un très bon pansement lorsque vous vous coincerez un doigt dans une des portes du labyrinthe du Minotoror.",ep:0,g:119,l:111,wd:false,fm:true,w:10,s:59,p:31525};
I.u[8413] = {n:"Marteau du Mominotor",nn:"MARTEAU DU MOMINOTOR",t:7,d:"Le bloc de pierre à l\'extrémité de ce marteau, est enroulé dans des bandelettes du Mominotor afin d\'absorber les fluides qui s\'échapperont des créatures que vous écraserez, ou tout simplement leur faire moins mal, et ne pas les tuer trop rapidement.",ep:0,g:60,l:112,wd:false,fm:true,w:10,an:15,tw:true,e:[5,4,1,1,50,50,false,false],c:"CC>100&CI>200&CW>150",s:59,p:100};
I.u[8414] = {n:"Dagues du Dragoeuf",nn:"DAGUES DU DRAGOEUF",t:5,d:"Ces dagues sont idéales pour découper le haut d\'un oeuf de Dragoeuf cuit à la coque. Elles sont aussi très légères, ce qui permet de courir vite lorsque le reste de la communauté des Dragoeufs s\'en aperçoit.",ep:20,g:58,l:128,wd:true,fm:true,w:10,an:13,tw:true,e:[10,3,1,1,30,30,false,true],c:"CS>150&CI>150&CA>100",p:39854};
I.u[8415] = {n:"Bottes du Mominotor",nn:"BOTTES DU MOMINOTOR",t:11,d:"Rembourrées et décorées avec des bandelettes de Mominotor, ces bottes sont à la fois très confortables et très chaudes. Elles vous rappellent peut-être quelqu\'un.",ep:0,g:92,l:110,wd:false,fm:true,w:10,s:59,p:31255};
I.u[8416] = {n:"Cape du Mominotor",nn:"CAPE DU MOMINOTOR",t:17,d:"Confectionnée à partir de bandelettes du Mominotor tressées entre elles, cette cape vous donnera un style tout droit sorti d\'outre-tombe. Vos ennemis risquent de crever de jalousie.",ep:0,g:122,l:111,wd:false,fm:true,w:10,s:59,p:31780};
I.u[8417] = {n:"Pelle à Gâteau",nn:"PELLE A GATEAU",t:8,d:"Cette petite pelle a été offerte a un jeune Enutrof, il y est resté si attaché qu\'il l\'utilisait encore juste avant sa mort.",ep:20,g:43,l:131,wd:true,fm:true,w:10,an:13,tw:true,e:[10,3,1,1,30,30,false,true],c:"CC>250&CA>100",p:42514};
I.u[8418] = {n:"Ceinture du Mominotor",nn:"CEINTURE DU MOMINOTOR",t:10,d:"Cette grosse bandelette de Mominotor possède une valeur ajoutée extraordinaire, dont l\'origine reste cependant méconnue. Mais qu\'importe, elle se vendra cher, et sa malédiction accélèrera la putréfaction des hanches des amis à qui vous offrirez cette ceinture, ce qui est il faut bien l\'avouer, tout simplement inestimable.",ep:0,g:108,l:112,wd:false,fm:true,w:10,s:59,p:32255};
I.u[8419] = {n:"Pelle Moh",nn:"PELLE MOH",t:8,d:"Avec cette pelle, Le capitaine Machab a éventré Grobill Dick, le grand Kralamoure Blanc.",ep:20,g:42,l:115,wd:true,fm:true,w:20,an:16,tw:true,e:[10,4,1,1,50,50,false,true],c:"CC>200",p:33653};
I.u[8420] = {n:"Pelle Dragoeuf",nn:"PELLE DRAGOEUF",t:8,d:"Les Dragoeufs aiment creuser des galeries, mais ne se servent pas de pelles, la communauté Enutrof a longtemps étudié l\'énigme de celle-ci, et a fini par conclure qu\'il était moins important de savoir d\'où elle venait que de taper avec.",ep:20,g:45,l:127,wd:true,fm:true,w:20,an:16,tw:true,e:[10,4,1,1,40,40,false,true],c:"CC>200&CS>100",p:42154};
I.u[8421] = {n:"Certificat de Mise en Chanil : Vilain Petit Corbac",nn:"CERTIFICAT DE MISE EN CHANIL : VILAIN PETIT CORBAC",t:77,d:"Ce certificat décrit les caractéristiques du Vilain Petit Corbac que vous avez placé au chanil.",ep:3,g:41,l:1,wd:false,fm:true,w:1,p:100};
