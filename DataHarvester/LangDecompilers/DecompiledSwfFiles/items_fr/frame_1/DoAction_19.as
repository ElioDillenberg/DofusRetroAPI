I.u[8249] = {n:"Plume de Buveur",nn:"PLUME DE BUVEUR",t:53,d:"Cette plume usée et imbibée d\'alcool fort, est très inflammable. Très pratique pour cuisiner un Buveur rôti.",ep:13,g:2,l:1,wd:true,fm:true,w:1,p:1};
I.u[8250] = {n:"Poil de Renarbo",nn:"POIL DE RENARBO",t:54,d:"Le Renarbo adore se travestir en Corbac, personne ne sait réellement pourquoi, même si les esprits les plus mal tournés ont leur avis sur la question...",ep:13,g:5,l:1,wd:true,fm:true,w:1,p:1};
I.u[8251] = {n:"Poil de Péki",nn:"POIL DE PEKI",t:54,d:"Qui aurait pu croire que le Péki accepterait de se délester d\'une touffe de poils pour vos beaux yeux ? Personne, à part Joche Banette, célèbre idiot du village d\'Amakna, réputé pour son affection maladive pour les Pékis empaillés.",ep:18,g:6,l:1,wd:true,fm:true,w:1,p:1};
I.u[8252] = {n:"Plume de Corbac Apprivoisé",nn:"PLUME DE CORBAC APPRIVOISE",t:53,d:"Cette plume bien grasse est celle d\'un Corbac Apprivoisé, qui a passé la majeure partie de son existence, enfermé, et gavé jusqu\'à ce qu\'il occupe totalement l\'espace de sa cage.",ep:13,g:3,l:1,wd:true,fm:true,w:1,p:1};
I.u[8253] = {n:"Amulette en Mousse",nn:"AMULETTE EN MOUSSE",t:1,d:"Cette amulette est souvent confondue avec un jouet à trouver dans des pochettes surprises, alors qu\'en fait elle se gagne au stand de tir.",ep:1,g:106,l:11,wd:true,fm:true,w:2,s:63,p:525};
I.u[8254] = {n:"Anneau en Mousse",nn:"ANNEAU EN MOUSSE",t:9,d:"Offert en alliance, cet anneau permet de faire passer deux messages à l\'être aimé : " + "\"" + "Je t\'aime" + "\"" + " et " + "\"" + "Va faire la vaisselle" + "\"" + ". Idéal pour ne pas perdre son temps en répétant tous les soirs la même chose.",ep:1,g:112,l:10,wd:true,fm:true,w:2,s:63,p:500};
I.u[8255] = {n:"Pelle en Mousse",nn:"PELLE EN MOUSSE",t:8,d:"Cette arme terriblement puissante contre les mottes de terre, a marqué plus d\'un aventurier débutant. Une fois mouillée, elle devient molle et peut être roulée, mais il faut généralement être à deux pour y arriver.",ep:1,g:44,l:10,wd:true,fm:true,w:10,an:16,e:[5,5,1,1,50,50,false,true],s:63,p:500};
I.u[8256] = {n:"Bottes en Mousse",nn:"BOTTES EN MOUSSE",t:11,d:"Avec ces bottes, un Eniripsa peut marcher sur l\'eau, ce qui ne sert à rien puisqu\'il peut voler, mais ça impressionne toujours les foules. Les autres pourront toujours se servir de ces bottes pour se gratter les pieds en marchant, ou tout simplement pour les nettoyer.",ep:1,g:84,l:11,wd:true,fm:true,w:5,s:63,p:525};
I.u[8257] = {n:"Ceinture en Mousse",nn:"CEINTURE EN MOUSSE",t:10,d:"Cette ceinture permettait à Vald de prendre son bain sans danger car aucun Iop n\'a encore réussi à apprendre à nager.",ep:1,g:102,l:10,wd:true,fm:true,w:5,s:63,p:500};
I.u[8258] = {n:"Cape en Mousse",nn:"CAPE EN MOUSSE",t:17,d:"Cette cape est idéale pour le camouflage en milieu naturel, mais uniquement s\'il n\'y a personne pour vous voir.",ep:1,g:109,l:10,wd:true,fm:true,w:1,s:63,p:505};
I.u[8259] = {n:"Coiffe en Mousse",nn:"COIFFE EN MOUSSE",t:16,d:"Sur la notice de cette coiffe, on peut lire :" + "\"" + "Offrez cette coiffe à votre mari, accrochez-la solidement sur sa tête et traînez-le par les pieds dans votre cuisine. Voilà le sol est propre désormais" + "\"" + ". Cela résume un peu la conception moderne du ménage et de la répartition des tâches ménagères au sein des couples épanouïs et qui s\'assument.",ep:1,g:143,l:11,wd:true,fm:true,w:10,s:63,p:525};
I.u[8260] = {n:"Bandeau Kaliptus",nn:"BANDEAU KALIPTUS",t:16,d:"Avec ce bandeau, les Iops n\'auront jamais été aussi séduisants !",ep:20,g:124,l:133,wd:true,fm:true,w:10,p:43500};
I.u[8262] = {n:"La Meulette",nn:"LA MEULETTE",t:1,d:"La Meulette renferme une étrange substance trouvée dans les entrailles du Meulou, d\'aucuns prétendent qu\'il s\'agit de l\'essence du monstre, d\'autres pensent qu\'il avait juste dévoré un Kirboule pas très frais.",ep:20,g:107,l:143,wd:true,fm:true,w:1,s:55,p:44850};
I.u[8263] = {n:"Anneau du Meulou",nn:"ANNEAU DU MEULOU",t:9,d:"Attention, cet anneau rendra votre doigt particulièrement malodorant. Une odeur qui risque d\'en couvrir une autre. C\'est peut-être pour votre doigt et votre entourage une très bonne chose...",ep:20,g:113,l:126,wd:true,fm:true,w:10,s:55,p:40190};
I.u[8264] = {n:"Bottes du Meulou",nn:"BOTTES DU MEULOU",t:11,d:"Ces bottes sont encore animées d\'une partie de l\'appétit vorace du Meulou. Si vos pieds sentent le Clakoss, elles risquent de vous les mordre.",ep:18,g:85,l:131,wd:true,fm:true,w:20,s:55,p:42840};
I.u[8265] = {n:"Cape du Meulou",nn:"CAPE DU MEULOU",t:17,d:"Le Meulou a la queue si belle et si longue qu\'elle traîne au sol. Certains aventuriers jaloux la lui ont arrachée et la portent, espérant sûrement y gagner en virilité.",ep:20,g:110,l:139,wd:true,fm:true,w:10,s:55,p:44500};
I.u[8266] = {n:"Ceinture du Meulou",nn:"CEINTURE DU MEULOU",t:10,d:"Cette ceinture colorée est dotée d\'une superbe touffe violacée sur le devant, vous pouvez également la porter à l\'envers pour vous travestir en Wabbit.",ep:20,g:103,l:125,wd:true,fm:true,w:1,s:55,p:1};
I.u[8267] = {n:"Coiffe du Meulou",nn:"COIFFE DU MEULOU",t:16,d:"En dépeçant le Meulou, des chasseurs se sont aperçus qu\'il ne possède qu\'une seule grosse corne qui traverse son crâne de part en part, ce qui ne gène nullement les Iops qui la portent avec enthousiasme non dissimulé.",ep:4,g:144,l:141,wd:true,fm:true,w:10,s:55,p:44600};
I.u[8268] = {n:"Collier du Minotoror",nn:"COLLIER DU MINOTOROR",t:1,d:"Ce collier permettait au Minotoror de ne pas avoir de puces, de tiques, ou même de poux. De toute évidence, vous en aviez besoin.",ep:18,g:112,l:111,wd:true,fm:true,w:1,s:54,p:31260};
I.u[8269] = {n:"Anneau du Minotoror",nn:"ANNEAU DU MINOTOROR",t:9,d:"Contrairement à ce que certains aventuriers rapportent, cet anneau n\'a jamais été porté par le Minotoror, incapable d\'enfiler un tel anneau autour de ses sabots.",ep:21,g:117,l:105,wd:true,fm:true,w:1,s:54,p:28010};
I.u[8270] = {n:"Bracelet du Minotot",nn:"BRACELET DU MINOTOT",t:9,d:"Ce bracelet permet de protéger entièrement le bras d\'un Eniripsa, mais triple son poids.",ep:21,g:116,l:151,wd:true,fm:true,w:10,s:57,p:45425};
I.u[8271] = {n:"Anneau du Dragon Cochon",nn:"ANNEAU DU DRAGON COCHON",t:9,d:"Cet anneau en cuir et en ivoire permet de stimuler plus facilement vos partenaires.",ep:18,g:115,l:108,wd:true,fm:true,w:1,s:58,p:29600};
I.u[8272] = {n:"Collier du Minotot",nn:"COLLIER DU MINOTOT",t:1,d:"Ce collier permettait aux Minotot, de protéger ses cervicales et d\'éviter de périr à cause d\'un stupide coup du Wabbit.",ep:21,g:111,l:158,wd:true,fm:true,w:20,s:57,p:47800};
I.u[8273] = {n:"Collier du Dragon Cochon",nn:"COLLIER DU DRAGON COCHON",t:1,d:"Ce collier dispose d\'une petite boucle, qui permettait d\'accrocher une laisse au cou du Dragon Cochon, lorsque son propriétaire voulait l\'emmener faire une petite promenade nocturne.",ep:21,g:110,l:100,wd:true,fm:true,w:1,s:58,p:25470};
I.u[8274] = {n:"Hache du Minotoror",nn:"HACHE DU MINOTOROR",t:19,d:"Cette hache a permis de trancher la tête d\'un des plus dangereux Minotorors que la province d\'Amakna ait connu. Le plus incroyable dans cette histoire c\'est qu\'elle semble vous laisser indifférent.",ep:21,g:44,l:109,wd:true,fm:true,w:10,an:17,tw:true,e:[5,5,1,1,40,50,false,true],s:54,p:30110};
I.u[8275] = {n:"Sceptre du Minotot",nn:"SCEPTRE DU MINOTOT",t:4,d:"Ce Sceptre orné à son extrémité d\'une tête de Serpentin en pierres précieuses, permet d\'effrayer les jeunes aventuriers, afin de les faire fuir, et de botter leurs petites fesses toutes roses.",ep:21,g:67,l:151,wd:true,fm:true,w:50,an:12,tw:true,e:[5,4,1,1,40,50,false,false],s:57,p:46200};
I.u[8276] = {n:"Bottes du Minotoror",nn:"BOTTES DU MINOTOROR",t:11,d:"De toute évidence, ces terribles bottes en poils et en cuir de Minotoror, sont bien trop lourdes pour vous faire gagner la légendaire course à pieds de la baie d\'Amakna. Cependant, elles devraient vous permettre d\'écraser facilement les pieds des autres concurrents sur la ligne de départ.",ep:21,g:89,l:113,wd:true,fm:true,w:40,s:54,p:32290};
I.u[8277] = {n:"Sandales du Minotot",nn:"SANDALES DU MINOTOT",t:11,d:"Ces incroyables sandales vous permettront de vous baigner dans les eaux les plus dangereuses, sans risquer de vous abîmer les pieds. Attention cependant, elles ont la réputation de rayer le fond des baignoires.",ep:21,g:88,l:163,wd:true,fm:true,w:20,s:57,p:48400};
I.u[8278] = {n:"Mules du Dragon Cochon",nn:"MULES DU DRAGON COCHON",t:11,d:"Une bonne paire de mules et vous vous sentez de suite plus fort, plus puissant, plus performant. Alors que ce ne sont que des mules.",ep:20,g:87,l:93,wd:true,fm:true,w:5,s:58,p:22800};
I.u[8279] = {n:"Cape du Minotoror",nn:"CAPE DU MINOTOROR",t:17,d:"Prenez un Minotoror, attachez-le fermement, prenez un rabot géant, et pelez-lui (en un seul passage) le dos. Voilà, c\'est fait, vous avez désormais entre les mains, une cape du Minotoror.",ep:21,g:117,l:109,wd:true,fm:true,w:10,s:54,p:30500};
I.u[8280] = {n:"Cape du Minotot",nn:"CAPE DU MINOTOT",t:17,d:"Pour sortir en soirée, ou séduire au bal masqué du Captain Chafer, c\'est la cape qu\'il vous faut.",ep:21,g:116,l:156,wd:true,fm:true,w:40,s:57,p:48850};
I.u[8281] = {n:"Cape du Dragon Cochon",nn:"CAPE DU DRAGON COCHON",t:17,d:"Cette cape ne fera pas de vous un petit Cochon de Lait, mais elle vous permettra peut-être de vous travestir l\'espace d\'une soirée, afin de séduire une belle Cochonne.",ep:20,g:118,l:103,wd:true,fm:true,w:10,s:58,p:27500};
I.u[8282] = {n:"Ceinture du Minotoror",nn:"CEINTURE DU MINOTOROR",t:10,d:"Cette ceinture est si souple, qu\'elle peut servir à fouetter les fesses des enfants trop turbulents. Avec un peu de chance, ses ferrures permettront d\'arracher quelques lambeaux de chair, et les terribles garnements qui vous servent de marmaille auront compris une bonne fois pour toute la leçon.",ep:21,g:106,l:111,wd:true,fm:true,w:1,s:54,p:1};
I.u[8283] = {n:"Ceinture du Minotot",nn:"CEINTURE DU MINOTOT",t:10,d:"Cette ceinture rigide, permettait au Minotot, de contenir son énorme bedaine, afin de ne pas basculer en avant au moindre courant d\'air.",ep:21,g:105,l:149,wd:true,fm:true,w:30,s:57,p:49950};
I.u[8284] = {n:"Coiffe du Minotot",nn:"COIFFE DU MINOTOT",t:16,d:"Cette coiffe se transmet dans la famille du Minotot, de génération en génération. Ce qui explique en partie son odeur de pellicules en décomposition.",ep:21,g:151,l:157,wd:true,fm:true,w:20,s:57,p:47652};
I.u[8285] = {n:"Coiffe du Dragon Cochon",nn:"COIFFE DU DRAGON COCHON",t:16,d:"Affublé de cet énorme crâne, vous aurez l\'air d\'un Grizboule à poils durs, mais sans poils. De quoi laisser sans voix vos admiratrices.",ep:20,g:150,l:98,wd:true,fm:true,w:10,s:58,p:24500};
I.u[8286] = {n:"Cape Dragoeuf",nn:"CAPE DRAGOEUF",t:17,d:"Cette cape recouverte d\'écailles vous protègera d\'après la légende, de la foudre, des haricots, et surtout des femmes fatales.",ep:11,g:115,l:127,wd:true,fm:true,w:10,s:56,p:41000};
I.u[8287] = {n:"Casque Dragoeuf",nn:"CASQUE DRAGOEUF",t:16,d:"Ce casque absolument terrifiant et d\'une taille démesurée ferait passer un petit chérubin comme vous, pour un impitoyable guerrier venu de l\'au delà. Cloué sur un mur, il fera un excellent porte manteau.",ep:11,g:149,l:128,wd:true,fm:true,w:10,s:56,p:41500};
I.u[8288] = {n:"Ceinture du Dragoeuf",nn:"CEINTURE DU DRAGOEUF",t:10,d:"Les esprits les plus pervertis d\'Amakna, racontent que cette ceinture pouvait servir de prothèse. Personne ne comprend pourquoi, et heureusement il n\'en n\'est rien, ce n\'est qu\'une banale ceinture du Dragoeuf.",ep:20,g:104,l:124,wd:true,fm:true,w:1,s:56,p:1};
I.u[8289] = {n:"Anneau du Dragoeuf",nn:"ANNEAU DU DRAGOEUF",t:9,d:"Cet anneau directement taillé dans une écaille géante de Dragoeuf protègera une petite partie d\'une de vos phalanges. Sûrement la plus importante.",ep:20,g:114,l:128,wd:true,fm:true,w:10,s:56,p:41468};
I.u[8290] = {n:"Amulette du Dragoeuf",nn:"AMULETTE DU DRAGOEUF",t:1,d:"Confectionnée à partir d\'écailles de Dragoeufs, cette amulette protégera efficacement votre pomme d\'Adam, ou vos amygdales.  De quoi faire la différence sur un champ de bataille.",ep:11,g:108,l:124,wd:true,fm:true,w:1,s:56,p:38930};
I.u[8291] = {n:"Bottes du Dragoeuf",nn:"BOTTES DU DRAGOEUF",t:11,d:"Ces bottes sont parfaites pour tuer le Bouftou lors d\'une partie de Boufball ou pour faire souffrir un peu plus vos ennemis lors d\'un botte-fesses.",ep:20,g:86,l:129,wd:true,fm:true,w:20,s:56,p:41500};
I.u[8292] = {n:"Epée du Dragoeuf",nn:"EPEE DU DRAGOEUF",t:6,d:"Aiguisée comme le rasoir du soir, cette lame vous permettra tout de même de vous raser le matin, mais uniquement les aisselles.",ep:11,g:68,l:127,wd:true,fm:true,w:30,an:14,e:[8,5,1,1,40,50,false,true],c:"CS>200&CA>200",p:41025};
I.u[8293] = {n:"Hache du Dragoeuf",nn:"HACHE DU DRAGOEUF",t:19,d:"Cette hache fend le beurre aussi facilement que les crânes, à moins que ça ne soit l\'inverse.",ep:20,g:43,l:126,wd:true,fm:true,w:20,an:17,tw:true,e:[5,4,1,1,40,50,false,false],c:"CC>180&CW>80",p:40854};
I.u[8294] = {n:"Marteau du Dragoeuf",nn:"MARTEAU DU DRAGOEUF",t:7,d:"Plus lourd qu\'un crâne vide de Krokeur, mais moins solide qu\'un fémur de Kirboule des cavernes, ce marteau reste néanmoins une arme redoutable.",ep:20,g:59,l:123,wd:true,fm:true,w:30,an:15,e:[10,5,1,1,45,50,false,true],c:"CI>200&CC>100",p:38542};
I.u[8295] = {n:"Arc du Dragoeuf",nn:"ARC DU DRAGOEUF",t:2,d:"Cet arc est si puissant qu\'il permet d\'envoyer des oeufs pourris de Dragoeufs au dessus des murailles de Brâkmar. Quel beau gâchis !",ep:20,g:55,l:122,wd:true,fm:true,w:20,an:10,tw:true,e:[10,5,2,8,40,50,false,true],c:"CS>300&CV>250",p:38015};
I.u[8296] = {n:"Baguette du Dragoeuf",nn:"BAGUETTE DU DRAGOEUF",t:3,d:"Cette baguette semble plus efficace lorsqu\'elle est utilisée comme un gourdin ou comme une tapette à Moskitos.",ep:21,g:60,l:124,wd:true,fm:true,w:10,an:11,e:[5,3,2,5,30,30,false,true],c:"CI>100&CA>200",p:37884};
I.u[8297] = {n:"Bâton du Dragoeuf",nn:"BATON DU DRAGOEUF",t:4,d:"Ce bâton est muni à son extrémité, d\'une grosse pointe dont personne ne connaît l\'origine. En revanche, les aventuriers avertis connaissent sa destination...",ep:20,g:66,l:124,wd:true,fm:true,w:30,an:12,tw:true,e:[5,5,1,1,30,50,false,true],c:"CS>300&CA>150&CV>300",p:39800};
I.u[8298] = {n:"Epis de Shika",nn:"EPIS DE SHIKA",t:1,d:"Shika assistait Farle lors des moissons, en récupérant les épis magiques qui tombaient sous sa faux. Elle prit l\'habitude au fil des moissons, de les accrocher autour de son cou. Privés de leurs racines et du reste de leur tige, ces épis magiques continuent tout de même de mûrir chaque année à la même période.",ep:18,g:113,l:80,wd:true,fm:true,w:5,s:53,p:16500};
I.u[8299] = {n:"Bracelet Magique de Shika",nn:"BRACELET MAGIQUE DE SHIKA",t:9,d:"Ce bracelet couvrait les bras de Shika, la protégeant des coups de soleil, et des coups de bec des Corbacs attirés par sa beauté rayonnante. Ils sont encore imprégnés de sa sueur suave.",ep:21,g:118,l:80,wd:true,fm:true,w:5,s:53,p:16500};
I.u[8300] = {n:"Bâton de Shika",nn:"BATON DE SHIKA",t:4,d:"Ce bâton taillé sur mesure, permettait à Shika de créer les meules de foin les plus hautes de tout Amakna. La légende raconte que leur taille était si importante, que les marins s\'en servaient de points de repères pour savoir s\'ils approchaient ou non de la côte d\'Amakna.",ep:20,g:69,l:80,wd:true,fm:true,w:10,an:12,e:[10,5,1,1,45,45,false,false],s:53,p:16500};
I.u[8301] = {n:"Sabots de Shika",nn:"SABOTS DE SHIKA",t:11,d:"Ces sabots ont été conçus pour permettre à Shika de ne jamais écraser les semis, lorsqu\'elle ensemençait les plaines de Snossios. Elle y fit rajouter des talons hauts, pour qu\'elle puisse courir librement le matin dans les éteules, sans remplir ses sabots de rosée, ou sans abîmer ses chevilles fragiles.",ep:21,g:91,l:80,wd:true,fm:true,w:10,s:53,p:16600};
I.u[8302] = {n:"Cape de Shika",nn:"CAPE DE SHIKA",t:17,d:"Des fleurs des moissons ont été cousues sur cette cape confectionnée à partir de céréales magiques tissées entre elles. Elle est animée d\'une magie si puissante, que Shika s\'en servait de protection contre la foudre, les soirs d\'orage.",ep:20,g:120,l:80,wd:true,fm:true,w:10,s:53,p:16500};
I.u[8303] = {n:"Shikature",nn:"SHIKATURE",t:10,d:"Cette ceinture en cuir protégeait les cuisses de Shika, lorsqu\'elle traversait des champs de blé en courant pour rattraper les Bouftous égarés de son troupeau. Recouverte de pollen de céréales magiques, on raconte qu\'elle brillait au clair de lune, et permettait à Farle de ne pas sectionner les jambes de sa propre femme lorsqu\'il fauchait des champs entiers de céréales une fois la nuit tombée.",ep:21,g:107,l:80,wd:true,fm:true,w:10,s:53,p:16500};
I.u[8304] = {n:"Shikacoiffe",nn:"SHIKACOIFFE",t:16,d:"Recouverte d\'une multitude de fleurs des moissons, cette coiffe donnait une allure guillerette à Shika, lorsqu\'elle partait au petit matin, labourer les collines Koln.",ep:20,g:156,l:80,wd:true,fm:true,w:10,s:53,p:16500};
I.u[8305] = {n:"Relique du Mominotor",nn:"RELIQUE DU MOMINOTOR",t:24,d:"Cette étrange relique est indispensable pour pouvoir rencontrer le Minotoror.",ep:21,g:119,l:1,wd:false,fm:true,w:0,p:1};
I.u[8306] = {n:"Relique du Déminoboule",nn:"RELIQUE DU DEMINOBOULE",t:24,d:"Cette étrange relique est indispensable pour pouvoir rencontrer le Minotoror.",ep:21,g:120,l:1,wd:false,fm:true,w:0,p:1};
I.u[8307] = {n:"Clef de la salle du Minotot",nn:"CLEF DE LA SALLE DU MINOTOT",t:84,d:"Cette clef construite à partir de diverses ressources, vous permettra de rencontrer l\'incroyable Minotot. Une rencontre qui vous marquera à vie, lorsque le Minotot vous aura envoyé quelques coups de sabots dans le derrière.",ep:13,g:15,l:1,wd:true,fm:true,w:1,p:10};
I.u[8308] = {n:"Carapace de Scaratos",nn:"CARAPACE DE SCARATOS",t:107,d:"Cette petite carapace luisante est très pratique pour créer de beaux bijoux à moindre frais, que vous revendrez une fortune, une fois que vous aurez persuadé un Astrubien, que c\'est une puissante relique qui permet de voler dans les airs en se rendant invisible.",ep:21,g:735,l:1,wd:true,fm:true,w:1,p:10};
I.u[8309] = {n:"Plume du Serpiplume",nn:"PLUME DU SERPIPLUME",t:53,d:"Cette plume est recouverte de poussière de pierre magique, ce qui explique qu\'une unité de plume de Serpiplume, pèse plus lourd que trois unités de plume de Tofu. Tout est une question de logique.",ep:21,g:4,l:1,wd:true,fm:true,w:10,p:1};
I.u[8310] = {n:"Gelée de Dégelée",nn:"GELEE DE DEGELEE",t:110,d:"Ce pot de gelée contient une substance à l\'odeur repoussante et au goût épouvantable, et pourtant, vous rêvez tous les jours secrètement de vous enduire de cette substance. Un autre trouble de votre comportement, qui doit en cacher d\'autres...",ep:21,g:736,l:1,wd:true,fm:true,w:1,p:1};
I.u[8311] = {n:"Poil de Gamino",nn:"POIL DE GAMINO",t:54,d:"Ce poil semble recouvert d\'une sorte de gomina à base de musc de Bitouf à poils ras, et de graisse de Kirboule des cavernes. On aurait presque envie de les manger.",ep:21,g:9,l:1,wd:true,fm:true,w:1,p:1};
I.u[8312] = {n:"Peau de Minoskito",nn:"PEAU DE MINOSKITO",t:59,d:"Cette peau usée par le sable fin et le sel de mer est fine comme une vieille chaussette usée par le sable fin et le sel de mer. De là à dire que le Minoskito ressemble à une vieille chaussette, il n\'y a qu\'un pas, que vous ne franchirez pas.",ep:21,g:604,l:1,wd:true,fm:true,w:1,p:1};
I.u[8313] = {n:"Fémur du Minotoror",nn:"FEMUR DU MINOTOROR",t:47,d:"Cet os est assez solide pour servir de poutrelle, ou pour casser des briques par lots de dix. Très pratique pour détruire la maison de ses voisins, mais complètement inutile pour les personnes saines d\'esprit.",ep:2,g:644,l:1,wd:true,fm:true,w:1,p:1};
I.u[8314] = {n:"Peau de Kraméléhon",nn:"PEAU DE KRAMELEHON",t:59,d:"Cette peau brûlée protège efficacement les peaux sensibles, des rayons du soleil. A porter très près du corps.",ep:21,g:603,l:1,wd:true,fm:true,w:1,p:1};
I.u[8315] = {n:"Peau de Mandrine",nn:"PEAU DE MANDRINE",t:59,d:"Cette peau tachetée a la réputation d\'être recouverte d\'un poison capable de transformer n\'importe qui en Karaboule desséché. Inquiétant, mais tout de même tentant.",ep:21,g:602,l:1,wd:true,fm:true,w:1,p:1};
I.u[8316] = {n:"Le Tifoux : Bien l\'élever",nn:"LE TIFOUX : BIEN L\'ELEVER",t:25,d:"Ce livre écrit par Oshimo Li\'kross Babine explique comment élever un Tifoux.",ep:3,g:10,l:1,wd:false,fm:true,w:1,u:true,p:20};
I.u[8317] = {n:"El Scarador : Bien l\'élever",nn:"EL SCARADOR : BIEN L\'ELEVER",t:25,d:"Ce livre écrit par Oshimo Li\'kross Babine explique comment élever El Scarador.",ep:3,g:10,l:1,wd:false,fm:true,w:1,u:true,p:20};
I.u[8318] = {n:"Abra Kadabra : Bien l\'élever",nn:"ABRA KADABRA : BIEN L\'ELEVER",t:25,d:"Ce livre écrit par Oshimo Li\'kross Babine explique comment élever Abra Kadabra.",ep:3,g:10,l:1,wd:false,fm:true,w:1,u:true,p:20};
I.u[8319] = {n:"Bouloute : Bien l\'élever",nn:"BOULOUTE : BIEN L\'ELEVER",t:25,d:"Ce livre écrit par Oshimo Li\'kross Babine explique comment élever Bouloute.",ep:3,g:10,l:1,wd:false,fm:true,w:1,u:true,p:20};
I.u[8320] = {n:"Clef de l\'antre du Dragon Cochon",nn:"CLEF DE L\'ANTRE DU DRAGON COCHON",t:84,d:"Après être entré et vous être perdu dans le labyrinthe du Dragon Cochon, vous vous êtes aperçu que vous aviez oublié de prendre vos clefs de l\'antre du Dragon Cochon. Heureusement, vos amis les Porcos avaient un double des clefs sur eux.",ep:15,g:16,l:1,wd:true,fm:true,w:1,p:100};
I.u[8321] = {n:"Poil de Déminoboule",nn:"POIL DE DEMINOBOULE",t:54,d:"Inutile d\'insister, ces poils ne sont pas comestibles.",ep:21,g:664,l:100,wd:true,fm:true,w:1,p:10};
I.u[8322] = {n:"Poil de Mominotor",nn:"POIL DE MOMINOTOR",t:54,d:"Sous ses bandelettes, le Mominotor cache un duvet très doux qui rendrait jalouse la plus coquette des princesses.",ep:21,g:663,l:100,wd:true,fm:true,w:1,p:10};
I.u[8323] = {n:"Cuir de Déminoboule",nn:"CUIR DE DEMINOBOULE",t:56,d:"Craquelé et rugueux, ce cuir vous rappellera les joues de votre dulcinée.",ep:21,g:657,l:100,wd:true,fm:true,w:1,p:10};
I.u[8324] = {n:"Cuir de Mominotor",nn:"CUIR DE MOMINOTOR",t:56,d:"Le Mominotor a toujours eu la peau très fragile, c\'est pour cela qu\'il doit se recouvrir de nombreuses bandelettes. Les mauvaises langues racontent que c\'est pour cacher d\'autres choses assez surprenantes...",ep:21,g:656,l:100,wd:true,fm:true,w:1,p:10};
I.u[8325] = {n:"Coiffe du Mominotor",nn:"COIFFE DU MOMINOTOR",t:16,d:"Si vous voulez préserver votre visage des affres du temps, nous vous conseillons cette coiffe. Attention, elle est totalement inefficace pour vous protéger des pertes de mémoire.",ep:0,g:157,l:110,wd:false,fm:true,w:10,s:59,p:100};
I.u[8326] = {n:"Bandelette du Mominotor",nn:"BANDELETTE DU MOMINOTOR",t:15,d:"Enroulée autour des cornes, mais surtout des parties intimes du Mominotor, ces bandelettes se sont imprégnées pendant de longues années de toute la magie du Mominotor. A la fois malodorantes et terrifiantes, ces bandelettes ont leur place dans les collections de tous les archéologues de renom.",ep:21,g:734,l:100,wd:true,fm:true,w:1,p:10};
I.u[8327] = {n:"Corne de Déminoboule",nn:"CORNE DE DEMINOBOULE",t:47,d:"Cette corne impressionnante n\'est à première vue d\'aucune utilité, et même en y regardant de plus près, elle se révèle bien peu utile.  Un peu comme une armée de tiques par exemple.",ep:21,g:643,l:100,wd:true,fm:true,w:1,p:10};
I.u[8328] = {n:"Corne de Mominotor",nn:"CORNE DE MOMINOTOR",t:47,d:"Enroulée dans des bandelettes magiques, cette corne doit de toute évidence valoir très cher, être très précieuse, sentir très mauvais, et être maudite. Un peu comme une momie.",ep:21,g:642,l:100,wd:true,fm:true,w:1,p:10};
I.u[8330] = {n:"Coiffe du Dragoeuf volant",nn:"COIFFE DU DRAGOEUF VOLANT",t:16,d:"Cette petite coiffe permet au Dragoeuf volant de se jeter sur ses ennemis sans risquer de se faire de jolies bosses sur le crâne. Utilisée depuis longtemps sur les grands chantiers d\'Amakna, cette coiffe a connu un regain de popularité après l\'invasion d\'Astrub par l\'illustre troupe des Meulous débridés de la montagne de Kruup. Il n\'y a aucune logique dans cette histoire, mais quelle importance ?",ep:13,g:152,l:72,wd:true,fm:true,w:10,p:16000};
I.u[8331] = {n:"Coiffe de la Dragueuse",nn:"COIFFE DE LA DRAGUEUSE",t:16,d:"Equipé de cette coiffe, vous pourrez sans peine vous travestir et séduire le premier Astrubien un peu trop crédule que vous voudrez arnaquer, en lui proposant une folle nuit d\'amour derrière un grand orme, pour 100 kamas seulement.",ep:18,g:153,l:95,wd:true,fm:true,w:10,p:23000};
I.u[8333] = {n:"Cape du Papa Nowel",nn:"CAPE DU PAPA NOWEL",t:17,d:"Cette mystérieuse cape fait son apparition lors des saisons froides, et permet aux imposteurs de se faire passer pour un vieillard généreux.",ep:1,g:119,l:19,wd:true,fm:true,w:10,p:1205};
I.u[8334] = {n:"Chapeau du Papa Nowel",nn:"CHAPEAU DU PAPA NOWEL",t:16,d:"Ce chapeau vous permettra de cacher les cicatrices que vous avez sur le crâne. Vous pourrez enfin plaire aux enfants.",ep:1,g:155,l:22,wd:true,fm:true,w:10,p:1478};
I.u[8335] = {n:"Tout petit cadeau de Nowel",nn:"TOUT PETIT CADEAU DE NOWEL",t:89,d:"L\'équipe d\'Ankama vous souhaite de joyeuses et agréables fêtes. Ce cadeau vous rendra heureux comme vous ne l\'avez jamais été.",ep:1,g:3,l:1,wd:false,fm:true,w:1,u:true,p:1};
I.u[8336] = {n:"Petit cadeau de Nowel",nn:"PETIT CADEAU DE NOWEL",t:89,d:"L\'équipe d\'Ankama vous souhaite de joyeuses et agréables fêtes. Ce cadeau vous rendra heureux comme vous ne l\'avez jamais été.",ep:1,g:3,l:1,wd:false,fm:true,w:1,u:true,p:1};
I.u[8337] = {n:"Cadeau de Nowel",nn:"CADEAU DE NOWEL",t:89,d:"L\'équipe d\'Ankama vous souhaite de joyeuses et agréables fêtes. Ce cadeau vous rendra heureux comme vous ne l\'avez jamais été.",ep:1,g:3,l:1,wd:false,fm:true,w:1,u:true,p:1};
I.u[8338] = {n:"Epée de Nowel",nn:"EPEE DE NOWEL",t:6,d:"Cette étrange épée ne vous rendra pas invincible, mais vous pourrez peut-être vous en servir pour tuer des boules de neige.",ep:1,g:69,l:1,wd:false,fm:true,w:1,an:14,e:[1,1,1,1,2,2,false,true],p:1};
I.u[8339] = {n:"Grand cadeau de Nowel",nn:"GRAND CADEAU DE NOWEL",t:89,d:"L\'équipe d\'Ankama vous souhaite de joyeuses et agréables fêtes. Ce cadeau vous rendra heureux comme vous ne l\'avez jamais été.",ep:1,g:3,l:1,wd:false,fm:true,w:1,u:true,p:1};
I.u[8340] = {n:"Incroyable cadeau de Nowel",nn:"INCROYABLE CADEAU DE NOWEL",t:89,d:"L\'équipe d\'Ankama vous souhaite de joyeuses et agréables fêtes. Ce cadeau vous rendra heureux comme vous ne l\'avez jamais été.",ep:1,g:3,l:1,wd:false,fm:true,w:1,u:true,p:1};
I.u[8341] = {n:"Bottananas",nn:"BOTTANANAS",t:11,d:"Ces bottes sentent le jus d\'ananas macéré dans du substrat de chaussette trouée. L\'avantage, c\'est qu\'on ne vous demandera jamais de les enlever lorsque vous irez chez un ami.",ep:1,g:90,l:1,wd:true,fm:true,w:10,s:74,p:100};
I.u[8342] = {n:"Clef du Sanctuaire des Dragoeufs",nn:"CLEF DU SANCTUAIRE DES DRAGOEUFS",t:84,d:"Cette clef confectionnée à base de coquilles de Dragoeufs vous permettra d\'accéder au terrible Sanctuaire des Dragoeufs, réputé pour ses habitants peu loquaces.",ep:10,g:18,l:1,wd:true,fm:true,w:1,p:100};
I.u[8343] = {n:"Clef du Donjon des Dragoeufs",nn:"CLEF DU DONJON DES DRAGOEUFS",t:84,d:"Vous pensiez être arrivé au bout de vos peines en rentrant dans le Sanctuaire des Dragoeufs, mais vous vous êtes lamentablement trompé. Cette clef vous mènera peut-être vers une mort certaine.",ep:10,g:17,l:1,wd:true,fm:true,w:1,p:100};
I.u[8344] = {n:"Corne de Dragoss Noir",nn:"CORNE DE DRAGOSS NOIR",t:47,d:"Moins pointue qu\'une aiguille de Xélor, mais plus solide, cette corne permet aux Dragoss de transformer un botte-fesses, en perce-fesses.",ep:11,g:648,l:50,wd:true,fm:true,w:1,p:10};
I.u[8345] = {n:"Corne de Dragoss de Saphire",nn:"CORNE DE DRAGOSS DE SAPHIRE",t:47,d:"Moins pointue qu\'une aiguille de Xélor, mais plus solide, cette corne permet aux Dragoss de transformer un botte-fesses, en perce-fesses.",ep:11,g:647,l:50,wd:true,fm:true,w:1,p:10};
