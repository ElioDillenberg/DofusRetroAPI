I.u[8348] = {n:"Coquille de Dragoss Noir",nn:"COQUILLE DE DRAGOSS NOIR",t:111,d:"Cette coquille peut éventuellement servir à protéger ce que vous avez de plus précieux.",ep:11,g:738,l:50,wd:true,fm:true,w:1,p:10};
I.u[8349] = {n:"Coquille de Dragoss de Saphir",nn:"COQUILLE DE DRAGOSS DE SAPHIR",t:111,d:"Cette coquille peut éventuellement servir à protéger ce que vous avez de plus précieux.",ep:11,g:739,l:50,wd:true,fm:true,w:1,p:10};
