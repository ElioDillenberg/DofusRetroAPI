I.u[8050] = {n:"Peau de Koalak Immature",nn:"PEAU DE KOALAK IMMATURE",t:59,d:"Le petit Koalak Immature à qui vous avez arraché cette peau, n\'était pas d\'accord pour vous la donner, mais qu\'importe, vous étiez plus fort et plus cruel que lui.",ep:20,g:3,l:1,wd:true,fm:true,w:1,p:10};
I.u[8052] = {n:"Peau de Maître Koalak",nn:"PEAU DE MAITRE KOALAK",t:59,d:"Cette peau vielle et usée recouvrait les fesses du Maître Koalak. Elle comporte encore quelques belles traces rouges, témoins d\'une vie agitée et un peu folle.",ep:20,g:5,l:1,wd:true,fm:true,w:1,p:10};
I.u[8053] = {n:"Peau de Chevaucheur Koalak",nn:"PEAU DE CHEVAUCHEUR KOALAK",t:59,d:"Cette peau déchirée a été usée contre celle de la monture du Chevaucheur Koalak.  De toute évidence, c\'est un  gage de qualité.",ep:20,g:6,l:1,wd:true,fm:true,w:1,p:10};
I.u[8054] = {n:"Peau de Drakoalak",nn:"PEAU DE DRAKOALAK",t:59,d:"Cette peau devrait vous rappeler l\'odeur des étables du château d\'Amakna. Très pratique pour effrayer les âmes sensibles, et les tuer avant qu\'elles ne décident de vous accompagner dans un donjon.",ep:20,g:7,l:1,wd:true,fm:true,w:1,p:10};
I.u[8055] = {n:"Os de Mama Koalak",nn:"OS DE MAMA KOALAK",t:47,d:"La Mama Koalak possède des os particulièrement résistants qui ne cèdent pas sous son poids. On ne peut pas en dire autant de Francisque Kabroule, le célèbre Ménestrel, qui perdit l\'usage de ses deux bras en se faisant écraser par les énormes cuisses de la Mama Koalak. La curiosité est un vilain défaut.",ep:20,g:638,l:1,wd:true,fm:true,w:1,p:10};
I.u[8056] = {n:"Fémur de Koalak Farouche",nn:"FEMUR DE KOALAK FAROUCHE",t:47,d:"Une fois taillé en forme de dague, cet os peut faire office de dague, ou de perce-furoncle.",ep:20,g:639,l:1,wd:true,fm:true,w:1,p:10};
I.u[8057] = {n:"Tibia de Koalak Fossoyeur",nn:"TIBIA DE KOALAK FOSSOYEUR",t:47,d:"Cet os peut servir de pelle pour déterrer les cadavres et les dépouiller, si vous aimez passer vos vacances près des cimetières. Accessoirement, il sert de tibia au Koalak Fossoyeur.",ep:20,g:640,l:1,wd:true,fm:true,w:1,p:10};
I.u[8058] = {n:"Cubitus de Momie Koalak",nn:"CUBITUS DE MOMIE KOALAK",t:47,d:"Cet os a gardé toute sa splendeur grâce aux nombreuses bandelettes de la Momie Koalak, qui protègent des attaques magiques les plus puissantes.",ep:20,g:641,l:1,wd:true,fm:true,w:1,p:10};
I.u[8059] = {n:"Poils de Koalak Griotte",nn:"POILS DE KOALAK GRIOTTE",t:54,d:"Ces poils sont recouverts de pollen de Blop, ce qui leur donne un goût délectable, mais une odeur déplorable.",ep:20,g:660,l:1,wd:true,fm:true,w:1,p:10};
I.u[8060] = {n:"Poils de Koalak Koko",nn:"POILS DE KOALAK KOKO",t:54,d:"Ces poils sont recouverts de pollen de Blop, ce qui leur donne un goût délectable, mais une odeur déplorable.",ep:20,g:662,l:1,wd:true,fm:true,w:1,p:10};
I.u[8061] = {n:"Poils de Koalak Reinette",nn:"POILS DE KOALAK REINETTE",t:54,d:"Ces poils sont recouverts de pollen de Blop, ce qui leur donne un goût délectable, mais une odeur déplorable.",ep:20,g:661,l:1,wd:true,fm:true,w:1,p:10};
I.u[8062] = {n:"Poils de Koalak Indigo",nn:"POILS DE KOALAK INDIGO",t:54,d:"Ces poils sont recouverts de pollen de Blop, ce qui leur donne un goût délectable, mais une odeur déplorable.",ep:20,g:659,l:1,wd:true,fm:true,w:1,p:10};
I.u[8063] = {n:"Queue de Koalak Sanguin",nn:"QUEUE DE KOALAK SANGUIN",t:65,d:"Cette ridicule petite queue aurait pû vous appartenir, tant vous êtes insignifiant. Et pourtant il n\'en n\'est rien, estimez-vous heureux.",ep:20,g:641,l:1,wd:true,fm:true,w:1,p:10};
I.u[8064] = {n:"Boomerang du Koalak Sanguin",nn:"BOOMERANG DU KOALAK SANGUIN",t:15,d:"Ce morceau de bois d\'une forme étrange a voyagé longtemps dans les airs et entre les rangs ennemis avant de se briser. On raconte qu\'il revient toujours vers son propriétaire une fois lancé, mais rarement dans ses mains...",ep:20,g:721,l:1,wd:true,fm:true,w:1,p:10};
I.u[8065] = {n:"Poils de Barbe du Warko Marron",nn:"POILS DE BARBE DU WARKO MARRON",t:54,d:"Ces poils sont urticants, assez pour transformer les bises du Warko Marron en armes de destruction massives.",ep:20,g:657,l:1,wd:true,fm:true,w:1,p:10};
I.u[8066] = {n:"Poils de Barbe du Warko Violet",nn:"POILS DE BARBE DU WARKO VIOLET",t:54,d:"Ces poils de barbe sont urticants, ce qui fait de cette barbe à titre de comparaison, une arme de guerre bien plus destructrice que votre haleine de Bwork. Tout simplement effrayant.",ep:20,g:658,l:1,wd:true,fm:true,w:1,p:10};
I.u[8067] = {n:"Bottes du Champion",nn:"BOTTES DU CHAMPION",t:11,d:"Certains aventuriers racontent que ces bottes permettent de marcher sur les pièges sans les déclencher. Attention, c\'est peut-être un piège...",ep:1,g:72,l:1,wd:false,fm:true,w:10,s:45,p:10};
I.u[8068] = {n:"Cape du Champion",nn:"CAPE DU CHAMPION",t:17,d:"Cette splendide cape dispose d\'épaulettes intégrées afin de vous donner plus de prestance pour enfin ressembler à un véritable champion.",ep:1,g:96,l:1,wd:false,fm:true,w:10,ce:true,s:45,p:10};
I.u[8069] = {n:"Coiffe du Champion",nn:"COIFFE DU CHAMPION",t:16,d:"Cette imposante coiffe vous donnera de toute évidence la grosse tête, si vous ne l\'avez pas déjà.",ep:1,g:127,l:1,wd:true,fm:true,w:100,ce:true,s:45,p:10};
I.u[8070] = {n:"Ceinture du Champion",nn:"CEINTURE DU CHAMPION",t:10,d:"Avec cette ceinture autour des hanches, vous cacherez facilement ce qui ne vous met pas en valeur.",ep:1,g:88,l:1,wd:false,fm:true,w:10,s:45,p:10};
I.u[8071] = {n:"Mini Champion",nn:"MINI CHAMPION",t:18,d:"Cette petite créature n\'accepte de suivre que les champions, elle risque donc de vous suivre quelques jours, pour ensuite s\'apercevoir que vous êtes un imposteur. Dès lors, c\'est vous qui la suivrez comme un petit familier.",ep:1,g:32,l:1,wd:true,fm:true,w:10,ce:true,c:"PZ=1",s:45,p:1};
I.u[8072] = {n:"Dofus Kaliptus",nn:"DOFUS KALIPTUS",t:23,d:"Personne ne connaît le dragon à l\'origine de ce Dofus. On raconte juste qu\'il aurait ingéré une quantité effroyable de feuilles de Kaliptus, avant de pondre et de mourir d\'une terrible crise d\'aérophagie.",ep:1,g:10,l:6,wd:false,fm:true,w:1,p:100000};
I.u[8073] = {n:"Clef du Repaire de Skeunk",nn:"CLEF DU REPAIRE DE SKEUNK",t:84,d:"Cette clef vous a été donnée par Rigroulak en échange d\'une panoplie du Koulosse, elle vous permettra de rentrer dans le repaire de Skeunk.",ep:13,g:11,l:1,wd:true,fm:true,w:1,p:100};
I.u[8075] = {n:"Boomerang du Dok Alako",nn:"BOOMERANG DU DOK ALAKO",t:15,d:"Ce morceau de bois d\'une forme étrange a voyagé longtemps dans les airs et entre les rangs ennemis avant de se briser. On raconte qu\'il revient toujours vers son propriétaire une fois lancé, mais rarement dans ses mains...",ep:20,g:722,l:1,wd:true,fm:true,w:1,p:10};
I.u[8076] = {n:"Boomerang du Maître Koalak",nn:"BOOMERANG DU MAITRE KOALAK",t:15,d:"Ce morceau de bois d\'une forme étrange a voyagé longtemps dans les airs et entre les rangs ennemis avant de se briser. On raconte qu\'il revient toujours vers son propriétaire une fois lancé, mais rarement dans ses mains...",ep:20,g:723,l:1,wd:true,fm:true,w:1,p:10};
I.u[8077] = {n:"Boomerang du Warko Marron",nn:"BOOMERANG DU WARKO MARRON",t:15,d:"Ce morceau de bois d\'une forme étrange a voyagé longtemps dans les airs et entre les rangs ennemis avant de se briser. On raconte qu\'il revient toujours vers son propriétaire une fois lancé, mais rarement dans ses mains...",ep:20,g:724,l:1,wd:true,fm:true,w:1,p:10};
I.u[8078] = {n:"Planche en Bois de Kaliptus",nn:"PLANCHE EN BOIS DE KALIPTUS",t:95,d:"Cette planche permet aux bricoleurs de construire des trucs en bois, comme un cercueil en planches par exemple.",ep:20,g:22,l:70,wd:true,fm:true,w:10,p:10};
I.u[8079] = {n:"Transformation Fantôme Petit Chacha Blanc",nn:"TRANSFORMATION FANTOME PETIT CHACHA BLANC",t:27,d:"Vous avez été transformé en fantôme afin de pouvoir pénétrer dans le sanctuaire des familiers.",ep:20,g:3,l:1,wd:false,fm:true,w:0,m:true,p:1};
I.u[8080] = {n:"Transformation Fantôme Petit ChienChien Noir",nn:"TRANSFORMATION FANTOME PETIT CHIENCHIEN NOIR",t:27,d:"Vous avez été transformé en fantôme afin de pouvoir pénétrer dans le sanctuaire des familiers.",ep:20,g:3,l:1,wd:false,fm:true,w:0,m:true,p:1};
I.u[8081] = {n:"Sac de Bois de Charme",nn:"SAC DE BOIS DE CHARME",t:100,d:"Cet énorme sac contient une quantité impressionnante de Bois de Charme.",ep:1,g:55,l:1,wd:true,fm:true,w:250,u:true,p:100};
I.u[8082] = {n:"Etoffe du Fauchalak",nn:"ETOFFE DU FAUCHALAK",t:55,d:"Cette étoffe sent la mort, alors que vous sentez le mort. La différence est notable.",ep:20,g:13,l:1,wd:true,fm:true,w:1,p:100};
I.u[8083] = {n:"Os de Pékeualak",nn:"OS DE PEKEUALAK",t:47,d:"Cet os est spongieux, ce qui lui permet de flotter si le Pékeualak tombe à l\'eau en essayant de remonter une grosse prise.",ep:20,g:10,l:1,wd:true,fm:true,w:1,p:100};
I.u[8084] = {n:"Peau de Piralak",nn:"PEAU DE PIRALAK",t:59,d:"Cette peau recouverte de balafres est devenue plus résistante que la roche, tout en gardant une souplesse remarquable. C\'est la peau idéale pour les martinets et les sandales de mauvais goût.",ep:20,g:8,l:1,wd:true,fm:true,w:1,p:100};
I.u[8085] = {n:"Poils de Koalak Forestier",nn:"POILS DE KOALAK FORESTIER",t:54,d:"Ces poils sont recouverts de sève de Kaliptus, ce qui leur confère un aspect luisant et brillant. Malheureusement, ils sont inutilisables en haute couture à cause de leur odeur de Dragodinde morte.",ep:20,g:4,l:1,wd:true,fm:true,w:1,p:100};
I.u[8086] = {n:"Coiffe déchiquetée de Drakoalak",nn:"COIFFE DECHIQUETEE DE DRAKOALAK",t:15,d:"Cette étrange coiffe cachait jadis la calvitie naissante du Drakoalak. Avec un peu de travail, elle pourra peut-être bientôt cacher la vôtre.",ep:20,g:725,l:1,wd:true,fm:true,w:1,p:100};
I.u[8087] = {n:"Sort Maîtrise de la Hache",nn:"SORT MAITRISE DE LA HACHE",t:73,d:"(Double clic pour apprendre le sort) \nCe parchemin vous permettra de faire plus de dégâts en utilisant une hache.",ep:20,g:8,l:20,wd:false,fm:true,w:1,u:true,p:10000};
I.u[8088] = {n:"Baguette Gyver",nn:"BAGUETTE GYVER",t:3,d:"Cette baguette faite de bric et de broc renferme une énergie surprenante. On raconte qu\'elle est capable de se déplacer elle-même, pour choisir sa cible et défendre son maître même lorsque ce dernier est en train d\'agoniser. Le Chouque aurait même dit à propos de cette baguette " + "\"" + "Je crois que votre baguette est ouverte" + "\"" + ". Pauvre Chouque.",ep:20,g:56,l:128,wd:true,fm:true,w:10,an:11,e:[5,5,2,5,40,50,false,true],c:"CI>250",p:41257};
I.u[8089] = {n:"Baguette Iots",nn:"BAGUETTE IOTS",t:3,d:"Iots était un perroquet qui répétait toujours tout, on raconte que cette baguette fut sculptée dans son bec.",ep:20,g:57,l:138,wd:true,fm:true,w:20,an:11,tw:true,e:[5,4,3,4,40,50,false,true],c:"CI>150&CA>100&CC>100&CS>100",p:44125};
I.u[8090] = {n:"Bâton des Caraïbes",nn:"BATON DES CARAIBES",t:4,d:"Ce bâton a déjà mordu des gens, ou été mordu par des gens, ou des gens sont mordus de lui, on ne sait pas trop.",ep:20,g:64,l:114,wd:true,fm:true,w:10,an:12,tw:true,e:[5,5,1,1,35,50,false,true],c:"CI>200&CA>150",p:33520};
I.u[8091] = {n:"La Canne Bière",nn:"LA CANNE BIERE",t:4,d:"Ce bâton vient du sud, et par tous les chemins, il y revient.",ep:20,g:65,l:134,wd:true,fm:true,w:20,an:12,tw:true,e:[20,4,1,1,150,150,false,true],c:"CS>150&CA>150&CI>150",p:43550};
I.u[8092] = {n:"Les Dagues du Bouwin",nn:"LES DAGUES DU BOUWIN",t:5,d:"Le bouwin n\'est pas fin, le bouwin n\'est pas malin, mais quand il a ses dagues en main, personne ne cherche à provoquer le bouwin.",ep:20,g:56,l:135,wd:true,fm:true,w:20,an:13,tw:true,e:[10,3,1,1,30,50,false,true],c:"CS>250&CA>100",p:43510};
I.u[8093] = {n:"Dagues Ristocrates",nn:"DAGUES RISTOCRATES",t:5,d:"Porter ces dagues ne fera pas de vous un héros, mais si vous les rangez dans vos sous-vêtements, vous finirez peut-être par ressembler à une héroïne.",ep:0,g:64,l:58,wd:true,fm:true,w:1,e:[-1,-1,1,-1,-1,-1,false,false],p:4253};
I.u[8094] = {n:"L\'Epée Rilleuse",nn:"L\'EPEE RILLEUSE",t:6,d:"Cette épée illustre bien un vieux proverbe Iop, quand l\'ennemi périt, l\'épée rit.",ep:20,g:66,l:169,wd:true,fm:true,w:30,an:14,e:[10,5,1,1,50,50,false,true],c:"CS>450&CV>450",p:49010};
I.u[8095] = {n:"Le Sabre Ayassalama",nn:"LE SABRE AYASSALAMA",t:6,d:"Cette épée fut forgée en visant uniquement l\'esthétisme afin de décorer une cheminée de grande valeur, mais son efficacité lors des tueries lui vaut d\'être maniée régulièrement. Les gens n\'ont décidément aucun respect pour l\'art.",ep:20,g:67,l:143,wd:true,fm:true,w:20,an:14,e:[15,4,1,1,80,50,false,true],c:"CI>300&CS>130&(PG=6|PG=8|PG=11)",p:44587};
I.u[8096] = {n:"Marteau Mawoque",nn:"MARTEAU MAWOQUE",t:7,d:"Son précédent propriétaire dansait avec les Mulous. Il est mort dévoré.",ep:20,g:56,l:140,wd:true,fm:true,w:20,an:15,e:[5,5,1,1,30,45,false,true],c:"CI>250&CC>250",p:44541};
I.u[8097] = {n:"Marteau Xiko",nn:"MARTEAU XIKO",t:7,d:"Ce marteau fait voir la vie en rose avant de vous réduire le crâne en bouillie.",ep:20,g:57,l:129,wd:true,fm:true,w:30,an:15,e:[5,4,1,1,40,15,false,true],c:"CS>250&CI>150",p:42658};
I.u[8098] = {n:"Lame Usicke",nn:"LAME USICKE",t:19,d:"Cette hache à la forme exceptionnelle, dispose d\'une lame vibrante qui permet d\'achever ses ennemis en musique.",ep:0,g:38,l:1,wd:false,fm:true,w:1,an:17,e:[10,4,1,1,50,50,false,true],p:1};
I.u[8099] = {n:"La Hache Menbien",nn:"LA HACHE MENBIEN",t:19,d:"Cette arme a été trempée dans une fontaine de foudre pure puis saupoudrée de poussière d\'étoile, mais il paraît que c\'est entièrement faux.",ep:20,g:39,l:116,wd:true,fm:true,w:10,an:17,tw:true,e:[7,5,1,1,35,50,false,false],c:"CA>120&CV>150",p:34562};
I.u[8100] = {n:"La Hache Ticot",nn:"LA HACHE TICOT",t:19,d:"Cette hache est percée de trous en divers endroits, ce qui accompagne ses coups d\'élégants sifflements guillerets, cachant les cris de douleur de vos ennemis. C\'est regrettable.",ep:20,g:40,l:112,wd:true,fm:true,w:15,an:17,e:[5,4,1,1,40,50,false,false],c:"CS>150&CW>150",p:30300};
I.u[8101] = {n:"Le Fendoir Tichaud",nn:"LE FENDOIR TICHAUD",t:19,d:"Cette hache fut forgée par Brouce Boulgoure lorsqu\'il s\'essayait à la boulangerie pour faire plaisir à son ami Xavier le boulanger. Il voulait faire une pelle à tarte.",ep:20,g:41,l:128,wd:true,fm:true,w:1,an:17,e:[5,4,1,1,20,30,false,false],c:"CI>230&CA>60&CV>250",p:444};
I.u[8102] = {n:"Pierre du Craqueleur Légendaire",nn:"PIERRE DU CRAQUELEUR LEGENDAIRE",t:51,d:"On dit de cette pierre qu\'elle pourrait être le coeur du Craqueleur Légendaire. Ce qui est certain c\'est que le Craqueleur Légendaire n\'avait jamais le coeur lourd, cette pierre est donc d\'une légèreté incroyable, ce qui permet d\'en faire des objets adaptés aux personnages qui recherchent avant tout la mobilité, l\'agilité et protection. Tout simplement génial.",ep:5,g:457,l:120,wd:true,fm:true,w:50,p:100};
I.u[8103] = {n:"Arc Rainier",nn:"ARC RAINIER",t:2,d:"Cet arc est en réalité la chevelure fossilisée d\'un Sadida endormi trop profondément, sur laquelle des Araknes ont tissé leurs toiles.",ep:20,g:53,l:121,wd:true,fm:true,w:20,an:10,tw:true,e:[5,5,2,6,30,50,false,true],c:"CS>100&CA>100&CI>100&CC>100&CV>200",p:34857};
I.u[8104] = {n:"Arc Chtelion",nn:"ARC CHTELION",t:2,d:"Cet arc d\'une couleur discutable est parfait pour parader au Zaap de Bonta et à défaut de vous faire passer pour un coq, il attirera toutes les poulettes. Tantôt renversant, tantôt révulsant, il viendra peut-être à bout de vos ennemis sans que vous ne décochiez la moindre flèche.",ep:20,g:54,l:129,wd:true,fm:true,w:20,an:10,tw:true,e:[7,4,2,6,35,40,false,true],c:"CI>300&CV>300&CA>100",p:42510};
I.u[8106] = {n:"Potion d\'oubli de métier : Bricoleur",nn:"POTION D\'OUBLI DE METIER : BRICOLEUR",t:44,d:"Cette potion vous fera oublier le métier de Bricoleur.",ep:7,g:21,l:1,wd:false,fm:true,w:1,u:true,p:10};
I.u[8107] = {n:"Livre Honorifique des Bardes",nn:"LIVRE HONORIFIQUE DES BARDES",t:25,d:"Ce livre a été remis solennellement à Nolliuos, pour avoir remporté le concours de Bardise de l\'an 636. Qu\'il soit témoigné ici de ses mérites et de son talent ! Hujikan, Doux-Leurre, et Madahine sont également présents dans ce recueil. Longue vie à eux et à leur plume !",ep:1,g:19,l:1,wd:false,fm:true,w:1,u:true,p:10};
I.u[8108] = {n:"Amulette du Tofu",nn:"AMULETTE DU TOFU",t:1,d:"Légère comme une plume de Tofu, cette amulette se porte autour du cou ou du front, pour les aventuriers en quête d\'originalité.",ep:3,g:95,l:22,wd:true,fm:true,w:5,s:50,p:1525};
I.u[8109] = {n:"Anneau du Tofu",nn:"ANNEAU DU TOFU",t:9,d:"Cet anneau vraiment insignifiant tant il est ridicule, peut tout de même se révéler indispensable. En effet, vos ennemis risquent d\'avoir tellement pitié de vous, qu\'ils vous achèveront, plutôt que de vous laisser souffrir atrocement. Il faut toujours voir le bon côté des choses.",ep:3,g:100,l:21,wd:true,fm:true,w:5,s:50,p:1350};
I.u[8110] = {n:"Baguette du Tofu",nn:"BAGUETTE DU TOFU",t:3,d:"Contrairement à ce que certains aventuriers peuvent croire, cette baguette n\'est pas utilisée par les Tofus (bien trop stupides pour savoir se servir d\'une telle arme), mais contre les Tofus. En effet, les plumes de Tofu qui la composent permettent aux chasseurs de simuler la parade nuptiale des Tofus pour ensuite mieux les attraper. Mais jusqu\'où iront ces terribles chasseurs pour satisfaire leurs pulsions meurtrières ?",ep:3,g:58,l:25,wd:true,fm:true,w:10,an:11,e:[5,6,2,2,50,50,false,false],s:50,p:100};
I.u[8111] = {n:"Pantoufles du Tofu",nn:"PANTOUFLES DU TOFU",t:11,d:"A la fois souples et légères, ces pantoufles restent chaudes l\'hiver et fraîches l\'été. Malheureusement, elles ne supportent pas les lavages en machine.",ep:3,g:73,l:23,wd:true,fm:true,w:10,s:50,p:1575};
I.u[8112] = {n:"Cape du Tofu",nn:"CAPE DU TOFU",t:17,d:"Cette étrange cape semble avoir été coupée en deux. Mais pourquoi aurait-elle était coupée en deux ? Personne ne le sait, même si tout le monde a son avis sur la question.",ep:3,g:97,l:26,wd:true,fm:true,w:10,s:50,p:2000};
I.u[8113] = {n:"Ceinture du Tofu",nn:"CEINTURE DU TOFU",t:10,d:"Idéale pour étrangler les Tofus, cette ceinture permet également aux chasseurs de fouetter les aventuriers qui s\'aventurent sur leur domaine en période de chasse. Il est donc déconseillé de plaisanter pas avec nos amis les chasseurs, ce ne sont pas forcément les Tofus qui ont un coup dans l\'aile.",ep:3,g:89,l:22,wd:true,fm:true,w:5,s:50,p:1553};
I.u[8114] = {n:"Kaskofu",nn:"KASKOFU",t:16,d:"Ce casque de chantier est devenu un objet de mode incontournable pour les aventurières coquettes. Mais où va le monde ?",ep:3,g:128,l:23,wd:true,fm:true,w:5,s:50,p:1575};
I.u[8116] = {n:"Scaracoiffe Dorée",nn:"SCARACOIFFE DOREE",t:16,d:"Cette coiffe est parfaite pour cacher votre acné, ou votre visage de chérubin.",ep:6,g:129,l:58,wd:true,fm:true,w:10,s:51,p:8900};
I.u[8117] = {n:"Scaracape Dorée",nn:"SCARACAPE DOREE",t:17,d:"Plus dorée qu\'une Scaracape classique et moins noire qu\'une Scaracape Noire, cette cape est parfaite pour cacher vos excroissances. Elle servait autrefois au Scarabosse Doré qui l\'utilisait pour cacher son horrible bosse. De nos jours, cette cape est avant tout utilisée pour cacher les horribles postérieurs. Autant dire que cette cape est populaire.",ep:6,g:98,l:58,wd:true,fm:true,w:10,s:51,p:8980};
I.u[8118] = {n:"Baguette du Scarabosse Doré",nn:"BAGUETTE DU SCARABOSSE DORE",t:3,d:"Cette baguette magique est parfaite pour dépoussiérer les vieux meubles ou les vieilles personnes.",ep:6,g:59,l:59,wd:true,fm:true,w:10,an:11,e:[10,4,2,4,30,60,false,false],s:51,p:9300};
I.u[8119] = {n:"Scarature Dorée",nn:"SCARATURE DOREE",t:10,d:"Si vous aimez parader entièrement nu sur les plages de l\'île de Moon, vous devriez rapidement tomber amoureux de cette ceinture, réputée pour être un excellent cache-misère.",ep:6,g:90,l:51,wd:true,fm:true,w:10,s:51,p:6825};
I.u[8120] = {n:"Amulette Scarabosse Doré",nn:"AMULETTE SCARABOSSE DORE",t:1,d:"Portée sur un torse velu, cette amulette a tout pour plaire tant elle se marie bien avec les touffes de poils.",ep:6,g:96,l:45,wd:true,fm:true,w:5,s:51,p:5375};
I.u[8121] = {n:"Anneau du Scarabosse Doré",nn:"ANNEAU DU SCARABOSSE DORE",t:9,d:"Le Sacrabosse est une étrange créature. Bossue et laide depuis sa naissance, elle s\'est retrouvée être la risée des autres Scarafeuilles. C\'est peut-être pour cela que vous vous sentez si proche de cette créature.",ep:6,g:101,l:43,wd:true,fm:true,w:5,s:51,p:5000};
I.u[8122] = {n:"Scarabottes Dorées",nn:"SCARABOTTES DOREES",t:11,d:"Ces bottes crochues permettent d\'escalader facilement les falaises les plus abruptes, ou de labourer les champs sans charrue.",ep:6,g:74,l:54,wd:true,fm:true,w:10,s:51,p:7725};
I.u[8123] = {n:"Amulette Paysanne",nn:"AMULETTE PAYSANNE",t:1,d:"Cette amulette est parfumée à l\'huile de Tournesol Sauvage et aux fines herbes. Elle peut donc servir d\'appât pour les ruminants dangereux que vous voudrez piéger, comme les Bouftous, ou les belles-mères par exemple.",ep:1,g:97,l:20,wd:true,fm:true,w:5,s:47,p:1255};
I.u[8124] = {n:"Ceinturemuda du Paysan",nn:"CEINTUREMUDA DU PAYSAN",t:10,d:"Cette ceinture est franchement exceptionnelle, puisqu\'un bermuda en fibre de lin y est attaché. C\'est la ceinture idéale pour ceux qui aiment se délester de leurs vêtements rapidement, et passer à l\'action sur le champ. Attention cependant, pour des raisons d\'hygiène, cette ceinture est interdite dans les piscines municipales.",ep:1,g:91,l:20,wd:true,fm:true,w:10,s:47,p:1300};
I.u[8125] = {n:"Bob du Paysan",nn:"BOB DU PAYSAN",t:16,d:"Pour se protéger du soleil lorsque vous travaillez dans les champs, rien de mieux qu\'un Ploukosse. Pour se protéger du soleil lorsque vous travaillez dans les champs et que vous êtes pauvre, rien de mieux que le célèbre Bob du Paysan.",ep:1,g:130,l:19,wd:true,fm:true,w:10,s:47,p:1200};
I.u[8126] = {n:"Mitaines Mitées du Paysan",nn:"MITAINES MITEES DU PAYSAN",t:9,d:"Ces mitaines mitées sont de véritables bijoux. On raconte qu\'elles sont recouvertes de poussière de diamant, ce qui leur confère une résistance extraordinaire. Elles disposent d\'une face abrasive, idéale pour frotter le derrière des Bouftous, ou celui de votre épouse.",ep:1,g:102,l:12,wd:true,fm:true,w:5,s:47,p:600};
I.u[8127] = {n:"Faux usée du Paysan",nn:"FAUX USEE DU PAYSAN",t:22,d:"Cette faux robuste mais abîmée a déjà beaucoup servi et coupe assez mal. En revanche, elle est très efficace pour arracher les chaires tendres et superficielles. Très utile pour lutter contre les varices de vos adversaires.",ep:1,g:2,l:17,wd:true,fm:true,w:1,an:17,e:[5,3,1,1,50,50,false,false],s:47,p:950};
I.u[8128] = {n:"Bottes Paysannes",nn:"BOTTES PAYSANNES",t:11,d:"Ces bottes sont rembourrées avec de la paille de blé, ce qui permet d\'absorber la transpiration et de cultiver des champignons toxiques à moindre frais. Grâce à leur semelle molle, ces bottes sont recommandées par les arbitres de la fédération de Boufball.",ep:1,g:75,l:18,wd:true,fm:true,w:5,s:47,p:1100};
I.u[8129] = {n:"Amuloumulette",nn:"AMULOUMULETTE",t:1,d:"Derrière ce surnom stupide, se cache en réalité une amulette relativement puissante, confectionnée à partir de crocs, de dents et d\'ossements de Mulous, ce qui explique la terrible odeur qui s\'en dégage. A défaut d\'être assortie à votre cou, elle sera assortie à votre haleine.",ep:1,g:98,l:57,wd:true,fm:true,w:10,s:32,p:8550};
I.u[8130] = {n:"Hache du Mulou",nn:"HACHE DU MULOU",t:19,d:"Cette terrible hache a permis de décapiter plus d\'un aventurier imprudent. Et pourtant, elle coupe assez mal, il faut parfois une dizaine de tentatives pour parvenir à couper un cou un peu résistant.",ep:1,g:42,l:55,wd:true,fm:true,w:10,an:17,tw:true,e:[5,6,1,1,50,50,false,false],s:32,p:8125};
I.u[8131] = {n:"Sac du Paysan",nn:"SAC DU PAYSAN",t:81,d:"Ce sac très en vogue chez les jeunes écoliers, est idéal pour entreposer des farines animales et végétales. Une fois ouvert, puis déplié et déchiré le long des coutures, il pourra vous servir de jupe si vous n\'avez pas peur de montrer vos cuisses velues.",ep:1,g:3,l:19,wd:true,fm:true,w:1,s:47,p:1153};
I.u[8132] = {n:"Scarature Noire",nn:"SCARATURE NOIRE",t:10,d:"Cette ceinture manque cruellement d\'originalité. Elle semble cependant assortie à votre coiffure.",ep:6,g:93,l:43,wd:true,fm:true,w:10,s:49,p:4955};
I.u[8133] = {n:"Scaracoiffe Noire",nn:"SCARACOIFFE NOIRE",t:16,d:"C\'est la coiffe parfaite pour ceux qui veulent avoir des idées noires.",ep:6,g:132,l:53,wd:true,fm:true,w:10,s:49,p:7350};
I.u[8134] = {n:"Scaracape Noire",nn:"SCARACAPE NOIRE",t:17,d:"Si vous n\'avez pas les moyens de vous acheter une Corbacape Mastralis, mais que vous aimez faire peur aux Araknes en vous habillant en noir, c\'est la cape qu\'il vous faut.",ep:6,g:100,l:53,wd:true,fm:true,w:10,s:49,p:7575};
I.u[8135] = {n:"Clef du Donjon des Bworks",nn:"CLEF DU DONJON DES BWORKS",t:84,d:"Cette clef surprenante vous permettra de rentrer dans le donjon des Bworks, mais pas d\'en sortir vivant.",ep:1,g:234,l:1,wd:true,fm:true,w:1,p:100};
I.u[8136] = {n:"Scaranneau Noir",nn:"SCARANNEAU NOIR",t:9,d:"Cet anneau magnifique peut être porté autour du gros orteil. Idéal pour cacher vos verrues et vos ongles incarnés.",ep:6,g:104,l:48,wd:true,fm:true,w:10,s:49,p:6200};
I.u[8137] = {n:"Viscères du Scarabosse Doré",nn:"VISCERES DU SCARABOSSE DORE",t:15,d:"Ces viscères sont en partie responsables des digestions difficiles du Scarabosse Doré. Vous êtes donc autorisé à les faire frire sans remord.",ep:6,g:726,l:70,wd:true,fm:true,w:5,p:100};
I.u[8138] = {n:"Carapace de Scarafeuille Noir",nn:"CARAPACE DE SCARAFEUILLE NOIR",t:107,d:"Une fois retournée, cette carapace fera un excellent cendrier.",ep:6,g:727,l:40,wd:true,fm:true,w:5,p:100};
I.u[8139] = {n:"Clef du Donjon des Scarafeuilles",nn:"CLEF DU DONJON DES SCARAFEUILLES",t:84,d:"Cette magnifique clef dorée, vous permettra de rentrer dans le donjon des Scarafeuilles. Vous pensiez avoir tout vu ?",ep:1,g:233,l:1,wd:true,fm:true,w:1,p:100};
I.u[8140] = {n:"Antennes du Scarafeuille Noir",nn:"ANTENNES DU SCARAFEUILLE NOIR",t:15,d:"Le Scarafeuille noir n\'a jamais trouvé la moindre utilité à ses propres antennes. La nature est vraiment mal faite.",ep:6,g:728,l:40,wd:true,fm:true,w:1,p:100};
I.u[8141] = {n:"Ailes du Scarafeuille Noir",nn:"AILES DU SCARAFEUILLE NOIR",t:104,d:"Contrairement à ce que certains aventuriers peuvent penser, les ailes du Scarafeuille Noir ne sont pas noires, mais blanches. Encore un mystère de la nature qui alimente les débats mouvementés de la taverne.",ep:6,g:729,l:40,wd:true,fm:true,w:1,p:100};
I.u[8142] = {n:"Clef Secrète du Tofulailler Royal",nn:"CLEF SECRETE DU TOFULAILLER ROYAL",t:84,d:"Cette clef vous donne accès au Tofulailler Royal. Une bonne dérouillée vous y attend.",ep:3,g:12,l:1,wd:true,fm:true,w:1,p:1};
I.u[8143] = {n:"Clef des Champs",nn:"CLEF DES CHAMPS",t:84,d:"Cette clef ne vous permettra pas de vous évader du Donjon des Champs, mais plutôt de vous y enfermer.",ep:1,g:13,l:1,wd:true,fm:true,w:1,p:100};
I.u[8144] = {n:"Mojo du Meulou",nn:"MOJO DU MEULOU",t:15,d:"Cette petite fiole contient toute la puissance virile du Meulou. Attention, son contenu n\'est pas comestible.",ep:2,g:730,l:1,wd:true,fm:true,w:1,p:1};
I.u[8145] = {n:"Bière de Bwork",nn:"BIERE DE BWORK",t:37,d:"La Bière de Bwork enivre un Féca à 12 pas et un Sadida de quinze pieds. On ne sait pas vraiment si elle est faite par les Bworks, à partir de Bworks, ou les deux.",ep:1,g:1,l:1,wd:false,fm:true,w:1,u:true,p:1};
I.u[8146] = {n:"Bottes Du Craqueleur Légendaire",nn:"BOTTES DU CRAQUELEUR LEGENDAIRE",t:11,d:"Si vous aimez danser et écraser les pieds de vos partenaires, ces bottes sont faites pour vous.",ep:5,g:77,l:86,wd:true,fm:true,w:10,s:52,p:19150};
I.u[8147] = {n:"Casque du Craqueleur Légendaire",nn:"CASQUE DU CRAQUELEUR LEGENDAIRE",t:16,d:"Version améliorée du casque du Craqueleur, ce roc vous protègera efficacement des chutes de pierres, mais pas des chutes de cheveux.",ep:5,g:133,l:80,wd:true,fm:true,w:10,s:52,p:16530};
I.u[8148] = {n:"Marteau du Craqueleur Légendaire",nn:"MARTEAU DU CRAQUELEUR LEGENDAIRE",t:7,d:"Cet énorme marteau est en fait une épée de Craqueleur, munie d\'une pierre du Craqueleur Légendaire. Cette création surprenante saura surprendre vos adversaires, juste avant de les écraser.",ep:5,g:58,l:81,wd:true,fm:true,w:10,an:15,e:[7,5,1,1,40,50,false,false],c:"CA>100&CI>50",s:52,p:16850};
I.u[8149] = {n:"Craquanneau Légendaire",nn:"CRAQUANNEAU LEGENDAIRE",t:9,d:"Ce rocher serti de pierres précieuses est idéal pour marquer les tempes ou le front de vos adversaires de façon définitive.",ep:18,g:105,l:80,wd:true,fm:true,w:10,s:52,p:16775};
I.u[8150] = {n:"Amulette du Craqueleur Légendaire",nn:"AMULETTE DU CRAQUELEUR LEGENDAIRE",t:1,d:"D\'apparence modeste, cette amulette renferme bien des secrets notamment celui précisant quels secrets elle renferme.",ep:5,g:99,l:84,wd:true,fm:true,w:10,s:52,p:18350};
I.u[8151] = {n:"Bouloute",nn:"BOULOUTE",t:18,d:"Ce petit monstre vous suivra partout, et se nourrira des restes de ses congénères. Tantôt terrifiant, tantôt amusant, il égayera votre vie, avant de peut-être vous la prendre définitivement.",ep:3,g:35,l:1,wd:true,fm:true,w:10,c:"PZ=1",p:1};
I.u[8152] = {n:"Ceinture du Craqueleur Légendaire",nn:"CEINTURE DU CRAQUELEUR LEGENDAIRE",t:10,d:"Le Craqueleur légendaire porte cette ceinture autour de ses orteils, vous autour de la taille, ou des chevilles peut-être...",ep:5,g:94,l:80,wd:true,fm:true,w:10,s:52,p:16506};
I.u[8153] = {n:"Abra Kadabra",nn:"ABRA KADABRA",t:18,d:"Comme vous, ce petit Abraknyde ne supportait plus sa famille, et a décidé de se rebeller. Aujourd\'hui, il vous accompagne et comptabilise tous les membres de sa famille que vous réussirez à exterminer.",ep:3,g:34,l:1,wd:true,fm:true,w:10,c:"PZ=1",p:1};
I.u[8154] = {n:"El Scarador",nn:"EL SCARADOR",t:18,d:"Ce charmant petit coléoptère venu du sud est en fait redoutable. Humilié par ses amis lorsqu\'il était jeune, il a décidé de se venger, et vous aidera à botter le derrière de tous les Scarafeuilles que vous rencontrerez.",ep:3,g:36,l:1,wd:true,fm:true,w:10,c:"PZ=1",p:1};
I.u[8155] = {n:"Tifoux",nn:"TIFOUX",t:18,d:"Cette splendide créature est un lointain cousin des célèbres Lenalds de l\'île des Wabbits. Plus téméraire, mais également beaucoup plus indisciplinée, elle saura vous apporter son aide durant vos combats, mais également faire preuve d\'une fourberie sans nom, en vous aidant à vaincre les membres de sa propre famille.",ep:3,g:37,l:1,wd:true,fm:true,w:10,c:"PZ=1",p:1};
I.u[8156] = {n:"Clef du Donjon des Canidés",nn:"CLEF DU DONJON DES CANIDES",t:84,d:"Cette clef vous permettra de rentrer dans le terrible donjon des Canidés, et de vous faire par la même occasion, étriper dans la joie et la bonne humeur.",ep:1,g:235,l:1,wd:true,fm:true,w:1,p:1};
I.u[8157] = {n:"Pétale Magique du Tournesol Affamé",nn:"PETALE MAGIQUE DU TOURNESOL AFFAME",t:35,d:"Comme tous les objets magiques, ce pétale impressionne, et dégage une aura de puissance proportionnelle à la crédulité des aventuriers qui l\'observent.",ep:1,g:2,l:20,wd:true,fm:true,w:1,p:100};
I.u[8158] = {n:"Plume Graisseuse du Tofu Ventripotent",nn:"PLUME GRAISSEUSE DU TOFU VENTRIPOTENT",t:53,d:"Le Tofu Ventripotent se badigeonne régulièrement de graisse pour que ses plumes ne se détachent jamais de son plumage. Une technique vieille comme le monde, qui permet à certains Enutrofs de garder leurs cheveux, même après la puberté.",ep:3,g:1,l:50,wd:true,fm:true,w:1,p:10};
I.u[8159] = {n:"Carapace du Scarabosse Doré",nn:"CARAPACE DU SCARABOSSE DORE",t:107,d:"Portée sur la tête, cette carapace vous protègera des intempéries et des chutes de guano.",ep:6,g:731,l:70,wd:true,fm:true,w:5,p:100};
I.u[8160] = {n:"Antennes du Scarabosse Doré",nn:"ANTENNES DU SCARABOSSE DORE",t:15,d:"Si vous voulez ressembler à Korgamone le Chapon des Karpates, c\'est très simple, enfoncez-vous ces antennes dans les narines, et chantez à travers la campagne en remuant le postérieur de droite à gauche.",ep:6,g:732,l:70,wd:true,fm:true,w:1,p:100};
I.u[8161] = {n:"Ailes du Scarabosse Doré",nn:"AILES DU SCARABOSSE DORE",t:104,d:"Si vous avez toujours rêvé de voler comme un Piou adipeux, accrochez ces ailes à votre ceinture, et jetez-vous depuis la tour des ordres de Brâkmar. Avec de la chance, vous ressemblerez à un Corbac écrasé, vous offrirez aux Brâkmariens, un très bon moment de rigolade, et aux Corbacs, un excellent repas.",ep:6,g:733,l:70,wd:true,fm:true,w:1,p:100};
I.u[8162] = {n:"Tête de Citwouille",nn:"TETE DE CITWOUILLE",t:16,d:"Vous aurez l\'air encore plus stupide que d\'habitude avec cette Citwouille magique sur la tête, mais rappelez-vous de la malédiction d\'Halouine, qui transforme certaines créatures en d\'étranges bêtes difformes... \nIl semblerait que seule cette coiffe puisse vous prémunir de cette terrible malédiction.",ep:1,g:134,l:1,wd:true,fm:true,w:1,p:1};
I.u[8163] = {n:"Kabuto",nn:"KABUTO",t:16,d:"Porté autrefois par les forces de l\'ordre des plus grandes cités, ce casque est aujourd\'hui utilisé pour effrayer ceux qui contestent l\'ordre établi et cherchent à en apprendre trop sur ce qui régit leur univers.",ep:20,g:135,l:101,wd:true,fm:true,w:10,p:26000};
I.u[8164] = {n:"Cadeau Bouloute",nn:"CADEAU BOULOUTE",t:89,d:"En posant votre oreille sur ce cadeau ou en le secouant vigoureusement, vous entendrez peut-être d\'étranges bruits qui vous révéleront la présence d\'une créature encore vivante. Ne perdez pas une seule seconde, ouvrez-le vite, vous y trouverez un superbe familier, un livre vous expliquant comment l\'élever, et des poudres d\'Eniripsa pour le soigner.",ep:1,g:2,l:1,wd:true,fm:true,w:1,c:"PZ=1",u:true,p:1};
I.u[8165] = {n:"Cadeau Abra Kadabra",nn:"CADEAU ABRA KADABRA",t:89,d:"En posant votre oreille sur ce cadeau ou en le secouant vigoureusement, vous entendrez peut-être d\'étranges bruits qui vous révéleront la présence d\'une créature encore vivante. Ne perdez pas une seule seconde, ouvrez-le vite, vous y trouverez un superbe familier, un livre vous expliquant comment l\'élever, et des poudres d\'Eniripsa pour le soigner.",ep:1,g:2,l:1,wd:false,fm:true,w:1,c:"PZ=1",u:true,p:1};
I.u[8166] = {n:"Cadeau El Scarador",nn:"CADEAU EL SCARADOR",t:89,d:"En posant votre oreille sur ce cadeau ou en le secouant vigoureusement, vous entendrez peut-être d\'étranges bruits qui vous révéleront la présence d\'une créature encore vivante. Ne perdez pas une seule seconde, ouvrez-le vite, vous y trouverez un superbe familier, un livre vous expliquant comment l\'élever, et des poudres d\'Eniripsa pour le soigner.",ep:1,g:2,l:1,wd:false,fm:true,w:1,c:"PZ=1",u:true,p:1};
I.u[8167] = {n:"Cadeau Tifoux",nn:"CADEAU TIFOUX",t:89,d:"En posant votre oreille sur ce cadeau ou en le secouant vigoureusement, vous entendrez peut-être d\'étranges bruits qui vous révéleront la présence d\'une créature encore vivante. Ne perdez pas une seule seconde, ouvrez-le vite, vous y trouverez un superbe familier, un livre vous expliquant comment l\'élever, et des poudres d\'Eniripsa pour le soigner.",ep:1,g:2,l:1,wd:false,fm:true,w:1,c:"PZ=1",u:true,p:1};
I.u[8168] = {n:"Cadeau d\'Halouine",nn:"CADEAU D\'HALOUINE",t:89,d:"Halouine était de loin le plus grand cultivateur de Citwouilles de tout le continent. Malheureusement, il était également l\'apprenti d\'un Bwork Mage un peu trop versé dans les arcanes. Afin de gagner le concours de la plus grosse Citwouille cultivée au purin de Porkass, Halouine tricha en faisant appel au Bwork Mage, qui tenta une incantation inédite sur les Citwouilles. Ceux qui goûtèrent ces Citwouilles magiques, finirent transformés en d\'étranges créatures difformes pendant plusieurs jours, et ce chaque année à la même période. Cette malédiction semble même parfois se transmettre aux aventuriers qui s\'approchent trop près des créatures maudites. Pour conjurer le sort, les habitants d\'Amakna et de ses alentours, se mettent une Citwouille sur la tête pendant plusieurs jours, en espérant que cela les sauve. Le plus surprenant c\'est qu\'il semblerait que cela fonctionne.",ep:1,g:1,l:1,wd:false,fm:true,w:1,p:1};
I.u[8169] = {n:"Transformation Bouftou d\'Halouine",nn:"TRANSFORMATION BOUFTOU D\'HALOUINE",t:27,d:"Vous avez été transformé en Bouftou d\'Halouine. Vous devez essayer de vaincre un autre aventurier pour vous débarrasser de cette terrible malédiction.",ep:1,g:4,l:1,wd:false,fm:true,w:0,m:true,p:1};
I.u[8170] = {n:"Transformation Tofu d\'Halouine",nn:"TRANSFORMATION TOFU D\'HALOUINE",t:27,d:"Vous avez été transformé en Tofu d\'Halouine. Vous devez essayer de vaincre un autre aventurier pour vous débarrasser de cette terrible malédiction.",ep:1,g:4,l:1,wd:false,fm:true,w:0,m:true,p:1};
I.u[8171] = {n:"Fantôme d\'Abra Kadabra",nn:"FANTOME D\'ABRA KADABRA",t:90,d:"Ce Fantôme n\'a pas encore quitté le monde des vivants, il ne tient qu\'à vous de trouver une façon de le ressusciter, ou de l\'abandonner lâchement une bonne fois pour toutes.",ep:3,g:34,l:1,wd:true,fm:true,w:1,p:1};
I.u[8172] = {n:"Fantôme de Bouloute",nn:"FANTOME DE BOULOUTE",t:90,d:"Ce Fantôme n\'a pas encore quitté le monde des vivants, il ne tient qu\'à vous de trouver une façon de le ressusciter, ou de l\'abandonner lâchement une bonne fois pour toutes.",ep:3,g:35,l:1,wd:true,fm:true,w:1,p:1};
I.u[8173] = {n:"Fantôme d\'El Scarador",nn:"FANTOME D\'EL SCARADOR",t:90,d:"Ce Fantôme n\'a pas encore quitté le monde des vivants, il ne tient qu\'à vous de trouver une façon de le ressusciter, ou de l\'abandonner lâchement une bonne fois pour toutes.",ep:3,g:36,l:1,wd:true,fm:true,w:1,p:1};
I.u[8174] = {n:"Fantôme de Tifoux",nn:"FANTOME DE TIFOUX",t:90,d:"Ce Fantôme n\'a pas encore quitté le monde des vivants, il ne tient qu\'à vous de trouver une façon de le ressusciter, ou de l\'abandonner lâchement une bonne fois pour toutes.",ep:3,g:37,l:1,wd:true,fm:true,w:1,p:1};
