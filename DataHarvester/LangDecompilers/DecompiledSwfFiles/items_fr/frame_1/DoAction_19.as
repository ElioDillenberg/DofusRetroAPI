I.u[7999] = {n:"Capananas",nn:"CAPANANAS",t:17,d:"Certains aventuriers racontent que cette cape est aussi rugueuse que la peau d\'un Crocodaille, et d\'autres racontent même qu\'elle est encore plus rugueuse que la peau des fesses d\'une femelle Bwork. Quoi qu\'il en soit, faites attention à la porter du bon côté, si vous aussi, vous ne voulez pas finir par avoir les fesses rugueuses.",ep:1,g:94,l:1,wd:true,fm:true,w:10,s:74,p:100};
I.u[8000] = {n:"Bworky",nn:"BWORKY",t:18,d:"Cette petite créature est un Bwork, resté enfermé vivant dans un bocal pendant plusieurs décennies. Il a donc pris la forme du bocal, et restera nain toute sa vie. Sa bonne humeur et son humour légendaire en font le compagnon idéal pour les donjons perdus d\'avance, ou les soirées mondaines endiablées. Attention, c\'est un adepte des apéritifs, et il ne supporte que les noix et noisettes, même s\'il ne rechigne pas avaler un gland de temps en temps.",ep:3,g:31,l:1,wd:true,fm:true,w:10,c:"PZ=1",p:1};
I.u[8001] = {n:"Bandelette du Guerrier Koalak",nn:"BANDELETTE DU GUERRIER KOALAK",t:15,d:"Cette bandelette normalement enroulée le long des bras du Guerrier Koalak, lui permet de se moucher le nez sans utiliser de mouchoirs. Lorsqu\'elle a beaucoup servi, cette bandelette peut-être accrochée à un plafond pour coller les Moskitos de passage dans les habitations.",ep:20,g:719,l:70,wd:true,fm:true,w:5,p:100};
I.u[8002] = {n:"Etoffe de Dok Alako",nn:"ETOFFE DE DOK ALAKO",t:55,d:"Cette étoffe semble imprégnée d\'une magie ancestrale difficilement prévisible et maîtrisable. Encore un point commun avec votre dulcinée.",ep:20,g:720,l:40,wd:true,fm:true,w:5,p:1};
I.u[8003] = {n:"Amulette du Koalak",nn:"AMULETTE DU KOALAK",t:1,d:"Cette amulette ne ressemble à aucune autre amulette, tout simplement car aucune amulette ne lui ressemble.",ep:20,g:94,l:60,wd:true,fm:true,w:10,s:46,p:9425};
I.u[8004] = {n:"Anneau du Koalak",nn:"ANNEAU DU KOALAK",t:9,d:"Ce magnifique anneau est porté par les Koalaks les plus téméraires, ceux qui ont été assez courageux pour le voler aux aventuriers d\'Astrub à la sortie de la banque.",ep:20,g:99,l:53,wd:true,fm:true,w:10,s:46,p:7375};
I.u[8005] = {n:"Arc du Koalak",nn:"ARC DU KOALAK",t:2,d:"Cet arc surprenant permet de voler la vie de ses adversaires, et parfois même, de voler vos adversaires à la vie, ce qui ne signifie rien en soi, mais reste tout de même très impressionnant.",ep:20,g:52,l:61,wd:true,fm:true,w:10,an:10,tw:true,e:[5,4,2,4,50,50,false,true],s:46,p:9775};
I.u[8006] = {n:"Bottes du Koalak",nn:"BOTTES DU KOALAK",t:11,d:"Ces bottes sont en réalité des pieds de Koalaks, sauvagement arrachés à leur propriétaires, puis vidés de leur contenu avant d\'être rembourrés avec des poils de Koalaks. A la fois terrifiantes et séduisantes, ces bottes feront de vous la terreur d\'Astrub.",ep:20,g:71,l:60,wd:true,fm:true,w:10,s:46,p:9600};
I.u[8007] = {n:"Cape du Koalak",nn:"CAPE DU KOALAK",t:17,d:"Cette cape vous protégera de la majorité des attaques, mais pas du ridicule.",ep:20,g:95,l:61,wd:true,fm:true,w:10,s:46,p:9675};
I.u[8008] = {n:"Ceinture du Koalak",nn:"CEINTURE DU KOALAK",t:10,d:"Cette ceinture en feuilles de Kaliptus diffuse une délicieuse odeur, capable d\'en masquer d\'autres. Elle est en outre parfaite pour masquer vos rondeurs.",ep:20,g:87,l:60,wd:true,fm:true,w:10,s:46,p:9508};
I.u[8009] = {n:"Coiffe du Koalak",nn:"COIFFE DU KOALAK",t:16,d:"Cette coiffe est ornée d\'énormes oreilles de Koalak, qui permettent d\'amplifier les sons émis par les créatures qui vous entourent. Attention, elle peut détruire vos tympans si vos amis sont adeptes des vents de panique.",ep:20,g:125,l:60,wd:true,fm:true,w:10,s:46,p:9425};
I.u[8010] = {n:"Démonteur",nn:"DEMONTEUR",t:93,d:"Permet de récupérer un équipement installé dans un enclos.",ep:1,g:96,l:1,wd:false,fm:true,w:10,ut:true,p:1};
I.u[8011] = {n:"Les Prophètes, les cultes, la Foi",nn:"LES PROPHETES, LES CULTES, LA FOI",t:25,d:"Quelques bonnes histoires de cultes qui raviront le mystique de base.",ep:1,g:19,l:1,wd:false,fm:true,w:1,u:true,p:10};
I.u[8012] = {n:"Poudre de résurrection",nn:"POUDRE DE RESURRECTION",t:48,d:"Cette poudre permet de ramener à la vie un familier fantôme, à condition de savoir comment nourrir un fantôme de familier.",ep:20,g:51,l:1,wd:true,fm:true,w:1,p:100};
I.u[8013] = {n:"Transformation Fantôme Chacha",nn:"TRANSFORMATION FANTOME CHACHA",t:27,d:"Vous avez été transformé en fantôme afin de pouvoir pénétrer dans le sanctuaire des familiers.",ep:20,g:3,l:1,wd:false,fm:true,w:0,m:true,p:1};
I.u[8016] = {n:"Fantôme de Koalak Sanguin",nn:"FANTOME DE KOALAK SANGUIN",t:90,d:"Ce Fantôme n\'a pas encore quitté le monde des vivants, il ne tient qu\'à vous de trouver une façon de le ressusciter, ou de l\'abandonner lâchement une bonne fois pour toutes.",ep:3,g:43,l:1,wd:true,fm:true,w:1,p:1};
