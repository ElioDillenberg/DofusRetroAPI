I.u[728] = {n:"Sort Maîtrise de la Baguette",nn:"SORT MAITRISE DE LA BAGUETTE",t:73,d:"(Double clic pour apprendre le sort) \nCe parchemin vous permettra de faire plus de dégâts en utilisant une baguette.",ep:1,g:5,l:20,wd:true,fm:true,w:1,u:true,p:10000};
I.u[729] = {n:"Sort Maîtrise des Dagues",nn:"SORT MAITRISE DES DAGUES",t:73,d:"(Double clic pour apprendre le sort) \nCe parchemin vous permettra de faire plus de dégâts en utilisant une dague.",ep:2,g:6,l:20,wd:true,fm:true,w:1,u:true,p:10000};
I.u[730] = {n:"Sort Maîtrise de la Pelle",nn:"SORT MAITRISE DE LA PELLE",t:73,d:"(Double clic pour apprendre le sort) \nCe parchemin vous permettra de faire plus de dégâts en utilisant une pelle.",ep:7,g:7,l:20,wd:true,fm:true,w:1,u:true,p:10000};
I.u[731] = {n:"Parchemin de Sort Spécial Brokle",nn:"PARCHEMIN DE SORT SPECIAL BROKLE",t:75,d:"(Double clic pour apprendre le sort) \nCe parchemin vous permet d\'apprendre le sort Brokle... Réservé aux Iops uniquement.",ep:2,g:4,l:1,wd:true,fm:true,w:1,c:"PG=8",u:true,p:1};
I.u[732] = {n:"Alliance de Silimelle",nn:"ALLIANCE DE SILIMELLE",t:9,d:"Cette magnifique alliance a été volée à Silimelle la Brasseuse de Bière, lors d\'une soirée un peu trop arrosée à la taverne. Voilà ce qui arrive lorsque l\'on paie des coups à des inconnus...",ep:1,g:30,l:26,wd:true,fm:true,w:1,p:2000};
I.u[733] = {n:"Bottes de Ragalde",nn:"BOTTES DE RAGALDE",t:11,d:"Ragalde était sûrement un des Guerriers Iop les plus téméraires, ces bottes magiques lui ont permis d\'occire bon nombre d\'indésirables.",ep:1,g:7,l:49,wd:true,fm:true,w:1,p:5000};
I.u[734] = {n:"Ceinture de Sécurité",nn:"CEINTURE DE SECURITE",t:10,d:"Cette ceinture permettait aux gardes du Château d\'Amakna de contenir les émeutes et autres débordements lors des fêtes populaires.",ep:1,g:7,l:17,wd:true,fm:true,w:10,p:1000};
I.u[737] = {n:"Dofus Emeraude",nn:"DOFUS EMERAUDE",t:23,d:"Plus vert qu\'un Tofu malade, le Dofus Emeraude fut créé par Aerafal, le Dragon de l\'Air. Pour créer quelque chose d\'aussi vert, il devait avoir le mal de l\'air. Un comble, non ?",ep:1,g:2,l:6,wd:true,fm:true,w:5,p:1};
I.u[739] = {n:"Dofus Turquoise",nn:"DOFUS TURQUOISE",t:23,d:"Le Dofus Turquoise fut créé par Aguabrial, le Dragon de l\'Eau. Quand on y pense, c\'est fou ce qu\'un Dragon peut faire avec de l\'eau...",ep:1,g:3,l:6,wd:true,fm:true,w:5,p:1};
I.u[742] = {n:"Bâton Cornu",nn:"BATON CORNU",t:4,d:"" + "\"" + "Ce bâton a dû faire, toutes les guerres, pour être aussi fort aujourd\'hui" + "\"" + " tels ont été les derniers mots de Francisque Kabroule, le célèbre Ménestrel Forgeur de Haches, avant de périr écrasé par un œuf de Tofu géant.",ep:1,g:1,l:29,wd:true,fm:true,w:10,an:12,e:[5,4,1,1,30,200,false,false],c:"CC>20&CI>30",p:2350};
I.u[743] = {n:"Brioche Dorée",nn:"BRIOCHE DOREE",t:33,d:"La recette de cette Brioche a été élaborée par Xavier le Boulanger en personne qui voulait en faire une brioche délicieuse et légère à la fois. On peut sans se tromper, dire qu\'il a réussi.",ep:1,g:8,l:60,wd:true,fm:true,w:1,u:true,ut:true,p:1};
I.u[744] = {n:"Cape de Bowisse",nn:"CAPE DE BOWISSE",t:17,d:"Cette cape est comme son porteur, unique en son genre et trop souvent sous-estimée. \nElle est constituée d\'un assemblage de peaux de larves vertes, ce qui lui donne un aspect rugueux des plus sympathique.",ep:1,g:1,l:20,wd:true,fm:true,w:10,p:1275};
I.u[745] = {n:"Lingot d\'Or",nn:"LINGOT D\'OR",t:40,d:"Ce lingot est obtenu en faisant fondre de l\'Or à l\'état brut.",ep:1,g:241,l:10,wd:true,fm:true,w:1,p:0};
I.u[746] = {n:"Ebonite",nn:"EBONITE",t:40,d:"Cet alliage très rare permet de fabriquer des objets très rares et souvent mortels. En effet, l\'Ebonite est un métal qui une fois rouillé, devient un véritable poison.",ep:1,g:658,l:20,wd:true,fm:true,w:20,p:0};
I.u[747] = {n:"Aluminite",nn:"ALUMINITE",t:40,d:"Cet alliage est d\'une légèreté impressionnante, ce qui en fait un métal de choix pour les petites dagues ou les épées courtes.",ep:1,g:660,l:10,wd:true,fm:true,w:10,p:0};
I.u[748] = {n:"Magnésite",nn:"MAGNESITE",t:40,d:"Cet alliage original est presque magique, il brille et vibre la nuit, ce qui peut être suffisant pour effrayer un Bwork un peu trop crédule.",ep:1,g:659,l:30,wd:true,fm:true,w:30,p:0};
I.u[749] = {n:"Bakélélite",nn:"BAKELELITE",t:40,d:"Cet alliage n\'est pas des plus solide, mais correctement aiguisé, il devient aussi tranchant que la Lame d\'un Chafer.",ep:1,g:487,l:40,wd:true,fm:true,w:40,p:0};
I.u[750] = {n:"Kouartz",nn:"KOUARTZ",t:40,d:"Cet alliage très étrange est capable de renfermer une énergie impressionnante, le rendant très instable dans certaines conditions. Les meilleurs Forgerons raffolent de cet alliage, qui leur permet de forger des armes aux pouvoirs presque légendaires.",ep:1,g:664,l:60,wd:true,fm:true,w:60,p:0};
I.u[752] = {n:"Sang du Vampire",nn:"SANG DU VAMPIRE",t:15,d:"Ce sang est magique, ce qui en fait un ingrédient indispensable pour le Boudin Noir magique. Les effets secondaires sont cependant encore inconnus.",ep:2,g:89,l:15,wd:true,fm:true,w:1,p:100};
I.u[754] = {n:"Cape du Vampire",nn:"CAPE DU VAMPIRE",t:17,d:"Cette cape rouge sang a baigné dans le sang pendant des années. Ce type de cape est très prisée lors des Fêtes Dalouhines.",ep:7,g:17,l:29,wd:true,fm:true,w:13,p:2500};
I.u[756] = {n:"Slip en Cuir Moulant du Vampire",nn:"SLIP EN CUIR MOULANT DU VAMPIRE",t:56,d:"Pour se protéger des coups critiques aux endroits critiques, le Vampire utilise parfois son Slip en Cuir lorsqu\'il doit partir en chasse. Ce Slip lui permet également de protéger les endroits critiques des coups de soleil.",ep:3,g:622,l:15,wd:true,fm:true,w:1,p:100};
I.u[757] = {n:"Gelée Bleutée",nn:"GELEE BLEUTEE",t:110,d:"Cette gelée est particulièrement collante, un peu comme une Bloqueuse enragée. Étrangement, elle colle particulièrement bien les plumes de Tofus. On se demande bien à quoi cela peut bien servir...",ep:5,g:42,l:3,wd:true,fm:true,w:1,p:1};
I.u[758] = {n:"Cape du Justicier",nn:"CAPE DU JUSTICIER",t:17,d:"Cette cape ne peut être portée que par les grands justiciers d\'Amakna, c\'est à dire presque personne.",ep:1,g:2,l:30,wd:true,fm:true,w:10,c:"Ps=1&Pa>20",p:2550};
I.u[759] = {n:"Cape du Poolay Chogrelotant",nn:"CAPE DU POOLAY CHOGRELOTANT",t:17,d:"Cette magnifique cape a été confectionnée avec les plumes du dernier Poolay d\'Amakna. Véritable rareté, les collectionneurs rêvent de l\'avoir dans leur collection.",ep:8,g:3,l:33,wd:true,fm:true,w:4,p:5000};
I.u[760] = {n:"Pain au curry sans curry",nn:"PAIN AU CURRY SANS CURRY",t:33,d:"Un prototype de Kwaker Vans, son goût et son efficacité sont toujours une surprise, malheureusement l\'odeur, elle, est toujours la même.",ep:2,g:12,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:1};
I.u[761] = {n:"Poils de souris",nn:"POILS DE SOURIS",t:54,d:"Ces poils de souris sont très fins et doux, et sont de ce fait très recherchés pour la confection de manteaux. Le manteau en peau et poils de souris fait fureur dans la haute société d\'Amakna, il paraît que ça fait classe d\'en porter.",ep:1,g:549,l:1,wd:true,fm:true,w:1,p:1};
I.u[764] = {n:"Amulette du Petit Hibou",nn:"AMULETTE DU PETIT HIBOU",t:1,d:"Cette petite amulette ravira les héros débutants.",ep:1,g:1,l:1,wd:true,fm:true,w:2,p:50};
I.u[765] = {n:"Faux de Farle Ingalsse",nn:"FAUX DE FARLE INGALSSE",t:22,d:"Cette Faux a appartenu à Farle Ingalsse en personne, elle est suffisamment aiguisée pour faucher tous les types de céréales et tous les types de monstres qui se cachent dans les champs de céréales.",ep:3,g:1,l:13,wd:true,fm:true,w:10,an:17,e:[5,4,1,1,45,50,false,false],c:"CC>10",p:650};
I.u[766] = {n:"Koliet Aclou",nn:"KOLIET ACLOU",t:1,d:"Cette amulette permet d\'occasionner plus de dégâts pour les aventuriers qui débutent.",ep:1,g:34,l:1,wd:true,fm:true,w:1,p:20};
I.u[767] = {n:"Gantie Royal",nn:"GANTIE ROYAL",t:9,d:"Malgré un nom pompeux, cet anneau rend juste un peu plus fort, et un peu plus bête.",ep:1,g:8,l:1,wd:true,fm:true,w:1,p:100};
I.u[768] = {n:"Camaïneux",nn:"CAMAINEUX",t:9,d:"Cet anneau influe sur le cours de votre destinée, il vous rend plus résistant, mais moins chanceux. Vous augmentez vos chances de vivre longtemps en diminuant vos chances de vivre heureux.",ep:1,g:33,l:1,wd:true,fm:true,w:1,p:100};
I.u[769] = {n:"Einekeineux",nn:"EINEKEINEUX",t:11,d:"Les bottes qui portent chance. Le problème c\'est qu\'elles vous rendent moins résistant en combat.",ep:1,g:5,l:2,wd:true,fm:true,w:1,p:200};
I.u[770] = {n:"Fecaflip",nn:"FECAFLIP",t:11,d:"Cette magnifique paire de bottes convertit un peu de votre force en intelligence.",ep:1,g:3,l:2,wd:true,fm:true,w:1,p:200};
I.u[771] = {n:"Mangeuse de Frêne",nn:"MANGEUSE DE FRENE",t:19,d:"Cette hache n\'a rien d\'exceptionnel, mais elle reste tout de même appréciée des Bûcherons débutants, qui l\'aiment pour sa simplicité d\'utilisation .",ep:1,g:9,l:8,wd:true,fm:true,w:10,an:17,e:[5,4,1,1,45,45,false,false],c:"CS>10&CC>10",p:400};
I.u[772] = {n:"Cape Edépée",nn:"CAPE EDEPEE",t:17,d:"On prétend que cette cape légendaire permet de devenir presque invincible si on la combine à l\'épée légendaire des escrimeurs, le Dard Tagnan et au chapeau légendaire des Six Rameaux de Bergers Rahks.",ep:7,g:4,l:22,wd:true,fm:true,w:10,p:1500};
I.u[773] = {n:"Cape Ellinie",nn:"CAPE ELLINIE",t:17,d:"Cette cape vient du pays des Patitaliennes, un endroit où l\'on parle avec les mains. Cela explique la grande liberté de mouvement laissée par celle-ci. Elle est en outre d\'une légèreté surprenante.",ep:1,g:5,l:14,wd:true,fm:true,w:1,p:775};
I.u[774] = {n:"Cape Ulais",nn:"CAPE ULAIS",t:17,d:"Venant du Mont Taigüe ou du monde Aigüe selon la manière dont vous le prononcez, cette cape a conduit à l\'empoisonnement de son ancienne propriétaire, mais bizarrement elle augmente la résistance de son porteur.",ep:1,g:7,l:14,wd:true,fm:true,w:3,p:775};
I.u[775] = {n:"Cape Abilité",nn:"CAPE ABILITE",t:17,d:"Tissée magiquement avec un soin infini, la Cape Abilité permet à son porteur de discerner les points faibles de ses adversaires pour leur porter des coups destructeurs.",ep:5,g:9,l:17,wd:true,fm:true,w:10,p:1000};
I.u[776] = {n:"Cape Sulcorpe",nn:"CAPE SULCORPE",t:17,d:"Ayant appartenu à une certaine Mulba, qui s\'en servait pour transporter des objets encombrants dans sa quête des Sept Poules de Cristal venues des étoiles, cette cape augmente considérablement la force.",ep:5,g:8,l:14,wd:true,fm:true,w:3,p:775};
I.u[777] = {n:"Cape du Coq Hû",nn:"CAPE DU COQ HU",t:17,d:"Le Coq ayant donné ses plumes pour la confection de cette cape s\'est débattu pendant plusieurs jours et plusieurs nuits avant de finalement tomber à bout de force et de se laisser déplumer. " + "\"" + "Hû !" + "\"" + " s\'exclama son propriétaire, lui aussi à bout de souffle.",ep:5,g:10,l:44,wd:true,fm:true,w:10,p:5275};
I.u[778] = {n:"Cape de Mori Arty",nn:"CAPE DE MORI ARTY",t:17,d:"Cette authentique cape de méchant est une preuve de votre appartenance à la bande du diaboliquement génial Mori Arty, dont on raconte que seule sa malchance égalait sa stupéfiante intelligence. Ne peut être portée que par les plus grands méchants.",ep:2,g:11,l:6,wd:true,fm:true,w:1,c:"Ps=2&Pa>30",p:20000};
I.u[779] = {n:"Cape Maj\'Hic",nn:"CAPE MAJ\'HIC",t:17,d:"Cette cape d\'apparence quelconque n\'est en fait pas tout à fait quelconque. Elle rendrait invisible son porteur aux yeux de ceux qui ne le regardent pas. Impressionnant !",ep:8,g:12,l:34,wd:true,fm:true,w:15,p:3300};
I.u[781] = {n:"Cape Itou Lascione",nn:"CAPE ITOU LASCIONE",t:17,d:"Cette superbe cape a appartenu au Grand Stratéquerre. Ce célèbre seigneur de guerre et fin stratège, avide de conquête de l\'univers, a bien vite abandonné tous ses projets après avoir revêtu cette cape. Étrange !",ep:8,g:7,l:21,wd:true,fm:true,w:10,p:1400};
I.u[782] = {n:"Agride",nn:"AGRIDE",t:19,d:"Cette hache est assez particulière, elle est à la base destinée à l\'apprentissage du métier de Bûcheron. Correctement utilisée, elle peut sectionner en deux un jeune Abraknyde trop insouciant, ce qui avouons-le, est très rare.",ep:1,g:11,l:27,wd:true,fm:true,w:20,an:17,e:[5,4,1,1,45,45,false,false],c:"CW>15",p:2200};
I.u[783] = {n:"Amulette du Bûcheron",nn:"AMULETTE DU BUCHERON",t:1,d:"Cette amulette, exclusivement fabriquée à partir de bois précieux, est très prisée des Bûcherons qui lui attribuent bien souvent des pouvoirs qu\'elle n\'a pas.",ep:1,g:9,l:22,wd:true,fm:true,w:2,p:1500};
I.u[784] = {n:"Bottes du Chat Buté",nn:"BOTTES DU CHAT BUTE",t:11,d:"Le Chat Buté est le surnom donné à un célèbre Ecaflip. Aucun Ecaflip n\'aura été plus têtu que lui, et en combat seule sa chance le sauvait de son inexistante intelligence.  \n" + "\"" + "Qui qu\'c\'est qu\'y\'est buté comme un caillou ?" + "\"" + ", s\'exclamait-il souvent.",ep:1,g:3,l:2,wd:true,fm:true,w:1,p:200};
I.u[785] = {n:"Anneau Fortifiant",nn:"ANNEAU FORTIFIANT",t:9,d:"Cet anneau, transmis de Grand père Enutrof en Père Enutrof, procure à son porteur une formidable force. Celui-ci n\'en est qu\'une pâle copie, mais il saura tout de même satisfaire les collectionneurs, qu\'ils soient Enutrof ou non.",ep:1,g:29,l:11,wd:true,fm:true,w:1,p:525};
I.u[786] = {n:"Pendentif du Hibou Chétif",nn:"PENDENTIF DU HIBOU CHETIF",t:1,d:"Née d\'une erreur de manipulation d\'un bijoutier, cette amulette augmente l\'intelligence de son porteur. En revanche, elle donne généralement un puissant mal de crâne qui a pour effet d\'amoindrir la condition physique de son propriétaire.",ep:1,g:1,l:2,wd:true,fm:true,w:1,p:200};
I.u[787] = {n:"Anneau Rexik",nn:"ANNEAU REXIK",t:9,d:"Cet étrange petit anneau a la faculté d\'augmenter l\'agilité de son porteur, pourtant étrangement frappé d\'un manque de chance.",ep:1,g:31,l:1,wd:true,fm:true,w:1,p:100};
I.u[789] = {n:"Ceinture du Kobeer",nn:"CEINTURE DU KOBEER",t:10,d:"Cette ceinture fait fureur auprès de la jeune population d\'Amakna. Un couturier Sadida en est à l\'origine. Lors de sa première vente, il s\'exclama : " + "\"" + "La vie est courte, jouez durement !" + "\"" + ".",ep:2,g:9,l:1,wd:true,fm:true,w:1,p:10};
I.u[790] = {n:"Cape de Bonne Espérance",nn:"CAPE DE BONNE ESPERANCE",t:17,d:"Les effets magiques de cette cape sont souvent surestimés, ce qui a causé la perte de nombreux aventuriers, notamment, le tristement célèbre Ménestrel Francisque Kabroule.",ep:8,g:5,l:30,wd:true,fm:true,w:8,p:2525};
I.u[791] = {n:"Abraknydi Vivitus",nn:"ABRAKNYDI VIVITUS",t:4,d:"C\'est un Bâton en racines d\'Abraknyde encore vivantes. Attention, ce dernier a une fâcheuse tendance à se retourner contre son maître. Il peut être très efficace lorsqu\'il décide de s\'attaquer à la même cible que celle de son maître.",ep:2,g:1,l:34,wd:true,fm:true,w:10,an:12,e:[5,2,1,1,50,30,false,false],c:"CC>30&CI>34&CW>15",p:3400};
I.u[792] = {n:"Sève d\'Abraknyde",nn:"SEVE D\'ABRAKNYDE",t:15,d:"Cette sève sirupeuse ne sert à rien sauf si vous être prêt à attendre qu\'elle se fossilise en Ambre. On raconte cependant, qu\'une Enutrofette astucieuse et particulièrement séduisante a trouvé un moyen pour accélérer ce processus.",ep:1,g:162,l:45,wd:true,fm:true,w:2,p:50};
I.u[793] = {n:"Potion de Vieillesse",nn:"POTION DE VIEILLESSE",t:12,d:"Cette potion permet d\'accélérer certains processus chimiques, comme la formation de précipités. C\'est un peu comme une levure surpuissante, le mauvais goût en moins.",ep:1,g:2,l:1,wd:true,fm:true,w:1,p:100};
I.u[795] = {n:"Parchemin de Force",nn:"PARCHEMIN DE FORCE",t:76,d:"(Double click pour gagner de la force) \nCe parchemin permet de gagner de la force de manière définitive sans prendre de risques. \nAttention cependant, vous ne pourrez l\'utiliser que si votre force est inférieure à 50.",ep:1,g:34,l:1,wd:true,fm:true,w:1,c:"Cs<50",u:true,p:2500};
