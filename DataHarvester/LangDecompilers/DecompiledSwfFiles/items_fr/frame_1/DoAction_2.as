I.u[848] = {n:"Vieil Os de Dragoeuf",nn:"VIEIL OS DE DRAGOEUF",t:47,d:"Ce vieil os est magique, correctement utilisé, il devrait permettre de confectionner des objets très puissants.",ep:11,g:15,l:70,wd:true,fm:true,w:20,p:100};
I.u[849] = {n:"Anneau Forcesque",nn:"ANNEAU FORCESQUE",t:9,d:"Ce petit anneau donne de la Force à son porteur.",ep:1,g:5,l:1,wd:true,fm:true,w:1,p:50};
I.u[850] = {n:"Anneau Agilesque",nn:"ANNEAU AGILESQUE",t:9,d:"Étrangement, les collectionneurs Amaknéens s\'arrachent cet anneau alors qu\'il ne vaut pas tripette.",ep:1,g:1,l:1,wd:true,fm:true,w:1,p:50};
I.u[851] = {n:"Anneau Vitalesque",nn:"ANNEAU VITALESQUE",t:9,d:"Ce petit anneau augmente la vie de son porteur.",ep:1,g:2,l:1,wd:true,fm:true,w:1,p:50};
I.u[852] = {n:"Petit Anneau Magique",nn:"PETIT ANNEAU MAGIQUE",t:9,d:"Ce petit anneau augmente l\'intelligence de son porteur.",ep:1,g:3,l:1,wd:true,fm:true,w:1,p:50};
I.u[853] = {n:"Petit Anneau du Chanceux",nn:"PETIT ANNEAU DU CHANCEUX",t:9,d:"Ce petit anneau augmente la chance de son porteur.",ep:1,g:42,l:1,wd:true,fm:true,w:1,p:50};
I.u[854] = {n:"Amulette du Petit Ours",nn:"AMULETTE DU PETIT OURS",t:1,d:"Cette petite amulette augmente la force de son porteur",ep:1,g:5,l:1,wd:true,fm:true,w:1,p:50};
I.u[856] = {n:"Grenouillette",nn:"GRENOUILLETTE",t:1,d:"Cette petite amulette augmente l\'agilité de son porteur.",ep:1,g:2,l:1,wd:true,fm:true,w:1,p:50};
I.u[859] = {n:"Ceinture d\'Agilité",nn:"CEINTURE D\'AGILITE",t:10,d:"Cette modeste ceinture augmente l\'agilité de son porteur.",ep:1,g:8,l:1,wd:true,fm:true,w:1,p:50};
I.u[860] = {n:"Ceinture de Chance",nn:"CEINTURE DE CHANCE",t:10,d:"Cette modeste ceinture augmente la chance de son porteur.",ep:1,g:3,l:1,wd:true,fm:true,w:1,p:50};
I.u[861] = {n:"Petites Bottes de Force",nn:"PETITES BOTTES DE FORCE",t:11,d:"Ces bottes augmentent la force de leur porteur au détriment de son agilité, ce qui tend à le rendre différent d\'un Tofu, sans pour autant le rendre moins idiot.",ep:1,g:3,l:2,wd:true,fm:true,w:1,p:200};
I.u[862] = {n:"Sandales d\'Intelligence",nn:"SANDALES D\'INTELLIGENCE",t:11,d:"Ces bottes permettent d\'augmenter l\'intelligence de leur porteur, tout en lui conférant un style vraiment à la mode par les temps qui courent grâce à leur semelle en Bois de Noyer qui offre une adhérence remarquable.",ep:1,g:4,l:8,wd:true,fm:true,w:1,p:400};
I.u[863] = {n:"Bottes Agilesques",nn:"BOTTES AGILESQUES",t:11,d:"Ces bottes permettent d\'augmenter l\'agilité de leur porteur, afin qu\'il puisse se prendre pour un Tofu plus facilement, lorsque l\'envie lui prend de faire l\'idiot.",ep:1,g:5,l:8,wd:true,fm:true,w:1,p:400};
I.u[864] = {n:"Bottes de Chance",nn:"BOTTES DE CHANCE",t:11,d:"Ces bottes augmentent la chance de leur porteur afin de l\'aider à trouver des objets très puissants, comme de la bave de Bouftou par exemple.",ep:1,g:1,l:8,wd:true,fm:true,w:1,p:100};
I.u[867] = {n:"Dragolyre",nn:"DRAGOLYRE",t:1,d:"Cette splendide amulette confectionnée à partir de restes de Dragoeuf, est suffisamment impressionnante pour faire battre en retrait la majorité des ennemis normalement constitués.",ep:11,g:9,l:32,wd:true,fm:true,w:2,p:4700};
I.u[878] = {n:"Parchemin de Boulanger",nn:"PARCHEMIN DE BOULANGER",t:13,d:"(Double clic pour utiliser) \nCe parchemin permet de gagner des points d\'expérience dans le métier de Boulanger.",ep:1,g:33,l:1,wd:true,fm:true,w:1,u:true,p:1000};
I.u[879] = {n:"Parchemin de Cordonnier",nn:"PARCHEMIN DE CORDONNIER",t:13,d:"(Double clic pour utiliser) \nCe parchemin permet de gagner des points d\'expérience dans le métier de Cordonnier.",ep:1,g:34,l:1,wd:true,fm:true,w:1,u:true,p:1000};
I.u[880] = {n:"Laine de Bouftou Royal",nn:"LAINE DE BOUFTOU ROYAL",t:57,d:"Cette laine est royale, elle est douce et légère, mais a tendance à démanger les peaux sensibles. Cette laine est en revanche très usée donc très fragile, étant donné l\'âge du Bouftou Royal.",ep:1,g:207,l:35,wd:true,fm:true,w:1,p:4000};
I.u[881] = {n:"Laine de Boufton Blanc",nn:"LAINE DE BOUFTON BLANC",t:57,d:"Cette laine est extrêmement douce, car elle provient du duvet d\'un Boufton. Attention tout de même, cette laine est encore imprégnée d\'une odeur qui pourrait inciter la mère du Boufton à vous lécher si vous entrez en contact avec cette laine.",ep:1,g:208,l:2,wd:true,fm:true,w:1,p:1};
I.u[882] = {n:"Laine du Chef de Guerre Bouftou",nn:"LAINE DU CHEF DE GUERRE BOUFTOU",t:57,d:"Cette laine sent très mauvais, mais elle a appartenu à un Chef de Guerre Bouftou, ce qui lui confère une grande valeur sentimentale auprès des guerriers.",ep:1,g:209,l:19,wd:true,fm:true,w:1,p:1};
I.u[883] = {n:"Cuir de Boufton Blanc",nn:"CUIR DE BOUFTON BLANC",t:56,d:"Ce cuir très lisse est particulièrement adapté à la confection de sous-vêtements en cuir, le Slip en Cuir du Chafer en est un exemple frappant.",ep:1,g:534,l:2,wd:true,fm:true,w:1,p:1};
I.u[884] = {n:"Cuir de Boufton Noir",nn:"CUIR DE BOUFTON NOIR",t:56,d:"Ce cuir luisant est très apprécié des Cordonniers et des tailleurs, pour la confection de petites bottes ou de chapeaux.",ep:1,g:16,l:2,wd:true,fm:true,w:1,p:1};
I.u[885] = {n:"Laine de Boufton Noir",nn:"LAINE DE BOUFTON NOIR",t:57,d:"Cette laine rugueuse n\'est pas très utilisée par les tailleurs, elle est en revanche très appréciée des Enutrofs, qui s\'en servent pour cacher leur calvitie naissante.",ep:1,g:210,l:2,wd:true,fm:true,w:1,p:1};
I.u[886] = {n:"Cuir de Bouftou Royal",nn:"CUIR DE BOUFTOU ROYAL",t:56,d:"Ce cuir est royal, il est d\'une souplesse extraordinaire, et les crevasses qu\'il arbore, sont les témoins de la longue vie du Bouftou Royal. Il n\'existe pas meilleur cuir dans toute la province d\'Amakna.",ep:1,g:535,l:35,wd:true,fm:true,w:1,p:4000};
I.u[887] = {n:"Cuir du Chef de Guerre Bouftou",nn:"CUIR DU CHEF DE GUERRE BOUFTOU",t:56,d:"" + "\"" + "Ce cuir a du faire toutes les guerres, pour être aussi dur aujourd\'hui" + "\"" + " telles étaient les paroles du célèbre Ménestrel Francisque Kabroule, juste avant de mourir asphyxié par l\'odeur d\'un Chef de Guerre Bouftou.",ep:1,g:534,l:19,wd:true,fm:true,w:1,p:1};
I.u[888] = {n:"Grandes Bottes de Concentration",nn:"GRANDES BOTTES DE CONCENTRATION",t:11,d:"Ces bottes sont si confortables qu\'elles permettent au héros qui les chausse d\'accroître sa concentration.",ep:1,g:6,l:26,wd:true,fm:true,w:15,p:1950};
I.u[889] = {n:"Puissantes Bottes de Concentration",nn:"PUISSANTES BOTTES DE CONCENTRATION",t:11,d:"Ces bottes sont si confortables qu\'elles permettent au héros qui les chausse d\'accroître sa concentration.",ep:1,g:6,l:27,wd:true,fm:true,w:15,p:2200};
I.u[890] = {n:"Bottes du Bouftou",nn:"BOTTES DU BOUFTOU",t:11,d:"Ces bottes sont confectionnées en cuir de Bouftou, elles permettent aux éleveurs de s\'approcher plus facilement des Bouftous pour les tuer, grâce à l\'odeur qu\'elles dégagent.",ep:1,g:3,l:1,wd:true,fm:true,w:3,p:150};
I.u[891] = {n:"Bottes du Grand Bouftou",nn:"BOTTES DU GRAND BOUFTOU",t:11,d:"Ces bottes sont confectionnées en cuir de Bouftou, elles permettent aux éleveurs de s\'approcher plus facilement des Bouftous pour les tuer, grâce à l\'odeur qu\'elles dégagent.",ep:1,g:3,l:2,wd:true,fm:true,w:4,p:200};
