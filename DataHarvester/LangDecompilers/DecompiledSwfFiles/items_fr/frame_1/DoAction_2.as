I.u[836] = {n:"Abranneau",nn:"ABRANNEAU",t:9,d:"Cet anneau, forgé dans de l\'écorce d\'Abraknyde possède de grands pouvoirs et implique donc de grandes responsabilités. Cependant, malgré sa valeur, des rumeurs laissent entendre qu\'il est maudit.",ep:1,g:11,l:5,wd:true,fm:true,w:1,p:100};
I.u[837] = {n:"Bottes de Kluh",nn:"BOTTES DE KLUH",t:11,d:"Ces bottes augmentent la force de leur porteur, mais risquent de le rendre un peu idiot au bout d\'un certain temps, voire de le transformer en Iop en cas d\'utilisation régulière et prolongée",ep:1,g:3,l:8,wd:true,fm:true,w:1,p:400};
I.u[838] = {n:"Sang d\'Oni",nn:"SANG D\'ONI",t:15,d:"Cette fiole de sang d\'Oni est très recherchée par les alchimistes expérimentés qui désirent donner à certaines armes, des propriétés particulièrement effrayantes.",ep:4,g:662,l:75,wd:true,fm:true,w:1,p:100};
I.u[840] = {n:"Poils du Minotoror",nn:"POILS DU MINOTOROR",t:54,d:"Ces poils sentent très mauvais, mais sont d\'une résistance à toute épreuve, porter sur soi une étoffe en poils de Minotoror fera de vous une personne redoutée de vos adversaires, et appréciée des mouches.",ep:2,g:201,l:125,wd:true,fm:true,w:1,p:100};
I.u[841] = {n:"Anneau de Satisfaction",nn:"ANNEAU DE SATISFACTION",t:9,d:"Cet anneau permettra a votre héros d\'augmenter ses PA, en contrepartie de sa force et de son intelligence.",ep:1,g:12,l:8,wd:true,fm:true,w:1,p:10};
I.u[842] = {n:"Coquille de Dragoeuf Doré",nn:"COQUILLE DE DRAGOEUF DORE",t:111,d:"Cette coquille est presque incassable, cependant, en la polissant pendant des générations, les meilleurs Bijoutiers peuvent utiliser ses facultés magiques pour confectionner des amulettes aux pouvoirs exceptionnels.",ep:11,g:202,l:64,wd:true,fm:true,w:5,p:1};
