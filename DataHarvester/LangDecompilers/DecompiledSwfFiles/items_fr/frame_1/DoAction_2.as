I.u[910] = {n:"Bottes d\'Hogmeiser",nn:"BOTTES D\'HOGMEISER",t:11,d:"Ces bottes ont appartenu à Hogmeiser pendant des années, avant qu\'il ne les perdent lors d\'un pari stupide avec Grizmine. La morale de l\'histoire, c\'est qu\'il ne faut jamais jouer avec un Ecaflip.",ep:2,g:11,l:76,wd:true,fm:true,w:20,p:15000};
I.u[911] = {n:"Bottes Usées d\'Hogmeiser",nn:"BOTTES USEES D\'HOGMEISER",t:11,d:"Ces bottes usées ont appartenu à Hogmeiser pendant des années, elles sont tellement usées, qu\'elles sont surtout handicapantes. Un très bon Cordonnier devrait pouvoir les réparer avec de la chance.",ep:1,g:11,l:1,wd:true,fm:true,w:10,p:10};
I.u[912] = {n:"Bottes Dorées d\'Hogmeiser",nn:"BOTTES DOREES D\'HOGMEISER",t:11,d:"Ces magnifiques bottes d\'Hogmeiser sont recouvertes d\'une fine couche de pierres précieuses, pour les rendre insensibles à certaines attaques magiques. C\'est avec ces bottes que Brouce Boulgoure a écrasé la tête du Roi des Abraknydes il y a très longtemps, pour animer l\'ouverture de la foire du Trool.",ep:2,g:12,l:99,wd:true,fm:true,w:30,p:25000};
I.u[916] = {n:"Amulette de Silicate",nn:"AMULETTE DE SILICATE",t:1,d:"Silicate a confectionné cette Amulette un matin d\'hiver, pour un célèbre aventurier du nom de Rimb Vardi, qui pourchassait un vieux Bwork Mage, responsable de plusieurs catastrophes naturelles déclenchées par sa magie trop approximative.",ep:1,g:9,l:37,wd:true,fm:true,w:2,p:3700};
I.u[917] = {n:"La Feuille d\'Automne",nn:"LA FEUILLE D\'AUTOMNE",t:1,d:"Cette Broche a été confectionnée un matin d\'automne au milieu d\'une forêt de Hêtres et d\'Ormes non loin de l\'île d\'Olag-lacustre. Elle est considérée par nombre de bûcherons comme une véritable relique.",ep:7,g:64,l:89,wd:true,fm:true,w:2,p:20500};
I.u[918] = {n:"Ambre Ancestrale",nn:"AMBRE ANCESTRALE",t:50,d:"Cette ambre provient d\'un Abraknyde Ancestral, elle a donc une valeur inestimable, et peut conférer des propriétés magiques à certains objets si elle est travaillée par des mains habiles.",ep:9,g:108,l:100,wd:true,fm:true,w:2,p:200};
I.u[919] = {n:"Dagues Ancestrales",nn:"DAGUES ANCESTRALES",t:5,d:"Ces puissantes dagues, forgées à l\'ancienne à partir d\'Ambre Ancestrale, ont servi pendant la colonisation de l\'île de Moon, avant que cette dernière ne redevienne sauvage.",ep:9,g:8,l:34,wd:true,fm:true,w:10,an:13,e:[10,3,1,1,30,50,false,false],c:"CA>34&CI>10",p:3300};
I.u[920] = {n:"Bois Ancestral",nn:"BOIS ANCESTRAL",t:38,d:"Ce bois provient d\'un Orme de l\'île d\'Olag-lacustre, qui a été foudroyé un matin d\'automne, par un Bwork Mage qui maîtrisait assez mal ses sorts élémentaires.",ep:2,g:678,l:100,wd:true,fm:true,w:1,p:0};
I.u[922] = {n:"Marteau du Forgeur de Haches",nn:"MARTEAU DU FORGEUR DE HACHES",t:7,d:"Outil indispensable à la création de haches.",ep:1,g:2,l:1,wd:true,fm:true,w:15,an:15,e:[5,4,1,1,50,50,false,false],p:150};
I.u[923] = {n:"Fendeuse d\'Abraknyde",nn:"FENDEUSE D\'ABRAKNYDE",t:19,d:"Cette hache de guerre a été forgée par Kademlia Boulgoure, pour combattre les plus terrifiants Abraknydes. La légende raconte que cette arme légendaire a tué en une nuit plus d\'Abraknydes qu\'il n\'existe d\'arbres en Amakna.",ep:7,g:12,l:74,wd:true,fm:true,w:190,an:17,e:[5,4,1,1,45,45,false,false],c:"CS>148&CW>10&CV>30",p:14400};
