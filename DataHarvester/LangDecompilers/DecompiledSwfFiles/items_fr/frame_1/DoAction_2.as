I.u[357] = {n:"La Queue",nn:"LA QUEUE",t:6,d:"Epée symbolisant la partie de leur corps dont les Ecaflips sont le plus fier. Cette épée résume bien la façon de faire des joueurs.",ep:1,g:18,l:13,wd:true,fm:true,w:20,an:14,e:[20,4,1,1,15,15,false,false],c:"CS>26&CA>10",p:650};
I.u[358] = {n:"La Queue Infernale",nn:"LA QUEUE INFERNALE",t:6,d:"La Queue Infernale est vraiment infernale... De quoi rendre votre adversaire ou vos coéquipiers complètement fous, et surtout de transformer vos coéquipiers en adversaires si vous vous y prenez mal. En revanche, le cas inverse ne semble jamais se produire.",ep:1,g:18,l:21,wd:true,fm:true,w:20,an:14,e:[20,4,1,1,15,15,false,false],c:"CS>42&CA>10",p:1400};
I.u[359] = {n:"L\'Ecaliseur",nn:"L\'ECALISEUR",t:9,d:"Cet anneau a été offert il y bien longtemps à Isidor, un chat malin, par le roi Allister, qui reconnu l\'agilité inégalable de cet Ecaflip.",ep:1,g:35,l:29,wd:true,fm:true,w:3,p:2400};
I.u[360] = {n:"Oreilles du Tiwabbit",nn:"OREILLES DU TIWABBIT",t:106,d:"Ces petites oreilles recouvertes de poils bruns sentent le mulou mort une fois bouillies. Elles sont en revanche très appréciées lorsqu\'elles sont fourrées avec de la gelée royale.",ep:8,g:30,l:11,wd:true,fm:true,w:1,p:10};
I.u[361] = {n:"Cawotte",nn:"CAWOTTE",t:68,d:"C\'est l\'unique nourriture du Tiwabbit Kiafin, ce qui explique peut-être l\'origine de sa débilité profonde.",ep:2,g:31,l:20,wd:true,fm:true,w:1,p:2};
I.u[362] = {n:"Peau de Larve Bleue",nn:"PEAU DE LARVE BLEUE",t:59,d:"Cette peau toute lisse constitue un très bon ingrédient pour les potions magiques.",ep:1,g:32,l:2,wd:true,fm:true,w:1,p:2};
I.u[363] = {n:"Peau de Larve Orange",nn:"PEAU DE LARVE ORANGE",t:59,d:"Cette peau toute lisse constitue un excellent ingrédient pour les potions magiques.",ep:1,g:33,l:9,wd:true,fm:true,w:1,p:2};
I.u[364] = {n:"Peau de Larve Verte",nn:"PEAU DE LARVE VERTE",t:59,d:"Cette peau toute lisse constitue un excellent ingrédient pour les potions magiques.",ep:1,g:34,l:7,wd:true,fm:true,w:1,p:100};
I.u[365] = {n:"Patte d\'Arakne",nn:"PATTE D\'ARAKNE",t:103,d:"Une patte très fine, mais très poilue. Très prisée des alchimistes pour ses propriétés euphorisantes, c\'est la petite faiblesse qui les perdra.",ep:1,g:35,l:2,wd:true,fm:true,w:1,p:10};
I.u[366] = {n:"Bec du Tofu",nn:"BEC DU TOFU",t:47,d:"Ce ridicule petit bec de Tofu, d\'apparence anodine, est cependant très aiguisé. Les archers d\'Amakna les utilisent depuis des lustres comme pointes de flèches.",ep:1,g:37,l:4,wd:true,fm:true,w:1,p:2};
I.u[367] = {n:"Oeuf de Tofu",nn:"OEUF DE TOFU",t:105,d:"Cet oeuf a la particularité de sentir l\'oeuf pourri même lorsqu\'il n\'est pas pourri, imaginez un peu ce qu\'il sent lorsqu\'il est réellement pourri. Personne ne sait en revanche, qui du Tofu ou de l\'Oeuf de Tofu est arrivé en premier en Amakna.",ep:1,g:38,l:4,wd:true,fm:true,w:1,p:1};
I.u[368] = {n:"Gelée à la Fraise",nn:"GELEE A LA FRAISE",t:110,d:"Cette délicieuse gelée à la fraise a rendu plus d\'un aventurier un peu trop gourmand malade. Comme le dit si bien le fameux dicton, " + "\"" + "tout est poison, rien n\'est poison, c\'est la dose qui compte" + "\"" + ". Dans le cas de la gelée à la fraise, évitez l\'overdose.",ep:5,g:40,l:28,wd:true,fm:true,w:1,p:100};
I.u[369] = {n:"Gelée à la Menthe",nn:"GELEE A LA MENTHE",t:110,d:"Cette délicieuse gelée à la menthe a rendu plus d\'un aventurier un peu trop gourmand malade. Elle a un effet apaisant sur le moral de toute votre équipe si vous la consommez en cas de mauvaise haleine.",ep:5,g:41,l:10,wd:true,fm:true,w:1,p:100};
I.u[370] = {n:"Gelée Bleutée Royale",nn:"GELEE BLEUTEE ROYALE",t:110,d:"Cette délicieuse gelée est issue de la digestion et de la fermentation de miel par une Gelée Royale. Immangeable sous sa forme brute, cette gelée n\'en reste pas moins, un ingrédient de choix pour les confiseries.",ep:7,g:407,l:50,wd:true,fm:true,w:1,p:100};
I.u[371] = {n:"Sourcil de Moskito",nn:"SOURCIL DE MOSKITO",t:15,d:"Le Moskito dispose de longs et fins sourcils qui mettent en valeur son regard de grand charmeur. La grande sensibilité du moskito fait que ses sourcils sont souvent imprégnés de larmes aux propriétés méconnues.",ep:1,g:44,l:1,wd:true,fm:true,w:1,p:1};
I.u[372] = {n:"Bandeau du Black Tiwabbit",nn:"BANDEAU DU BLACK TIWABBIT",t:15,d:"Ce bandeau donne une allure de guerrier au Black Tiwabbit qui le porte. Cette tradition de porter un bandeau vient paraît-il du Wabbit Wambo.",ep:8,g:29,l:11,wd:true,fm:true,w:1,p:2};
I.u[373] = {n:"Tige de Pissenlit Diabolique",nn:"TIGE DE PISSENLIT DIABOLIQUE",t:36,d:"Cette tige, souple comme le roseau, se plie, mais ne casse pas, ce qui en fait un composant de choix pour la réalisation des fouets et autres martinets, indispensables à l\'éducation d\'un enfant.",ep:1,g:45,l:6,wd:true,fm:true,w:1,p:10};
I.u[374] = {n:"Langue du Pissenlit Diabolique",nn:"LANGUE DU PISSENLIT DIABOLIQUE",t:36,d:"La couleur verte de cette langue provient de la consommation abusive de gelée à la menthe dont le Pissenlit Diabolique est friand.",ep:1,g:555,l:5,wd:true,fm:true,w:1,p:2};
I.u[375] = {n:"Dent du Tofu Maléfique",nn:"DENT DU TOFU MALEFIQUE",t:47,d:"Le Tofu Maléfique a troqué son ridicule petit bec de Tofu contre de ridicules grandes dents. Il est difficile de savoir ce qui est le plus dangereux sans tester soi-même.",ep:1,g:47,l:3,wd:true,fm:true,w:1,p:1};
I.u[376] = {n:"Aile du Tofu Maléfique",nn:"AILE DU TOFU MALEFIQUE",t:104,d:"Cette ridicule petite aile ne sert pas à grand-chose au Tofu Maléfique, si ce n\'est à brasser de l\'air pour déséquilibrer les Moskitos.",ep:1,g:48,l:3,wd:true,fm:true,w:1,p:1};
I.u[377] = {n:"Epine du Champ Champ",nn:"EPINE DU CHAMP CHAMP",t:15,d:"Cette épine venimeuse protège le Champ Champ des attaques au corps à corps, mais l\'empêche en revanche de faire des câlins sans tuer ceux qu\'il aime. Heureusement pour lui, le Champ Champ utilise du Sporme qu\'il envoie à distance pour se reproduire.",ep:1,g:49,l:5,wd:true,fm:true,w:1,p:1};
I.u[378] = {n:"Sporme du Champ Champ",nn:"SPORME DU CHAMP CHAMP",t:15,d:"Cette sporme est la " + "\"" + "petite graine" + "\"" + " qui permet au Champ Champ de disséminer ses petits un peu partout, et surtout n\'importe où. Attention, les spormes du Champ Champ sont attirées par les pieds malodorants et l\'odeur des Bottes du Bowisse.",ep:1,g:50,l:5,wd:true,fm:true,w:1,p:1};
I.u[379] = {n:"Pince du Crabe",nn:"PINCE DU CRABE",t:47,d:"Cette pince de taille démesurée est presque creuse, elle permet juste au Crabe d\'épater la galerie et de se faire passer pour un boss. Elle est en revanche réputée incassable.",ep:1,g:51,l:4,wd:true,fm:true,w:1,p:1};
I.u[380] = {n:"Feuille de Menthe Sauvage",nn:"FEUILLE DE MENTHE SAUVAGE",t:36,d:"C\'est l\'aliment de base de la Gelée Verte, si vous sentez une odeur de menthe, n\'en déduisez donc pas obligatoirement qu\'il y a de la menthe près de vous.",ep:1,g:52,l:30,wd:true,fm:true,w:1,p:0};
I.u[381] = {n:"Fraise",nn:"FRAISE",t:46,d:"C\'est l\'aliment de base de la Gelée à la fraise, si vous sentez une odeur de fraise, vous êtes peut-être sur son territoire de chasse.",ep:7,g:53,l:28,wd:true,fm:true,w:1,p:1};
I.u[382] = {n:"Ongle de Chevaucheur de Karne",nn:"ONGLE DE CHEVAUCHEUR DE KARNE",t:47,d:"Cet ongle sale et usé a traîné n\'importe où, mais surtout dans des endroits peu recommandables. Il est donc préférable de ne pas à votre tour le mettre n\'importe où et encore moins dans des endroits peu recommandables.",ep:3,g:54,l:16,wd:true,fm:true,w:1,p:1};
I.u[383] = {n:"Corne de Bouftou",nn:"CORNE DE BOUFTOU",t:47,d:"Les Cornes de Bouftou sont luisantes et douces comme du cuir, pour des raisons inconnues, les Osamodas les affectionnent particulièrement.",ep:1,g:55,l:9,wd:true,fm:true,w:1,p:1};
I.u[384] = {n:"Laine de Bouftou",nn:"LAINE DE BOUFTOU",t:57,d:"Cette laine épaisse sent le Bwork qui se néglige, elle est cependant très chaude et surtout, elle ne démange pas les peaux sensibles, ce que les Eniripsa ne manquent pas d\'apprécier pour leurs sous-vêtements.",ep:1,g:56,l:9,wd:true,fm:true,w:1,p:1};
I.u[385] = {n:"Bave de Bouftou",nn:"BAVE DE BOUFTOU",t:15,d:"Cette bave purulente tire son odeur du fait que le Bouftou passe son temps à se lécher la laine, ce qui est également à l\'origine de l\'haleine exécrable du Bouftou.",ep:1,g:57,l:9,wd:true,fm:true,w:1,p:1};
I.u[386] = {n:"Groin du Sanglier",nn:"GROIN DU SANGLIER",t:15,d:"Ce groin est un ustensile de choix pour débusquer les plus gros glands qui traînent dans les sous-bois. Contrairement au groin du Dragon Cochon, celui-ci est immangeable du fait de son odeur répugnante.",ep:1,g:58,l:14,wd:true,fm:true,w:1,p:1};
I.u[387] = {n:"Défense du Sanglier",nn:"DEFENSE DU SANGLIER",t:47,d:"Cette défense permet au Sanglier de se défendre et accessoirement de curer son groin débusqueur de glands. N\'oubliez donc pas de les nettoyer à l\'eau vive avant d\'en faire de la poudre.",ep:1,g:59,l:14,wd:true,fm:true,w:1,p:1};
I.u[388] = {n:"Poils du Sanglier",nn:"POILS DU SANGLIER",t:54,d:"Ces poils particulièrement résistants sont utilisés entre autres pour la confection de pinceaux et de balais. Certaines personnes subissent d\'importantes allergies au contact de ces poils, il est donc recommandé de ne pas s\'y frotter avec vigueur.",ep:1,g:60,l:14,wd:true,fm:true,w:1,p:1};
I.u[389] = {n:"Huile de Tournesol Sauvage",nn:"HUILE DE TOURNESOL SAUVAGE",t:60,d:"Cette huile est fortement recommandée par les diététiciens en raison de la forte proportion d\'acides gras essentiels qu\'elle contient. De plus, son contenu élevé en gras polyinsaturé et faible en gras saturé en fait un choix judicieux.",ep:1,g:61,l:12,wd:true,fm:true,w:1,p:100};
I.u[390] = {n:"Huile de Noix",nn:"HUILE DE NOIX",t:60,d:"Huile au goût très fruité, utilisée en pâtisserie pour parfumer certains gâteaux. Cette huile faible en gras saturé se conserve peu de temps, même dans une bouteille qui n\'a pas été ouverte.",ep:4,g:62,l:32,wd:true,fm:true,w:1,p:100};
I.u[391] = {n:"Noix de Cajou",nn:"NOIX DE CAJOU",t:58,d:"Cette Noix de Cajou permet de faire de l\'huile si elle est récoltée encore verte. Une fois séchée, elle peut toujours servir de jouet pour les enfants de Iop un peu simplets.",ep:4,g:63,l:32,wd:true,fm:true,w:1,p:10};
I.u[392] = {n:"Noix de Pécan",nn:"NOIX DE PECAN",t:58,d:"Cette Noix de Pécan permet de faire de l\'huile, mais il est préférable de la consommer séchée au regard des effets laxatifs de son huile.",ep:4,g:64,l:32,wd:true,fm:true,w:1,p:10};
I.u[393] = {n:"Gland",nn:"GLAND",t:58,d:"Les sangliers adorent les glands, et tout le monde ou presque se demande bien pourquoi.",ep:1,g:65,l:14,wd:true,fm:true,w:1,p:10};
I.u[394] = {n:"Noisette",nn:"NOISETTE",t:58,d:"Cette petite Noisette est délicieuse lorsqu\'elle est associée à une barre chocolatée. Dommage que le chocolat n\'existe pas à Amakna.",ep:1,g:66,l:14,wd:true,fm:true,w:1,p:1};
I.u[395] = {n:"Trèfle à 5 feuilles",nn:"TREFLE A 5 FEUILLES",t:36,d:"Ce trèfle constitue un élément essentiel de la nourriture des Tabis. Les éleveurs font tout de même attention dans la mesure où le Trèfle à 5 feuilles provoque des ballonnements chez les tabis.",ep:1,g:67,l:20,wd:true,fm:true,w:1,p:0};
I.u[396] = {n:"Huile de Palme",nn:"HUILE DE PALME",t:60,d:"De goût et parfum légers, cette huile convient bien pour tous les usages, de la friture à la préparation de vinaigrettes.",ep:3,g:68,l:32,wd:true,fm:true,w:1,p:100};
I.u[397] = {n:"Huile de Koode",nn:"HUILE DE KOODE",t:60,d:"Cette huile très rare est un vrai élixir. Elle est si lubrifiante qu\'elle permet aux rouages et aux engrenages de ne jamais s\'user. Elle est issue d\'un savant mélange de diverses huiles.",ep:4,g:69,l:32,wd:true,fm:true,w:1,p:100};
I.u[398] = {n:"Fruit de Palme",nn:"FRUIT DE PALME",t:46,d:"Ce fruit étrange permet d\'obtenir une huile raffinée une fois pressé. Dommage que ce fruit soit presque impressable à cause de sa coquille aussi dure que la semelle des bottes du Bowisse.",ep:3,g:81,l:32,wd:true,fm:true,w:1,p:10};
I.u[399] = {n:"Huile de Sésame",nn:"HUILE DE SESAME",t:60,d:"Cette huile extraite des grains de sésame se distingue par son goût riche et délicat de noisettes grillées. Dommage que son odeur se rapproche plus de celle du Bouftou grillé que de la feuille de menthe sauvage.",ep:1,g:71,l:1,wd:true,fm:true,w:1,p:100};
I.u[400] = {n:"Orge",nn:"ORGE",t:34,d:"L\'orge est une céréale utilisée pour la bière mais aussi pour le bétail et parfois pour les bétails buveurs de bière.",ep:1,g:82,l:10,wd:true,fm:true,w:2,p:0};
I.u[401] = {n:"Houblon",nn:"HOUBLON",t:34,d:"La fleur du plant femelle de Houblon renferme les principes actifs utiles au brasseur, notamment les résines amères et les huiles essentielles. " + "\"" + "Cela me rappelle mes cours de Biologie, sauf que je n\'ai jamais suivi de cours de Biologie" + "\"" + ". Tels étaient les mots de Gabrielle Naye à la vue de cette description, il faut bien l\'avouer légèrement romancée.",ep:1,g:80,l:30,wd:true,fm:true,w:2,p:0};
I.u[405] = {n:"Malt",nn:"MALT",t:34,d:"C\'est le nom donné à l\'Orge germée. C\'est un ingrédient de choix pour les bières de qualité. Attention cependant, le malt est vicieux et a tendance à absorber la bière lors de sa germination, un peu comme le Tot après une dure journée de chasse aux Tofus.",ep:1,g:83,l:60,wd:true,fm:true,w:2,p:0};
I.u[406] = {n:"Estomac de Wo Wabbit",nn:"ESTOMAC DE WO WABBIT",t:15,d:"Cet estomac a la faculté de se dilater très facilement, ce qui simplifie grandement les digestions difficiles du Wo Wabbit qui a tendance à abuser des liqueurs de Cawottes fermentées.",ep:8,g:146,l:39,wd:true,fm:true,w:1,p:10};
I.u[407] = {n:"Pic du Prespic",nn:"PIC DU PRESPIC",t:47,d:"Pour se curer les dents ou se créer une nouvelle pointe de flèche, rien de mieux que le Pic du Prespic.",ep:1,g:134,l:18,wd:true,fm:true,w:1,p:50};
I.u[408] = {n:"Dentier du Chafer d\'élite",nn:"DENTIER DU CHAFER D\'ELITE",t:47,d:"Le Chafer d\'élite a les dents moisies par l\'alcool, c\'est pour cela qu\'il utilise un dentier. Ce dentier coûte une fortune car ses dents sont en or et l\'haleine fétide du Chafer d\'élite a doté ses dents de pouvoirs imprévisibles.",ep:9,g:129,l:43,wd:true,fm:true,w:1,p:50};
I.u[409] = {n:"Poils de Barbe du Bwork Mage",nn:"POILS DE BARBE DU BWORK MAGE",t:54,d:"Ces poils roux sentent très mauvais lorsqu\'il pleut, pour des raisons assez obscures. Cependant, les Bworks Mages semblent tirer une partie de leurs pouvoirs magiques de ces poils de barbe, ce qui pourrait intéresser certains alchimistes.",ep:4,g:131,l:23,wd:true,fm:true,w:1,p:20};
I.u[410] = {n:"Livre du Bwork Mage",nn:"LIVRE DU BWORK MAGE",t:25,d:"Ce livre est une vraie pièce de collection, c\'est de là que le Bwork Mage puise toute son inspiration et sa vigueur. Chaque page de ce livre recèle de nombreux secrets quant à l\'anatomie de certaines créatures d\'Amakna.",ep:4,g:130,l:1,wd:true,fm:true,w:1,p:100};
I.u[411] = {n:"Bec du Kwak de Glace",nn:"BEC DU KWAK DE GLACE",t:47,d:"Ce bec extraordinairement solide permet au Kwak de Glace de faire des trous dans la glace, juste pour le plaisir de faire des trous dans la glace. Les kwaks de Glace sont décidément bien étranges...",ep:1,g:133,l:24,wd:true,fm:true,w:1,p:20};
I.u[412] = {n:"Bec du Kwak de Flamme",nn:"BEC DU KWAK DE FLAMME",t:47,d:"Ce bec extraordinairement solide permet au Kwak de Flamme d\'éteindre des braises entre son bec, juste pour le plaisir d\'éteindre des braises entre son bec. Les kwaks de Flamme sont décidément bien étranges...",ep:5,g:132,l:24,wd:true,fm:true,w:1,p:20};
I.u[413] = {n:"Bec du Kwak du Vent",nn:"BEC DU KWAK DU VENT",t:47,d:"Ce bec extraordinairement solide permet au Kwak du Vent de se prendre des vents sans broncher, juste pour le plaisir de se prendre des vents sans broncher. Les kwaks du vent sont décidément bien étranges...",ep:5,g:110,l:24,wd:true,fm:true,w:1,p:20};
I.u[414] = {n:"Plume du Kwak de Glace",nn:"PLUME DU KWAK DE GLACE",t:53,d:"Cette plume a la particularité de résister au givre. C\'est pour cela qu\'on ne rencontre jamais de Kwak de Glace givré.",ep:1,g:112,l:24,wd:true,fm:true,w:1,p:20};
I.u[415] = {n:"Plume du Kwak de Flamme",nn:"PLUME DU KWAK DE FLAMME",t:53,d:"Cette plume a la particularité de résister aux flammes. C\'est pour cela qu\'on ne rencontre jamais de Kwak de Flamme enflammés.",ep:5,g:111,l:24,wd:true,fm:true,w:1,p:20};
I.u[416] = {n:"Plume du Kwak de Vent",nn:"PLUME DU KWAK DE VENT",t:53,d:"Cette plume a la particularité de résister aux bourrasques de vent. C\'est pour cela que le Kwak de Vent peut encaisser de multiples vents jour après jour sans jamais voir son moral ni son ardeur affaiblis.",ep:5,g:113,l:24,wd:true,fm:true,w:1,p:20};
I.u[417] = {n:"Boue du Boo",nn:"BOUE DU BOO",t:15,d:"La Boo est très agressive, elle est constituée de morceaux de boue mis bout à bout, le seul moyen d\'en venir à bout c\'est de la dessécher, mais elle est tellement humide que vous n\'êtes pas près d\'en voir le bout.",ep:2,g:116,l:17,wd:true,fm:true,w:1,p:1};
I.u[418] = {n:"Poils de Barbe du Grand Pa Wabbit",nn:"POILS DE BARBE DU GRAND PA WABBIT",t:54,d:"Le Grand Pa Wabbit possède une barbe postiche qui sent très mauvais, un peu comme le Père Nowel, mais en pire. A force de baver sans cesse devant les Grand Ma Wabbit, ses poils de barbe ont fini par s\'imprégner de sa bave aux pouvoirs méconnus.",ep:8,g:114,l:39,wd:true,fm:true,w:1,p:100};
I.u[419] = {n:"Oreille du Grand Pa Wabbit",nn:"OREILLE DU GRAND PA WABBIT",t:106,d:"Les oreilles du Grand Pa Wabbit sont pointues et rigides, mais sont recouvertes d\'une épaisse fourrure zébrée du plus bel effet. C\'est sans conteste une des fourrures les plus recherchées, ne serait-ce que parce qu\'elle ne sent absolument rien.",ep:8,g:115,l:39,wd:true,fm:true,w:1,p:1};
I.u[420] = {n:"Ficelle en Lin",nn:"FICELLE EN LIN",t:15,d:"La ficelle de lin est très solide, elle permet de faire de jolies coutures bien solides ou alors de ligoter ses amis pour leur montrer qui est réellement le chef de la guilde.",ep:1,g:118,l:1,wd:true,fm:true,w:1,p:10};
I.u[421] = {n:"Fleur de Lin",nn:"FLEUR DE LIN",t:35,d:"La Fleur de Lin peut être bleue ou blanche, suivant la période de l\'année à laquelle la graine a été semée. En décoction ou en infusion, elle procure une sensation de bien-être peu commune.",ep:1,g:117,l:1,wd:true,fm:true,w:1,p:0};
I.u[422] = {n:"Graine de Lin",nn:"GRAINE DE LIN",t:58,d:"Cette toute petite graine contient énormément de bonnes choses. Les chasseurs de Kwaks savent que ces derniers en sont friands et n\'hésitent pas à les attirer dans leurs pièges avec ces graines luisantes.",ep:1,g:119,l:1,wd:true,fm:true,w:2,p:1};
I.u[423] = {n:"Lin",nn:"LIN",t:34,d:"La tige de Lin n\'a rien de spécial, si ce n\'est qu\'elle est capable de se redresser 7 fois après avoir été pliée par les tempêtes. C\'est sûrement pour cela que sa fibre est si souple.",ep:1,g:122,l:40,wd:true,fm:true,w:2,p:0};
I.u[424] = {n:"Fibre de Lin",nn:"FIBRE DE LIN",t:55,d:"C\'est la fibre idéale pour confectionner les plus belles étoffes, elle est rare et chère mais tellement supérieure aux fibres classiques qu\'aucun tisserand ne pourrait s\'en passer.",ep:13,g:120,l:46,wd:true,fm:true,w:1,p:1};
I.u[425] = {n:"Chanvre",nn:"CHANVRE",t:34,d:"La Tige de Chanvre peut être utilisée lors de la recherche d\'effets hallucinogènes, mais également par les tisserands qui exploitent leurs fibres pour des raisons toutes autres.",ep:1,g:121,l:70,wd:true,fm:true,w:2,p:0};
I.u[426] = {n:"Fibre de Chanvre",nn:"FIBRE DE CHANVRE",t:55,d:"Cette fibre n\'est pas très belle, mais elle est particulièrement résistante. De plus, dans des moments d\'introspection sur soi-même, il est toujours possible de la fumer.",ep:13,g:123,l:46,wd:true,fm:true,w:1,p:1};
I.u[427] = {n:"Graine de Chanvre",nn:"GRAINE DE CHANVRE",t:58,d:"La Graine de Chanvre est très utilisée pour calmer les troupeaux de Tabis en rut, il suffit de leur en administrer quelques-unes et ces derniers deviennent soudainement moins farouches.",ep:1,g:157,l:10,wd:true,fm:true,w:1,p:10};
I.u[428] = {n:"Fleur de Chanvre",nn:"FLEUR DE CHANVRE",t:35,d:"Cette fleur permet de faire des potions aux effets aléatoires, qui peuvent aller de la paralysie totale à l\'invulnérabilité aux gaz de Wabbits.",ep:1,g:156,l:10,wd:true,fm:true,w:1,p:0};
I.u[429] = {n:"Pointe de Flèche du Bwork Archer",nn:"POINTE DE FLECHE DU BWORK ARCHER",t:15,d:"Cette pointe de flèche rouillée est faite à partir d\'un métal rare, qu\'on ne trouve que sur ces pointes de flèche.",ep:4,g:125,l:24,wd:true,fm:true,w:1,p:1};
I.u[430] = {n:"Os Invisible du Chafer Invisible",nn:"OS INVISIBLE DU CHAFER INVISIBLE",t:47,d:"Cet os est invisible, grâce aux propriétés magiques qui lui ont été conférées. Il doit sûrement pouvoir être utilisé pour des potions magiques...",ep:1,g:126,l:23,wd:true,fm:true,w:1,p:20};
I.u[431] = {n:"Pierre du Craqueleur",nn:"PIERRE DU CRAQUELEUR",t:51,d:"Cette pierre est presque vivante, elle tire sa vie des pierres précieuses qu\'elle renferme. Elle est quasiment incassable, sauf par un Craqueleur. Il n\'y a pas mieux pour se faire un bon gros marteau en pierre.",ep:2,g:127,l:31,wd:true,fm:true,w:5,p:1};
I.u[432] = {n:"Côtes du Rib",nn:"COTES DU RIB",t:47,d:"Les côtes du Rib sont vraiment très bonnes une fois grillées, il n\'y a pas beaucoup à manger autour, mais c\'est tellement fin, que ça se mange sans fin.",ep:3,g:158,l:33,wd:true,fm:true,w:1,p:100};
I.u[433] = {n:"Fémur du Chafer Archer",nn:"FEMUR DU CHAFER ARCHER",t:47,d:"Voilà un os bien étrange, il est particulièrement friable, et dégage une odeur de curry éventé lorsqu\'il est brûlé, voilà qui pourrait bien arranger certains cuisiniers peu scrupuleux.",ep:1,g:101,l:34,wd:true,fm:true,w:1,p:100};
I.u[434] = {n:"Ecorce d\'Abraknyde",nn:"ECORCE D\'ABRAKNYDE",t:96,d:"Ce morceau d\'écorce est rugueux, crevassé, recouvert de petites aspérités et sent étrangement bon, un peu comme la peau des pieds d\'un Enutrof.",ep:1,g:103,l:32,wd:true,fm:true,w:1,p:1};
I.u[435] = {n:"Racine d\'Abraknyde",nn:"RACINE D\'ABRAKNYDE",t:98,d:"Ce morceau de racine a connu divers substrats à force d\'accompagner l\'Abraknyde lors de promenades dans les sous-bois. Mais ça n\'explique pas pourquoi ce morceau de racine est magique.",ep:1,g:102,l:32,wd:true,fm:true,w:1,p:1};
I.u[436] = {n:"Bâton en Racine d\'Abraknyde",nn:"BATON EN RACINE D\'ABRAKNYDE",t:4,d:"Ce bâton taillé dans une grosse racine d\'Abraknyde est très efficace au corps à corps, attention à ne pas le laisser au contact de l\'eau, il pourrait reprendre racines et se retourner contre son maître.",ep:1,g:1,l:30,wd:true,fm:true,w:1,an:12,e:[5,2,1,1,20,50,false,false],c:"CI>30&CC>5",p:2650};
I.u[437] = {n:"Bourgeon d\'Abraknyde",nn:"BOURGEON D\'ABRAKNYDE",t:108,d:"Lors de la puberté, les Abraknydes se recouvrent de vilains bourgeons gorgés de sève juteuse riche en vitamines. Plus tard, ces bourgeons donnent naissance à de nouvelles pousses qui elles-mêmes donneront naissance à des bourgeons.",ep:1,g:104,l:32,wd:true,fm:true,w:1,p:1};
I.u[438] = {n:"Queue du Mulou",nn:"QUEUE DU MULOU",t:65,d:"La queue du Mulou ressemble à s\'y méprendre à une énorme bûche velue, et pourtant, il n\'en est rien, ne vous méprenez pas.",ep:1,g:105,l:70,wd:true,fm:true,w:1,p:50};
I.u[439] = {n:"Canine du Mulou",nn:"CANINE DU MULOU",t:47,d:"Cette canine tranchante est surtout recherchée en tant que porte bonheur, pourtant, elle n\'apporte à son porteur que des mauvaises odeurs.",ep:1,g:106,l:70,wd:true,fm:true,w:1,p:1};
I.u[440] = {n:"Griffe du Mulou",nn:"GRIFFE DU MULOU",t:15,d:"Cette griffe bien sale permet de faire des armes redoutables.",ep:1,g:107,l:70,wd:true,fm:true,w:1,p:1};
I.u[441] = {n:"Cuivre",nn:"CUIVRE",t:39,d:"Le Cuivre est un métal très utile pour confectionner les armes bon marché, c\'est presque un ersatz de l\'or si on fait abstraction de toutes les qualités de l\'or.",ep:1,g:108,l:10,wd:true,fm:true,w:5,p:0};
I.u[442] = {n:"Bronze",nn:"BRONZE",t:39,d:"Ce métal entre souvent dans la fabrication d\'articles ornementaux, pour embellir vos armes à moindre frais, c\'est le métal idéal.",ep:1,g:109,l:20,wd:true,fm:true,w:5,p:0};
I.u[443] = {n:"Kobalte",nn:"KOBALTE",t:39,d:"Le nom « Kobalte » signifie « cuivre du diable » en Sadida ancien, c\'est un métal adapté à la forge d\'armes légères et élégantes, mais avant tout mortelles.",ep:1,g:77,l:30,wd:true,fm:true,w:5,p:0};
I.u[444] = {n:"Etain",nn:"ETAIN",t:39,d:"L\'Etain est un métal mou et lourd, un peu comme le Wo Wabbit, mais en pire.",ep:1,g:78,l:50,wd:true,fm:true,w:5,p:0};
I.u[445] = {n:"Manganèse",nn:"MANGANESE",t:39,d:"Contrairement à ce que tout le monde pense, ce métal ne sert pas à faire des gouttières, mais plutôt à faire des armes terrifiantes, comme le redoutable " + "\"" + "Tranchant Manganésien" + "\"" + " qui a estourbi plus d\'un Tofu lors des fêtes du Boulgour.",ep:1,g:397,l:40,wd:true,fm:true,w:5,p:0};
I.u[446] = {n:"Bauxite",nn:"BAUXITE",t:39,d:"Ce métal est très apprécié pour sa couleur argentée et pour son tranchant lorsqu\'il est correctement aiguisé.",ep:1,g:76,l:70,wd:true,fm:true,w:5,p:0};
I.u[447] = {n:"Charbon",nn:"CHARBON",t:39,d:"Le Charbon ne dispose d\'aucune propriété magique particulière, contrairement à ce que l\'on pourrait croire. Cependant, il a une fâcheuse tendance à attirer la foudre, à la contenir, et à la libérer au mauvais moment.",ep:5,g:75,l:1,wd:true,fm:true,w:5,p:0};
I.u[448] = {n:"Silex",nn:"SILEX",t:51,d:"Ce caillou est particulièrement tranchant, il a égorgé plus d\'une taupe imprudente.",ep:1,g:74,l:22,wd:true,fm:true,w:5,p:1};
I.u[449] = {n:"Bois d\'Ebène",nn:"BOIS D\'EBENE",t:38,d:"Ce bois tient son nom d\'un malentendu, il y a bien longtemps, la seule famille de Bûcherons qui savait où se trouvaient ces arbres, s\'appelait Baine, et partout, on parlait du " + "\"" + "Bois des Baine" + "\"" + ", à force d\'écrire n\'importe comment voici ce qui arrive.",ep:7,g:73,l:70,wd:true,fm:true,w:5,p:0};
I.u[450] = {n:"Pierre de Granit",nn:"PIERRE DE GRANIT",t:51,d:"Ce minéral est presque inusable, mais aussi très lourd, ce qui limite son utilisation dans la forge d\'armes. En le travaillant longtemps, on peut parfois en extraire des pierres précieuses.",ep:1,g:1,l:30,wd:true,fm:true,w:20,p:1};
I.u[454] = {n:"Hache de Bûcheron",nn:"HACHE DE BUCHERON",t:19,d:"Cette hache est indispensable au bûcheron s\'il veut correctement exercer son métier. Le cas échéant, elle peut également lui permettre de se débarrasser de quelques monstres...",ep:1,g:1,l:1,wd:true,fm:true,w:10,an:17,e:[5,4,1,1,40,40,false,false],p:150};
I.u[455] = {n:"Marteau de l\'Aurore",nn:"MARTEAU DE L\'AURORE",t:7,d:"Ce ridicule petit marteau servait à Zokrim le paysan, qui s\'en servait pour planter ses piquets de clôtures à l\'aurore. Personne ne sait pourquoi Zokrim plantait ses piquets à l\'aurore, mais la légende raconte qu\'il avait de bonnes raisons.",ep:1,g:2,l:26,wd:true,fm:true,w:20,an:15,e:[5,4,1,1,50,30,false,false],c:"CS>10&CI>16",p:2000};
I.u[456] = {n:"Tueuse de Chênes",nn:"TUEUSE DE CHENES",t:19,d:"Cette hache permet de couper facilement les arbres, même les plus gros. Elle est surtout appréciée pour son poids important qui lui permet de couper un Bwork en trois facilement, pour peu que son porteur soit capable de la soulever.",ep:7,g:2,l:33,wd:true,fm:true,w:50,an:17,e:[5,4,1,1,40,40,false,false],c:"CS>66&CA>10",p:3150};
I.u[458] = {n:"Amulette du Chafer",nn:"AMULETTE DU CHAFER",t:1,d:"Le Chafer est quelqu\'un de sensible et distingué, il garde toujours avec lui une amulette faite des crânes de ses cousins tombés au combat. Le plus étrange, c\'est que le Chafer n\'a jamais eu de cousins.",ep:1,g:3,l:19,wd:true,fm:true,w:4,p:1200};
I.u[459] = {n:"Planche en Frêne",nn:"PLANCHE EN FRENE",t:95,d:"Cette planche en Frêne permet aux bricoleurs de construire des trucs en bois, comme un cercueil en planches par exemple.",ep:1,g:93,l:1,wd:true,fm:true,w:10,p:10};
I.u[460] = {n:"Bois de Chêne",nn:"BOIS DE CHENE",t:38,d:"Ce bois d\'excellente qualité est particulièrement résistant, il permet de faire presque tout, des gourdins, des maisons ou encore des bateaux, mais la meilleure façon d\'utiliser ce bois reste d\'en faire des fûts à bière (Hips !).",ep:1,g:92,l:30,wd:true,fm:true,w:5,p:0};
I.u[461] = {n:"Bois d\'If",nn:"BOIS D\'IF",t:38,d:"Ce bois particulièrement souple ne casse jamais, ce qui en fait un bois de choix pour la réalisation d\'arcs. Il est en revanche difficile à trouver, les paysans d\'Amakna ayant tendance à tuer les jeunes pousses d\'If qui rendent malades leurs Bouftous.",ep:1,g:94,l:50,wd:true,fm:true,w:5,p:0};
I.u[462] = {n:"Arc en Racine d\'Abraknyde",nn:"ARC EN RACINE D\'ABRAKNYDE",t:2,d:"Le bois d\'Abraknyde étant toujours vivant une fois coupé, cet arc est très difficile à utiliser. Cependant, il n\'y a pas mieux pour ridiculiser un Abraknyde, que de le tuer avec un arc issu des racines de ses propres frères.",ep:1,g:1,l:28,wd:true,fm:true,w:10,an:10,tw:true,e:[10,4,2,7,30,50,false,true],c:"CS>10&CA>28",p:2250};
I.u[463] = {n:"Ambre",nn:"AMBRE",t:50,d:"Cette magnifique pierre précieuse est constituée de résine d\'arbre fossilisée. On y trouve parfois à l\'intérieur, un petit Moskito pris au piège il y a des lustres, ce qui ne manque pas de rajouter de la valeur à cette pierre.",ep:1,g:96,l:32,wd:true,fm:true,w:4,p:20};
I.u[464] = {n:"Ambre d\'Abraknyde",nn:"AMBRE D\'ABRAKNYDE",t:50,d:"Cette magnifique pierre précieuse est constituée de résine d\'Abraknyde fossilisée. On y trouve parfois à l\'intérieur, une Arakne prise au piège il y a des lustres dans la sève de son Maître. Voilà une mort bien méritée pour ce genre de Monstre.",ep:1,g:97,l:32,wd:true,fm:true,w:5,p:40};
I.u[465] = {n:"Cristal",nn:"CRISTAL",t:50,d:"Le Cristal est une pierre translucide, mais très cassante. Et oui, même chez les pierres précieuses, rien n\'est parfait.",ep:1,g:98,l:50,wd:true,fm:true,w:20,p:5000};
I.u[466] = {n:"Saphir",nn:"SAPHIR",t:50,d:"Cette pierre précieuse toute bleue est bien adaptée à la fabrication d\'armes aux effets magiques, peut-être parce que les créateurs de jeu en mal d\'inspiration utilisent toujours le bleu pour représenter le mana.",ep:1,g:99,l:50,wd:true,fm:true,w:20,p:5000};
I.u[467] = {n:"Rubis",nn:"RUBIS",t:50,d:"Cette pierre précieuse aux couleurs rougeoyantes comme les plumes du Kwak de Flammes est froide au toucher comme les plumes du Kwak de Glace.",ep:1,g:100,l:50,wd:true,fm:true,w:20,p:5000};
I.u[468] = {n:"Pain d\'Amakna",nn:"PAIN D\'AMAKNA",t:33,d:"Ce petit pain tout chaud à la mie bien molle et à la croûte dorée permet de regagner des points de vie perdus au combat. C\'est le pain le plus populaire, car généralement le moins cher.",ep:1,g:13,l:1,wd:true,fm:true,w:1,u:true,ut:true,p:1};
I.u[469] = {n:"Baguette en Racine d\'Abraknyde",nn:"BAGUETTE EN RACINE D\'ABRAKNYDE",t:3,d:"Cette baguette utilise les propriétés magiques de la Racine d\'Abraknyde qui est encore vivante, méfiez-vous des effets néfastes de cette baguette, il lui arrive parfois de se retourner contre son maître.",ep:1,g:3,l:33,wd:true,fm:true,w:20,an:11,e:[5,4,2,2,50,40,false,true],c:"CI>33&CW>10",p:3100};
I.u[470] = {n:"Bois d\'Orme",nn:"BOIS D\'ORME",t:38,d:"Le Bois d\'Orme est un des bois les plus recherchés, courant autrefois, il a été décimé il y a des lustres par la Grafioze, une sorte d\'envoûtement terrible lancé par un Puissant Bwork Mage des Cavernes de la pire espèce.",ep:3,g:677,l:90,wd:true,fm:true,w:5,p:0};
I.u[471] = {n:"Bois d\'Erable",nn:"BOIS D\'ERABLE",t:38,d:"Les feuilles d\'Erable ont permis à des gens peu inspirés de se faire un drapeau original, le sirop d\'Erable a permis de donner du goût aux crêpes. Quant à son bois, on cherche encore à quoi il peut bien servir.",ep:1,g:153,l:40,wd:true,fm:true,w:5,p:0};
I.u[472] = {n:"Bois de Charme",nn:"BOIS DE CHARME",t:38,d:"Le Bois de Charme doit son nom aux Sadidas, qui s\'accouplent toujours sous les Charmes, pour des raisons qui ne sont révélées que dans le livre du Bwork Mage.",ep:1,g:138,l:80,wd:true,fm:true,w:5,p:0};
I.u[473] = {n:"Bois de Châtaignier",nn:"BOIS DE CHATAIGNIER",t:38,d:"L\'odeur du Bois de Châtaignier repousse les mouches, sauf celles qui aiment les châtaignes et celles qui ont perdu l\'odorat, ce qui explique pourquoi l\'odeur du bois de Châtaignier ne repousse en fait aucune mouche.",ep:1,g:86,l:10,wd:true,fm:true,w:5,p:0};
I.u[474] = {n:"Bois de Merisier",nn:"BOIS DE MERISIER",t:38,d:"Ce bois est recherché en ébénisterie du fait de sa texture compacte et de sa couleur brun rougeâtre qu\'il acquiert avec le temps.",ep:1,g:139,l:60,wd:true,fm:true,w:5,p:0};
I.u[476] = {n:"Bois de Noyer",nn:"BOIS DE NOYER",t:38,d:"Le Bois de Noyer est très utilisé pour confectionner des meubles. Pour des raisons obscures, certains charpentiers se refusent à l\'utiliser pour leurs bateaux.",ep:1,g:95,l:20,wd:true,fm:true,w:5,p:0};
I.u[477] = {n:"Sang de Dragon Cochon",nn:"SANG DE DRAGON COCHON",t:15,d:"Cette fiole est remplie d\'un sang qui conférait au Dragon Cochon des pouvoirs diaboliques. A consommer avec modération, ou a faire cuire si vous aimez le boudin noir de Dragon Cochon.",ep:2,g:544,l:70,wd:true,fm:true,w:10,p:100};
I.u[478] = {n:"Elagueuse de Charme",nn:"ELAGUEUSE DE CHARME",t:19,d:"Cette hache doit son nom à Woodee la Bûcheronne, qui savait user de ses charmes pour détourner l\'attention des autres Bûcherons et ainsi garder secret l\'emplacement des rares Charmes dont elle voulait préserver l\'existence.",ep:7,g:3,l:47,wd:true,fm:true,w:20,an:17,e:[5,4,1,1,40,40,false,false],c:"CW>20&CI>20&CC>20",p:5900};
I.u[479] = {n:"Ivoire",nn:"IVOIRE",t:47,d:"L\'Ivoire est très recherché par les collectionneurs, pour sa blancheur légendaire, et par les Forgerons, pour sa capacité à rendre les armes mortelles. Ce qui est certain, c\'est que dans la région d\'Amakna, l\'Ivoire a vraiment la côte.",ep:1,g:88,l:40,wd:true,fm:true,w:10,p:100};
I.u[480] = {n:"Minerai Etrange",nn:"MINERAI ETRANGE",t:39,d:"Ce Minerai a été extrait d\'une mystérieuse carrière inconnue. Contrairement à tous les objets étranges d\'Amakna, celui-ci n\'est pas magique du tout.",ep:1,g:90,l:24,wd:true,fm:true,w:5,p:100};
I.u[481] = {n:"Crâne du Dragon Cochon",nn:"CRANE DU DRAGON COCHON",t:47,d:"Malgrès sa faible contenance, ce crâne est une vraie pièce de collection, sa relative ressemblance avec un crâne de Iop, a permis à de nombreux étudiants en médecine d\'expliquer l\'origine des faiblesses intellectuelles de certains Iop.",ep:2,g:91,l:70,wd:true,fm:true,w:50,p:100};
I.u[482] = {n:"Pelle Teuze",nn:"PELLE TEUZE",t:8,d:"Après une soirée un peu trop arrosée à la taverne qui a débouché sur une bagarre, un Enutrof s\'est aperçu que cette pelle conçue à l\'origine pour les grands travaux de construction était une excellente arme.",ep:1,g:8,l:27,wd:true,fm:true,w:40,an:16,e:[5,4,1,1,40,40,false,false],c:"CS>7&CC>20",p:2200};
I.u[483] = {n:"Grande Pelle Teuze",nn:"GRANDE PELLE TEUZE",t:8,d:"Après une soirée un peu trop arrosée à la taverne qui a débouché sur une bagarre, un Enutrof s\'est aperçu que cette pelle conçue à l\'origine pour les grands travaux de construction était une excellente arme.",ep:1,g:8,l:29,wd:true,fm:true,w:40,an:16,e:[5,4,1,1,40,40,false,false],c:"CS>9&CC>20",p:2500};
I.u[484] = {n:"Puissante Pelle Teuze",nn:"PUISSANTE PELLE TEUZE",t:8,d:"Après une soirée un peu trop arrosée à la taverne qui a débouché sur une bagarre, un Enutrof s\'est aperçu que cette pelle conçue à l\'origine pour les grands travaux de construction était une excellente arme.",ep:1,g:8,l:31,wd:true,fm:true,w:40,an:16,e:[5,4,1,1,40,40,false,false],c:"CS>11&CC>20",p:2800};
I.u[485] = {n:"Slip en cuir du Chafer Lancier",nn:"SLIP EN CUIR DU CHAFER LANCIER",t:56,d:"Lorsque vient l\'hiver, le Chafer Lancier ne sort jamais sans son slip en cuir, qui lui permet de garder ses articulations bien au chaud ou de distraire ses adversaires le cas échéant.",ep:1,g:152,l:44,wd:true,fm:true,w:10,p:100};
I.u[486] = {n:"Cuir de Sanglier",nn:"CUIR DE SANGLIER",t:56,d:"Ce cuir est particulièrement résistant, mais sent le sanglier mâle en rût, ce qui explique pourquoi les paysans locaux s\'en servent pour piéger les femelles sangliers, qui sont trop stupides pour déjouer un stratagème si fourbe.",ep:1,g:16,l:14,wd:true,fm:true,w:10,p:1};
I.u[487] = {n:"Cuir du Dragon Cochon",nn:"CUIR DU DRAGON COCHON",t:56,d:"A force de se prendre pour un Dragon, le Dragon Cochon s\'est brûlé les poils de la couenne, ce qui explique en partie l\'odeur de cochon grillé que dégage ce cuir.",ep:2,g:142,l:70,wd:true,fm:true,w:10,p:6000};
I.u[489] = {n:"Loupe",nn:"LOUPE",t:20,d:"La loupe permet d\'examiner avec attention les éléments du décor pour y déceler des trésors.",ep:1,g:3,l:1,wd:true,fm:true,w:10,an:3,e:[1,6,1,1,100,10,false,false],p:10};
I.u[490] = {n:"Fiole",nn:"FIOLE",t:20,d:"Cette fiole, vitale à tout alchimiste qui se respecte, permet de réaliser les meilleures potions.",ep:1,g:2,l:1,wd:true,fm:true,w:3,an:4,e:[1,6,1,1,200,2,false,false],p:10};
I.u[491] = {n:"Sertisseur",nn:"SERTISSEUR",t:5,d:"Cet objet permet au Bijoutier de travailler ses joailleries.",ep:1,g:16,l:1,wd:true,fm:true,w:10,e:[5,4,1,1,50,50,false,false],p:150};
I.u[492] = {n:"Le Pétrisseur",nn:"LE PETRISSEUR",t:20,d:"Outil indispensable si vous voulez exercer le métier de boulanger-pâtissier.",ep:1,g:1,l:1,wd:true,fm:true,w:8,an:4,tw:true,e:[50,6,1,1,500,10,false,false],p:10};
I.u[493] = {n:"Marteau de Forgeur de Marteaux",nn:"MARTEAU DE FORGEUR DE MARTEAUX",t:7,d:"Outil indispensable à la création de marteaux.",ep:1,g:2,l:1,wd:true,fm:true,w:15,an:15,e:[5,4,1,1,50,50,false,false],p:150};
I.u[494] = {n:"Marteau de Forgeur d\'Epées",nn:"MARTEAU DE FORGEUR D\'EPEES",t:7,d:"Outil indispensable à la création d\'épées.",ep:1,g:2,l:1,wd:true,fm:true,w:15,an:15,e:[5,4,1,1,50,50,false,false],p:150};
I.u[495] = {n:"Marteau de Forgeur de Dagues",nn:"MARTEAU DE FORGEUR DE DAGUES",t:7,d:"Outil indispensable à la création de dagues.",ep:1,g:2,l:1,wd:true,fm:true,w:15,an:15,e:[5,4,1,1,50,50,false,false],p:150};
I.u[496] = {n:"Marteau de Forgeur de Pelles",nn:"MARTEAU DE FORGEUR DE PELLES",t:7,d:"Outil indispensable à la création de pelles.",ep:1,g:2,l:1,wd:true,fm:true,w:15,an:15,e:[5,4,1,1,50,50,false,false],p:150};
I.u[497] = {n:"Pioche du Mineur",nn:"PIOCHE DU MINEUR",t:21,d:"Outil indispensable à la collecte des minerais.",ep:1,g:1,l:1,wd:true,fm:true,w:20,an:17,e:[5,4,1,1,50,50,false,false],p:150};
I.u[498] = {n:"Tailladeuse de Bâton",nn:"TAILLADEUSE DE BATON",t:5,d:"Outil indispensable à la création des bâtons.",ep:1,g:17,l:1,wd:true,fm:true,w:10,e:[5,4,1,1,50,50,false,false],p:150};
I.u[499] = {n:"Tailladeuse de Baguette",nn:"TAILLADEUSE DE BAGUETTE",t:5,d:"Outil indispensable à la création des baguettes.",ep:1,g:17,l:1,wd:true,fm:true,w:10,e:[5,4,1,1,50,50,false,false],p:150};
I.u[500] = {n:"Tailladeuse d\'Arc",nn:"TAILLADEUSE D\'ARC",t:5,d:"Outil indispensable à la création des arcs.",ep:1,g:17,l:1,wd:true,fm:true,w:10,e:[5,4,1,1,50,50,false,false],p:150};
I.u[501] = {n:"Sève d\'Abraknyde Retravaillée",nn:"SEVE D\'ABRAKNYDE RETRAVAILLEE",t:15,d:"Cette sève sirupeuse a été retravaillée par un Alchimiste de renom, pour lui permettre de redonner vie aux Racines d\'Abraknydes. De toute évidence, ce genre de sève ne doit être manipulée que par des mains expertes.",ep:2,g:162,l:30,wd:true,fm:true,w:2,p:50};
I.u[502] = {n:"Trancheuse d\'If",nn:"TRANCHEUSE D\'IF",t:19,d:"Cette hache est une des seules à pouvoir couper des arbres envoûtés, on peut donc par extension, affirmer qu\'elle peut trancher tout ce qui est envoûté, vivant ou non.",ep:1,g:4,l:17,wd:true,fm:true,w:20,an:17,e:[10,4,1,1,40,50,false,false],c:"CS>10&CI>35",p:950};
I.u[515] = {n:"Boulgourde des Clairières",nn:"BOULGOURDE DES CLAIRIERES",t:19,d:"Cette hache a appartenu à Brouce Boulgoure le fameux Bûcheron, marié jadis avec Kassiopée Baine, fille d\'un autre grand Bûcheron. Cette arme lui permit de se défendre maintes fois lorsqu\'il partait la nuit couper des forêts entières.",ep:7,g:5,l:62,wd:true,fm:true,w:60,an:17,tw:true,e:[15,6,1,1,50,40,false,false],c:"CI>124&CC>30&CV>10",p:10100};
I.u[519] = {n:"Poudre de Perlinpainpain",nn:"POUDRE DE PERLINPAINPAIN",t:48,d:"Comme son nom l\'indique cette poudre est faite à partir de graines de Lin et de Pain. Mais ça n\'explique pas pourquoi cette poudre est magique.",ep:1,g:2,l:1,wd:true,fm:true,w:1,p:1};
I.u[520] = {n:"Pain aux Graines de Pavot",nn:"PAIN AUX GRAINES DE PAVOT",t:33,d:"La confection de ce pain est délicate, car les boulangers qui en respirent les fumées lors de la cuisson, sont victimes de symptômes étranges et se mettent à danser autour de leur boulangerie.",ep:1,g:10,l:20,wd:true,fm:true,w:1,u:true,ut:true,p:1};
I.u[521] = {n:"Pain aux Graines de Sésame",nn:"PAIN AUX GRAINES DE SESAME",t:33,d:"Les Kwaks les plus audacieux n\'hésitent pas à se déguiser en boulanger pour subtiliser les Graines de Sésame, mais cette supercherie ne fonctionne jamais sauf chez les Iop Boulangers.",ep:1,g:11,l:20,wd:true,fm:true,w:1,u:true,ut:true,p:1};
I.u[522] = {n:"Pain aux Graines de Lin",nn:"PAIN AUX GRAINES DE LIN",t:33,d:"Ce pain est riche en protéines, mais pauvre en goût. Et oui, la vie est toujours une question de choix.",ep:1,g:12,l:20,wd:true,fm:true,w:1,u:true,ut:true,p:1};
I.u[524] = {n:"Pain aux Noisettes",nn:"PAIN AUX NOISETTES",t:33,d:"Ce pain au goût subtil est garanti sans colorants, ni conservateurs, tout le contraire de la Gelée Royale.",ep:1,g:12,l:20,wd:true,fm:true,w:1,u:true,ut:true,p:1};
I.u[526] = {n:"Pain aux Céréales",nn:"PAIN AUX CEREALES",t:33,d:"Ce pain composé d\'une variété de céréales sans précédent, est suffisamment riche en protéines pour rendre la vie à un Chafer.",ep:1,g:6,l:60,wd:true,fm:true,w:1,u:true,ut:true,p:1};
I.u[527] = {n:"Pain aux Noix",nn:"PAIN AUX NOIX",t:33,d:"Ce pain est cuit avec du bois de Noyer et les noix sont grillées avant d\'être fourrées dans la mie.",ep:4,g:8,l:40,wd:true,fm:true,w:1,u:true,ut:true,p:1};
I.u[528] = {n:"Pain au Blé Complet",nn:"PAIN AU BLE COMPLET",t:33,d:"Ce pain est enrichi en blé complet, ce qui en fait un pain de qualité pour les gens exigeants.",ep:1,g:7,l:20,wd:true,fm:true,w:2,u:true,ut:true,p:1};
I.u[529] = {n:"Farine d\'Orge",nn:"FARINE D\'ORGE",t:52,d:"Cette farine s\'obtient au moulin, en écrasant des grains d\'orge, elle permet de donner une légèreté à la mie de pain qui n\'a pas d\'équivalent, à moins, bien entendu, de faire emploi d\'une magie occulte lors de la cuisson de vos pains.",ep:1,g:256,l:1,wd:true,fm:true,w:2,p:0};
I.u[530] = {n:"Farine de Seigle",nn:"FARINE DE SEIGLE",t:52,d:"Cette farine hérite de tous les bienfaits du Seigle. Selon la légende, le Seigle qui germe à l\'aurore, ne mûrit jamais. Par extension, certains paysans font leur enfants à l\'aurore en espérant qu\'ils ne vieillissent jamais.",ep:1,g:257,l:1,wd:true,fm:true,w:2,p:0};
I.u[531] = {n:"Farine d\'Avoine",nn:"FARINE D\'AVOINE",t:52,d:"Cette farine ne lève pas à la cuisson, ce qui permet de faire des pains vraiment denses, particulièrement utiles lorsqu\'il faut distribuer des pains aux monstres.",ep:1,g:250,l:1,wd:true,fm:true,w:2,p:0};
I.u[532] = {n:"Seigle",nn:"SEIGLE",t:34,d:"Cette céréale sombre a la faculté de pousser dans des conditions difficiles, c\'est une des rares céréales à subsister après de fortes sécheresses.",ep:1,g:123,l:50,wd:true,fm:true,w:2,p:0};
I.u[533] = {n:"Avoine",nn:"AVOINE",t:34,d:"Cette céréale est utilisée pour nourrir les jeunes Tabis, lorsqu\'ils n\'ont pas encore les dents assez solides pour ruminer. Les paysans font tout de même attention dans la mesure où l\'avoine cause parfois des crises d\'aérophagie chez le bétail sensible.",ep:1,g:154,l:20,wd:true,fm:true,w:2,p:0};
I.u[534] = {n:"Farine de Malt",nn:"FARINE DE MALT",t:52,d:"Cette farine blanche est obtenue en écrasant des grains d\'Orge germés, ce qui donne un goût parfois acidulé au pain, et dans le meilleur des cas, un goût de bière éventée, utile pour les sevrages progressifs des amateurs de bière. Une fois cuite, cette farine permet en effet de mieux supporter les cuites.",ep:1,g:255,l:1,wd:true,fm:true,w:2,p:0};
I.u[535] = {n:"Farine de Houblon",nn:"FARINE DE HOUBLON",t:52,d:"Cette farine permet de donner un délicieux goût de bière au pain, sans avoir à utiliser de la bière pour faire son pain. Il faut parfois reconnaître que la nature est bien faite.",ep:1,g:253,l:1,wd:true,fm:true,w:2,p:0};
I.u[536] = {n:"Pain aux Flocons d\'Avoine",nn:"PAIN AUX FLOCONS D\'AVOINE",t:33,d:"Ce pain à la mie particulièrement légère est parfait pour les personnes qui n\'ont pas été gâtées par Mère Nature, et qui n\'ont qu\'un tout petit estomac.",ep:1,g:5,l:10,wd:true,fm:true,w:1,u:true,ut:true,p:1};
I.u[537] = {n:"Pomme de Terre",nn:"POMME DE TERRE",t:68,d:"De manière générale, lorsque vous tombez sur cet objet, c\'est que vous êtes soit au milieu d\'un champ, soit au fond d\'un cachot.",ep:1,g:135,l:1,wd:true,fm:true,w:20,p:10};
I.u[538] = {n:"Pommes de Terre épluchées",nn:"POMMES DE TERRE EPLUCHEES",t:68,d:"Cette Pomme de terre est dépourvue de sa pelure, ce qui en fait une denrée commestible.",ep:1,g:136,l:1,wd:true,fm:true,w:20,p:10};
I.u[539] = {n:"Pain de Seigle",nn:"PAIN DE SEIGLE",t:33,d:"Ce pain est fait à partir de farine de Seigle, ce qui lui donne à la mie, une couleur brune particulière, mais surtout un léger goût sucré.",ep:1,g:9,l:20,wd:true,fm:true,w:1,u:true,ut:true,p:1};
I.u[543] = {n:"Pierre de Diamant",nn:"PIERRE DE DIAMANT",t:51,d:"Correctement polie, cette pierre peut donner un magnifique Diamant.",ep:1,g:144,l:30,wd:true,fm:true,w:20,p:15};
I.u[544] = {n:"Pierre d\'Emeraude",nn:"PIERRE D\'EMERAUDE",t:51,d:"Correctement polie, cette pierre peut donner une magnifique Emeraude.",ep:1,g:145,l:30,wd:true,fm:true,w:20,p:15};
I.u[545] = {n:"Pierre de Cristal",nn:"PIERRE DE CRISTAL",t:51,d:"Correctement polie, cette pierre peut donner un magnifique Cristal.",ep:1,g:141,l:30,wd:true,fm:true,w:20,p:15};
I.u[546] = {n:"Pierre de Saphir",nn:"PIERRE DE SAPHIR",t:51,d:"Correctement polie, cette pierre peut donner un magnifique Saphir",ep:1,g:151,l:30,wd:true,fm:true,w:20,p:15};
I.u[547] = {n:"Pierre de Rubis",nn:"PIERRE DE RUBIS",t:51,d:"Correctement polie, cette pierre peut donner un magnifique Rubis.",ep:1,g:150,l:30,wd:true,fm:true,w:20,p:15};
I.u[548] = {n:"Potion de Rappel",nn:"POTION DE RAPPEL",t:12,d:"Cette potion permet de revenir au dernier point de sauvegarde.",ep:1,g:17,l:1,wd:true,fm:true,w:1,u:true,p:10};
I.u[577] = {n:"Faux du Paysan",nn:"FAUX DU PAYSAN",t:22,d:"Cette faux n\'est pas très aiguisée et surtout un peu rouillée, ce qui en fait une arme peu dangereuse. En revanche, pour faucher, c\'est l\'outil idéal.",ep:1,g:1,l:1,wd:true,fm:true,w:10,an:17,e:[5,4,1,1,50,50,false,false],p:150};
I.u[578] = {n:"Pierre à Aiguiser",nn:"PIERRE A AIGUISER",t:51,d:"Je ne vous jette pas la pierre Pierre !",ep:2,g:127,l:1,wd:true,fm:true,w:20,p:1};
I.u[579] = {n:"Coupe Cuir",nn:"COUPE CUIR",t:5,d:"Outil indispensable à la création de chaussures et de ceintures.",ep:1,g:18,l:1,wd:true,fm:true,w:10,e:[5,4,1,1,50,50,false,false],p:150};
I.u[580] = {n:"Potion Ghetto Raide",nn:"POTION GHETTO RAIDE",t:12,d:"Cette potion permet de retrouver quelques points d\'énergie.",ep:1,g:18,l:10,wd:true,fm:true,w:1,u:true,p:20};
I.u[582] = {n:"Farine Blanche",nn:"FARINE BLANCHE",t:52,d:"Cette farine nécessite plusieurs céréales pour être obtenue, elle est cependant très appréciée pour sa qualité panifiable. Et pourtant au premier regard, rien ne la distingue d\'une farine classique.",ep:1,g:7,l:1,wd:true,fm:true,w:2,p:0};
I.u[583] = {n:"Farine Bise",nn:"FARINE BISE",t:52,d:"Cette farine mi-blanche tire son nom de son inventeur, une jolie paysanne qui aimait faire des bises, les joues pleines de farine. Quelle coquine.",ep:1,g:7,l:20,wd:true,fm:true,w:2,p:0};
I.u[586] = {n:"Farine Paysanne",nn:"FARINE PAYSANNE",t:52,d:"C\'est en affrontant la femme de Brouce Boulgoure à coups de sacs de farine, que Shika Ingalsse a eu l\'idée de créer cette farine devenue populaire. Les combats de femmes apportent parfois beaucoup plus que de la distraction pour la plèbe.",ep:1,g:258,l:30,wd:true,fm:true,w:2,p:0};
I.u[587] = {n:"Farine Complète",nn:"FARINE COMPLETE",t:52,d:"Cette farine est issue d\'une multitude de céréales, ce qui en fait une farine particulièrement complète, très utile pour les pains complets.",ep:1,g:259,l:40,wd:true,fm:true,w:2,p:0};
I.u[592] = {n:"Corde d\'Ivan Nowé",nn:"CORDE D\'IVAN NOWE",t:2,d:"Cet arc a appartenu à Ivan Nowé, un Crâ téméraire, qui a pourfendu plus d\'un Tofu égaré. Cet arc a la particularité de faire des coups pour le moins surprenants qui laissent perplexes puis agonisants les ennemis.",ep:7,g:1,l:36,wd:true,fm:true,w:10,an:10,tw:true,e:[10,4,2,7,15,50,false,true],c:"CS>30&CA>36",p:3600};
I.u[593] = {n:"Orchidée Freyesque",nn:"ORCHIDEE FREYESQUE",t:35,d:"Cette fleur est d\'une rareté et d\'une beauté inimaginable, mais elle est presque introuvable. En effet, les Crapauds ont tendance à les ramener dans leur campement pour nourrir leur progéniture.",ep:1,g:192,l:40,wd:true,fm:true,w:1,p:0};
I.u[594] = {n:"Edelweiss",nn:"EDELWEISS",t:35,d:"Cette fleur est d\'une rareté et d\'une beauté inimaginable, mais elle est presque introuvable. Pour cause, sa magie est si puissante qu\'elle serait capable de faire revivre certains morts.",ep:1,g:191,l:50,wd:true,fm:true,w:1,p:0};
I.u[596] = {n:"Canne à Pêche Courte",nn:"CANNE A PECHE COURTE",t:20,d:"Cette petite canne à pêche est idéale pour s\'amuser à pêcher les petits poissons dans les mares d\'eau.",ep:1,g:8,l:1,wd:true,fm:true,w:1,an:18,e:[1,4,1,1,100,100,false,false],p:200};
I.u[597] = {n:"Filet simple de capture",nn:"FILET SIMPLE DE CAPTURE",t:99,d:"Ce filet très résistant permet de capturer la plus sauvage des montures, à condition d\'avoir réussi à l\'hypnotiser ou à la distraire un peu avant.",ep:20,g:1,l:1,wd:true,fm:true,w:1,an:0,p:10};
I.u[598] = {n:"Greu-Vette",nn:"GREU-VETTE",t:41,d:"Les Greu-Vettes sont appréciées pour leur couleur rose et leur goût raffiné. Elles sont cependant détestées pour leur mauvais caractère légendaire et leurs antennes gominées.",ep:1,g:292,l:1,wd:true,fm:true,w:1,p:10};
I.u[600] = {n:"Kralamoure",nn:"KRALAMOURE",t:41,d:"Le Kralamoure est un grand romantique, ses yeux de petit Tofu lui donnent un air un peu niais, mais ne vous y méprenez pas, le Kralamoure est très agressif envers les étrangers.",ep:2,g:309,l:20,wd:true,fm:true,w:1,p:10};
I.u[602] = {n:"Requin Marteau-Faucille",nn:"REQUIN MARTEAU-FAUCILLE",t:41,d:"Ce Requin rouge est un requin militant, d\'instinct grégaire, l\'individu n\'existe pas au sein du groupe, même si leur chef, le Requin Stakanoviou est porté au rang d\'idole.",ep:2,g:322,l:80,wd:true,fm:true,w:2,p:1};
I.u[603] = {n:"Poisson Chaton",nn:"POISSON CHATON",t:41,d:"Ce poisson a la particularité d\'avoir des moustaches et de ronronner sous l\'eau, ce qui ne manque pas de faire peur aux Poissons Volants.",ep:1,g:277,l:1,wd:true,fm:true,w:1,p:50};
I.u[607] = {n:"Truite Vaseuse",nn:"TRUITE VASEUSE",t:41,d:"La Truite Vaseuse est une cousine éloignée du Saumoon agressif de l\'île de Moon. Cette truite est aussi bonne cuite que crue, qui l\'eut cru ?",ep:1,g:395,l:1,wd:true,fm:true,w:1,p:50};
I.u[610] = {n:"Amulette en Crabe",nn:"AMULETTE EN CRABE",t:1,d:"Cette amulette blanchâtre est confectionnée avec du Crabe, ce qui en fait une amulette très fragile mais originale.",ep:1,g:30,l:24,wd:true,fm:true,w:2,p:1800};
I.u[616] = {n:"Amulette du Marin",nn:"AMULETTE DU MARIN",t:1,d:"Cette amulette, sertie d\'écailles de poisson, est censée porter chance à son propriétaire lorsqu\'il part en mer les jours de tempête.",ep:1,g:29,l:22,wd:true,fm:true,w:2,p:2000};
I.u[617] = {n:"Amulette du Pirate",nn:"AMULETTE DU PIRATE",t:1,d:"Cette amulette terrifiante a impressionné plus d\'un marin, la porter sur soi suffit à faire de vous une personne redoutée.",ep:1,g:20,l:41,wd:true,fm:true,w:2,p:4500};
I.u[619] = {n:"Bâton du Maître des Bouftous",nn:"BATON DU MAITRE DES BOUFTOUS",t:4,d:"Ce bâton permettait à Farle Ingalsse, éleveur de Bouftous, d\'invoquer plus facilement des Tofus gardiens de troupeaux. Au fil du temps, ce bâton s\'est révélé être une excellente arme de combat.",ep:7,g:6,l:85,wd:true,fm:true,w:20,tw:true,e:[10,3,1,1,30,100,false,false],c:"CI>60&CW>20&CA>40",p:10000};
I.u[620] = {n:"Découpeuse de Kralamoure",nn:"DECOUPEUSE DE KRALAMOURE",t:6,d:"Cette épée a permis au célèbre marin Mobby Diqueriveurse, de découper en tranches, les tentacules du célèbre Kralamoure Géant de l\'île de Moon, réputé pour étouffer les marins saouls et saouler les marins étouffés.",ep:1,g:8,l:61,wd:true,fm:true,w:50,an:14,e:[10,7,1,1,50,50,false,false],c:"CV>60&CW>60",p:9900};
I.u[629] = {n:"Chapeau Pointu",nn:"CHAPEAU POINTU",t:16,d:"Ce magnifique chapeau permettra à votre héros d\'être la plus belle pour aller danser au bal masqué (hohé hohé).",ep:5,g:1,l:21,wd:true,fm:true,w:5,p:1400};
I.u[638] = {n:"Amulette du Pêcheur de Somoon",nn:"AMULETTE DU PECHEUR DE SOMOON",t:1,d:"Pêcher un Somoon relève de l\'exploit, mais avec cette amulette sur soi, plus rien n\'est impossible.",ep:12,g:24,l:35,wd:true,fm:true,w:5,p:2500};
I.u[639] = {n:"Amulette des Abîmes",nn:"AMULETTE DES ABIMES",t:1,d:"Cette amulette a été retrouvée dans l\'épave échouée du célèbre pêcheur de Kralamoure, Moby Diqueriveurse, qui était également populaire pour ses boutades vaseuses au bar des pirates.",ep:14,g:22,l:47,wd:true,fm:true,w:5,p:6000};
I.u[640] = {n:"Arc des Rivages",nn:"ARC DES RIVAGES",t:2,d:"Cet arc permettait aux pêcheurs de nettoyer les rivages de toute la vermine environnante sans avoir à se lever de leur siège, et cela depuis la fenêtre du bar des pirates.",ep:14,g:38,l:46,wd:true,fm:true,w:10,an:10,tw:true,e:[10,4,2,10,30,45,false,true],c:"CA>46",p:5600};
I.u[641] = {n:"Coton Ancestral",nn:"COTON ANCESTRAL",t:15,d:"Ce Coton est doux comme le pelage d\'une jeune Ecaflipette, blanc comme un nuage de printemps, mais fragile comme une plume de Tofu.",ep:1,g:657,l:6,wd:true,fm:true,w:1,p:500};
I.u[642] = {n:"Cocon de Ver à Soie",nn:"COCON DE VER A SOIE",t:15,d:"Cet énorme cocon blanchâtre, peut être déroulé pour obtenir un magnifique fil de soie. \nL\'aventurier Harry Hane l\'aurait même utilisé pour pouvoir sortir du labyrinthe du Dark Vlad.",ep:1,g:489,l:6,wd:true,fm:true,w:1,p:100};
I.u[643] = {n:"Fil de Soie",nn:"FIL DE SOIE",t:15,d:"Ce fil est d\'une finesse et d\'une souplesse sans équivalent. C\'est de loin la matière préférée du Chafer dès qu\'il s\'agit de s\'équiper en sous-vêtements de combat.",ep:1,g:486,l:5,wd:true,fm:true,w:1,p:100};
I.u[646] = {n:"Poils de Black Tiwabbit",nn:"POILS DE BLACK TIWABBIT",t:54,d:"Ce poil tout noir à l\'avantage ne de pas être salissant, ce qui ne manquera pas d\'intéresser les héros peu portés sur l\'hygiène et la coquetterie. Attention cependant, ce poil a tendance à déteindre au contact d\'une sueur abondante et corrosive.",ep:8,g:195,l:11,wd:true,fm:true,w:1,p:50};
I.u[648] = {n:"Poils de Wabbit",nn:"POILS DE WABBIT",t:54,d:"Ce poil n\'est pas très prisé par les Tailleurs expérimentés qui lui préfèrent le poil des autres Wabbits, mais il a le mérite de faire fuir les Moskitos à cause de son odeur de Citronnelle Sauvage.",ep:8,g:196,l:18,wd:true,fm:true,w:1,p:1};
I.u[649] = {n:"Poils de Wo Wabbit",nn:"POILS DE WO WABBIT",t:54,d:"Ce poil est si gras, qu\'il permet de garder les étoffes imperméables aux intempéries.",ep:8,g:197,l:39,wd:true,fm:true,w:1,p:40};
I.u[650] = {n:"Poils de Grand Pa Wabbit",nn:"POILS DE GRAND PA WABBIT",t:54,d:"Ce poil malodorant est délicat à utiliser en couture à cause de sa tendance à friser au contact du jus de Cawotte.",ep:8,g:198,l:39,wd:true,fm:true,w:1,p:50};
I.u[651] = {n:"Etoffe du Mulou",nn:"ETOFFE DU MULOU",t:55,d:"Cette étoffe en Poils de Mulou est très rare, mais sa constitution lui permet de résister à toutes formes d\'agressions physiques et parfois même magiques.",ep:1,g:199,l:70,wd:true,fm:true,w:1,p:100};
I.u[652] = {n:"Etoffe du Sanglier",nn:"ETOFFE DU SANGLIER",t:55,d:"Cette étoffe en Poils de Sanglier est rugueuse et malodorante, mais protège efficacement des attaques physiques.",ep:1,g:220,l:13,wd:true,fm:true,w:1,p:50};
I.u[653] = {n:"Etoffe du Prespic",nn:"ETOFFE DU PRESPIC",t:55,d:"Sous ses pics, le Prespic dispose d\'une fourrure rousse très douce, qui dégage une odeur désagréable lorsqu\'il pleut. Heureusement pour vous, il ne pleut jamais en Amakna, mais évitez d\'utiliser cette étoffe pour vous confectionner un caleçon de bain.",ep:1,g:661,l:17,wd:true,fm:true,w:1,p:100};
I.u[654] = {n:"Etoffe du Wabbit",nn:"ETOFFE DU WABBIT",t:55,d:"Cette étoffe en Poils de Barbe du Grand Pa Wabbit est très touffue, mais également irritante, un peu comme une grosse chenille urticante.",ep:8,g:15,l:18,wd:true,fm:true,w:1,p:100};
I.u[657] = {n:"Bâton du Maître des Tofus",nn:"BATON DU MAITRE DES TOFUS",t:4,d:"Ce bâton permettait à Shika Ingalsse, éleveuse de Tofus, d\'invoquer plus facilement des Tofus gardiens de troupeaux. Au fil du temps, ce bâton s\'est révélé être une excellente arme de combat.",ep:7,g:6,l:56,wd:true,fm:true,w:10,an:12,tw:true,e:[5,3,1,1,30,50,false,false],c:"CI>40&CW>10&CS>30",p:5600};
I.u[658] = {n:"Bâton du Maître des Tabis",nn:"BATON DU MAITRE DES TABIS",t:4,d:"Ce bâton permettait à Gramine Ingalsse, éleveuse de Tabis, d\'invoquer plus facilement des Tofus gardiens de troupeaux. Au fil du temps, ce bâton s\'est révélé être une excellente arme de combat.",ep:7,g:6,l:34,wd:true,fm:true,w:10,an:12,tw:true,e:[5,3,1,1,30,50,false,false],c:"CI>20&CW>5",p:3400};
I.u[673] = {n:"Mangeuse de Châtaignier",nn:"MANGEUSE DE CHATAIGNIER",t:19,d:"Cette hache n\'a rien de particulier si ce n\'est que la légèreté naturelle du Bois de Châtaignier, permet à cette hache d\'avoir un manche aussi léger qu\'une plume de jeune Tofu Maléfique.",ep:1,g:6,l:8,wd:true,fm:true,w:10,an:17,e:[5,4,1,1,45,45,false,false],c:"CS>8&CA>3",p:400};
I.u[674] = {n:"Fendeuse de Merisier",nn:"FENDEUSE DE MERISIER",t:19,d:"" + "\"" + "Cette hache a tout pour plaire" + "\"" + " disait le fameux slogan de Brouce Boulgoure le célèbre bûcheron. En effet, son manche en Bois de Merisier permet d\'amortir les vibrations lors de l\'abattage des arbres ou des bworks au crâne un peu trop solide.",ep:1,g:7,l:32,wd:true,fm:true,w:20,an:17,e:[5,4,1,1,45,45,false,false],c:"CV>40",p:2900};
I.u[675] = {n:"Trancheuse de Noyer",nn:"TRANCHEUSE DE NOYER",t:19,d:"Cette hache à l\'aspect rudimentaire, n\'en est pas moins efficace. C\'est avec cet outil que Brouce Boulgoure le Bûcheron a gagné le concours de tranchage de Bworks nains. Voilà qui en dit long sur l\'efficacité de cette hache.",ep:1,g:8,l:21,wd:true,fm:true,w:20,an:17,e:[5,4,1,1,45,45,false,false],c:"CV>30",p:1350};
I.u[676] = {n:"Mangeuse d\'Erable",nn:"MANGEUSE D\'ERABLE",t:19,d:"Cette hache a la particularité de ne jamais rouiller, pour éviter de désagréables surprises lorsque les Bûcherons gourmands décident de lécher le sirop d\'érable sur la lame après une éprouvante journée d\'abattage d\'érables.",ep:1,g:10,l:25,wd:true,fm:true,w:20,an:17,e:[5,4,1,1,45,45,false,false],c:"CA>30",p:1850};
I.u[677] = {n:"Cape du Pirate",nn:"CAPE DU PIRATE",t:17,d:"Cette cape a appartenu à un vieux pirate du nom de Zokrim. Il devait être plutôt malchanceux...",ep:1,g:11,l:1,wd:true,fm:true,w:10,p:1};
I.u[678] = {n:"Parchemin d\'Ivoire",nn:"PARCHEMIN D\'IVOIRE",t:13,d:"(Double clic pour gagner de l\'expérience) \nCe parchemin permet de gagner de l\'expérience sans prendre de risques.",ep:1,g:25,l:25,wd:true,fm:true,w:1,u:true,p:1};
I.u[679] = {n:"Parchemin Blanc",nn:"PARCHEMIN BLANC",t:13,d:"(Double clic pour gagner de l\'expérience) \nCe parchemin permet de gagner de l\'expérience sans prendre de risques.",ep:1,g:3,l:1,wd:true,fm:true,w:1,u:true,p:1};
I.u[680] = {n:"Parchemin Doré",nn:"PARCHEMIN DORE",t:13,d:"(Double clic pour gagner de l\'expérience) \nCe parchemin permet de gagner de l\'expérience sans prendre de risques.",ep:1,g:26,l:50,wd:true,fm:true,w:1,u:true,p:1};
I.u[683] = {n:"Petit Parchemin de Force",nn:"PETIT PARCHEMIN DE FORCE",t:76,d:"(Double clic pour gagner de la force) \nCe parchemin permet de gagner de la force de manière définitive sans prendre de risque. \nAttention cependant, vous ne pourrez l\'utiliser que si votre force est inférieure à 25.",ep:1,g:21,l:1,wd:true,fm:true,w:1,c:"Cs<25",u:true,p:1000};
I.u[684] = {n:"Parchemin de Sorts",nn:"PARCHEMIN DE SORTS",t:76,d:"(Double clic pour gagner des Points de Sorts) \nCe parchemin permet de gagner des Points de Sorts de manière définitive sans prendre de risques.",ep:1,g:666,l:1,wd:true,fm:true,w:1,u:true,p:15000};
I.u[686] = {n:"Petit Parchemin d\'Intelligence",nn:"PETIT PARCHEMIN D\'INTELLIGENCE",t:76,d:"(Double clic pour gagner des Points d\'Intelligence) \nCe parchemin permet de gagner des Points d\'Intelligence de manière définitive sans prendre de risques. \nAttention cependant, vous ne pourrez l\'utiliser que si votre intelligence est inférieure à 25.",ep:2,g:23,l:1,wd:true,fm:true,w:1,c:"Ci<25",u:true,p:1000};
I.u[690] = {n:"Farine de Lin",nn:"FARINE DE LIN",t:52,d:"Cette farine est très étrange, car elle est n\'est pas issue d\'une céréale, ce qui explique pourquoi elle ne ressemble à aucune autre farine.",ep:1,g:254,l:1,wd:true,fm:true,w:2,p:0};
I.u[692] = {n:"Pain Consistant",nn:"PAIN CONSISTANT",t:33,d:"Ce pain est fait à partir de Farine de Lin, ce qui explique pourquoi il est si gras, et pourquoi sa mie est brune. Très riche en huiles essentielles, il faut cependant avouer qu\'il a très mauvais goût.",ep:1,g:12,l:1,wd:true,fm:true,w:2,u:true,ut:true,p:1};
I.u[694] = {n:"Dofus Pourpre",nn:"DOFUS POURPRE",t:23,d:"C\'est le Dragon du Feu Ignemikhal qui créa le Dofus Pourpre, un Œuf dont le moins qu\'on puisse dire, c\'est qu\'il sent le soufre - mais il sent bien d\'autres choses encore...",ep:1,g:1,l:6,wd:true,fm:true,w:5,p:1};
I.u[695] = {n:"Parchemin de Bûcheron",nn:"PARCHEMIN DE BUCHERON",t:13,d:"(Double clic pour gagner des Points d\'expérience)",ep:1,g:27,l:1,wd:true,fm:true,w:1,u:true,p:1000};
I.u[696] = {n:"La Coiffe du Ploukosse",nn:"LA COIFFE DU PLOUKOSSE",t:16,d:"La Coiffe du Ploukosse, sous ses dehors " + "\"" + "has been" + "\"" + ", est très efficace lors des combats pour un simple chapeau de paille.",ep:1,g:2,l:20,wd:true,fm:true,w:4,p:1300};
I.u[697] = {n:"Makroute",nn:"MAKROUTE",t:16,d:"Makroute est un chien très affectueux... Le problème, c\'est que lorsqu\'il trouve un nouveau copain, il ne le lâche plus... Il a bien dû se faire 30 millions d\'amis de cette façon...",ep:2,g:3,l:6,wd:true,fm:true,w:6,p:30000};
I.u[698] = {n:"Choipo Podlard",nn:"CHOIPO PODLARD",t:16,d:"Le Choipo Podlard vient d\'une grande école de magie. Créature vivante, il a rencontré les plus grands magiciens de notre temps, tels que Mme Magoa AlaGale, Mr Doublegore et le célèbre Remy Krokeur.",ep:1,g:4,l:33,wd:true,fm:true,w:6,p:3000};
I.u[699] = {n:"Le Troma",nn:"LE TROMA",t:16,d:"Le Troma est aussi craignos qu\'un Rasboul en poil de Bitouf. (Pour ceux qui ne savent pas ce qu\'est un Rasboul en poil de bitouf, référez-vous aux grands livres des Craps à Poules).",ep:1,g:5,l:22,wd:true,fm:true,w:10,p:1525};
I.u[700] = {n:"Pandawa",nn:"PANDAWA",t:16,d:"Ah le Pandawa, vraiment quelle grosse tête il a. Comme son pote Makroute, il passe son temps sur la tête de son maître. Nul besoin de se déplacer, lorsqu\'il est motivé, il arrive même à subtiliser la nourriture de son porteur.",ep:2,g:6,l:6,wd:true,fm:true,w:10,c:"PG!10",p:12000};
I.u[701] = {n:"Le Oiram",nn:"LE OIRAM",t:16,d:"Le Oiram a fait de nombreux adeptes de part le monde, du fait de son côté très classieux... Ce chapeau a longtemps été porté par un plombier qui passait son temps à sauter et courir...",ep:1,g:7,l:22,wd:true,fm:true,w:12,c:"PG!10",p:1525};
I.u[702] = {n:"Kritter",nn:"KRITTER",t:16,d:"Ce crâne de démon reconstruit à partir d\'ossements divers et variés apporte grâce et élégance à son porteur... Vos amies vont vous jalouser...",ep:1,g:8,l:30,wd:true,fm:true,w:9,c:"PG!10",p:2600};
I.u[703] = {n:"Dora",nn:"DORA",t:16,d:"Ce chapeau, en forme de corbeille à fruits en osier renversée, est en fait une corbeille à fruits renversée magique. Ce qui ne manquera pas d\'impressionner votre entourage, surtout s\'il est composé d\'apprentis Bworks Mages.",ep:2,g:9,l:36,wd:true,fm:true,w:20,p:3550};
I.u[704] = {n:"Dantgoule",nn:"DANTGOULE",t:16,d:"Ce chapeau, confectionné à partir de Slips en cuir de Chafer Lancier tressés entre eux, vous rendra de toute évidence irrésistible auprès des femelles Bwork Mages, réputées pour leur féminité débordante, accentuée par leur barbe rousse.",ep:2,g:10,l:36,wd:true,fm:true,w:10,p:3550};
I.u[705] = {n:"Masque Tromatisant",nn:"MASQUE TROMATISANT",t:16,d:"Si vous voulez être traumatisé, il vous faut absolument cette magnifique noix de coco trouée. Nous rappelons à l\'utilisateur que l\'intérieur est équipé d\'une paille et de boissons rafraîchissantes.",ep:12,g:11,l:47,wd:true,fm:true,w:10,p:6000};
I.u[706] = {n:"Crystal O\'Boul",nn:"CRYSTAL O\'BOUL",t:16,d:"Hugne " + "\"" + "le sans mémoire" + "\"" + " possédait une magnifique boule de crystal que lui avait donné son pépé. Bien décidé à la conserver toute sa vie, il la plaça sur son chapeau, qu\'il ne quittait jamais... Il est mort de fatigue, en essayant de la retrouver.",ep:4,g:12,l:24,wd:true,fm:true,w:12,s:38,p:1775};
I.u[707] = {n:"Minotokorno",nn:"MINOTOKORNO",t:16,d:"Ce chapeau en Corne de Minotoror est très efficace pour impressionner ses adversaires en se faisant passer pour un Minotoror. Cette ruse ne semble fonctionner que sur les adversaires simplets. Un Iop ou un Tofu devrait faire l\'affaire.",ep:2,g:13,l:33,wd:true,fm:true,w:10,s:54,p:3000};
I.u[708] = {n:"Houde",nn:"HOUDE",t:16,d:"Ce chapeau en peau de larve verte magique donne à son porteur une allure guillerette, mais également un peu stupide. On ne peut pas tout avoir dans la vie, le porteur de ce chapeau en est d\'ailleurs un excellent exemple.",ep:1,g:14,l:30,wd:true,fm:true,w:8,p:2525};
I.u[709] = {n:"Lorko Kasko",nn:"LORKO KASKO",t:16,d:"Ce chapeau en écorce enchantée appartenait à Lorko, un Bûcheron réputé qui n\'a pas hésité à défier le célèbre Bûcheron Brouce Boulgoure, lors d\'un fameux duel de déracinage d\'Orme à mains nues dans la clairière de Liroye Merline.",ep:9,g:15,l:37,wd:true,fm:true,w:9,c:"PG!10",p:3825};
I.u[710] = {n:"Korko Klako",nn:"KORKO KLAKO",t:16,d:"Lo Korko Klako ost ouno chapo kilo bo. Tros ostotiko, tros bolo, il so porto par los plus bo dos Sadida.",ep:1,g:16,l:17,wd:true,fm:true,w:9,c:"PG=10",p:1000};
I.u[711] = {n:"Jouik Krampe",nn:"JOUIK KRAMPE",t:16,d:"Ce magnifique masque en cuir appartenait à Jouik Krampe, cousin de Zeurg. Tous deux étaient très portés sur les vêtements en cuir moulant. Un peu comme le Captain Chafer, la perversité en moins.",ep:8,g:17,l:26,wd:true,fm:true,w:15,c:"PG=10",p:2025};
I.u[712] = {n:"Gulliver",nn:"GULLIVER",t:16,d:"Ce chapeau confectionné à partir d\'oreilles cousues entre elles, vous rendra tout simplement unique tant il est original. On raconte qu\'il a jadis permis à Rakmar Bilgout de séduire la reine des Bouftous, pour ensuite l\'égorger plus facilement.",ep:13,g:18,l:33,wd:true,fm:true,w:10,p:3075};
I.u[713] = {n:"Parchemin de Forgeur d\'Epées",nn:"PARCHEMIN DE FORGEUR D\'EPEES",t:13,d:"(Double clic pour gagner des Points d\'expérience)",ep:1,g:28,l:1,wd:true,fm:true,w:1,u:true,p:1000};
I.u[714] = {n:"Parchemin de Forgeur de Dagues",nn:"PARCHEMIN DE FORGEUR DE DAGUES",t:13,d:"(Double clic pour gagner des Points d\'expérience)",ep:1,g:29,l:1,wd:true,fm:true,w:1,u:true,p:1000};
